    var saveAndClose = function(i) {
        var $othAppPopup = $('#' + applications[i].popupAttr.divId);
        var dontShowAgain = $othAppPopup.find('#' + applications[i].popupAttr.checkboxId).is(':checked');
        if (dontShowAgain) {
            applications[i].enabled = false;
            WorkloadDwrUtil.setShowPopupInfo(applications[i].app_id, false);
        }
        $othAppPopup.closeWin();
    };

    var stopPropagation = function(e) {
        if (!!e) {
            e.preventDefault();
            e.stopPropagation();
        }
    };

    var onClickOthAppLink = function(i, othAppLink) {
        return function(e) {
            if (applications[i].enabled) {
                var $othAppPopup = $('#' + applications[i].popupAttr.divId);
                $othAppPopup.find('#' + applications[i].popupAttr.buttonId)
                        .off('click')
                        .on('click', function(e) {
                            stopPropagation(e);
                            !!othAppLink && window.open(othAppLink, '_blank');
                            saveAndClose(i);
                        });
                $othAppPopup.createWin();
                stopPropagation(e);
            } else {
                !!othAppLink && window.open(othAppLink, '_blank');
                stopPropagation(e);
            }
        };
    };
    
    var isRenderPopupOnPage = function (i) {
        var popupPages = applications[i].popupPages;
        if (!!popupPages) {
            return popupPages.some(function (page) {
                return (page.indexOf("?") !== -1 && window.location.search.substring(1).indexOf(page.substring(page.indexOf('?') + 1)) !== -1)
                        || (window.location.pathname.indexOf(page) !== -1);
            });
        }
        return false;
    };
    
    var initPages = function(i) {
        isRenderPopupOnPage(i) && onClickOthAppLink(i)();
    };
    
    var initPopup = function(i) {
        $('.content-wrap:eq(0)').append($(applications[i].popup));
    };

    var addPopupEvent = function(i, $e, url) {
        if (!$e.data('othAppPopup')) {
            $e
                .data('othAppPopup', true)
                .off('click')
                .on('click', onClickOthAppLink(i, url));
        }
    };

    var findLinks = function(i) {
        var redirectPages = applications[i].redirectPages;
        if (!!redirectPages) {
            var result = [];
            redirectPages.forEach(function(link) {
                $('a[href*="' + link + '"]').each(function(index, element) {
                    result.push(element);
                });
            });
            return (result.length > 0) ? result : null;
        }
        return null;
    };
    
    var initLinks = function(i) {
        var linksArray = findLinks(i);
        if (!!linksArray) {
            linksArray.forEach(function(link) {
                addPopupEvent(i, $(link), link.href);
            });
        }
    };
    
    var findElements = function(i) {
        var redirectLink = applications[i].redirectLink;
        if (!!redirectLink) {
            var result = [];
            $('[' + redirectLink + ']').each(function(index, element) {
                result.push(element);
            });
            return (result.length > 0) ? result : null;
        }
        return null;
    };
    
    var initElements = function(i) {
        var elementsArray = findElements(i);
        if (elementsArray) {
            elementsArray.forEach(function(element) {
                var $e = $(element);
                addPopupEvent(i, $e, $e.attr(applications[i].redirectLink));
            });
        }
    };

    var initPopupEvents = function(i) {
        initLinks(i);
        initElements(i);
    };

    var initMutationObserver = function(i) {
        var initMutationObserver = function(i) {
            new MutationObserver(
                    (function(ix){return function() { initPopupEvents(ix); } })(i))
            .observe(document.body, {
                childList: true,
                subtree: true
            });
        };
    };

    $(function() {
        var applicationIds = [];
        
        applications.forEach(function(element, index) {
            if (isRenderPopupOnPage(index) || findLinks(index) || findElements(index)) {
                applicationIds.push(element.app_id);
            }
        });
        
        if (applicationIds.length > 0) {
            WorkloadDwrUtil.getShowPopupsInfo(applicationIds, function(data) {
                if (data) {
                    data.forEach(function(element) {
                        var index = applications.map(function(element){
                            return element.app_id;
                        }).indexOf(+element.id);

                        if (index >= 0) {
                            if (element.active) {
                                    initPopup(index);
                                    initPages(index);
                                    initPopupEvents(index);
                                    initMutationObserver(index);
                            } else {
                                applications[index].enabled = false;
                                initElements(index);
                            }
                        }
                    });
                }
            });
        }
    });
})(jQuery);


var STARTUP_PAGE = STARTUP_PAGE || (function ($) {
    $(function () {
        $('body').append(
            '<div id="setStartupPageConfirmation" class="modernPopup" style="display: none; width: 400px;">' +
                '<div class="modernPopup-title">' +
                       'Change Startup Page In UPSA' +
                '</div>' +
                '<div class="modernPopup-content modernPopup-content-padding">' +
                       '<p>Do you want to set this page  as your startup page in UPSA?</p>' +
                '</div>' +
                '<div class="modernPopup-footer buttons-wrap">' +
                    '<div id="cancelSetStartupPage" style="display: inline-block; float: right;">' +
                        '<input type="button" class="modernButton grey-button" value="Cancel"/>' +
                    '</div>' +
                    '<div id="applySetStartupPage" style="display: inline-block; float: right;">' +
                        '<input type="button" class="modernButton" value="Apply"/>' +
                    '</div>' +
                '</div>' +
            '</div>'
        );

        var confirmationPopup = $('#setStartupPageConfirmation');
        var pinLink = $("#startupPageLink .icon-thin-pin");

        var setStartupPagePinSelected = function (isSelected) {
            pinLink.toggleClass('selected', isSelected).attr('title', isSelected
                ? 'This is your UPSA startup page. To restore the default settings click this pin again.'
                : 'Pin this page to set it as your UPSA startup page.'
            );
        };

        $('#applySetStartupPage').on('click', function(){
            WorkloadDwrUtil.setUpsaStartupPage(window.location.href, function(data){
                setStartupPagePinSelected(true);
                confirmationPopup.closeWin();
            });
        });

        $('#cancelSetStartupPage').on('click', function(){
            confirmationPopup.closeWin();
        });

        pinLink.on("click", function () {
            WorkloadDwrUtil.isUpsaStartPage(window.location.href, function (data) {
                if (!data) {
                    confirmationPopup.createWin();
                } else {
                    WorkloadDwrUtil.setUpsaStartupPage(null, function(){
                        setStartupPagePinSelected(false);
                    });
                }

            });
        });

        var prevUrl = window.location.href;
        var timer = null;

        var chackStartPage = function(url) {
            WorkloadDwrUtil.isUpsaStartPage(url, function (data) {
                setStartupPagePinSelected(data);
                !timer && setUrlChangeTimer();
            });
        };

        var setUrlChangeTimer = function() {
            timer = setInterval(function() {
                var currUrl = window.location.href;
                if (currUrl !== prevUrl) {
                    prevUrl = currUrl;
                    clearInterval(timer);
                    timer = null;
                    chackStartPage(currUrl);
                }
            }, 1000);
        };

        chackStartPage(window.location.href);

    });
})(jQuery);
