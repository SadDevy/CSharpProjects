};





p.getEmployeeListByIds = function(p0, callback) {
return dwr.engine._execute(p._path, 'LookupDwrUtil', 'getEmployeeListByIds', arguments);
};





p.getActiveTopLocationsForCountries = function(p0, callback) {
return dwr.engine._execute(p._path, 'LookupDwrUtil', 'getActiveTopLocationsForCountries', arguments);
