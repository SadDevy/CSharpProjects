jQuery(document).ready(function () {
    var $summaryFilter = jQuery(".summaryFilter");
    var $filterHideControl = jQuery(".filter-hide-control-wrap");
    var $filterButtons = jQuery(".buttonWrapper");
    var heightContentFilter = jQuery(".summaryFilterWrap .summaryFilter").outerHeight() + jQuery(".summaryFilterWrap .buttonWrapper").outerHeight();
    if ($summaryFilter.length > 0 && $filterHideControl.length === 0 && !getLocalStorage(location.pathname) && !getLocalStorage('wasClickApply')) {
        $summaryFilter.hide();
        $filterButtons.hide();
        clearLocalStorage('wasClickApply');
        localStorage.setItem(location.pathname, 'true');
        localStorage.setItem(location.pathname + '_filter', 'hide');
    }
    else if (getLocalStorage('wasClickApply')) {
        localStorage.setItem(location.pathname + '_filter', 'visible');
    }
    else {
        localStorage.setItem(location.pathname + '_filter', 'hide');
    }

    if (jQuery(".filter-hide-control-wrap").length === 0) {
        if (localStorage.getItem(location.pathname + '_filter') === 'visible') {
            jQuery(".summaryFilterControls").append('<div class="filter-hide-control-wrap" title="Hide/Show Summary Filter">' +
                    '<i class="icon-angle-up filter-hide-control"></i></div>');
        }
        else {
            jQuery(".summaryFilterControls").append('<div class="filter-hide-control-wrap" title="Hide/Show Summary Filter">' +
                    '<i class="icon-angle-down filter-hide-control"></i></div>');
            $summaryFilter.hide();
            $filterButtons.hide();
        }
        var $control = jQuery(".filter-hide-control");
        if (!getLocalStorage('wasClickApply')) {
            $control.removeClass("icon-angle-up");
            $control.addClass("icon-angle-down");
            localStorage.setItem(location.pathname + '_filter', 'hide');
        }

        jQuery(".summaryFilterHeader").click(function (e) {
            var $target = jQuery(e.target);

            if ($target.closest('#settings-popup').length > 0
                    || $target.closest('#settings').length > 0
                    || $target.closest('#breakdownSelect').length > 0
                    || $target.closest('.period-block').length > 0
                    || e.target.tagName === 'INPUT'
                    || e.target.className === 'icon-calendar'
                    || $target.closest('div').attr("id") === "projectStartDateCalendar"
                    || $target.closest('div').attr("class") === "calendarLayout"
                    || $target.closest('table').attr("class") === "navigationLayout") {
                return;
            }

            if ($summaryFilter.is(":visible")) {
                $summaryFilter.slideUp(function () {
                    jQuery(window).trigger("resize");
                });
                if (typeof $assigneeTable !== "undefined") {
                    $filterButtons.slideUp();
                    $assigneeTable.animate({"top": "-=" + heightContentFilter + ""}, 400);
                    jQuery(window).scroll();
                }
                else {
                    $filterButtons.slideUp();
                }
                $control.removeClass("icon-angle-up");
                $control.addClass("icon-angle-down");
                localStorage.setItem(location.pathname + '_filter', 'hide');
            } else {
                $summaryFilter.slideDown(function () {
                    jQuery(window).trigger("resize");
                });

                if (typeof $assigneeTable !== "undefined") {
                    $filterButtons.slideDown();
                    $assigneeTable.animate({"top": "+=" + heightContentFilter + ""}, 400);
                    jQuery(window).scroll();
                }
                else {
                    $filterButtons.slideDown();
                }
                $control.removeClass("icon-angle-down");
                $control.addClass("icon-angle-up");
                localStorage.setItem(location.pathname + '_filter', 'visible');
            }
        });
        jQuery(window).trigger("resize");
    }
    if (localStorage.getItem('wasClickApply')) {
        clearLocalStorage('wasClickApply');
    }
});
var MENU = MENU || {};

MENU._menuTime = null;

MENU._clearTimeout = function () {
    if (MENU._menuTime) {
        clearTimeout(MENU._menuTime);
        MENU._menuTime = null;
    }
};

MENU._bufferSecondLevelMenuLeftPosition = {};

MENU._isClicked = false;


MENU.enableSecondMenu = function ($hoveredWrap, $hoveredItem, activeSecondLevelMenu) {
    MENU._showMenu($hoveredWrap, activeSecondLevelMenu);
};

MENU._showMenu = function ($hoveredWrap, activeSecondLevelMenu) {
    $hoveredWrap.toggleClass('selected-menu').show();
    if (!_.isUndefined(activeSecondLevelMenu)) {
        $hoveredWrap.find("a[href='" + activeSecondLevelMenu + "']").addClass('submenu-selected');
    }
};

MENU.behaveInit = function ($) {

    var menuItemState = 0;

    function mEnter(event) {
        var $hoveredItem = $(this),
            $hoveredWrap = $hoveredItem.find('> div.insideListWrap:first'),
            $pageActiveItem = $('ul#top-list-menu > .active');
        $pageActiveItem.find("> a.haveMenu").toggleClass("hovered-menu", false);
        $hoveredItem.parents('ul:first').find('li div.insideListWrap').hide();
        $hoveredItem.parents('ul:first').find('a.hovered-menu').toggleClass("hovered-menu");
        $hoveredItem.parents('ul:first').find('li div.selected-menu').toggleClass("selected-menu");
        $hoveredItem.find("> a.haveMenu").toggleClass("hovered-menu");
        MENU.enableSecondMenu($hoveredWrap, $hoveredItem);
        menuItemState = 1;
    }

    function mLeave() {
        $('.insideListWrap').hide();
        menuItemState = 0;
    }

    function __hideMainMenu() {
        setTimeout(function () {
            var $menuItem = jQuery('#mainMenu div.insideListWrap.selected-menu'),
                $pageActiveItem = jQuery('ul#top-list-menu > .active');
            if (menuItemState === 0) {
                $menuItem.parents("li").find("a.haveMenu").toggleClass("hovered-menu", false);
                $menuItem.hide().toggleClass("selected-menu");
                $pageActiveItem.find("> a.haveMenu").toggleClass("hovered-menu", true);
                $pageActiveItem.find("> div.insideListWrap:first").show();
            }
        }, 500);
    }

    MENU._hoverIntent = function(enable) {
        if (enable) {
            $('#mainMenu > ul > li.toplevel')
                    .off('mouseenter', mEnter)
                    .off('mouseleave', mLeave);

            $('#mainMenu > ul > li.toplevel').hoverIntent({
                over: mEnter,
                out: mLeave,
                timeout: 200
            });
        } else {
            $('#mainMenu > ul > li.toplevel')
                    .off('mouseenter')
                    .off('mouseleave')
                    .removeProp('hoverIntent_t')
                    .removeProp('hoverIntent_s')
                    .on('mouseenter', mEnter)
                    .on('mouseleave', mLeave);
        }
    };

    $("#topMenu").mouseleave(__hideMainMenu);

    MENU._hoverIntent(true);
};


(function ($) {
    $(document).ready(function () {
        $("#hiddenQuery").prop("autocomplete", "off");

        MENU.behaveInit($);

    });


})(jQuery);

MENU.aimInit = function ($) {
    var $menu = $("#top-list-menu");

    $menu.menuAim({
        activate: activateAim,
        deactivate: deactivateAim,
        rowSelector: "> li",
        submenuDirection: "below"
    });

    function activateAim(cell) {
        var $cell = $(cell),
            submenuId = $cell.data("submenuId"),
            $submenu = $("#" + submenuId);
// Show the submenu
        $submenu.css({
            display: "block"
        });
// Keep the currently activated cell's highlighted look
        $cell.find("a").addClass("maintainHover");
    }

    function deactivateAim(cell) {
        var $cell = $(cell),
            submenuId = $cell.data("submenuId"),
            $submenu = $("#" + submenuId);
// Hide the submenu and remove the cell's highlighted look
        $submenu.css("display", "none");
        $cell.find("a").removeClass("maintainHover");
    }
};/**
 * Regular expression for to parsing date.
 * Delimiter symbols are '-', '/' and ' '(space).
 * Correct formats with '-': d-d-dd, dd-d-dd, d-dd-dd, dd-dd-dd,
 *                  d-d-dddd, dd-d-dddd, d-dd-dddd, dd-dd-dddd,
 *                  d-mmm-dd, dd-mmm-dd, d-mmm-dddd, dd-mmm-dddd
 * */
var dateRegExpr = /(\d{1,2})([-|\/|\s]*)(\d{1,2}|[A-Za-z]{3})([-|\/|\s]*)(\d{4}|\d{2})\s*$/;
var dateRegExprApp = /\d{2}-(Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec)-\d{4}$/;

/**
 * The regular expression for to parsing string on special symbols
 * */
var REG_SPECIAL_SYMBOLS = /[\.\/\\\@\#\$\%\^\&\*\(\)\-\+\'\"\]\[\}\{\>\<\`\~\=\_\|\,]+/;

/**
 * It checks string on presents of special symbols.
 *
 * @param value The variable is string for the check.
 *
 * @return true  - if special symbol is found in the string
 *         false - if special symbol isn't found in the string.
 *
 * @author Andrei_Khadziukou
 */
function checkOnSpecialSymbol(value){
    return REG_SPECIAL_SYMBOLS.test(value);
}

var arrayMonth = [];
//Array for checking month
arrayMonth["01"] = arrayMonth["JAN"] = "Jan";// january  - 0
arrayMonth["02"] = arrayMonth["FEB"] = "Feb";// february - 1
arrayMonth["03"] = arrayMonth["MAR"] = "Mar";// march    - 2
arrayMonth["04"] = arrayMonth["APR"] = "Apr";// april    - 3
arrayMonth["05"] = arrayMonth["MAY"] = "May";// may      - 4
arrayMonth["06"] = arrayMonth["JUN"] = "Jun";// june     - 5
arrayMonth["07"] = arrayMonth["JUL"] = "Jul";// july     - 6
arrayMonth["08"] = arrayMonth["AUG"] = "Aug";// august   - 7
arrayMonth["09"] = arrayMonth["SEP"] = "Sep";// september- 8
arrayMonth["10"] = arrayMonth["OCT"] = "Oct";// october  - 9
arrayMonth["11"] = arrayMonth["NOV"] = "Nov";// november - 10
arrayMonth["12"] = arrayMonth["DEC"] = "Dec";// december - 11

/**
 * Return true if leap year.
 */
function isLeapYear(year) {
    return (year % 4 !== 0 ? false :
            (year % 100 !== 0 ? true :
                    (year % 1000 !== 0 ? false : true)));
}

/**
 * Check the format of the date and change date to the format, which using in app.
 * obj - the field for check.
 * Return
 *      true  - if the format of the date is correct or empty.
 *      false - if the format of the date isn't correct.
 * */
function correctedFormatDate(obj){
	if (obj.value == '') {
		return true;
	}
    var isError = false;
    /*
     var date = toDateDD_MMM_YYYY(obj.value);
     if (dateRegExprApp.test(obj.value) && date != null && !isNaN(date)){
     return true;
     } */
    var arr = dateRegExpr.exec(obj.value);
    var day = null;
    var month = null;
    var year = null;
    if (arr != null && (arr[1] + arr[2] + arr[3] + arr[4] + arr[5]) == trim(obj.value)){
        isError = true;
        //parsing year
        switch (arr[5].length) {
        case 4:{
            year = arr[5];
            break;
        }
        case 2:{
            var curentYear = (new Date()).getFullYear();
            var centery = Math.floor(curentYear/100);
            var years   = curentYear%100;
            var parsYear = parseInt(arr[5], 10);
            prevYear = (centery - 1)*100 + parsYear;
            nextYear = centery*100 + parsYear;
            year = prevYear+"";
            if (nextYear - curentYear < 30 || parsYear < years){
                year = nextYear+"";
            }
            break;
        }
        default:
        	return false;
        }
        //parsing day
        day = arr[1];
        if(day == undefined || day < 1 || day > 31){
        	return false;
        }
        if (day.length === 1){
            day = "0" + day;
        }
        //parsing month
        var days = [0,31,isLeapYear(year)?29:28,31,30,31,30,31,31,30,31,30,31];
        switch (arr[3].length) {
            case 1:
                if (days[arr[3]] >= day) {
                    month = arrayMonth[("0" + arr[3]).toUpperCase()];
                }
                ;
                break;
            case 2:
                if (arr[3].charAt(0) !== "0") {
                    if (days[arr[3]] >= day) {
                        month = arrayMonth[arr[3].toUpperCase()];
                    }
                } else if (days[arr[3].charAt(1)] >= day) {
                    month = arrayMonth[arr[3].toUpperCase()];
                }
                ;
                break;
            case 3:
                var count = 0;
                for (var el in arrayMonth) {
                    if (el.length === 3) {
                        count++;
                        if (arrayMonth[el].toUpperCase() === arr[3].toUpperCase()) {
                            if (days[count] >= day) {
                                month = arrayMonth[arr[3].toUpperCase()];
                            }
                            break;
                        }
                    }
                }
                break;
        }

        if (month == undefined || month == null) {
            isError = false;
        }
        if (isError) {
            obj.value = day + "-" + month + "-" + year;
        }
    }
    return isError;
}

/**
 * Return true if form is valid; false otherwise.
 */
function isValid(form, errorsArray) {
    var result = [];
    if(errorsArray){
        result = errorsArray;
    }
    var fields = eval('struct_' + form.name);
    var resultIndex = 0;
    for (var i = 0; i < fields.length; i++) {
        var field = form[fields[i]['fieldName']];
        if (!field) {
            continue;
        }

        for (var j = 0; j < fields[i].actions.length; j++) {
            var action = fields[i].actions[j];
            var actionFunction = eval(action);
            var isValid = actionFunction(form, field, fields[i].param);
            if (!isValid) {
                addError(result, fields[i]['fieldName'], fields[i].messages[j]);
                resultIndex++;
                break;
            }
        }
    }
    if (result.length > 0) {
        processErrors(form, result);
    }
    return (result.length === 0);
}

/**
 * Add error to errors array and return it.
 */
function addError(errors, fieldName, message) {
    errors[errors.length] = {
        fieldName:fieldName,
        message:message
    };
    return errors;
}

/**
 * Process error messages after client or server validation.
 */
function processErrors(form, errors) {
    var message = '';
    for (var i = 0; i < errors.length; i++) {
        message += errors[i].message + '\n';
    }
//    UPSA.panelButtons.trigger("reenable");
    alert(message);
}

function validateConversion(field, param) {
    return true;
}

/**
 * Return true if field is not blank; false otherwise.
 */
function validateRequired(form, field, param) {
    var value = '';
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea' || field.type === 'file' ||
         field.type === 'checkbox' || field.type === 'select-one' || field.type === 'password') && (!field.disabled)) {
        if (field.type === "select-one") {
            var si = field.selectedIndex;
            if (si >= 0) {
                value = field.options[si].value;
            }
        } else if (field.type === 'checkbox') {
            if (field.checked) {
                value = field.value;
            }
        } else {
            value = field.value;
        }

        if (trim(value).length === 0) {
            return false;
        }
    } else if (field.type === "select-multiple") {
        var numOptions = field.options.length;
        var lastSelected = -1;
        for (var loop = numOptions - 1; loop >= 0; loop--) {
            if (field.options[loop].selected) {
                lastSelected = loop;
                value = field.options[loop].value;
                break;
            }
        }
        if (lastSelected < 0 || trim(value).length === 0) {
            return false;
        }
    } else if ((field.length > 0) && (field[0].type === 'radio' || field[0].type === 'checkbox')) {
        var isChecked = false;
        for (var loop = 0; loop < field.length; loop++) {
            if (field[loop].checked) {
                isChecked = true;
                break;
            }
        }
        if (isChecked < 0) {
            return false;
        }
    }
    return true;
}

function validateNotZero(form, field) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea' || field.type === 'file' || field.type === 'password') 
            && (!field.disabled)) {
       return trim(field.value) !== "0";
    }
    return true;
}

/**
 * Return true if field is a valid integer number of any size; false otherwise.
 */
function validateInteger(form, field, param) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea' || field.type === 'select-one' ||
         field.type === 'radio') && (!field.disabled)) {
        var value = '';
        if (field.type === 'select-one') {
            var si = field.selectedIndex;
            if (si >= 0) {
                value = field.options[si].value;
            }
        } else {
            value = field.value;
        }
        if (value.length > 0) {
            if (isNaN(value)) {
                return false;
            }
            validChars = '0123456789';
            for (var i = 0; i < value.length; i++) {
                if (validChars.indexOf(value.charAt(i)) < 0) {
                    return false;
                }
            }
        }
    }
    return true;
}

/**
 * Return true if field is a valid float number of any size; false otherwise.
 */
function validateDouble(form, field, param) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea' || field.type === 'select-one' ||
         field.type === 'radio') && (!field.disabled)) {
        var value = '';
        if (field.type === 'select-one') {
            var si = field.selectedIndex;
            if (si >= 0) {
                value = field.options[si].value;
            }
        } else {
            value = field.value;
        }
        if (value.length > 0) {
            if (isNaN(value)) {
                return false;
            }
            validChars = '0123456789.';
            for (var i = 0; i < value.length; i++) {
                if (validChars.indexOf(value.charAt(i)) < 0) {
                    return false;
                }
            }
        }
    }
    return true;
}

/**
 * Return true if field is in range min through max; false otherwise.
 */
function validateIntRange(form, field, param) {
    if (!field.disabled) {
        var value = '';
        if (field.type === 'hidden' || field.type === 'text' || field.type === 'textarea' || field.type === 'radio') {
            value = field.value;
        } else if (field.type === 'select-one') {
            var si = field.selectedIndex;
            if (si >= 0) {
                value = field.options[si].value;
            }
        }
        if (value.length > 0) {
            var iMin = parseInt(param['min']);
            var iMax = parseInt(param['max']);
            var iValue = parseInt(value);
            if (!(iValue >= iMin && iValue <= iMax)) {
                return false;
            }
        }
    }
    return true;
}

/**
 * Return true if field is a valid year in a range minYear through current year + maxYearIncrement; false otherwise.
 */
function validateYear(form, field, param) {
    if (!field.disabled) {
        var value = '';
        if (field.type === 'hidden' || field.type === 'text' || field.type === 'textarea') {
            value = field.value;
        } else if (field.type === 'select-one') {
            var si = field.selectedIndex;
            if (si >= 0) {
                value = field.options[si].value;
            }
        }
        if (value.length > 0) {
            var minYear = parseInt(param['minYear']);
            var maxYearIncrement = 0;
            if (param['maxYearIncrement']) {
                maxYearIncrement = parseInt(param['maxYearIncrement']);
            }
            var iValue = parseInt(value);
            var today = new Date();
            if ((iValue < minYear) || (iValue > today.getFullYear() + maxYearIncrement)) {
                return false;
            }
        }
    }
    return true;
}

/**
 * Return true if field matches regexp mask; false otherwise.
 */
function validateMask(form, field, param) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea' || field.type === 'file') &&
        (field.value.length > 0) && (!field.disabled)) {
            var mask = param['mask'];//if used format, which was definition in local code
            if (mask == ""){
                mask = dateRegExprApp;//used default format.
            }
            if (!field.value.match(mask)) {
                return false;
        }
    }
    return true;
}

/**
 * Return true if monthYear field has valid month format; false otherwise.
 */
function validateMonthYearMonth(form, field, param) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea') &&
        (field.value.length > 0) && (!field.disabled)) {
        var value = field.value;
        if (isNaN(toDateMMM_YYYY(value))) {
            return false;
        }
    }
    return true;
}

/**
 * Return true if monthYear field has valid date; false otherwise.
 */
function validateMonthYearDate(form, field, param) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea') &&
        (field.value.length > 0) && (!field.disabled)) {
        var value = field.value;
        var date = toDateMMM_YYYY(value);
        var parsedYear = date.getFullYear();
        var parsedMonth = date.getMonth();
        date = new Date();
        var todayYear = date.getFullYear();
        var todayMonth = date.getMonth();
        if (todayYear < parsedYear) {
            return false;
        } else if (todayYear == parsedYear) {
            if (todayMonth < parsedMonth) {
                return false;
            }
        }
        value = param['minMonthYear'];
        date = toDateMMM_YYYY(value);
        var minYear = date.getFullYear();
        var minMonth = date.getMonth();
        if (minYear > parsedYear) {
            return false;
        } else if (minYear == parsedYear) {
            if (minMonth > parsedMonth) {
                return false;
            }
        }
    }
    return true;
}

/**
 * Return true if start date is less than end date; false otherwise.
 */
function validateMonthYearRange(form, field, param) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea') &&
        (field.value.length > 0) && (!field.disabled)) {
        var value = field.value;
        var dateEnd = toDateMMM_YYYY(value);
        var startField = param['startDate'];
        value = form[startField].value;
        var dateStart = toDateMMM_YYYY(value);
        if (dateStart.getTime() > dateEnd.getTime()) {
            return false;
        }
    }
    return true;
}

function validateYearMonthDay(form, field, param) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea') &&
        (field.value.length > 0) && (!field.disabled)) {
        if (!field.value.match(param['mask'])) {
            return false;
        }
        var value = field.value;
        var date = toDateYYYY_MM_DD(value);
        if (date == null) {
            return false;
        }
        var day = getDayFromYYYY_MM_DD(value);
        if (date.getDate() != day) {
            return false;
        }
    }
    return true;
}

function validateMMMYYYY(field) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea') &&
        (field.value.length > 0) && (!field.disabled)) {

        var value = field.value;
        var date = toDateMMM_YYYY(value);
        if (date == null) {
            return false;
        }
    }
    return true;
}


function validateDayMonthYear(form, field, param) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea') &&
        (field.value.length > 0) && (!field.disabled)) {
        if (!field.value.match(param['mask'])) {
            return false;
        }
        var value = field.value;
        var date = toDateDD_MMM_YYYY(value);
        if (date == null || isNaN(date)) {
            return false;
        }else{
            if(date.getDate()!=getDayFromDD_MM_YYYY(value)){
                return false;
            }
        }
    }
    return true;
}

/**
 * Return true if start date is less than end date; false otherwise.
 */
function validateYearMonthDayGreaterThan(form, field, param) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea') &&
        (field.value.length > 0) && (!field.disabled)) {
        var value = field.value;
        var endDate = toDateYYYY_MM_DD(value);
        if (endDate == null) {
            return true;
        }
        var startField = param['startDate'];
        value = form[startField].value;
        var startDate = toDateYYYY_MM_DD(value);
        if (startDate == null) {
            return true;
        }
        if (startDate.getTime() > endDate.getTime()) {
            return false;
        }
    }
    return true;
}

/**
 * Return true if start date is less than end date; false otherwise. For dates in dd-Mon-yyyy format.
 */
function validateDayMonthYearGreaterThan(form, field, param) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea') &&
        (field.value.length > 0) && (!field.disabled)) {
        var value = field.value;
        var endDate = toDateDD_MMM_YYYY(value);
        if (endDate == null) {
            return true;
        }
        var startField = param['startDate'];
        value = form[startField].value;
        var startDate = toDateDD_MMM_YYYY(value);
        if (startDate == null) {
            return true;
        }
        if (startDate.getTime() > endDate.getTime()) {
            return false;
        }
    }
    return true;
}

/**
 * It check start date is less end date.
 *
 * @param startDateField The field contains start date.
 * @param endDateField The field contains end date.
 *
 * @return true If start date is less or equal end date.
 *         false If start date isn't less end date.
 * */
function checkStartDateLessEndDate(startDateField, endDateField){
    var startDate = toDateDD_MMM_YYYY(startDateField.value);
    var endDate = toDateDD_MMM_YYYY(endDateField.value);
    if (startDate.getTime() > endDate.getTime()) {
        return false;
    }
    return true;
}

function checkDateInPast(dateField) {
    var date = sliceToDay(toDateDD_MMM_YYYY(dateField.value));
    var today = sliceToDay(new Date());
    return date < today;
}

/**
 * Return true if date is greater or equal than today date; false otherwise.
 */
function validateDayGreaterOrEqualToday(form, field, param) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea') &&
        (field.value.length > 0) && (!field.disabled)) {
        var value = field.value;
        var date = toDateYYYY_MM_DD(value);
        /*
         * values of data variable is 'null' if date don't conform format 'YYYY_MM_DD'
         * */
        if (date == null){
            date = toDateDD_MMM_YYYY(value);
        }
        var todayDate = new Date();
        todayDate.setHours(0);
        todayDate.setMinutes(0);
        todayDate.setSeconds(0);
        todayDate.setMilliseconds(0);
        if (date.getTime() < todayDate.getTime()) {
            return false;
        }
    }
    return true;
}

/**
 * Trim spaces.
 */
function trim(s) {
    return s.replace(/^\s*/, '').replace(/\s*$/, '');
}

function sliceToDay(date) {
    date.setHours(0);
    date.setMinutes(0);
    date.setSeconds(0);
    date.setMilliseconds(0);
    return date;
}

/**
 * Parse date of MMM-YYYY format.
 */
function toDateMMM_YYYY(value) {
    var separatorPos = value.indexOf('-');
    value = value.replace('-00', '-10');
    value = value.substring(0, separatorPos) + ' 1, ' + value.substring(separatorPos + 1);
    return new Date(value);
}

/**
 * Parse date of DD_MMM_YYYY format.
 */
function toDateDD_MMM_YYYY(value) {
    var el = value.split("-");
    if(el[0] == undefined || el[0]<0 || el[0]>31){
        return null;
    }
    var r = el[1] + " " + el[0] + ", " + el[2];
    return new Date(r);
}

/**
 * Parse date of YYYY_MM_DD format.
 */
function toDateYYYY_MM_DD(value) {
    var el = value.split("-");
    if(el[1] == undefined || el[1]<0 || el[1]>12){
        return null;
    }
    if(el[2] == undefined || el[2]<0 || el[2]>31){
        return null;
    }
    var v = el[0] + "/" + el[1] + "/" + el[2];
    return new Date(v);
}

function getDayFromYYYY_MM_DD(value) {
    var el = value.split("-");
    if (typeof el[2] === 'undefined') {
        return -1;
    }
    return el[2];
}

function getDayFromDD_MM_YYYY(value){
        var el = value.split("-");
    if (typeof el[0] === 'undefined') {
        return -1;
    }
    return el[0];
}

function validateRequiredAny(form, field, param) {
    var valid = true;

    valid = validateRequired(form, field);
    if (!valid) {
        field = form[param.secondProperty];
        valid = validateRequired(form, field);
    }
    return valid;
}
function validateMaxLength(form, field, param) {
    if ((field.type === 'hidden' || field.type === 'text' || field.type === 'textarea') && (!field.disabled)) {
        var maxLength = parseInt(param['maxlength']);
        if (field.value.length > maxLength) {
            return false;
        }
    }
    return true;
}

/*
 Validate date object.
 This function require global variables (messageRequired, messageYearMonthDayDate, messageYearMonthDayFormat, messageFormatDateInvalide) (see layout.jsp).
 dateObject - field, which content the date for validation.
 form - form, which content field
 param - array of params( "name" - name of field, which was display
                          "mask" - the format of the for validation).
 Return
     array of errors.
 */
function validateDateObject(dateObject, form, param) {
    var errors = new Array();
    var fieldName = param["name"];
    //var param = new Object();
    var mess = "";
    if (fieldName == undefined) {
        if (dateObject.name != undefined) {
            fieldName = dateObject.name;
        }
    }
    //format was definition in local code
    //param['mask'] = /^[0-9]{4}-[0-9]{2}-[0-9]{2}$/;
    if (dateObject.value == '') {
        mess = messageRequired;
    } else if (!correctedFormatDate(dateObject)) {
        mess = messageFormatDateInvalide;
    } else if (!validateMask(form, dateObject, param)) {
        mess = messageYearMonthDayFormat;
    } else if (!validateDayMonthYear(form, dateObject, param)) {
        mess = messageYearMonthDayDate;
    }
    if (mess !== "") {
        addError(errors, dateObject.name, mess.replace("|", fieldName));
    }
    return errors;
}

    /*
    Validate date object for dates in dd-Mon-yyyy format.
    This function require global variables (messageRequired, messageYearMonthDayDate, messageFormatDateInvalide).
    */
function validateDateObject_DDMMMYYYY(dateObject, fieldName, form) {
    var errors = new Array();
    var param = new Object();
    var mess = "";
    param['mask'] = /^[0-9]{2}-[a-z,A-Z]{3}-[0-9]{4}$/;
    if (dateObject.value == '') {
        mess = messageRequired;
        addError(errors, dateObject.name, mess.replace("|", fieldName));
    } else if (!validateMask(form, dateObject, param)) {
        mess = messageFormatDateInvalide;
        addError(errors, dateObject.name, mess.replace("|", fieldName));
    } else if (!validateDayMonthYear(form, dateObject, param)) {
        mess = messageYearMonthDayDate;
        addError(errors, dateObject.name, mess.replace("|", fieldName));
    }
    return errors;
}
	/*
    Validate date object for dates in dd-Mon-yyyy format.
    This function require global variables (messageYearMonthDayDate, messageFormatDateInvalide).
    */
function validateCalendarDateObject_DDMMMYYYY(dateObject, fieldName) {
    if (fieldName == undefined && dateObject.name != undefined) {
        fieldName = dateObject.name;
    }
    var errors = [];
    var mess = "";

    if (!correctedFormatDate(dateObject)) {
        mess = messageDayMonthYearFormat;
    }
    if (mess != "") {
        addError(errors, dateObject.name, mess.replace("|", fieldName));
    }
    return errors;
}

function validateCalendarDateObject_MMMYYYY(dateObject, fieldName) {
    if (fieldName == undefined && dateObject.name != undefined) {
        fieldName = dateObject.name;
    }
    var errors = [];
    var mess = "";

    if (!validateMMMYYYY(dateObject)) {
        mess = messageMonthYearFormat;
    }
    if (mess != "") {
        addError(errors, dateObject.name, mess.replace("|", fieldName));
    }
    return errors;
}
	/*
    Validate date object for dates in yyyy-mm-dd format.
    This function require global variable (messageYearMonthDayDate).
    */
function validateCalendarDateObject_YYYYMMDD(dateObject, fieldName) {
    if (fieldName == undefined && dateObject.name != undefined) {
        fieldName = dateObject.name;
    }
    var param = new Object();
    param["name"] = fieldName;

    var errors = new Array();
    var mess = "";

    if (!validateYearMonthDay(null, dateObject, param)) {
        mess = messageYearMonthDayFormat;
    }
    if (mess != "") {
        addError(errors, dateObject.name, mess.replace("|", fieldName));
    }
    return errors;
}

function validateRequiredIf(form, field, param) {
    var checkField = jQuery("[name='" + param['checkField'] + "']");
    if (checkField.attr("type") === "checkbox") {
        if (jQuery("[name='" + param['checkField'] + "']").is(':checked')) {
            return validateRequired(form, field, param);
        }
    } else {
        if (jQuery("[name='" + param['checkField'] + "']").val() == param['checkFieldValue']) {
            return validateRequired(form, field, param);
        }
    }

    return true;
}

function truncTextElement(element, length) {
    if (length != 0 && element.value.length > length) {
        element.value = element.value.substr(0, length);
        return false;
    }
    return true;
}

    /**
     * It check new date is less then current date value.
     *
     * @param dateField The field contains date.
     * @param prevDate The previous date value.
     *
     * */
function checkDateLessCurrentDate(dateField, prevDate, errorMsg, isChangeNotValidVal, forbidEquality) {
    var result = false;
    var newDate = (toDateDD_MMM_YYYY(dateField.value)).setHours(0, 0, 0, 0);
    var currentDate = new Date().setHours(0, 0, 0, 0);
    if (newDate < currentDate || (forbidEquality && newDate == currentDate)) {
        alert(errorMsg);
        result = true;
        if (isChangeNotValidVal) {
            dateField.value = prevDate;
        }
    }
    return result;
}

function checkDateGreaterCurrentDate(dateField, prevDate, errorMsg, isChangeNotValidVal, forbidEquality) {
    var result = false;
    var newDate = (toDateDD_MMM_YYYY(dateField.value)).setHours(0, 0, 0, 0);
    var currentDate = new Date().setHours(0, 0, 0, 0);
    if (newDate > currentDate || (forbidEquality && newDate == currentDate)) {
        alert(errorMsg);
        result = true;
        if (isChangeNotValidVal) {
            dateField.value = prevDate;
        }
    }
    return result;
}

var UTF_LENGTH_VALIDATOR = UTF_LENGTH_VALIDATOR || {};

UTF_LENGTH_VALIDATOR.CRYPTO_LENGTH_MULTIPLYER = 1.5;

UTF_LENGTH_VALIDATOR.getUtfLength = function(string, isCrypto){
    string = string || "";

    var utf8length = 0;
    for (var n = 0; n < string.length; n++) {
        var c = string.charCodeAt(n);
        if (c < 128) {
            utf8length++;
        }
        else if((c > 127) && (c < 2048)) {
            utf8length = utf8length+2;
        }
        else {
            utf8length = utf8length+3;
        }
    }
    utf8length = isCrypto ? utf8length * UTF_LENGTH_VALIDATOR.CRYPTO_LENGTH_MULTIPLYER : utf8length;
    utf8length = utf8length | 0; // trunc length

    return utf8length;
};

UTF_LENGTH_VALIDATOR.getLengthSymbols = function(string, lengthByteLimit, isCrypto){
    string = string || "";

    var utf8length = 0;
    for (var n = 0; n < string.length; n++) {
        var c = string.charCodeAt(n);
        if (c < 128) {
            utf8length++;
        }
        else if((c > 127) && (c < 2048)) {
            utf8length = utf8length+2;
        }
        else {
            utf8length = utf8length+3;
        }
        if((isCrypto ?
            utf8length * UTF_LENGTH_VALIDATOR.CRYPTO_LENGTH_MULTIPLYER :
            utf8length)
            > lengthByteLimit){
            return n;
        }
    }

    return n;
};

UTF_LENGTH_VALIDATOR.addValidator = function(jElement, lengthLimit, isCrypto){
    jElement.on("keyup", function(){
        var current = jQuery(this),
            value = current.val(),
            lengthSymbols = UTF_LENGTH_VALIDATOR.getLengthSymbols(value,lengthLimit,isCrypto);
        if(lengthSymbols < value.length){
           current.val(value.substring(0,lengthSymbols));
        }
    });
};
if (!Array.prototype.filter) {
    Array.prototype.filter = function (fun /*, thisp*/) {
        "use strict";

        if (this == null) {
            throw new TypeError();
        }

        var t = Object(this);
        var len = t.length >>> 0;
        if (typeof fun !== "function") {
            throw new TypeError();
        }

        var res = [];
        var thisp = arguments[1];
        for (var i = 0; i < len; i++) {
            if (i in t) {
                var val = t[i]; // in case fun mutates this
                if (fun.call(thisp, val, i, t)) {
                    res.push(val);
                }
            }
        }

        return res;
    };
}

if (typeof String.prototype.trim !== 'function') {
    String.prototype.trim = function () {
        return this.replace(/^\s+|\s+$/g, '');
    };
}

Math.trunc = Math.trunc || function(x) {
    return x - x%1;
};

var isLtIE9 = function () {
    var dm = document.documentMode,
        ua = navigator.userAgent,
        isbadBrowser = false;

    if (ua.indexOf("MSIE") !== -1) {
        if (dm === 7 || dm === 8 || dm === 9 || dm === 10) {
            isbadBrowser = true;
        }
    }
    return isbadBrowser;
};

function getIEVersion() {
    var sAgent = window.navigator.userAgent;
    var Idx = sAgent.indexOf("MSIE");
    if (Idx > 0) {
        return parseInt(sAgent.substring(Idx + 5, sAgent.indexOf(".", Idx)));
    } else if (!!navigator.userAgent.match(/Trident\/7\./)) {
        return 11;
    }
    return 0;
}

var _ = {} || _; //global scope for common util functions

_.isUndefined = function (data) {
    return typeof data === "undefined" || data === null;
};

_.isDefine = function (data) {
    return typeof data !== "undefined";
};

(function ($) {
    $.fn.replaceOptions = function (options, firstItem) {
        var self, $option;

        this.empty();
        self = this;


        function _addOption(option) {
            $option = $("<option></option>")
                .attr("value", option.id)
                .text(option.name);
            self.append($option);
        }

        if(_.isDefine(firstItem)) {
            _addOption(firstItem);
        }

        $.each(options, function (index, option) {
            _addOption(option);
        });
    };
})(jQuery);

(function ($) {
    $.fn.replaceOptgroups = function (optgroups) {
        var self, $optgoup, $option;

        this.empty();
        self = this;


        function _addOptgroup(optgroup) {
            $optgoup = $("<optgroup></optgroup>")
                .attr("label", optgroup.categoryName);

            $.each(optgroup.options, function (option) {
                _addOption(optgroup.options[option]);
            });
            self.append($optgoup);
        }

        function _addOption(option) {

            $option = $("<option></option>")
                .attr("value", option.id)
                .text(option.name);
            $optgoup.append($option);
        }

        self.append("<option></option>");
        $.each(optgroups, function (optiongroup) {
            _addOptgroup(optgroups[optiongroup]);
        });

    };
})(jQuery);

_.switchVisibilityState = function (elementId, switcherId) {
    var $elem = jQuery("#"+elementId),
        $switcher = jQuery(switcherId);
    if ($elem.css("visibility") === "visible") {
        $switcher.text("+");
        $elem.css("visibility", "hidden");
    } else {
        $switcher.text("-");
        $elem.css("visibility", "visible");
    }
};

_.switchDisplayState = function (elementId, switcherId) {
    var $elem = jQuery("#" + elementId),
        $switcher = jQuery("#" + switcherId);
    if ($elem.css("display") === "block") {
        $switcher.text("+");
        $elem.css("display", "none");
    } else {
        $switcher.text("-");
        $elem.css("display", "block");
    }
};
_.switchTableDisplayState = function (elementId, switcherId) {
    var $elem = jQuery("#" + elementId),
        $switcher = jQuery("#" + switcherId);
    if ($elem.css("display") === "table") {
        $switcher.text("+");
        $elem.css("display", "none");
    } else {
        $switcher.text("-");
        $elem.css("display", "table");
    }
};

//TODO: add safety check
_.sendRedirect = function (urlToRedirect) {
    window.location = urlToRedirect;
};

_.openInNewTab = function (url) {
    window.open(url, '_blank');
};

_.openInThisTab = function (url) {
    window.location.href = url;
};

function escapeJS(str) {
    var re = new RegExp("\\n\\r", "g");
    var result = str.replace(re, ' ');
    re = new RegExp("\\r\\n", "g");
    result = str.replace(re, ' ');
    re = new RegExp("\\n", "g");
    result = str.replace(re, ' ');
    re = new RegExp("&", "g");
    result = result.replace(re, '&amp;');
    re = new RegExp("\\r", "g");
    result = result.replace(re, ' ');
    re = new RegExp("<", "g");
    result = result.replace(re, "&lt;");
    re = new RegExp(">", "g");
    result = result.replace(re, "&gt;");
    re = new RegExp("\"", "g");
    result = result.replace(re, "&quot;");
    re = new RegExp("'", "g");
    result = result.replace(re, "&apos;");
    return result;
}

function convertToOutTagFormat(str)
{
    str = str.replace(/&/g, "&amp;");
    str = str.replace(/>/g, "&gt;");
    str = str.replace(/</g, "&lt;");
    str = str.replace(/"/g, "&quot;");
    str = str.replace(/'/g, "&#039;");
    str = str.replace(/\n\r/g, " ");
    str = str.replace(/\r\n/g, " ");
    str = str.replace(/\n/g, " ");
    str = str.replace(/\r/g, " ");
    return str;
}

function isContainsHtmlTags(str) {
    return jQuery.ajax({
        url: 'rest/security/html-tag-checker',
        data: JSON.stringify(str),
        async: false,
        method: 'POST',
        contentType: 'application/json'
    }).responseJSON;
}

function escapeRegExp(str) {
    return str.replace(/[\-\[\]\/\{\}\(\)\*\+\?\.\\\^\$\|]/g, "\\$&");
}

/**
 * Find position element on the page.
 * @param element - the element
 * @returns {Array} coordinates of the element on the page
 */
function offsetPosition(element) {
    var offsetLeft = 0;
    var offsetTop = 0;
    if (element.offsetParent) {
        do {
            offsetLeft += element.offsetLeft;
            offsetTop  += element.offsetTop;
            } while (element = element.offsetParent);
    }
    return { offsetTop: Math.round(offsetTop), offsetLeft: Math.round(offsetLeft) };
}

function escJSID(id) {
    return '#' + id.replace(/(:|\.)/g,'\\$1');
}

function escJSName(name) {
    return  name.replace(/(:|\.)/g,'\\$1');
}

function keyPressedCode(ev) {
    var keyPressed;
    var event = ev || window.event;
    if(event) { //IE and other browsers copability
        keyPressed = event.keyCode;
    } else {
        keyPressed = ev.which;
    }
    return keyPressed;
}

function isKeyPressed(keycode, ev) {
    return keycode === keyPressedCode(ev);
}

function disableSavableField(jQueryField, disable) {
    if (disable) {
        if (jQueryField.length > 0 && jQueryField[0].nodeName === "SELECT" && jQueryField.hasClass("select2-offscreen")) {
            jQueryField.select2("enable", false);
        }
        var $hidden = jQuery("input[type=hidden][name='" + jQueryField.attr('name') + "']");
        if ($hidden.length === 0) {
            $hidden = jQuery('<input></input>').attr('type', 'hidden').attr('name', jQueryField.attr('name'));
            jQueryField.after($hidden);
        }
        $hidden.val(jQueryField[0].value);

        jQueryField.prop("disabled", true);
    } else {
        if (jQueryField.length > 0 && jQueryField[0].nodeName === "SELECT" && jQueryField.hasClass("select2-offscreen")) {
            jQueryField.select2("enable", true);
        }
        jQuery("input[type=hidden][name='" + jQueryField.attr("name") + "']").remove();
        jQueryField.prop("disabled", false);
    }
}

function formatDateDDMonYYYY(date) {
    var formatDate = "";
    if (date) {
          var yyyy = date.getFullYear();
          var months = new Array("Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec");
          var mon = months[date.getMonth()];
          var dd = date.getDate();
          if (dd < 10) {
              dd = '0' + dd;
          }
          formatDate = dd + '-' + mon + '-' + yyyy;
      } else {
          formatDate = 'N/A';
      }
      return formatDate;
}

function getDataField(data, path) {
    if (path === "") {
        return data;
    } else {
        var field = path.split(".");
        var func = field.pop();
        for (var i = 0, len = field.length; i < len; i++) {
            data = data[field[i]];
        }
        return data[func];
    }
}

function isPathNullData (data, path) {
    if (path === "") {
        return true;
    } else {
        var field = path.split(".");
        var func = field.pop();
        var isNullFlag = false;
        for (var i = 0, len = field.length && !isNullFlag; i < len; i++) {
           if (data[field[i]] == null) {
               return true;
           }
        }
        return false;
    }
}

function strval(value) {
    if (typeof value === "undefined" || value === null) {
        return "";
    } else {
        return value.toString();
    }
}

function concatNullableStrings() {
    var result = "", str;
    for (var i=0; i < arguments.length; i++) {
        str = strval(arguments[i]);
        if (result.length && str.length) {
            result += " ";
        }
        result += str;
    }
    return result;
}

var waitForFinalEvent = (function () {
    var timers = {};
    return function (callback, ms, uniqueId) {
        if (!uniqueId) {
            uniqueId = "Don't call this twice without a uniqueId";
        }
        if (timers[uniqueId]) {
            clearTimeout (timers[uniqueId]);
        }
        timers[uniqueId] = setTimeout(callback, ms);
    };
})();

function getUTF8StringLength(string) {
    var utf8length = 0;
    for (var n = 0; n < string.length; n++) {
        var c = string.charCodeAt(n);
        if (c < 128) {
            utf8length++;
        }
        else if ((c > 127) && (c < 2048)) {
            utf8length = utf8length + 2;
        }
        else {
            utf8length = utf8length + 3;
        }
    }
    return utf8length;
}

function getUTF8SymbolsCountBeforeSize(string, size) {
    var utf8length = 0, count = 0;
    for (var n = 0; n < string.length; n++) {
        var c = string.charCodeAt(n);
        if (c < 128) {
            utf8length++;
        }
        else if ((c > 127) && (c < 2048)) {
            utf8length = utf8length + 2;
        }
        else {
            utf8length = utf8length + 3;
        }
        if (utf8length <= size) {
            count++;
        } else {
            break;
        }
    }
    return count;
}

function checkInt(value) {
    if (value == '') {
        return false;
    }
    var regNum = /^(\d)*$/;
    return regNum.test(value);
}

/**
 * @param time passed is ms (ex. new Date() - someDateInPast)
 * @returns dateStructure{years,months,days,weeks,daysInWeek,hours,minutes,secs,milliSecs}
 */
function convertMsTimePassedToDateStructure(time) {
    var dateStructure = {};

    dateStructure.years = Math.floor(time / (1000*60*60*24*30*365));
    var timeRemains = time - dateStructure.years*(1000*60*60*24*30*365);

    dateStructure.months = Math.floor(time / (1000*60*60*24*30));
    timeRemains = time - dateStructure.months*(1000*60*60*24*30);

    dateStructure.days=Math.floor(timeRemains / (1000*60*60*24));
    timeRemains = timeRemains -  dateStructure.days*(1000*60*60*24);

    dateStructure.weeks = Math.floor(dateStructure.days / 7);
    dateStructure.daysInWeek = dateStructure.days - dateStructure.weeks * 7;

    dateStructure.hours = Math.floor(timeRemains / (1000*60*60));
    timeRemains = timeRemains -  dateStructure.hours*(1000*60*60);

    dateStructure.minutes = Math.floor(timeRemains / (1000*60));
    timeRemains = timeRemains -  dateStructure.minutes*(1000*60);

    dateStructure.secs = Math.floor(timeRemains / (1000));
    dateStructure.milliSecs = timeRemains -  dateStructure.secs*(1000);

    return dateStructure;
}

function synchronizeSelect(selectObject, name)  {
    var selectedValue = jQuery('option:selected', selectObject).val();
    jQuery("select[name='" + name + "'] option[value='" + selectedValue + "']").prop('selected', 'yes');
}

/*
 * Format a number with obligatory decimal part.
 *
 * Parameters
 *
 * number
 *   The number being formatted.
 *
 * obligDecimals
 *   Sets the number of obligatory decimal points.
 *
 * decimals
 *   Sets the number of decimal points.
 *
 *
 * If you need all decimal digits, you must not indicate decimals parameter, or set this parameter value null or ''.
 *
 * Examples:
 * we have number 1.14
 * if we will call our function with next parameters
 * nfObligatoryDecimal(1.14, 1, 2) return 1.14
 * nfObligatoryDecimal(1.146, 1, 2) return 1.15
 * nfObligatoryDecimal(1.146, 4, 2) return 1.1460
 * nfObligatoryDecimal(1.14, 1, 1) return 1.1
 * nfObligatoryDecimal(1.14, 1) return 1.14
 * nfObligatoryDecimal(1.1, 1) return 1.1
 * nfObligatoryDecimal(1, 1) return 1.0
 */
function nfObligatoryDecimal(number, obligDecimals, decimals){

    if(obligDecimals != null) {
        if(decimals == null || decimals.toString() === '') {
            decimals = number.toString().length - number.toString().indexOf(".");
        }

        if(decimals < obligDecimals) {
            decimals = obligDecimals;
        }
    }
    if (!isNaN(number)) {
        number = new Number(number);
    }
    number = number.toFixed(decimals);
    var diff = decimals - obligDecimals;
    if (diff > 0) {
        for(var i = 0; i < diff; i++) {
            if (number.substr(number.length - 1, 1) === 0) {
                number = number.substring(0, number.length - 1);
            } else {
                return number;
            }
        }
        if (obligDecimals == 0) {
            number = number.substring(0, number.length - 1);
            return number;
        }
    }

    return number;
}

function replaceSpaces(formName) {
    jQuery("[name='"+formName+"']").find(":input:not(input[type=button],input[type=submit],input[type=checkbox],select, :hidden, :image, .no-whitespace-trim)").each(function() {
        var $this = jQuery(this);
        var replaced = $this.val().replace(/^\s+|\s+$/g, "");
        replaced = replaced.replace(/(\t|\s{2,})/g, ' ');
        jQuery(this).val(replaced);
    });
}

function isSelectOptionExist (selectName, valueToFind) {
    return jQuery("select[name='"+selectName+"']").find("option[value='"+valueToFind+"']").length!==0;
}

function textTransform() {
    jQuery("[style*='text-transform: capitalize'], .capitalize").each(function() {
        var $this = jQuery(this);
        var val = $this.val();
        $this.val(val.capitalize());
    });
}

var POPUP_UTIL = {
    setPopupPosition : function(caller, popup) {
        var jPopup = jQuery(popup),
            jParentOffset = jPopup.offsetParent().offset(),
            popupHeight = jPopup.height(),
            popupWidth = jPopup.width(),

            jCaller = jQuery(caller),
            callerPositionX = jCaller.offset().left,
            callerPositionY = jCaller.offset().top,

            jWindow = jQuery(window),
            spaceTop = callerPositionY - jWindow.scrollTop() - 5,
            spaceBottom = jWindow.height() + jWindow.scrollTop() - callerPositionY - 5,
            spaceLeft = callerPositionX - jWindow.scrollLeft() - 5,
            spaceRight = jWindow.width() + jWindow.scrollLeft() - callerPositionX - 5;

        if (spaceBottom < popupHeight && spaceTop > spaceBottom) {
            jPopup.css("top", callerPositionY - jParentOffset.top - popupHeight - 5);
        } else {
            jPopup.css("top", callerPositionY - jParentOffset.top + 5);
        }
        if (spaceRight < popupWidth && spaceLeft > spaceRight) {
            jPopup.css("left", callerPositionX - jParentOffset.left - popupWidth - 5);
        } else {
            jPopup.css("left", callerPositionX - jParentOffset.left + 5);
        }
    }
};

function isNull (value) {
    return (typeof value === "undefined" || value === null || value === "");
}

function valueIsNull(control) {
    return isNull(control) || control.value === null || control.value.trim() === "";
}

function getMonday(d) {
    d = !!d && new Date(d) || new Date();
    var day = d.getDay(),
        diff = d.getDate() - day + (day === 0 ? -6 : 1);
    return new Date(d.setDate(diff));
}

function getSunday(d) {
    d = getMonday(d);
    return new Date(d.setDate(d.getDate() + 6));
}

function monthDiff(d1, d2) {
    var months;
    months = (d2.getFullYear() - d1.getFullYear()) * 12;
    months -= d1.getMonth() + 1;
    months += d2.getMonth();
    return months <= 0 ? 0 : months;
}

function htmlDecode(input){
    var e = document.createElement('div');
    e.innerHTML = input;
    return e.childNodes.length === 0 ? "" : e.childNodes[0].nodeValue;
}

function byteLengthUtf8(str) {
    // returns the byte length of an utf8 string
    var len = str.length;
    for (var i = str.length - 1; i >= 0; i--) {
        var code = str.charCodeAt(i);
        if (code > 0x7f && code <= 0x7ff) {
            len++;
        } else if (code > 0x7ff && code <= 0xffff) {
            len+=2;
        }
        if (code >= 0xDC00 && code <= 0xDFFF) { //trail surrogate
            i--;
        }
    }

    return len;
}

function disableElement(element) {
    jQuery(element).addClass('no-pointer-events half-transparent');
}

function enableElement(element) {
    jQuery(element).removeClass('no-pointer-events half-transparent');
}

function arrayContainsId(array, id) {
    var result = false;
    for (var i = 0; i < array.length; i++) {
        if (array[i].id == id) {
            result = true;
        }
    }
    return result;
}

function debounce(fn, delay) {
    var timer = null;
    return function () {
        var context = this, args = arguments;
        clearTimeout(timer);
        timer = setTimeout(function () {
            fn.apply(context, args);
        }, delay);
    };
}

function deepCopy(data) {
    var obj = JSON.parse(JSON.stringify(data));
    return typeof obj === 'string' ? JSON.parse(obj) : obj;
}

function compare(a, b) {
    return a < b ? -1 : a > b ? 1 : 0;
}

// Use this function to open any URL with security protection against Tabnabbing
function securedWindowOpener(url, name, windowFeatures){
    var newWindow = window.open(url, name, 'noopener,noreferrer,' + windowFeatures);
    newWindow.opener = null;
};/**
 * Locks the page with absolutely positioned div to prevent unnecessary
 * user interaction or just to show that something is really going on
 * @author Maksim Golunko, 2012
 *
 * @param options js objects param
 * lockerID - locker id. default is 'pageLocker'
 * disableInputs - boolean is all form elements need to be disabled. default is false
 * opacity - div's opacity. default is 0
 * zIndex - div's zIndex. default is 1001
 * changeCursor - is mouse cursor should be changed when the page is locked(cursor:wait). default is true
 * background - div's background color. default is '#F7F7F7'.
 * workloadBodyOnly - boolean is page content only should be locked (not including top menu, search, etc.). default is false
 */
function showPageLocker(options) {
    options = jQuery.extend({
        lockerID: "pageLocker",
        disableInputs: false,
        opacity: 0.0,
        zIndex: 1001,
        changeCursor: true,
        background: "#222",
        workloadBodyOnly: false,
        animate: false
    }, options);
    var locker = document.getElementById(options.lockerID);
    if (locker !== null) {
        hidePageLocker(options.lockerID, true);
    }
    if (options.disableInputs) {
        jQuery("input,textarea").prop("disabled", true);
        jQuery("select").select2("readonly", true);
    }
    locker = document.createElement("div");
    locker.id = options.lockerID;
    if (options.workloadBodyOnly) {
        var offset = jQuery("#workloadBody").offset();
        locker.style.top = offset.top + "px";
        locker.style.left = offset.left + "px";
    } else {
        locker.style.top = 0;
        locker.style.left = 0;
    }
    locker.style.position = "absolute";
    locker.style.zIndex = options.zIndex;
    locker.style.display = "block";
    locker.style.background = options.background;
    locker.style.opacity = options.opacity;
    locker.style.filter = "progid:DXImageTransform.Microsoft.Alpha(opacity=" + options.opacity * 100 + ")";
    if (options.changeCursor) {
        changeMouse("wait", locker);
    }
    var resizeFunc = function () {
        adjustPageLocker(options.lockerID);
    };
    var pageLocker = jQuery(locker);
    pageLocker.data("resizeFunc", resizeFunc);
    jQuery("body").append(locker);
    if (options.animate) {
        jQuery(locker).css('opacity', 0).fadeTo("fast", options.opacity);
    }
    jQuery(window).on("resize", resizeFunc);
    resizeFunc();
}

function adjustPageLocker(lockerID) {
    if (typeof lockerID === "undefined") {
        lockerID = "pageLocker";
    }
    var locker = jQuery("#" + lockerID);
    var win = jQuery(document);
    locker.height(0);
    locker.width(0);
    locker.height(win.height() - parseInt(locker.css("top"), 10) - (jQuery.browser.msie ? 21 : 0));
    locker.width(win.width() - parseInt(locker.css("left"), 10) - (jQuery.browser.msie ? 21 : 0));
}

function hidePageLocker(lockerID, instantly) {
    if (typeof lockerID === "undefined") {
        lockerID = "pageLocker";
    }
    var locker = document.getElementById(lockerID);
    if (locker !== null) {
        changeMouse('auto', locker);
        var resizeFunc = jQuery(locker).data("resizeFunc");
        jQuery(window).off("resize", resizeFunc);
        if (instantly) {
            jQuery(locker).remove();
        } else {
            jQuery(locker).fadeTo("fast", 0, function () {
                jQuery(locker).remove();
            });
        }
    }
}

function changeMouse(cursor, locker) { //HACK for Chrome / Safari
    if (jQuery.browser.webkit) {
        var wkch = document.createElement("div");
        wkch.style.overflow = "hidden";
        wkch.style.position = "absolute";
        wkch.style.left = "0px";
        wkch.style.top = "0px";
        wkch.style.width = "100%";
        wkch.style.height = "100%";
        var wkch2 = document.createElement("div");
        wkch2.style.width = "200%";
        wkch2.style.height = "200%";
        wkch.appendChild(wkch2);
        document.body.appendChild(wkch);
    }
    locker.style.cursor = cursor;
    if (jQuery.browser.webkit) {
        wkch.scrollLeft = 1;
        wkch.scrollLeft = 0;
        document.body.removeChild(wkch);
    }
};var UPSA = UPSA || {};

UPSA.panelButtons = [];
UPSA._initializeTopMenu = function () {
    UPSA.__initAjaxRetry();
    UPSA.__initDwrRetry();
    UPSA.__initMenuItemBehavior();
    UPSA.__initUserIcon();
    UPSA.__initSearchInput();
    UPSA.__initStickyPanel();
    UPSA.__initRowspanBorders();
    UPSA.__initTableHightlght();
    UPSA.__initOnboarding();
    UPSA.__initGlobalMenu();
};

UPSA._initializeGlobalEvents = function () {
    jQuery(document).on('keyup', function (e) {
        if (e.which === 27) {
            if (jQuery('.modernPopup:visible').data('disable-esc')) {
                return;
            }
            var confirmMsg = jQuery('.modernPopup:visible').data('confirm-exit');
            var exit = true;
            if (!!confirmMsg) {
                exit = (!!hasFormBeenChanged && hasFormBeenChanged()) && confirm(confirmMsg) || (hasFormBeenChanged && !hasFormBeenChanged());
            }
            exit && jQuery(".modernPopup:visible").closeWin();
        }
    });
    jQuery('a[href="#"]').on('click', function (e) {
        e.preventDefault();
    });
};

UPSA.__initGlobalMenu = function() {
    setTimeout(function() {
        if (UPSA.showGlobalMenu) {
            jQuery.get('https://menu.epam.com/scripts/menu.js?app=UPSA').done(function(data) {
                jQuery('head').append(jQuery('<script/>').text(data));
            });
        }
    }, 100);
};


UPSA.__initStickyPanel = function () {
    var panel = jQuery('.page-title-panel:eq(0)');
    if (panel.length === 1) {
        var buttonsPanel = jQuery('.page-title-bottom-panel-wrap:eq(0)');
        var panelTop = (buttonsPanel.length === 0) ? panel.offset().top : buttonsPanel.offset().top;
        jQuery(window).scroll(function () {
            if (jQuery(window).scrollTop() > panelTop) {
                panel.toggleClass("fixed", true);
                var singleRow = (panel.find('.page-title-bottom-panel').length === 0);
                panel.toggleClass("oneRow", singleRow).toggleClass("twoRows", !singleRow);
                panel.width(jQuery(window).width() - 100);
            } else {
                panel.removeClass("fixed");
                panel.parent().height(panel.parent().height());
                panel.css('width', panel.parents(".pageTitle-wrap").css('width'));
            }
        }).resize(function () {
            panel.css('width', (jQuery(window).scrollTop() > panelTop) ? jQuery(window).width() - 100: panel.parents(".pageTitle-wrap").css('width'));
        });
    }
};

UPSA.__initTableHightlght = function () {
    var toggleHighlight = function (target, color) {
        var row = jQuery(target).parent();
        var rowspan = jQuery(target).attr('rowspan');
        if (rowspan) {
            for (var i = 0; i < rowspan; i++) {
                var tmpcolor = typeof color === 'undefined' ? row.data('bg') : color;
                row.data('bg', row.css('background-color'));
                row.css('background-color', tmpcolor);
                row = row.next();
            }
        } else {
            row.children().not('[rowspan]').each(function () {
                var $this = jQuery(this);
                $this.data('bg', $this.css('background-color'));
                $this.css('background-color', color);
            });
            row.children('[rowspan=1]').each(function () {
                var $this = jQuery(this);
                $this.data('bg', $this.css('background-color'));
                $this.css('background-color', color);
            });
        }
    };

    jQuery('.general-info-table:not(.unhovered) tr:not(.unhovered) td[rowspan]').on('mouseover', function () {
        toggleHighlight(this, '#fbfbfb');
    });

    jQuery('.general-info-table:not(.unhovered) tr:not(.unhovered) td[rowspan]').on('mouseout', function () {
        toggleHighlight(this);
    });
};

UPSA.__initRowspanBorders = function () {
    jQuery('td[rowspan]').each(function () {
        var $cell = jQuery(this),
                $row = $cell.parent(),
                rowspan = $cell.attr('rowspan');
        for (var i = 0; i < rowspan - 1; i++) {
            $row = $row.next('tr');
            $row.addClass('rowspan-row');
        }
    });
};

UPSA.__initOnboarding = function () {
    if (UPSA.onboardingEnabled) {
        ONBOARD.init({
            before: function () {
                jQuery('#top-list-menu li').trigger('mouseout');
                MENU._hoverIntent(false);
            },
            after: function () {
                MENU._hoverIntent(true);
            }
        });
        ONBOARD.loadScenarios(function (versions) {
            jQuery('body').append(ONBOARD.createAlert(versions));
        });
        ONBOARD.getVersions().done(function (versionIds) {
            if (versionIds.length > 0) {
                UPSA.__onboardPreviousVersions = true;

                jQuery('#onboardDialogLink').show();
                jQuery('body').on('click', '#onboardDialogLink', function() {
                    var onboardStart = function(version) {
                        return function() {
                            ONBOARD.loadScenarios(function() {
                                jQuery('.onboard-alert').remove();
                                ONBOARD.run();
                            }, version);
                        };
                    };
                    if (versionIds.length > 1) {
                        var dialog = ONBOARD.createDialog(versionIds);
                        if (!!dialog) {
                            dialog.appendTo(jQuery('body'));
                            dialog.find('select').select2();
                            dialog.on('click', '#onboardStart', function() {
                                var version = jQuery('#onboardVersion').val();
                                dialog.closeWin();
                                onboardStart(version)();
                            });

                            dialog.on('click', '#onboardClose', function() {
                                dialog.closeWin();
                            });

                            dialog.createWin();
                        }
                    } else {
                        onboardStart(versionIds[0])();
                    }
                });
            }
        });
    }
};


UPSA.__initUserIcon = function () {
    jQuery("#menuProfileLink").click(function (e) {
        var $idCard = jQuery(".id-card");
        if ($idCard.css('visibility') === 'hidden') {
            $idCard.css('visibility', 'visible');
            jQuery("#menuProfileLink").addClass("hover-photo");
        } else {
            $idCard.css('visibility', 'hidden');
            e.stopPropagation();

        }
    });

    jQuery(document).mouseup(function (e) {
        var $idCard = jQuery(".id-card");

        if (!$idCard.is(e.target) && $idCard.has(e.target).length === 0) {
            $idCard.css('visibility', 'hidden');
            jQuery("#menuProfileLink").removeClass("hover-photo");
        }
    });
};

UPSA.__changeTabHelpLink = function (linkValue) {
    jQuery('.help-icon').qtip('option', 'content.text', "<a  class='help-item' id='upsaHelpLink' href='" + linkValue + "' target='_blank'>On-Line Help</a></br>" +
            (permissionMatrixLink !== "" ? "<a class='help-item' href='" + permissionMatrixLink + "' target='_blank'>Permission Matrix</a><br/>"
                    : "") +
            "<a class='help-item' href='" + whatsNewLink + "' target='_blank'>What's new</a><br/>" +
            (UPSA.onboardingEnabled ? "<a class='help-item' href='#' id='onboardDialogLink' style='display: none;'>Page Guide</a>" : "") +
            "<a class='help-item' href='rest/api/api.html' target='_blank'>UPSA API</a>");
};

UPSA.__initMenuItemBehavior = function () {
    if(UPSA.activeTopMenuItemId) {
        jQuery(".current-page").removeClass("current-page");
        jQuery("#" + UPSA.activeTopMenuItemId + " a").addClass("current-page");
    }

};

UPSA.__getMethodURL = function () {
    var href = window.location.href,
            indxHash = href.lastIndexOf("#"),
            indxSlash = href.lastIndexOf("/"),
            indxQuast = href.lastIndexOf("?"),
            indxAnd = href.lastIndexOf("&"),
            indexes = [];

    indexes.push(indxHash);
    indexes.push(indxQuast);
    indexes.push(indxAnd);
    var last = UPSA.__getMinInArray(indexes);
    return (_.isDefine(last) && last > 0) ?
            href.substring(indxSlash, last) : href.substr(indxSlash);
};

UPSA.__getMinInArray = function (indexes) {
    var filtered = indexes.filter(function (element, index, array) {
        return (element > 0);
    });
    return Math.min.apply(Math, filtered);
};

UPSA.__initAjaxRetry = function() {
    jQuery.ajaxPrefilter(function(options, originalOptions, jqXHR) {
        originalOptions.isRetry = false;

        var deferred = jQuery.Deferred();
        jqXHR.done(deferred.resolve);
        jqXHR.fail(function() {
            var args = Array.prototype.slice.call(arguments);
            if (jqXHR.status === 401 && !originalOptions.isRetry) {
                createAutoLoginFrame(UPSA.__ajaxRetryCallback(originalOptions, deferred));
            } else {
                deferred.rejectWith(jqXHR, args);
            }
        });

        return deferred.promise(jqXHR);
    });
};

UPSA.__ajaxRetryCallbackCount = 0;

UPSA.__ajaxRetryCallback = function(request, deferred) {
    var callbackName = 'retryCallback_' + UPSA.__ajaxRetryCallbackCount++;
    window[callbackName] = function() {
        request.isRetry = true;
        jQuery.ajax(request).then(deferred.resolve, deferred.reject);
    };

    return callbackName;
};

UPSA.__initDwrRetry = function() {
    var originalErrorHandler = dwr.engine._handleError;
    dwr.engine._handleError = function(batch, ex) {
        if (!batch.isRetry && ex.name === 'dwr.engine.http.401') {
            createAutoLoginFrame(UPSA.__dwrRetryCallback(batch));
        } else if (ex.name !== 'dwr.engine.incompleteReply') {
            originalErrorHandler(batch, ex);
        } else {
            !!console && console.error('Failed to execute DWR batch :' + ex);
        }
    };
};

UPSA.__dwrRetryCallback = function(batch) {
    var callbackName = 'retryCallback_' + UPSA.__ajaxRetryCallbackCount++;
    window[callbackName] = function() {
        dwr.engine._batch = batch;
        batch.preHooks = [];
        dwr.engine.endBatch({
            completed: false,
            isRetry: true
        });
    };

    return callbackName;
};

UPSA.clickCloseInit = function () {
    jQuery(document).mouseup(function (e) {
        var $clickClose = jQuery(".click-close");

        if (!$clickClose.is(e.target) && $clickClose.has(e.target).length === 0) {
            $clickClose.fadeOut(200);
        }
    });
};

function initializeGoToTopArrow(_gaq) {
    var win = jQuery(window);
    var arrowBlock = jQuery(".upArrowBlock");
    var isArrowBlockShown = false;
    var onScroll = function () {
        if (win.scrollTop() > 155) {
            isArrowBlockShown = true;
            arrowBlock.show();
        } else {
            isArrowBlockShown = false;
            arrowBlock.hide();
        }
    };
    onScroll();
    window.onscroll = onScroll;
    arrowBlock.click(function () {
        win.scrollTop(0);
        _gaq.push(['_trackEvent', 'Navigation', 'Go to page top']);
    });
}

function callOnLoadHandler() {
    if (window.onLoad) {
        onLoad();
    }
}

function registerUnloadAlert() {
    window.onbeforeunload = unloadAlert;
}

function unloadAlert(e) {
    // this is workaround for evading dwr warning handler triggering on page unload
    // jira bug - EPMUPSA-1762
    dwr.engine._unloading = true;
    if (!e) {
        e = event;
    }
    msg = "";
    try {
        msg = getUnloadAlert();
    } catch (e) {
    }
    if (msg !== "") {
        // if we want confirm from user
        dwr.engine._unloading = false;
        e.returnValue = msg;
        return msg;
    }
}

//this is 'hack', layout.js include on layout.jsp and this func must
//be implemented, when need errors process

function getUnloadAlert() {
    return "";
}

function onSearch(e) {
    if ((e == null) || (e.keyCode === 13)) {
        var query = document.getElementById("hiddenQuery").value;
        query = query.replace(/[\(\)]/g, '').replace(/^\s*/, '').replace(/\s*$/, '');
        if (query === '') {
            alert(searchAlert);
            return false;
        }
        document.getElementById("query").value = query;
        document.getElementById("hiddenQuery").value = query;
        document.charset = 'utf-8';
    }
    return true;
}

function prepareUrl(url) {
    if (url.indexOf(upsaBaseUrl) !== 0) {
        url = upsaBaseUrl + (url.indexOf('/') !== 0 ? '/' : '') + url;
    }
    return url;
}

function goToUrl(urlToGo) {
    urlToGo = prepareUrl(urlToGo);
    try {
        location.href = urlToGo;
    } catch (e) {
        //nothing
    }
    return false;
}

function openInNewTab(urlToGo) {
    urlToGo = prepareUrl(urlToGo);
    window.open(urlToGo, '_blank');
    return false;
}

function fixTableXScrollDivIE9() {
    //if div has inputs and overflow: auto when IE9 increase height on div selection/deselection
    // with overflow: scroll/hidden it works correct
    if (jQuery.browser.msie && jQuery.browser.version === "9.0") {
        if (jQuery("#tableXScrollDiv").scrollLeft(5).scrollLeft() === 0) {
            jQuery("#tableXScrollDiv").css("overflow-x", "hidden");
        } else {
            jQuery("#tableXScrollDiv").css("overflow-x", "scroll");
            jQuery("#tableXScrollDiv").scrollLeft(0);
        }
    }
}

/**
 * Shows animated loader img
 * @param settings object with settings:
 * container - jquery object to center image against, default is #workloadBody
 * loaderImgId - id to be used for image, default is 'loaderImg' which is styled in common.css
 */
function showLoaderImg(settings) {
    settings = jQuery.extend({container: jQuery("#workloadBody"), loaderImgId: "loaderImg"}, settings);
    var width = settings.container.width() / 2;
    if (jQuery("#" + settings.loaderImgId).length === 0) {
        var loaderImg = "<div id='" + settings.loaderImgId + "' style='zIndex:51;position:fixed;top:50%;left:" + width + "px;'></div>";
        settings.container.after(loaderImg);
    }
}

function hideLoaderImg(loaderImgId) {
    if (typeof loaderImgId === "undefined") {
        loaderImgId = "loaderImg";
    }
    jQuery("#" + loaderImgId).remove();
}

function lockSaveButtons(lock) {
    if (lock) {
        jQuery('.saveButton').each(function () {
            jQuery(this).data('locked', typeof (jQuery(this).attr('disabled')) !== 'undefined');
            jQuery(this).attr('disabled', true);
        });
    } else {
        jQuery('.saveButton').each(function () {
            jQuery(this).attr('disabled', jQuery(this).data('locked'));
        });
    }
}

