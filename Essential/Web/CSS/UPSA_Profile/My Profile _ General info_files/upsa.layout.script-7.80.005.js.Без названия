                ColorOverlay: '#F7F7F7',
                OpacityOverlay: 0.3
            });
}

function isScrollingNeeded(wrapper,scroll) {
        if(jQuery(wrapper + ' table').width() <= jQuery(scroll).width()) {
                return false;
            }
        return true;
    }

function setCdnActiveCookie(name, value) {
    var date = new Date();
    var minutes = 5;
    date.setTime(date.getTime() + (minutes * 60 * 1000));
    document.cookie = name + "=" + value + "; expires=" + date.toGMTString() + "; path=/";
}

function initTableScroll(wrapper, scroll) {
    var $wrapper = jQuery(wrapper),
            $scroll = jQuery(scroll);

    if ($wrapper.length && $scroll.length) {
        $scroll.width(jQuery(wrapper).width());

        jQuery(scroll + ' > div').width(jQuery(wrapper + ' table').width());

        $scroll.scroll(function () {
            jQuery(wrapper).scrollLeft(jQuery(this).scrollLeft());
        });

        $wrapper.scroll(function () {
            $scroll.scrollLeft(jQuery(this).scrollLeft());
        });

        jQuery(window).scroll(function () {
            var windowBottom = jQuery(window).scrollTop() + jQuery(window).height();
            var tableBottom = $wrapper.offset().top + $wrapper.height();
            if(isScrollingNeeded(wrapper,scroll)) {
                $scroll.css('visibility', windowBottom >= tableBottom ? 'hidden' : 'visible');
            }
            else {
                $scroll.css('visibility', 'hidden');
            }

            $scroll.width($wrapper.width());
        });

        $wrapper.children('div').innerWidth(jQuery(wrapper + ' table').width());

        jQuery(window).resize(function () {
            $scroll.width($wrapper.width());
            $scroll.children('div').width($wrapper.children(':eq(0)').width());
            $wrapper.children('div').innerWidth(jQuery(wrapper + ' table').width());
            jQuery(window).scroll();
        });

        jQuery(window).scroll();
    }
}
;function DOMBuilder(tag, attr, content) {
    this.tag = tag;
    this.attributes = attr ? attr : {};
    this.content = [];
    if (typeof content !== "undefined") {
        this.content.push(content);
    }
}
DOMBuilder.prototype.prependChild = function (child) {
    this.content.unshift(child);
    return this;
};
DOMBuilder.prototype.appendChild = function (child) {
    this.content.push(child);
    return this;
};
DOMBuilder.prototype.innerHTML = function (content) {
    this.content = [];
    this.content.push(content);
    return this;
};
DOMBuilder.prototype.attr = function (name, value) {
    if (typeof value === "undefined") {
        return this.attributes[name];
    } else {
        this.attributes[name] = value;
    }
    return this;
};
DOMBuilder.prototype.appendAttr = function (name, value) {
    this.attributes[name] = concatNullableStrings(this.attributes[name], value);
    return this;
};
DOMBuilder.prototype.toString = function () {
    var string = "<" + this.tag;
    for (var attrName in this.attributes) {
        if (this.attributes.hasOwnProperty(attrName)) {
            string += " " + attrName + "=\"" + strval(this.attributes[attrName]) + "\"";
        }
    }
    if (this.content.length === 0) {
        string += "/>";
    } else {
        string += ">";
        for (var i = 0; i < this.content.length; i++) {
            string += strval(this.content[i]);
        }
        string += "</" + this.tag + ">";
    }
    return string;
};
DOMBuilder.prototype.jQuery = function() {
    return jQuery(this.toString());
};/*
