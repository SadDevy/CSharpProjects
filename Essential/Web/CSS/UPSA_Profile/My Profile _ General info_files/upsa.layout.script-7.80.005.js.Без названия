(function() {
if (dwr.engine._getObject("Survey") == undefined) {
var p;

p = {};
p._path = '/workload/dwr';





p.getSurvey = function(p0, callback) {
return dwr.engine._execute(p._path, 'Survey', 'getSurvey', arguments);
};

dwr.engine._setObject("Survey", p);
}
})();

var ONBOARD = ONBOARD || (function() {
    var _this = this;

    var OPTIONS = {
        shadowClass: 'onboard-shadow',
        shadowWhiteClass: 'onboard-shadow-white',
        wrapperClass: 'onboard-wrapper',
        wrapperLockedClass: 'locked',
        lockerClass: 'onboard-locker',
        messageBoxClass: 'onboard-message',
        messageArrowClass: 'onboard-message-arrow',
        messageArrowLeftClass: 'left',
        messageArrowRightClass: 'right',
        messageArrowBottomClass: 'bottom',
        messageHeaderClass: 'onboard-message-header',
        messageBodyClass: 'onboard-message-body',
        messageFooterClass: 'onboard-message-footer',
        nextBtnClass: 'next',
        alertClass: 'onboard-alert',
        alertCloseClass: 'onboard-alert-close',
        alertActionClass: 'onboard-alert-action',

        msgNext: 'Got it!',
        msgPrev: 'Back',
        msgDefaultTitle: 'Page Guide',
        msgStartTour: 'Page Guide',
        msgShowLater: 'Show me later',
        msgNeverShow: 'Don\'t show again',

        shadowOpacity: 0.75,
        shadowPadding: 10,
        arrowSize: 15,

        container: '#container',

        getPageName: function() {
            var pathParts = location.pathname
                .split('/')
                .filter(function(e) {
                    return !!e && !!e.trim();
                });

            return pathParts[pathParts.length - 1];
        },

        buildScenariosUrl: function() {
            return 'rest/v3/onboarding/scenario/' + OPTIONS.getPageName();
        },

        buildMarkSeenUrl: function(version) {
            return 'rest/v3/onboarding/markSeen/' + OPTIONS.getPageName() + '/' + version;
        }
    };

    var currentElement = null;
    var currentScroll = 0;
    var wrapper = null;
    var fullScenario = null;

    var initialized = false;

    var init = function(options) {
        OPTIONS = jQuery.extend(OPTIONS, options);
        _this.initialized = true;
    };

    var _trigger = function(settings) {
        var target = jQuery(settings.target).get(0);
        var listeners = jQuery._data(target, 'events');
        if (!!listeners && !!listeners[settings.event]) {
            listeners[settings.event].forEach(function(eventHandler) {
                var evt = {
                    type: eventHandler.origType
                };

                var handler = eventHandler.handler;

                handler.bind(target, evt)();
            });
        }
    };

    var focusOnElement = function(settings, lookupForward) {
        var finishCallback = null,
            target = null;

        if (!!settings) {
            do {
                (typeof settings.finish === 'function') && (finishCallback = settings.finish);
                (typeof settings.before === 'function') && (settings.before());
                target = settings.target;
                if (jQuery(target).is(':visible')) {
                    focus(target, _buildMsgBox(settings));
                    (typeof settings.after === 'function') && (settings.after());
                    return;
                } else {
                    settings = settings[lookupForward ? 'next' : 'prev'];
                    (typeof settings.after === 'function') && (settings.after());
                }
            } while (!!settings && !!settings.next);
        }

        (lookupForward && typeof finishCallback === 'function') && (finishCallback());
    };

    var runScenario = function(scenario) {
        if (wrapper === null) {
            var initialWidth = jQuery('body').width();
            wrapper = jQuery('<div/>')
                    .addClass(OPTIONS.wrapperClass)
                    .appendTo('body')
                    .append(jQuery(OPTIONS.container));
            var newWidth = jQuery('body').width();

            jQuery(window).resize(function() {
                !!wrapper && wrapper.css('width', jQuery('body').width() - (initialWidth - newWidth));
            });

            jQuery(window).resize();
        }

        typeof scenario.before === 'function' && scenario.before();
        var actions = scenario.actions.filter(function(e) {
            return jQuery(e.target).length > 0;
        });
        var processedActions = [];
        actions.forEach(function(e, i) {
            processedActions.push(e);
            if (Array.isArray(e.target)) {
                var target = null;
                e.target.forEach(function(selector) {
                    (target === null && jQuery(selector).length > 0) && (target = selector);
                });
                e.target = target || "";
            }
            (!!actions[i - 1]) && (e.prev = actions[i - 1]);
            (!!actions[i + 1]) && (e.next = actions[i + 1]);
            (!actions[i + 1] && typeof scenario.after === 'function') && (e.finish = scenario.after);
            (!!e.triggerBefore) && (e.before = function() {
                e.triggerBefore.forEach(_trigger);
            });
        });

        setTimeout(function() {
            focusOnElement(processedActions[0]);
        }, 300);
    };

    var markSeen = function(version) {
        jQuery.ajax({
            type: 'PUT',
            url: OPTIONS.buildMarkSeenUrl(version)
        });
    };

    var _buildShadows = function($elem) {
        var top = $elem.offset().top - OPTIONS.shadowPadding;
        var left = $elem.offset().left - OPTIONS.shadowPadding;
        var right = $elem.offset().left + $elem.outerWidth() + OPTIONS.shadowPadding;
        var bottom = $elem.offset().top + $elem.outerHeight() + OPTIONS.shadowPadding;

        var leftShadow = jQuery('<div/>').addClass(OPTIONS.shadowClass).css({
            top: 0,
            left: 0,
            width: left + 'px',
            height: '100%'
        });

        var rightShadow = jQuery('<div/>').addClass(OPTIONS.shadowClass).css({
            top: 0,
            left: right + 'px',
            width: '100%',
            height: '100%'
        });

        var topShadow = jQuery('<div/>').addClass(OPTIONS.shadowClass).css({
            top: 0,
            left: left + 'px',
            width: (right - left) + 'px',
            height: top + 'px'
        });

        var bottomShadow = jQuery('<div/>').addClass(OPTIONS.shadowClass).css({
            top: bottom,
            left: left + 'px',
            width: (right - left) + 'px',
            height: '100%'
        });

        var centerShadow = jQuery('<div/>')
            .addClass(OPTIONS.shadowWhiteClass)
            .css({
                top: top + 'px',
                left: left + 'px',
                width: (right - left) + 'px',
                height: (bottom - top) + 'px'
            });

        return [topShadow, rightShadow, bottomShadow, leftShadow, centerShadow];
    };

    var blur = function() {
        _this.currentElement = null;
        jQuery('.' + OPTIONS.wrapperClass).removeClass(OPTIONS.wrapperLockedClass);
        jQuery('html, body').scrollTop(_this.currentScroll);
        jQuery('.' + OPTIONS.shadowClass).remove();
        jQuery('.' + OPTIONS.shadowWhiteClass).remove();
        jQuery('.' + OPTIONS.messageBoxClass).remove();
    };

    var focus = function(element, message) {
        var $elem = jQuery(element);
        blur();
        _this.currentElement = $elem;
        _this.currentScroll = $elem.offset().top;

        if ($elem.outerHeight() / jQuery(window).height() > 0.80) {
            _this.currentScroll -= jQuery(window).height() / 2;
        } else {
            _this.currentScroll -= (jQuery(window).height() - $elem.outerHeight()) / 2;
        }

        var locker = jQuery('<div/>').addClass(OPTIONS.lockerClass).appendTo(wrapper);

        jQuery('html, body').animate({
            scrollTop: _this.currentScroll
        }, function() {
            jQuery('.' + OPTIONS.shadowClass).remove();
            jQuery('.' + OPTIONS.shadowWhiteClass).remove();
            wrapper.scrollTop(_this.currentScroll);
            locker.remove();
            wrapper.addClass(OPTIONS.wrapperLockedClass);

            var shadows = _buildShadows($elem);
            wrapper.append(shadows);
            shadows.forEach(function(e) {
                jQuery(e).animate({
                    opacity: OPTIONS.shadowOpacity
                });
            });

            message.appendTo(jQuery('body'));

            var elemLeft = $elem.offset().left;
            var elemRight = $elem.offset().left + $elem.outerWidth();
            var elemTop = $elem.offset().top;
            var elemHeight = $elem.outerHeight();
            var elemWidth = $elem.outerWidth();

            var msgHeight = message.outerHeight();
            var msgWidth = message.outerWidth();

            var msgTop = elemTop + (elemHeight - msgHeight) / 2;

            var posLeft = elemLeft - msgWidth - 2 * OPTIONS.shadowPadding - OPTIONS.arrowSize;
            var posRight = elemRight + 2 * OPTIONS.shadowPadding + OPTIONS.arrowSize;

            var arrow = message.find('.' + OPTIONS.messageArrowClass);
            arrow.css('top', msgHeight / 2 - OPTIONS.arrowSize);

            var msgLeft;
            if ((elemRight + 2 * msgWidth + 2 * OPTIONS.shadowPadding + OPTIONS.arrowSize) > jQuery(window).width()) {
                msgLeft = posLeft;
                arrow.addClass(OPTIONS.messageArrowRightClass).css('left', msgWidth - 1);
            } else {
                msgLeft = posRight;
                arrow.addClass(OPTIONS.messageArrowLeftClass);
            }

            if (msgLeft < 0 || (msgLeft + 2 * OPTIONS.shadowPadding + OPTIONS.arrowSize + msgWidth) > jQuery(window).width()) {
                arrow.removeClass(OPTIONS.messageArrowLeftClass)
                        .removeClass(OPTIONS.messageArrowRightClass)
                        .addClass(OPTIONS.messageArrowBottomClass)
                        .css('top', msgHeight)
                        .css('left', msgWidth / 2 - OPTIONS.arrowSize);
                msgTop = elemTop - 2 * OPTIONS.shadowPadding - OPTIONS.arrowSize - msgHeight;
                msgLeft = elemLeft + (elemWidth - msgWidth) / 2;
            }

            message.css({
                top: msgTop,
                left: msgLeft
            });
        });
    };

    var getVersions = function() {
        return jQuery.ajax({
            type: 'GET',
            url: 'rest/v3/onboarding/' + OPTIONS.getPageName() + '/versions'
        });
    };

    var _preProcessScenarios = function(data) {
        if (!!data) {
            var versions = Object.keys(data).sort();
            if (versions.length > 0) {
                fullScenario = {
                    before: OPTIONS.before,
                    after: function() {
                        typeof OPTIONS.after === 'function' && OPTIONS.after();
                        blur();
                        wrapper.children().appendTo(jQuery('body'));
                        jQuery('.' + OPTIONS.lockerClass).remove();
                        wrapper.remove();
                        wrapper = null;
                    },
                    actions: []
                };

                versions.forEach(function(e, i) {
                    var scenario = JSON.parse(data[e]);

                    fullScenario.actions = fullScenario.actions.concat(scenario.actions);
                    fullScenario.actions[fullScenario.actions.length - 1].after = function() {
                        markSeen(versions[i]);
                    };
                });
            }
        }
    };

    var _buildMsgBox = function(settings) {
        var msgBox = jQuery('<div/>')
                .addClass(OPTIONS.messageBoxClass);
        var msgHead = jQuery('<div/>')
                .addClass(OPTIONS.messageHeaderClass)
                .text(settings.title || OPTIONS.msgDefaultTitle)
                .appendTo(msgBox);
        var btnClose = jQuery('<div/>')
                .addClass('close')
                .append(jQuery('<i/>').addClass('icon-remove'))
                .appendTo(msgHead)
                .on('click', function() {
                    fullScenario.after();
                });
        var msgBody = jQuery('<div/>')
                .addClass(OPTIONS.messageBodyClass)
                .html(settings.message)
                .appendTo(msgBox);
        var msgArrow = jQuery('<div/>')
                .addClass(OPTIONS.messageArrowClass)
                .appendTo(msgBox);
        var msgFooter = jQuery('<div/>')
                .addClass(OPTIONS.messageFooterClass)
                .appendTo(msgBox);
        if (!!settings.prev) {
            var btnPrev = jQuery('<input/>')
                    .attr('type', 'button')
                    .val(OPTIONS.msgPrev)
                    .appendTo(msgFooter)
                    .on('click', function() {
                        focusOnElement(settings.prev, false);
                    });
        }
        var btnNext = jQuery('<input/>')
                .attr('type', 'button')
                .addClass(OPTIONS.nextBtnClass)
                .val(OPTIONS.msgNext)
                .appendTo(msgFooter);
        if (!!settings.next) {
            btnNext.on('click', function() {
                focusOnElement(settings.next, true);
            });
        } else {
            btnNext.on('click', function() {
                blur();
                !!settings.finish && settings.finish();
            });
        }

        return msgBox;
    };

    var _buildAlert = function(versions) {
        if (versions.length > 0) {
            var pulse = true;

            var pageGuideBlock = jQuery('<div/>')
                    .addClass(OPTIONS.alertClass);

            var pageGuideLink = jQuery('<a/>')
                    .attr('href', '#')
                    .text(OPTIONS.msgStartTour)
                    .click(function() {
                        jQuery(this).parent().remove();
                        ONBOARD.run();
                    }).appendTo(pageGuideBlock);

            var hidePageGuide = jQuery('<div/>')
                    .html(OPTIONS.msgShowLater)
                    .addClass(OPTIONS.alertActionClass)
                    .click(function() {
                        jQuery(this).parent().remove();
                    })
                    .appendTo(pageGuideBlock);

            var cancelPageGuide = jQuery('<div/>')
                    .html(OPTIONS.msgNeverShow)
                    .addClass(OPTIONS.alertActionClass)
                    .click(function() {
                        ONBOARD.markSeen(versions[versions.length - 1]);
                        jQuery(this).parent().remove();
                    })
                    .appendTo(pageGuideBlock);

            var closePageGuide = jQuery('<div/>')
                    .addClass(OPTIONS.alertCloseClass)
                    .append(jQuery('<i/>').addClass('icon-remove'))
                    .click(function() {
                        pageGuideLink.hide();
                        jQuery(this).hide();
                        hidePageGuide.show();
                        cancelPageGuide.show();
                        pulse = false;
                    })
                    .insertAfter(pageGuideLink);

            (function doPulse(back) {
                if (pulse) {
                    pageGuideBlock.animate({
                        backgroundColor: back ? 'red' : 'black'
                    }, 500, function() {
                        doPulse(!back);
                    });
                } else {
                    pageGuideBlock.animate({
                        backgroundColor: 'black'
                    }, 500);
                }
            })(false);

            return pageGuideBlock;
        }

        return null;
    };

    var dialogTemplate = Hogan.compile(
        '<div class="onboard-message onboard-dialog">' +
            '<div class="onboard-message-header">' +
                OPTIONS.msgDefaultTitle +
                '<div class="close">' +
                    '<i class="icon-remove" id="onboardClose"></i>' +
                '</div>' +
            '</div>' +
            '<div class="onboard-message-body">' +
                '<span>See what\'s new since version </span>' +
                '<select id="onboardVersion" style="width: 115px;">' +
                    '{{#versions}}' +
                        '<option value="{{id}}">{{name}}</option>' +
                    '{{/versions}}' +
                '</select>' +
                '<input type="button" id="onboardStart" class="next" value="Go!" />' +
            '</dov>' +
        '</div>'
    );

    var _decodeVersion = function(versionId) {
        var major = versionId.substr(0, 2).replace(/^0+/, '');
        var minor1 = versionId.substr(2, 2).replace(/^0+/, '');
        var minor2 = versionId.substr(4, 2).replace(/^0+/, '');
        var build = versionId.substr(6, 3).replace(/^0+/, '');
        var hotfix = versionId.substr(9, 2).replace(/^0+/, '');

        return major
            + '.'
            + minor1
            + (minor2.length > 0 ? 'v' + minor2 : '')
            + '.'
            + Array(4 - build.length).join('0') + build
            + (hotfix.length > 0 ? '.' + hotfix : '');
    };

    var _buildDialog = function(versionIds) {
        if (!!versionIds && versionIds.length > 0) {
            var versions = [];
            versionIds.forEach(function(versionId) {
                versions.push({
                    id: versionId,
                    name: _decodeVersion(versionId)
                });
            });

            var dialog = jQuery(dialogTemplate.render({
                versions: versions
            }));

            return dialog;
        }

        return null;
    };

    var loadScenarios = function(callback, version) {
        !initialized && init();
        jQuery.ajax({
            type: 'GET',
            url: OPTIONS.buildScenariosUrl() + '/' + (!!version ? '?fromVersion=' + version : ''),
            success: function(data) {
                _preProcessScenarios(data);
                callback(!!data && Object.keys(data).sort() || []);
            }
        });
    };

    return {
        init: init,
        loadScenarios: loadScenarios,
        run: function() {
            !!fullScenario && runScenario(fullScenario);
        },
        runScenario: runScenario,
        markSeen: markSeen,
        getVersions: getVersions,
        createAlert: _buildAlert,
        createDialog: _buildDialog
    };
})();var OTHER_APPS = OTHER_APPS || (function($) {
    var default_footer = function(divId) {
        return [
            '<div class="modernPopup-footer">',
                '<div style="display: inline-block; float: left;">',
                    '<input type="checkbox" id="' + divId + 'DontShow" class="modernCheckbox"/>',
                    '<label for="' + divId + 'DontShow">Don\'t show me this message again</label>',
                '</div>',
                '<div style="display: inline-block; float: right;">',
                    '<input id="' + divId + 'CloseBtn" type="button" class="modernButton grey-button" value="Close"/>',
                '</div>',
            '</div>'
        ];
    };

    //!!! use next app_id for new popup applications !!!
    // use app_id => select -1*(min(action_id)+19) from c_config_actions where action_id < 0;
