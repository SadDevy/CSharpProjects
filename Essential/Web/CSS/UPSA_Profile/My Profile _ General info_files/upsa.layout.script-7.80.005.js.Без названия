nClosed|onCompleted|var|vPos|length|posTop|css|animate|hPos|remove|ShowOverlay|center|return|LongTrip|switch|ScrollTop|ScrollLeft|_isReadable|ShowTimeEffect|init|HideTimeEffect|VerticalPosition|click|pos|window|else|cursor|jError|200|bottom|HeightDoc|outerHeight|pointer|WidthDoc|stop|delay|this|jSuccess|left|outerWidth|div|_show|false|_showOverlay||null|posLeft|OpacityOverlay|defaults|overlay|HorizontalPosition|ColorOverlay|appendTo|clickOverlay|_construct|body|MinWidth|000|1500|extend|minWidth|el|html|backgroundColor|show|preventDefault|jQuery|width|right|height|scrollTop|scrollLeft'.split('|'),0,{}));function clearLocalStorage(key) {
  try {
    if( 'localStorage' in window && window['localStorage'] !== null ) {
      if(key === undefined) {
        localStorage.clear();
        return true;
      } else {
        localStorage.removeItem(key);
        return true;
      }
    } else {
      return undefined;
    }
  } catch (e) {
    return undefined;
  }
}

function getLocalStorage(key){
  try {
    if( 'localStorage' in window && window['localStorage'] !== null ) {
      return localStorage.getItem(key);
    } else {
      return undefined;
    }
  } catch (e) {
    return undefined;
  }
}

function setLocalStorage(key, value){
  try {
    if( 'localStorage' in window && window['localStorage'] !== null ) {
        localStorage.setItem(key, value);
        return true;
    }
    else {
        return false;
    }
  } catch (e) {
    return false;
  }
};var loadingFlag = true;

function getElement(name, form) {
    return form['' + name + ''];
}

function submitForm(form) {
    form.submit();

    showPageLocker({disableInputs: true});
}

function doNavigation(action, element) {
    if (document.readyState === 'loading') {
        return;
    }
    var form = getParentForm(element);
    if (action === 'first') {
        getElement('pNavigation.first', form).value = true;
    } else if (action === 'prev') {
        getElement('pNavigation.prev', form).value = true;
    } else if (action === 'next') {
        getElement('pNavigation.next', form).value = true;
    } else if (action === 'last') {
        getElement('pNavigation.last', form).value = true;
    }

    var div_id = getElement('pNavigation.container', form).value;

    if (div_id == null) {
        submitForm(form);
    } else {
        var callbackFunction = getElement('pNavigation.handler', form).value;
        if ((callbackFunction != null) && (callbackFunction != "")) {
            window[callbackFunction](
                getElement('pNavigation.first', form).value,
                getElement('pNavigation.prev', form).value,
                getElement('pNavigation.next', form).value,
                getElement('pNavigation.last', form).value,
                getElement('pNavigation.page', form).value,
                getElement('pNavigation.pageSize', form).value,
                getElement('pNavigation.maxRowNum', form).value,
                getElement('pNavigation.forwardName', form).value,
                getElement('pNavigation.container', form).value,
                false
            );
        } else {
            submitForm(form);
        }
    }
}

// Returns parent form of a given element
function getParentForm(element) {
    while (element != null && element.tagName !== 'FORM') {
        element = element.parentNode;
    }
    return element;
}

function goToPageNum(element, pageNum) {
    if (!loadingFlag) {
        return false;
    }
    //hack for IE, cause double clicking on input fiels lead to NPE
    jQuery("[id='logoTable']").focus();
    loadingFlag = false;
    var form = getParentForm(element);
    var pageCount = getElement('pNavigation.pageCount', form).value;
    var page = pageNum || getElement('goToPageValue', form).value.trim();
    if (page == null || page == '') {
        page = 1;
    }
    if (checkInt(page)) {
        var pageIntVal = parseInt(page, 10);
        if (pageIntVal > pageCount) {
            pageIntVal = pageCount;
        } else if (pageIntVal < 1) {
            pageIntVal = 1;
        }
        getElement('pNavigation.page', form).value = pageIntVal;
        if(getElement('goToPageValue', form)){
            getElement('goToPageValue', form).value = pageIntVal;
        }
        var div_id = getElement('pNavigation.container', form).value;

        if (div_id == null) {
            submitForm(form);
        } else {
            var callbackFunction = getElement('pNavigation.handler', form).value;
            if ((callbackFunction != null) && (callbackFunction != "")) {
                window[callbackFunction](
                    getElement('pNavigation.first', form).value,
                    getElement('pNavigation.prev', form).value,
                    getElement('pNavigation.next', form).value,
                    getElement('pNavigation.last', form).value,
                    getElement('pNavigation.page', form).value,
                    getElement('pNavigation.pageSize', form).value,
                    getElement('pNavigation.maxRowNum', form).value,
                    getElement('pNavigation.forwardName', form).value,
                    getElement('pNavigation.container', form).value,
                    false
                );
            } else {
                submitForm(form);
            }
        }
    } else {
        alert('You must enter a positive number.');
    }
    loadingFlag = true;
    return false;
}

function setPageSize(e) {
    e.stopPropagation();
    var element = e.target;
    if (!loadingFlag) {
        return false;
    }
    loadingFlag = false;
    var form = getParentForm(element);
    var pageSize = getElement('pageSizeValue', form).value;
    if (pageSize == null || pageSize == '') {
        pageSize = defaultPageSize;
    }
    if (checkInt(pageSize)) {
        var pageSizeIntVal = parseInt(pageSize, 10);
        if (pageSizeIntVal < minPageSize) {
            alert('Minimum number of items on the page is ' + minPageSize + '.');
            loadingFlag = true;
            return false;
        }
        if (pageSizeIntVal > maxPageSize) {
            alert('Maximum number of items on the page is ' + maxPageSize + '.');
            loadingFlag = true;
            return false;
        }
        if (pageSizeIntVal >= minPageSize && pageSizeIntVal <= maxPageSize) {
            getElement('pNavigation.pageSize', form).value = pageSizeIntVal;
            getElement('pageSizeValue', form).value = pageSizeIntVal;

            var div_id = getElement('pNavigation.container', form).value;

            if (div_id == null) {
                submitForm(form);
            } else {
                var callbackFunction = getElement('pNavigation.handler', form).value;
                if ((callbackFunction != null) && (callbackFunction != "")) {
                    window[callbackFunction](
                        getElement('pNavigation.first', form).value,
                        getElement('pNavigation.prev', form).value,
                        getElement('pNavigation.next', form).value,
                        getElement('pNavigation.last', form).value,
                        getElement('pNavigation.page', form).value,
                        getElement('pNavigation.pageSize', form).value,
                        getElement('pNavigation.maxRowNum', form).value,
                        getElement('pNavigation.forwardName', form).value,
                        getElement('pNavigation.container', form).value,
                        true
                    );

                } else {
                    submitForm(form);
                }
            }
        }
    } else {
        alert('You must enter a positive number.');
    }
    loadingFlag = true;
    return false;
}
;var UPSA = UPSA || {};

UPSA.__constructE3SRedirectUrl = function (value, e3sSearchURLType) {
    var searchValue = {
        text: value
    };
    var searchUrl = UPSA.__e3sBaseUrl + e3sSearchURLType + "?h=" + B64.encode(JSON.stringify(searchValue));
    return  searchUrl;
};

UPSA.searchByE3S = function (value) {
    var $searchTypeValue = "people",
        urlMapping = UPSA.__e3sSearchURLMapping,
        e3sSearchURLType = _.isUndefined(urlMapping[$searchTypeValue]) ? urlMapping["default"] : urlMapping[$searchTypeValue],
        urlToRedirect = UPSA.__constructE3SRedirectUrl(value, e3sSearchURLType);
    _.openInNewTab(urlToRedirect);
};

function typeaheadLinkClick(event, el) {
    event.stopPropagation();
}

function typeaheadLinkClickWithRedirect(event, el) {
    event.preventDefault();
    event.stopPropagation();
    _.openInNewTab(jQuery(el).prop("href"));
}

function openByType(suggestions, cb) {
    var type = suggestions[0].datum.type;
    if (type === "employee") {
        _.openInThisTab("employeeView.do?employeeId=" + suggestions[0].datum.employeeId);
    } else if (type === "project") {
        _.openInThisTab("resplanView.do?projectId=" + suggestions[0].datum.projectId);
    } else if (type === "case") {
        _.openInNewTab("projectCaseEdit.do?projectCaseId=" + suggestions[0].datum.caseId);
    } else if (type === "customer") {
        _.openInNewTab(UPSA.__e3sBaseUrl + "company_profile?upsaId=" + suggestions[0].datum.id);
    } else if (type === "autouser") {
        _.openInNewTab(UPSA.__e3sBaseUrl + "serviceAccount.do?id=" + suggestions[0].datum.id);
    } else if (type === "error") {
        UPSA.searchByE3S(jQuery("#hiddenQuery").val());
    }

    cb && cb();
}

function initTitles(suggestions) {
    if (suggestions.length > 0) {
        var empTitle = false;
        var projTitle = false;
        var caseTitle = false;
        var custTitle = false;
        var autoUserTitle = false;
        for (var i = 0; i < suggestions.length; i++) {
            var type = suggestions[i].datum.type;
            if (type === "employee" && !empTitle) {
                suggestions[i].datum.showTitle = empTitle = true;
            } else if (type === "project" && !projTitle) {
                suggestions[i].datum.showTitle = projTitle = true;
            } else if (type === "case" && !caseTitle) {
                suggestions[i].datum.showTitle = caseTitle = true;
            } else if (type === "customer" && !custTitle) {
                suggestions[i].datum.showTitle = custTitle = true;
            } else if (type === "autouser" && !autoUserTitle) {
                suggestions[i].datum.showTitle = autoUserTitle = true;
            }
        }
    }
}


function E3SSearchByError() {
    UPSA.searchByE3S(jQuery("#hiddenQuery").val());
}

UPSA.__searchEnterButtonEnabled = false;
UPSA.__searchEnterPressed = false;
UPSA.__searchDropDownEnabled = true;

UPSA.__initSearchInput = function () {
    var $hiddenQuery = jQuery("#hiddenQuery");
    $hiddenQuery.on("focus", function(){
        jQuery("#searchByName .icon-search").addClass('active-icon-search');
    });
    $hiddenQuery.on("blur", function(){
        jQuery("#searchByName .icon-search").removeClass('active-icon-search');
    });
    $hiddenQuery.on("typeahead:selected", function (e, selectedObj, searchName) {
        if (selectedObj.type !== "error") {
            var mapping = UPSA.__searchUrlMapping[selectedObj.type];
            var url = mapping.url + selectedObj.id;
            if (mapping.newTab) {
                _.openInNewTab(url);
            } else {
                _.openInThisTab(url);
            }
        }
        e.preventDefault();
    });

    var hiddenQueryKeypress = function (event) {
        var $this = jQuery(this),
            value = $this.val();
        if (event.keyCode === 13 && value.length >= 3) { //check if search completed
            UPSA.__searchEnterPressed = true;

            if (UPSA.__searchEnterButtonEnabled) {
                var suggestions = $this.data("suggestions");
                suggestions && openByType(suggestions);
            }
        }
    };

    $hiddenQuery.on('keypress', hiddenQueryKeypress);

    jQuery(document).mouseup(function (e) {
        var $dropdownMenu = jQuery(".tt-dropdown-menu"),
            $searchRegion = jQuery(".twitter-typeahead");

        if (!$searchRegion.is(e.target) && $searchRegion.has(e.target).length === 0) {
            $dropdownMenu.hide();
        }
    });


    $hiddenQuery.typeahead([
        {
            name: "global-search",
            minLength: 3,
            valueKey: "value",
            bottomSuggestionsTemplate: "<div>hell</div>",
            remote: {
                url: "rest/search/all/%QUERY" + "?minResult=3&maxResult=12",
                replace: function(URL, uriEncodedQuery) {
                    // Server assumes encoded ?, \, / and % symbols as part of URL
                    uriEncodedQuery = uriEncodedQuery
                .replace(/(%2B)/g, '@')
                .replace(/(%D1%85)/g, '@')
                            .replace(/(%25)/g, '@')
                            .replace(/%2(f|F)/g, '@')
                            .replace(/%5(c|C)/g, '@')
                            .replace(/%3(f|F)/g, '@');
                    URL = URL.replace('%QUERY', uriEncodedQuery);
                    return URL;
                },

                cache: true,
                beforeSend: function () {
                    UPSA.__searchEnterButtonEnabled = false;
                    if (!UPSA.__searchEnterPressed) {
                        UPSA.__searchEnterPressed = false;
                    }
                },
                onDone: function (suggestions) {

                    initTitles(suggestions);
                    if (suggestions.length >= 1 && suggestions[0].datum.type == 'error') {
                        if (suggestions[0].datum.errorCode == '401' && errorAuthenticationNotFound === suggestions[0].datum.value) {
                            createAutoLoginFrame('resendSearchQuery');
                            suggestions[0].datum.value = errorMessageSessionExpired;
                        }
                    }
                    $hiddenQuery.data("suggestions", suggestions);
                        if (suggestions.length >= 1 && UPSA.__searchEnterPressed) {
                            openByType(suggestions, function () {
                                UPSA.__searchEnterButtonEnabled = true;
                            });
                        }
                        if (!UPSA.__searchEnterButtonEnabled) {
                            UPSA.__searchEnterButtonEnabled = true;
                        }
                        UPSA.__searchEnterPressed = false;
                    }
            },
            filterDublicate: false,
            template: [
                '{{#employeeId}}',
                '{{#showTitle}}',
                '<div class="autocomplete-section">Employee</div>',
                '{{/showTitle}}',
                '<div class="autocomplete-employee-block active-{{active}}">',
                '<div class="autocomplete-employee-photo-wrap">' +
                    '<div class="autocomplete-employee-photo" style="background-image: url(\'photo/{{employeeId}}\');"></div></div>',
                '<div class="autocomplete-employee-text-block "><a onclick="typeaheadLinkClick(event, this)" target="" href = "employeeView.do?employeeId={{employeeId}}" ',
                'class="autocomplete-employee-name" data-suggest>{{employeeName}}</a>',
                '<span class = "status-{{active}}" style="font-size: 12px">not active</span>',
                '<span class="autocomplete-employee-title">{{#title}}{{title}}{{/title}}</span></br>' +

                ' {{#primarySkill}}{{primarySkill}} | {{/primarySkill}}',
                '<a onclick="typeaheadLinkClick(event, this)" target="" href = "employeeView.do?employeeId={{managerId}}">{{managerName}}</a>',
                '{{#orgUnitName}} | {{/orgUnitName}}',
                '<a href = "orgchart.do?unitId={{orgUnitId}}" onclick="typeaheadLinkClick(event, this)">{{orgUnitName}}</a></div>' +
                    '</div>',
                '{{/employeeId}}',

                '{{#autoUserId}}',
                '{{#showTitle}}',
                '<div class="autocomplete-section">Service Account</div>',
                '{{/showTitle}}',
                '<div class="autocomplete-autouser-block">',
                '<div class="autocomplete-autouser-text-block ">',
                  '<a onclick = "typeaheadLinkClick(event, this)" target="_blank" href = "serviceAccount.do?id={{id}}" class="autocomplete-autouser-name" data-suggest>{{value}} </a></br>',
                  '<span class="autocomplete-autouser-status">{{stateName}}</span>',
                '</div></div>',
                '{{/autoUserId}}',

                '{{#projectId}}',
                '{{#showTitle}}',
                '<div class="autocomplete-section">Project</div>',
                '{{/showTitle}}',
                '<div class="autocomplete-project-block">',
                '<div class="autocomplete-project-text-block "><a onclick = "typeaheadLinkClick(event, this)" target="" href = "resplanView.do?projectId={{projectId}}" class="autocomplete-project-name" data-suggest>{{projectName}} </a></br>',
                '<span class="autocomplete-project-status">{{stateName}} | </span>',
                '<a onclick="typeaheadLinkClickWithRedirect(event, this)" target="_blank" href = "' + UPSA.__e3sBaseUrl + 'company_profile?upsaId={{projectCustomerId}}' + '">{{customer}}</a> | ',
                '{{billingType}}',
                '</div>' +
                    '</div>',
                '{{/projectId}}',

                '{{#caseId}}',
                '{{#showTitle}}',
                '<div class="autocomplete-section">Opportunity</div>',
                '{{/showTitle}}',
                '<div class="autocomplete-case-block" sdt-link="projectCaseEdit.do?projectCaseId={{caseId}}">',
                '<div class="autocomplete-case-text-block "><a onclick = "typeaheadLinkClick(event, this)"  target="_blank" href = "projectCaseEdit.do?projectCaseId={{caseId}}" class="autocomplete-case-name" data-suggest>{{caseCode}} </a></br>',
                '<span class="autocomplete-case-status">{{stateName}} | </span>',
                '<a onclick="typeaheadLinkClickWithRedirect(event, this)" target="_blank" href = "' + UPSA.__e3sBaseUrl + 'company_profile?upsaId={{caseCustomerId}}' + '">{{caseCustomerName}}</a> | ',
                '{{targetStartDate}}',
                '</div>' +
                    '</div>',
                '{{/caseId}}',

                '{{#customerId}}',
                '{{#showTitle}}',
                '<div class="autocomplete-section">Customer</div>',
                '{{/showTitle}}',
                '<div class="autocomplete-customer-block active-{{active}}">',
                '<div class="autocomplete-customer-text-block "><a onclick="typeaheadLinkClickWithRedirect(event, this)" target="" href = "' + UPSA.__e3sBaseUrl + 'company_profile?upsaId={{id}}' + '" class="autocomplete-project-name" data-suggest>{{customerName}}</a> <span class = "status-{{active}}">not active</span></br>',
                '<span class="autocomplete-customer-title">{{#type}}{{type}} | {{/type}}</span>',
                '{{#industryName}}{{industryName}} | {{/industryName}}',
                '{{country}}',
                '</div>' +
                    '</div>',
                '{{/customerId}}',

                '{{#errorCode}}',
                '<div class="autocomplete-error-block">',
                    '<div class="autocomplete-error-text-block">{{value}}</div>' +
                    "<div class = 'error-link-to-e3s' onclick='E3SSearchByError();'>Try search in Telescope</div>" +
                '</div>',
                '{{/errorCode}}'

            ].join(''),
            engine: Hogan,
            limit: 36, //limit * 3
            maxLimit: 12
        }
    ]);
};
