var browserInfo = {
		isOpera : jQuery.browser.opera,
		isIe : jQuery.browser.msie,
		version : jQuery.browser.version
};


var multiSelectCollection = {
    /* CONSTANTS is used for customization */
    ID_DELIMITER : ',',
    VALUE_DELIMITER : ', ',
    RADIO_DELIMITER : ';',
    CATEGORIES_DELIMITER : '; ',
    CATEGORY_NAME_DELIMITER : ': ',
    ML_IMG_PATH : 'images/',
    CSS_DIR : 'css/',
    ML_IMG_BUTTON : 'mselectlookup.gif',
    ML_IMG_BUTTON_DISABLED : 'mselectlookup.gif',
    ML_IMG_RESET : 'mselectreset.gif',
    ML_IMG_RESET_DISABLED : 'mselectreset.gif',
    CUSTOM_VALUE_ATTR_NAME : 'customValue',
    CSS_FILE: 'multiselect.css',
    CSS_FILE_IEsummary: 'summary.css',
    LOOKUP_HINT: 'To choose multiple items click on every item you want to select in the dropdown sequentially',
    RESET_HINT: 'Reset',
    HIDE_TIMEOUT: 600,
    ALL_ELEMENT_TITLE: 'All',
    MATCHES_NOT_FOUND: 'No matches found',
    EMPTY_LIST_MESSAGE: 'Options list is empty!',
    CUST_TYPE_VALUE: 'multiSelect',
    CUST_TYPE: 'customType',
    CONTROL_CELL_WIDTH: 46,
    CONTROL_CELL_WIDTH_ALT: 35,
    DEFAULT_WIDTH: 130,
    CHECK_WIDTH: 10,
    CONTAINER_TYPE: 'MultiSelectContainerType',
    MAX_CONTAINER_HEIGHT: 290,
    HORIZONT_SCROLLBAR_HEIGHT: 17,
    SEARCH_INPUT_MAX_LENGTH: 20
};

function createSummaryMultiSelect(containerId)
{
    return (multiSelectCollection[containerId] = new classSummaryMultiSelect(containerId));
}

function classSummaryMultiSelect(containerId)
{

    /* SERVICE PROPERTIES */
    this.containerId = containerId;
    this.containerType = multiSelectCollection.CONTAINER_TYPE;
    this.containerObj = null;
    this.state = 0; // 0 - closed (default); 1 - opened
    this.selectedParsed = 0; // 0 - not parsed (default); 1 - parsed

    /* FORM FILED PROPERTIES */
    this.name = null;
    this.id = null;
    this.placeholder = null;

    /* CSS FILE */
    this.cssFile = multiSelectCollection.CSS_DIR + multiSelectCollection.CSS_FILE;
    this.cssFile_IEsummary = multiSelectCollection.CSS_DIR + multiSelectCollection.CSS_FILE_IEsummary;

    /* IMAGE PROPERTIES */
    this.multiSelectImg = multiSelectCollection.ML_IMG_PATH + multiSelectCollection.ML_IMG_BUTTON;
    this.multiSelectImgDisabled = multiSelectCollection.ML_IMG_PATH + multiSelectCollection.ML_IMG_BUTTON_DISABLED;
    this.multiSelectResetImg = multiSelectCollection.ML_IMG_PATH + multiSelectCollection.ML_IMG_RESET;
    this.multiSelectResetImgDisabled = multiSelectCollection.ML_IMG_PATH + multiSelectCollection.ML_IMG_RESET_DISABLED;

    this.inputWidth = null; // value of width of input field
    this.width = null; // value of width
    this.readonly = null; // Y - yes (default); N - no
    this.lookupHint = null;
    this.timer = null;
    this.treatAll = null;
    this.showCheckbox = null; // Y - yes ; N - no (default)
    this.showCategory = null; // Y - yes ; N - no (default)
    this.showRadioButtons = null; // Y - yes ; N - no (default)
    this.onClickRadioFunction = null;
    this.showWithoutSubcategories = null; // Y - yes ; N - no (default)
    
    this.selectParentsAndChildren = null; // Y - yes (default); N - no
    this.childrenAffectParent = null; // Y - yes (default); N - no
    this.oldValue = '';
    this.showSearchInput = null; // Y - yes ; N - no (default)

    /* METHODS */
    this.init = __summaryMultiSelect_init;
    this.render = __summaryMultiSelect_render;
    this.show = __summaryMultiSelect_show;

    this.getTop = __summaryMultiSelect_getTop;
    this.getLeft = __summaryMultiSelect_getLeft;
    this.setInputData = __summaryMultiSelect_setInputData;
    this.setList = __summaryMultiSelect_setList;
    this.parseSelected = __summaryMultiSelect_parseSelected;
    this.parseRadioSelected = __summaryMultiSelect_parseRadioSelected;
    this.resetMultiSelect = __summaryMultiSelect_resetMultiSelect;
    this.setValue = __summaryMultiSelect_setValue;
    this.getValue = __summaryMultiSelect_getValue;
    this.setDisabled = __summaryMultiSelect__setDisabled;
    this.setSelectedHint = __summaryMultiSelect_setSelectedHint;
    this.checkAllChilds = __summaryMultiSelect_checkAllChilds;
    this.parseClick = __summaryMultiSelect_parseClick;
    this.checkChild = __summaryMultiSelect_checkChild;
    this.checkParent = __summaryMultiSelect_checkParent;
    this.unSelectParent = __summaryMultiSelect_unSelectParent;
    this.selectParent = __summaryMultiSelect_selectParent;
    this.modifyDisplayObj = __summaryMultiSelect_modifyDisplayObj;
    this.getCheckbox = __summaryMultiSelect_getCheckbox;
    this.isAllChildSelected = __summaryMultiSelect_isAllChildSelected;
    this.getDisplayValue = __summaryMultiSelect_getDisplayValue;
    this.initRadioButtonTree = __summaryMultiSelect_initRadioButtonTree;
    this.clearChildRadioButtons = __summaryMultiSelect_clearChildRadioButtons;
    this.checkChildRadioButtons = __summaryMultiSelect_checkChildRadioButtons;

    /* EVENTS */
    this.onclick = null;
    this.customOnClick = null;
    this.resetCustomOnClick = null;
}

function __summaryMultiSelect_init(params)
{
    this.containerObj = document.getElementById(this.containerId);
    this.name = params.name;
    this.id = (params.id) ? params.id : params.name;
    this.placeholder = params.placeholder ? params.placeholder : '';
    this.width = (params.width)?params.width:multiSelectCollection.DEFAULT_WIDTH;
    this.popupWidth = (params.popupWidth)?params.popupWidth:this.width;
    this.inputWidth = this.width - multiSelectCollection.CONTROL_CELL_WIDTH;
    this.disabled = (params.disabled === 'Y')?'Y':'N';
    this.readonly = (params.readonly === 'N')?'N':'Y';
    this.treatAll = (params.treatAll === 'Y')?'Y':'N';
    this.lookupHint = (params.lookupHint)?params.lookupHint:multiSelectCollection.LOOKUP_HINT;
    this.showCheckbox = (params.showCheckbox === 'Y')?'Y':'N';
    this.showCategory = (params.showCategory === 'Y')?'Y':'N';
    this.showRadioButtons = (params.showRadioButtons === 'Y')?'Y':'N';
    this.onClickRadioFunction = (params.onClickRadioFunction) ? params.onClickRadioFunction : '';
    this.showWithoutSubcategories = (params.showWithoutSubcategories === 'Y')?'Y':'N';
    
    this.selectParentsAndChildren = (params.selectParentsAndChildren === 'Y')?'Y':'N';
    this.childrenAffectParent = (params.childrenAffectParent === 'Y')?'Y':'N';
    this.showSearchInput = (params.showSearchInput === 'Y')?'Y':'N';
}

function __summaryMultiSelect_render()
{
    if (!this.name || this.name === '')
    {
        alert('ERROR: NAME property is not defined!');
        this.containerObj.style.display = 'none';
        return false;
    }

    var html = new String();
    var onClick = '\'if (multiSelectCollection["' + this.containerId + '"].state) __summaryMultiSelect_closeMultiSelect("' +this.containerId+ '"); else __summaryMultiSelect_show.call(multiSelectCollection["'+this.containerId+'"],event);return false;\'';
    html = html.concat('<input type="Text" name="' + multiSelectCollection.CUST_TYPE_VALUE + '.' + this.name + '" ' + multiSelectCollection.CUST_TYPE + '="' + multiSelectCollection.CUST_TYPE_VALUE + '" onclick=' + onClick + ((this.inputWidth)?' style="width:' + this.inputWidth + 'px"':'') + ((this.readonly=='Y')?' readonly="readonly"':'') + ((this.disabled=='Y')?' disabled="true"':'') + (" placeholder=" + this.placeholder) +'>');
    html = html.concat('<input type="hidden" name="'+this.name+'" id="' + this.id +'" ' + multiSelectCollection.CUST_TYPE + '="' + multiSelectCollection.CUST_TYPE_VALUE + '"'+ ((this.disabled=='Y')?' disabled="true"':'') + '>');
    html = html.concat('<div class="buttonOpen" id="' + this.id + '_imgSet"' + ' title="' + this.lookupHint + '" onclick=' + onClick + ' style="cursor: ' + ((this.disabled == 'Y')? 'default' : 'pointer')+ '"></div>');
    html = html.concat('<div class="buttonClose" id="' + this.id + '_imgReset"' + ' title="' + multiSelectCollection.RESET_HINT + '" onclick=\'__summaryMultiSelect_resetMultiSelect.call(multiSelectCollection["' + this.containerId + '"]);' + this.onClickRadioFunction + 'return false;\' style="cursor: ' + ((this.disabled == 'Y')? 'default' : 'pointer')+ '"></div>');
    html = html.concat('<div class="mselect-box" style="display:none;position:absolute;width:0px;height:0px;z-index:6;"' +
        ' onMouseout="' + (browserInfo.isIe?'top.':'') + '__summaryMultiSelect_processOnOut(\'' + this.containerId + '\');return false;"' +
        ' onMouseover="' + (browserInfo.isIe?'top.':'') + '__summaryMultiSelect_processOnOver(\'' + this.containerId + '\');return false;"></div>');
    this.containerObj.className = "mselect";

    this.containerObj.innerHTML = html.valueOf();
    this.inputObj = this.containerObj.getElementsByTagName("INPUT")[1];
    this.displayObj = this.containerObj.getElementsByTagName("INPUT")[0];
    this.divObj = this.containerObj.getElementsByTagName("DIV")[2];
    if (this.treatAll === 'Y') {
        this.displayObj.value = multiSelectCollection.ALL_ELEMENT_TITLE;
    }
}

function __summaryMultiSelect__setDisabled(disabled) {
    var imgSet = document.getElementById(this.id + '_imgSet');
    var imgReset = document.getElementById(this.id + '_imgReset');
    if (disabled) {
        this.disabled = 'Y';
        if (imgSet && imgReset) {
            imgSet.src = this.multiSelectImgDisabled;
            imgSet.style.cursor = 'default';
            imgReset.src = this.multiSelectResetImgDisabled;
            imgReset.style.cursor = 'default';
            this.displayObj.disabled = true;
        }
    } else {
        this.disabled = 'N';
        if (imgSet && imgReset) {
            imgSet.src = this.multiSelectImg;
            imgSet.style.cursor = 'pointer';
            imgReset.src = this.multiSelectResetImg;
            imgReset.style.cursor = 'pointer';
            this.displayObj.disabled = false;
        }
    }
}

function __summaryMultiSelect_show()
{
    if (this.disabled === 'Y' || this.state) {
        return false;
    }

    for (var i in multiSelectCollection) {
        if (multiSelectCollection[i].containerType == multiSelectCollection.CONTAINER_TYPE && multiSelectCollection[i].state && multiSelectCollection[i].inputObj.onchange) {
            return false;
        }
    }

    if (this.entities == null || this.entities.length === 0) {
        alert(multiSelectCollection.EMPTY_LIST_MESSAGE);
        return;
    }
    
    var colspanSubcategory = "2";
    var colspanRadio = "";
    if (this.showRadioButtons === 'Y') {
    	colspanSubcategory = "3";
    	colspanRadio = 'colspan="2"';
    }

    var html = "";
    if (this.showSearchInput === 'Y' && this.entities.length > 10) {
        html = html + '<div class="mselect-search"><input id="mselect-input" class="mselect-input" type="text" placeholder="Search" autocomplete="off" autocorrect="off" autocapitilize="off" spellcheck="false" maxlength="' + multiSelectCollection.SEARCH_INPUT_MAX_LENGTH + '"></div>';
    }
    html = html + '<div class="mselect lookupLayerContent">';
    html = html + '<table id="multiSelectTable" width="100%" cellpadding="0" cellspacing="0" onclick="' + (browserInfo.isIe?'top.':'') + '__summaryMultiSelect_parseClick.call(' + (browserInfo.isIe?'top.':'') + 'multiSelectCollection[\'' + this.containerId + '\'], event, this);' + this.onClickRadioFunction + ' return true;">';
    if(this.treatAll === 'Y') {
        html = html + '<tr style="white-space: nowrap; font-size: 14px;" ' + multiSelectCollection.CUSTOM_VALUE_ATTR_NAME + '="">';
        if (this.showCheckbox === 'Y') {
            html = html + '<td style="width: ' + multiSelectCollection.CHECK_WIDTH + 'px;"><input type="checkbox"' + ((this.displayObj.value === multiSelectCollection.ALL_ELEMENT_TITLE)?' checked class="mselect modernCheckbox selected"':' class="mselect modernCheckbox unselected"')+ ' id="checkAllEntity" style="margin: 0 5px;"><label for="checkAllEntity"></label></td>';
        }
        html = html + '<td colspan="' + colspanSubcategory + '" class="data" style="white-space: nowrap;">' + multiSelectCollection.ALL_ELEMENT_TITLE + '</td></tr>';
    }
    if (this.showSearchInput === 'Y' && this.entities.length > 10) {
        html = html + '<tr id="matches-not-found" class="display-hide" style="white-space: nowrap; font-size: 14px;" ' + multiSelectCollection.CUSTOM_VALUE_ATTR_NAME + '="">';
        if (this.showCheckbox === 'Y') {
            html = html + '<td style="width: ' + multiSelectCollection.CHECK_WIDTH + 'px;"></td>';
        }
        html = html + '<td colspan="' + colspanSubcategory + '" style="white-space: nowrap; color: red;">' + multiSelectCollection.MATCHES_NOT_FOUND + '</td></tr>';
    }
    var radioShift = "";
    for (var i = 0; i < this.entities.length; i++) {
    	var elemCustomValue = this.entities[i].id;
        if (this.showCategory === 'Y') {
            // skip if this category haven't got subentities
        	if (this.showWithoutSubcategories === 'N') {
	            while (this.entities[i].category != null && this.entities[i + 1].category != null &&
	                    this.entities[i].category === 'Y' && this.entities[i + 1].category === 'Y') {
	                i++;
	            }
        	}
	            
            if (this.entities[i].category != null && this.entities[i].category === 'Y'){
                html = html + '<tr style="font-size: 14px;" ' + ((this.entities[i].selected)?' class="selected"':'') + ' ' + multiSelectCollection.CUSTOM_VALUE_ATTR_NAME + '="' + this.entities[i].id + '">';
                html = html + '<td style="width: ' + multiSelectCollection.CHECK_WIDTH + 'px;"><input type="checkbox"' + ((this.entities[i].selected)?' checked class="mselect modernCheckbox selected"':' class="mselect modernCheckbox unselected"')+ ' id="' + this.entities[i].id + '" style="margin: 0 5px;"><label for="' + this.entities[i].id + '"></label></td>';
                html = html + '<td colspan="' + colspanSubcategory + '" class="data" style="white-space: nowrap; font-weight:bold;">' + this.entities[i].name + '</td></tr>';
                radioShift = "";
            } else {            	
            	if (this.showRadioButtons === 'Y' && this.entities[i].radio === 'Y') {
            		elemCustomValue = this.entities[i].parent + this.entities[i].id;
            	}
                html = html + '<tr style="font-size: 14px;"' + ((this.entities[i].selected)?' class="selected"':'') + ' ' + multiSelectCollection.CUSTOM_VALUE_ATTR_NAME + '="' + elemCustomValue + '">';
                html = html + '<td></td>';
                if (this.showRadioButtons === 'Y' && this.entities[i].radio === 'Y') {                	
                	html = html + radioShift;
                	html = html + '<td style="width: ' + multiSelectCollection.CHECK_WIDTH + 'px;"><input type="radio"' + ' name="' + elemCustomValue + '" parent="' + this.entities[i].parent + '" ' + ((this.entities[i].selected)?' checked class="mselect modernRadio selected"':' class="mselect modernRadio unselected"')+ ' id="' + elemCustomValue + '" style="margin: 0 5px;"><label for="' + elemCustomValue + '"></label></td>';
                } else {
                	radioShift = '<td></td>';
                	html = html + '<td style="width: ' + multiSelectCollection.CHECK_WIDTH + 'px;"><input type="checkbox"' + ' parent="' + this.entities[i].parent + '" ' + ((this.entities[i].selected)?' checked class="mselect modernCheckbox selected"':' class="mselect modernCheckbox unselected"')+ ' id="' + this.entities[i].id + '" style="margin: 0 5px;"><label for="' + this.entities[i].id + '"></label></td>';
                }
                html = html + '<td class="data" ' + colspanRadio + ' style="white-space: nowrap;">' + this.entities[i].name + '</td></tr>';
            }
        } else {
            html = html + '<tr style="font-size: 14px;"' + ((this.entities[i].selected)?' class="selected"':'') + ' ' + multiSelectCollection.CUSTOM_VALUE_ATTR_NAME + '="' + elemCustomValue + '">';
            if (this.showCheckbox === 'Y') {
                html = html + '<td style="width: ' + multiSelectCollection.CHECK_WIDTH + 'px;"><input type="checkbox"' + ((this.entities[i].selected)?' checked class="mselect modernCheckbox selected"':' class="mselect modernCheckbox unselected"')+ ' id="' + this.entities[i].id + '" style="margin: 0 5px;"><label for="' + this.entities[i].id + '"></label></td>';
            }
            html = html + '<td class="data" style="white-space: nowrap;" colspan="' + colspanSubcategory + '">' + this.entities[i].name + '</td></tr>';
        }

    }

    html = html + '</table></div>';
    this.divObj.innerHTML = html;

    var search = jQuery(this.divObj.querySelector("#mselect-input"));
    if ( search.length > 0) {
        search.on("keyup-change input paste", debounce(function (event) {
        	__summaryMultiSelect_doFilter(search);
        }, 250));
    }
    //Hide other multiSelects
    __summaryMultiSelect_closeAll();
    this.divObj.style.visibility = "hidden";
    this.divObj.style.display = "block";
    var x = this.getLeft();
    var y = this.getTop() + this.containerObj.offsetHeight + (browserInfo.isIe? 0 : 1);

    //this.divObj.style.left = x;
    //this.divObj.style.top = y;

    //var actualWidth = browserInfo.isIe? this.containerObj.getElementsByTagName("TABLE")[0].offsetWidth:this.containerObj.getElementsByTagName("TABLE")[0].offsetWidth - 2;
    // var tempId = '#' + this.containerId.replace(/\./g, '\\.');
    // var tempWidth = jQuery(tempId).width();

    var actualWidth = jQuery.browser.msie ? this.popupWidth : this.popupWidth;
    if (jQuery.browser.msie) {
        actualWidth += 2;
    }
    var innerDivObj = this.containerObj.getElementsByTagName("DIV")[3];
    innerDivObj.style.width = actualWidth + 'px';

    this.divObj.style.width = actualWidth + 'px';
    var height = innerDivObj.scrollHeight + 3;

    if (innerDivObj.scrollWidth > innerDivObj.clientWidth) {
        height = height + multiSelectCollection.HORIZONT_SCROLLBAR_HEIGHT;
    }

    if (height >  multiSelectCollection.MAX_CONTAINER_HEIGHT) {
        height =  multiSelectCollection.MAX_CONTAINER_HEIGHT;
    } else {
        height = height + ((browserInfo.isIe)? 2 : 0);
    }

//    if (browserInfo.isIe) {
//        var strCssLink = '<link rel="stylesheet" type="text/css" href="' + this.cssFile + '" />';
//        var strCss_IEsummary_Link = ' <link rel="stylesheet" type="text/css" href="' + this.cssFile_IEsummary + '" />';
//
//        this.divObj.innerHTML = "<iframe id=\"multiSelectFrame\" name=\"multiSelectFrame\" src=\"javascript:'multiSelect'\" scrolling=\"no\" vspace=\"0\" hspace=\"0\" marginheight=\"0\" marginwidth=\"0\" frameborder=\"0\" width=\"100%\" height=\"100%\"></iframe>";
//        var iframeObj = this.divObj.getElementsByTagName("IFRAME")[0].contentWindow;
//        iframeObj.document.open();
//        iframeObj.document.write("<html><head>" + strCssLink + strCss_IEsummary_Link + "</head><body class='mselect'>" + html + "</body></html>");
//        iframeObj.document.close();
//        innerDivObj = iframeObj.document.getElementsByTagName("DIV")[0];
//        innerDivObj.style.width = actualWidth - 2;
//        iframeObj.frameElement.style.height = height + 2 + 'px';
//        iframeObj.frameElement.style.width = actualWidth + 'px';
//    }
    innerDivObj.style.height = height + 'px';

    this.divObj.style.visibility = "visible";
    this.state = 1;
    this.oldValue = this.inputObj.value;
    __summaryMultiSelect_registerEvents();
    
    if (this.showRadioButtons === 'Y') {
        __summaryMultiSelect_initRadioButtonTree(this.containerId);
    }    
    return true;
}

function __summaryMultiSelect_doFilter(control) {
    var $val = control.val().toUpperCase();
    var $parent = control.parents('.mselect-box').first();
    var $rows = $parent.find('td.data');
    for (var i = 0; i < $rows.length; i++) {
        if ($rows[i].innerText.toUpperCase().indexOf($val) > -1) {
            jQuery($rows[i]).parent().removeClass("display-hide");
            jQuery($rows[i]).parent().addClass("display-show");
        } else {
            jQuery($rows[i]).parent().removeClass("display-show");
            jQuery($rows[i]).parent().addClass("display-hide");
        }
    }
    var $visible = $parent.find('tr.display-show:not(#matches-not-found)');
    var $placeholder = $parent.find('#matches-not-found');
    if ($visible.length > 0) {
        $placeholder.removeClass("display-show");
        $placeholder.addClass("display-hide");
    } else {
        $placeholder.removeClass("display-hide");
        $placeholder.addClass("display-show");
    }
}

function __summaryMultiSelect_parseClick(e , obj) {
    srcEl = (e.target)?e.target:e.srcElement;
    var checkboxClick = false;
    var checkboxAll = document.getElementById("checkAllEntity");
    //if click on checkbox
    if (this.showCheckbox === 'Y' && srcEl.tagName === "INPUT") {
        checkboxClick = true;
        srcEl = srcEl.parentNode;
    }
    //if click on td
    if (srcEl.tagName === "TD") {
        srcEl = srcEl.parentNode;
    }
    //if click on radio
    if (this.showRadioButtons === 'Y' && !isNull(srcEl.previousSibling) && srcEl.previousSibling.type === "radio") {
    	srcEl = srcEl.parentNode.parentNode;
    }
    // var selectedArray = new Array();
    // var selectedNameArray = new Array();
    if (this.treatAll === 'Y' && obj.getElementsByTagName("TR")[0] == srcEl) {
        this.resetMultiSelect();
        return;
    }
    if (this.entities != null) {
        for (var i = 0; i < this.entities.length; i++) {
        	var elemCustomValue = "";
        	if (this.showRadioButtons === 'Y' && this.entities[i].radio === 'Y') {
        		elemCustomValue = this.entities[i].parent + this.entities[i].id;
        	} else {
        		elemCustomValue = this.entities[i].id;
        	}
            if (elemCustomValue == srcEl.getAttribute(multiSelectCollection.CUSTOM_VALUE_ATTR_NAME)) {
                var inputElem = srcEl.getElementsByTagName("INPUT")[0];
                if (this.showCheckbox === 'Y') {

                    if (!checkboxClick) {
                    	inputElem.checked = (inputElem.checked) ? false : true;
                    }
                    if (inputElem.type == "checkbox") {
                    	inputElem.className = (inputElem.checked) ? 'mselect modernCheckbox selected' : 'mselect modernCheckbox unselected';
                        if(checkboxAll && checkboxAll !== inputElem && checkboxAll.checked) {
                            checkboxAll.checked = false;
                            checkboxAll.className = 'mselect modernCheckbox unselected';
                        }
                    } else if (inputElem.type == "radio") {
                    	inputElem.className = (inputElem.checked) ? 'mselect modernRadio selected' : 'mselect modernRadio unselected';
                        if(checkboxAll && checkboxAll !== inputElem && checkboxAll.checked) {
                            checkboxAll.checked = false;
                            checkboxAll.className = 'mselect modernRadio unselected';
                        }
                    }
                }
                if (this.showRadioButtons === 'Y' && this.entities[i].radio === 'Y') {
                	var radioElems = document.querySelectorAll('[parent="' + this.entities[i].parent + '"]');
                	for (var j = 0; j < this.entities.length; j++) {
                		if (!isNull(this.entities[j].parent)) {
                			var currElem = this.entities[j].parent + this.entities[j].id;
                			for (var k = 0; k < radioElems.length; k++) {
                    			var nameAttrValue = radioElems[k].getAttribute("name");
                    			if (nameAttrValue == currElem) {
                    				this.entities[j].selected = false;
                    				radioElems[k].parentElement.parentElement.className = '';
                    				radioElems[k].className = 'mselect modernRadio unselected';
                    				radioElems[k].checked = false;
                    			}
                    		}
                		}                		
                	}
                	this.entities[i].selected = true;
                	srcEl.className = 'selected';
                	inputElem.className = 'mselect modernRadio selected';
                	inputElem.checked = true;
                } else {
                	if (!this.entities[i].selected) {
                        this.entities[i].selected = true;
                        srcEl.className = 'selected';
                        if (this.showRadioButtons === 'Y') {
				var checkedParentCheckboxes = jQuery(srcEl).attr("customvalue");
                            var elem = srcEl.nextElementSibling;
                            var firstInputElem = elem.getElementsByTagName("INPUT")[0];
                            if (firstInputElem.type == "radio") {
				                jQuery("input[type='radio'][parent='" + checkedParentCheckboxes +"']").parent().parent().show();
				                if (!__summaryMultiSelect_checkChildRadioButtons(checkedParentCheckboxes, this.entities)) {
							this.entities[i + 1].selected = true;
							elem.classname = 'selected';
					firstInputElem.className = 'mselect modernRadio selected';
					firstInputElem.checked = true;
				                }
                            } else if (firstInputElem.type == "checkbox") {
				                jQuery("input[type='checkbox'][parent='" + checkedParentCheckboxes +"'][class$=' unselected']").trigger("click");
                            }
                        }
                    //selectedArray[selectedArray.length] = this.entities[i].id;
                    //selectedNameArray[selectedNameArray.length] = this.entities[i].name;
                    } else {
                        this.entities[i].selected = false;
                        srcEl.className = '';
                        if (this.showRadioButtons === 'Y') {
				var checkedParentCheckboxes = jQuery(srcEl).attr("customvalue");
                            var elem = srcEl.nextElementSibling;
                            var flag = true;
                            var firstInputElem;
                            while (flag) {
				firstInputElem = elem.getElementsByTagName("INPUT")[0];
				if (firstInputElem.type == "radio") {
					var classAttr = firstInputElem.getAttribute("class");
					if (classAttr.endsWith(" unselected")) {
						elem = elem.nextElementSibling;
					} else {
						flag = false;
					}
				} else {
				                flag = false;
                                }
                            }
                            if (firstInputElem.type == "radio") {
				__summaryMultiSelect_clearChildRadioButtons(checkedParentCheckboxes, this.entities);
				elem.classname = '';
				firstInputElem.className = 'mselect modernRadio unselected';
				firstInputElem.checked = false;
                                jQuery("input[type='radio'][parent='" + checkedParentCheckboxes +"']").parent().parent().hide();
                            } else if (firstInputElem.type == "checkbox") {
				                jQuery("input[type='checkbox'][parent='" + checkedParentCheckboxes +"'][class$=' selected']").trigger("click");
                            }
                        }
                    }
                }
                
                if (this.showCategory === 'Y' && this.selectParentsAndChildren === 'Y') {
                    //if this is a category we need check all childs
                    if(this.entities[i].category != null && this.entities[i].category == 'Y' 
			            && srcEl.className !== "mselect" && srcEl.nextSibling.getElementsByTagName("INPUT")[0].type != "radio") {
                        this.checkAllChilds(this.entities[i].id, srcEl, inputElem.checked);
                    // else we need look for parent category and check it
                    } else if (this.entities[i].parent != null && this.childrenAffectParent === 'Y'){
                        this.checkParent(srcEl, this.entities[i], inputElem.checked);
                    }
                }
            } else {
	                if (this.entities[i].selected) {
	            //selectedArray[selectedArray.length] = this.entities[i].id;
	            //selectedNameArray[selectedNameArray.length] = this.entities[i].name;
	            }
            }
        }

        /*if (selectedArray.length > 0) {
                this.displayObj.value = selectedNameArray.join(multiSelectCollection.VALUE_DELIMITER);
            } else {
                this.displayObj.value = (this.treatAll == 'Y')?multiSelectCollection.ALL_ELEMENT_TITLE:'';
            }
            this.inputObj.value = selectedArray.join(multiSelectCollection.ID_DELIMITER);
            this.setSelectedHint(this.displayObj.value);*/

        //now we use method to show display string
        this.modifyDisplayObj();
    }
    if(this.customOnClick != null){
        this.customOnClick(this, srcEl, obj);
    }
}

function __summaryMultiSelect_setInputData(params) {
    var inputValue =  (params.value) ? params.value : null;
    var selectedArray;
    if (inputValue) {
	    if (!(this.showRadioButtons === 'Y')) {
	    	selectedArray = (inputValue)?inputValue.split(multiSelectCollection.ID_DELIMITER):null;
	    } else {
	    	selectedArray = inputValue;
	    }
    } else {
    	selectedArray = null;
    }
    this.entities = (params.list) ? params.list : null;
    if (!(this.showRadioButtons === 'Y')) {
    	return this.parseSelected(selectedArray);
    } else {
    	return this.parseRadioSelected(selectedArray);
    }
}

function __summaryMultiSelect_setList(list) {
    __summaryMultiSelect_closeMultiSelect(this.containerId, true);
    var inputValue =  this.inputObj.value;
    var selectedArray;
    if (inputValue) {
	    if (!(this.showRadioButtons === 'Y')) {
	    	selectedArray = (inputValue)?inputValue.split(multiSelectCollection.ID_DELIMITER):null;
	    } else {
	    	selectedArray = inputValue;
	    }
    } else {
    	selectedArray = null;
    }
    this.entities = list;
    if (!(this.showRadioButtons === 'Y')) {
    	return this.parseSelected(selectedArray);
    } else {
    	return this.parseRadioSelected(selectedArray);
    }
}

function __summaryMultiSelect_checkAllChilds(parentId, element, checkboxClick){
    for (var i = 0; i < this.entities.length; i++) {
        if (this.entities[i].parent != null && this.entities[i].parent == parentId){
            this.checkChild(element, this.entities[i], checkboxClick);
        }
    }
}

function __summaryMultiSelect_checkParent(element, entity, isChecked) {
    var srcEl = element;
    // var checkbox = srcEl.getElementsByTagName("INPUT")[0];
    var isRadio = element.getElementsByTagName("INPUT")[0].type == "radio";
    var parent = this.getCheckbox(srcEl, entity.parent);
    if (isChecked) {
        this.selectParent(entity.parent, parent);
        if (isRadio) {
        	var prevElem = srcEl.previousElementSibling;
        	while (isRadio == true) {
        		if (prevElem.getElementsByTagName("INPUT")[0].type == "radio") {
        			prevElem = prevElem.previousElementSibling;
        		} else {
        			isRadio = false;
        		}
        	}
        	var grandParent = this.getCheckbox(prevElem, prevElem.getElementsByTagName("INPUT")[0].getAttribute("parent"));
        	this.selectParent(prevElem.getElementsByTagName("INPUT")[0].getAttribute("parent"), grandParent);
        }
    } else {
	if (!(this.showRadioButtons === 'Y' && jQuery("input[type='checkbox'][parent='" + entity.parent +"'][class$=' selected']").length > 0)) {
		this.unSelectParent(entity.parent, parent);
	}
    }
}

function __summaryMultiSelect_isAllChildSelected(parentId) {
    var flag = true;
    for (var i = 0; i < this.entities.length; i++) {
        if (this.entities[i].parent != null && this.entities[i].parent == parentId) {
        	if (!this.entities[i].radio === 'Y') {
        		if (!this.entities[i].selected) {
                    flag = false;
                    break;
                }
        	} else {
        		if (this.entities[i].selected) {
                    flag = true;
                    break;
                }
        	}
        }
    }
    return flag;
}

function __summaryMultiSelect_selectParent(parentId, checkbox) {
	if (!isNull(parentId)) {
	    if (!this.isAllChildSelected(parentId)) {
	        return;
	    }
	    for (var i = 0; i < this.entities.length; i++) {
	        if (parentId == this.entities[i].id && 
	        		(this.entities[i].selected == false || isNull(this.entities[i].selected))) {
	            this.entities[i].selected = true;
	        }
	    }
	    if (checkbox.checked === false) {
	        checkbox.checked = true;
	        checkbox.className = 'mselect modernCheckbox selected';
	        checkbox.parentNode.parentNode.className = 'selected';
	    }
	}
	else {
		return;
	}
}

function __summaryMultiSelect_unSelectParent(parentId, checkbox) {
	if (!isNull(parentId)) {
	    for (var i = 0; i < this.entities.length; i++) {
	        if (parentId == this.entities[i].id && this.entities[i].selected == true) {
	            this.entities[i].selected = false;
	        }
	    }
	    if (checkbox.checked === true) {
	        checkbox.checked = false;
	        checkbox.className = 'mselect modernCheckbox unselected';
	        checkbox.parentNode.parentNode.className = '';
	    }
	} else {
		return;
	}
}

function __summaryMultiSelect_getCheckbox(element, id) {
    var srcEl = element;
    var parent = null;
    var flag = false;
    while(srcEl.tagName !== 'TABLE') {
        srcEl = srcEl.parentNode;
    }
    var tbody = srcEl.childNodes;
    var trs = tbody[0].childNodes;
    for (var i = 0 ; i < trs.length; i++) {
        var tds = trs[i].childNodes;
        for (var j = 0; j < tds.length; j++) {
            if (tds[j].hasChildNodes()) {
                if(tds[j].childNodes[0].tagName === 'INPUT') {
                    if (id == tds[j].childNodes[0].getAttribute('id')) {
                        parent = tds[j].childNodes[0];
                        flag = true;
                        break;
                    }
                }
            }
        }
        if (flag) {
            break;
        }
    }
    return parent;
}

function __summaryMultiSelect_modifyDisplayObj() {
    var selectedArray = new Array();
    var selectedArrayStr = "";
    for (var i = 0; i < this.entities.length; i++) {
    	var element = this.entities[i];
        if (element.selected) {
        	selectedArray[selectedArray.length] = element.id;
            if (this.showRadioButtons == "Y") {
	            if (element.radio == "Y") {
	            	selectedArrayStr = selectedArrayStr.concat(element.id).concat(multiSelectCollection.RADIO_DELIMITER);
	            } else if (!isNull(element.parent)) {
	            	selectedArrayStr = selectedArrayStr.concat(element.parent).concat(multiSelectCollection.ID_DELIMITER);
	            	selectedArrayStr = selectedArrayStr.concat(element.id).concat(multiSelectCollection.ID_DELIMITER);
	            } else if (!isNull(this.entities[i + 1]) && this.entities[i + 1].radio == "Y") {
	            	selectedArrayStr = selectedArrayStr.concat(element.id).concat(multiSelectCollection.ID_DELIMITER);
	            }
        	}
        }
    }
    if (selectedArray.length > 0) {
        this.displayObj.value = this.getDisplayValue();
    } else {
        this.displayObj.value = (this.treatAll === 'Y')?multiSelectCollection.ALL_ELEMENT_TITLE:'';
    }
    if (isNull(selectedArrayStr)) {
    	this.inputObj.value = selectedArray.join(multiSelectCollection.ID_DELIMITER);
    } else {
    	this.inputObj.value = selectedArrayStr;
    }    
    this.setSelectedHint(this.displayObj.value);
}

function __summaryMultiSelect_getDisplayValue(){
    var selectedNames = '';
    var currentCategory = '';
    var selectedInCategory = '';
    var hasSelected = false;
    for (var i = 0; i < this.entities.length; i++) {
        if(this.entities[i].category === 'Y' && this.showWithoutSubcategories !== 'Y'){
            if(hasSelected){
                selectedNames += currentCategory;
                selectedNames += selectedInCategory + multiSelectCollection.CATEGORIES_DELIMITER;
            }
            currentCategory = this.entities[i].name + multiSelectCollection.CATEGORY_NAME_DELIMITER;
            hasSelected = false;
            selectedInCategory = '';
        }else if (this.entities[i].selected) {
            selectedInCategory += selectedInCategory !== '' ? multiSelectCollection.VALUE_DELIMITER : '';
            selectedInCategory += this.entities[i].name;
            hasSelected = true;
        }
    }
    if(hasSelected){
        selectedNames += currentCategory;
        selectedNames += selectedInCategory + multiSelectCollection.CATEGORIES_DELIMITER;
    }
    return selectedNames;
}

function __summaryMultiSelect_checkChild(element, entity, isChecked) {
    // var srcEl = element;
    //var checkbox = srcEl.getElementsByTagName("INPUT")[0];
    var child = this.getCheckbox(element, entity.id);
    entity.selected = isChecked;
    child.checked = (isChecked) ? true : false;
    child.className = (isChecked) ? 'mselect modernCheckbox selected' : 'mselect modernCheckbox unselected';
    child.parentNode.parentNode.className = (isChecked) ? 'selected' : '';
}

function __summaryMultiSelect_parseSelected(selectedArray) {
    if (this.entities != null) {
        if (selectedArray != null && selectedArray.length > 0) {
            var correctedIdArray = new Array();
            var correctedNameArray = new Array();
            var i = 0;
            while (i < this.entities.length && selectedArray.length > 0) {
                var last;
                var j = selectedArray.length - 1;
                while (j >= 0) {
                    if (this.entities[i].id == selectedArray[j]) {
                        this.entities[i].selected = true;
                        correctedIdArray[correctedIdArray.length] = this.entities[i].id;
                        correctedNameArray[correctedNameArray.length] = this.entities[i].name;
                        last = selectedArray[selectedArray.length - 1];
                        selectedArray[j] = last;
                        selectedArray[selectedArray.length - 1] = null;
                        selectedArray.length--;
                        j--;
                        break;
                    } else {
                        this.entities[i].selected = false;
                    }
                    j--;
                }
                i++;
            }
            for (var k = i; k < this.entities.length; k++) {
                this.entities[k].selected = false;
            }
            if (correctedIdArray.length === 0 && this.treatAll === 'Y') {
                this.displayObj.value = multiSelectCollection.ALL_ELEMENT_TITLE;
            } else {
                this.displayObj.value = this.getDisplayValue();
            }
            this.inputObj.value = correctedIdArray.join(multiSelectCollection.ID_DELIMITER);
        } else {
            this.displayObj.value = (this.treatAll === 'Y')?multiSelectCollection.ALL_ELEMENT_TITLE:'';
            this.inputObj.value = '';
            for (var i = 0; i < this.entities.length; i++) {
                this.entities[i].selected = false;
            }
        }
    } else {
        this.displayObj.value = (this.treatAll === 'Y')?multiSelectCollection.ALL_ELEMENT_TITLE:'';
        this.inputObj.value = '';
    }
    this.oldValue = this.inputObj.value;
    this.setSelectedHint(this.displayObj.value);
    return true;
}

function __summaryMultiSelect_parseRadioSelected(selectedArray) {
    if (this.entities != null) {
        if (selectedArray != null && selectedArray.length > 0) {
            var correctedIdArray = new Array();
            var correctedNameArray = new Array();
            var inputArray = new Array(); 
            if (selectedArray.constructor === Array) {
            	inputArray = selectedArray;
            } else {
            	inputArray = selectedArray.split(multiSelectCollection.RADIO_DELIMITER);
            }
            var i = 0;
            for (i = 0; i < inputArray.length; i++) {
            	var radioElem = inputArray[i].split(multiSelectCollection.ID_DELIMITER);
            	var category = radioElem[0];
            	var subcategory = null;
            	var radio;
            	if (radioElem.length > 2) {
            		subcategory = radioElem[1];
            		radio = radioElem[2];
            	} else {
            		radio = radioElem[1];
            	}
            	for (j = 0; j < this.entities.length; j++) {
            		var isSelected = false;
            		var entityElem = this.entities[j];
            		var id = entityElem.id;
            		if ((id == category) || (id == subcategory) 
            				|| (entityElem.radio == "Y" && radio == id 
            					 && (entityElem.parent == category || entityElem.parent == subcategory))) {
            			isSelected= true;
            		}
            		if (isSelected == true) {
            			this.entities[j].selected = true;
                        correctedNameArray[correctedNameArray.length] = this.entities[j].name;
                    }
            	}
            }
            if (correctedNameArray.length === 0 && this.treatAll === 'Y') {
                this.displayObj.value = multiSelectCollection.ALL_ELEMENT_TITLE;
            } else {
                this.displayObj.value = this.getDisplayValue();
            }
            if (selectedArray.constructor === Array) {
                this.inputObj.value = selectedArray.join(multiSelectCollection.RADIO_DELIMITER);
            } else {
                this.inputObj.value = selectedArray;
            }
        } else {
            this.displayObj.value = (this.treatAll === 'Y')?multiSelectCollection.ALL_ELEMENT_TITLE:'';
            this.inputObj.value = '';
            for (var i = 0; i < this.entities.length; i++) {
                this.entities[i].selected = false;
            }
        }
    } else {
        this.displayObj.value = (this.treatAll === 'Y')?multiSelectCollection.ALL_ELEMENT_TITLE:'';
        this.inputObj.value = '';
    }
    this.oldValue = this.inputObj.value;
    this.setSelectedHint(this.displayObj.value);
    return true;
}

function __summaryMultiSelect_setValue(value)
{
    if (this.entities == null || this.entities.length === 0) {
        return false;
    }
    var selectedArray;
    if (value) {
	    if (!(this.showRadioButtons === 'Y')) {
	    	selectedArray = (value)?value.split(multiSelectCollection.ID_DELIMITER):null;
	    } else {
	    	selectedArray = (value)?value.split(multiSelectCollection.RADIO_DELIMITER):null;
            if (value.charAt(value.length - 1) == multiSelectCollection.RADIO_DELIMITER) {
                selectedArray.pop();
            }
	    }
    } else {
    	selectedArray = null;
    }
    if (!(this.showRadioButtons === 'Y')) {
    	return this.parseSelected(selectedArray);
    } else {
    	return this.parseRadioSelected(selectedArray);
    }
}

function __summaryMultiSelect_getValue()
{
    if (this.entities == null || this.entities.length === 0) {
        return '';
    }
    return this.inputObj.value;
}


function __summaryMultiSelect_processOnOut(i)
{
    multiSelectCollection[i].timer = setTimeout('__summaryMultiSelect_closeMultiSelect("' + i +'");', multiSelectCollection.HIDE_TIMEOUT);
}

function __summaryMultiSelect_processOnOver(i)
{
    __summaryMultiSelect_clearTimeout(i);
}

function __summaryMultiSelect_resetMultiSelect(withoutOnChange)
{
    if (this.disabled === 'Y') {
        return false;
    }
    if (this.entities != null) {
        for (var i = 0; i < this.entities.length; i++) {
            this.entities[i].selected = false;
        }
    }
    __summaryMultiSelect_closeMultiSelect(this.containerId, true);
    if (this.treatAll === 'Y') {
        this.displayObj.value = multiSelectCollection.ALL_ELEMENT_TITLE;
    } else {
        this.displayObj.value = '';
    }
    this.inputObj.value = '';
    this.setSelectedHint(this.displayObj.value);
    if (!withoutOnChange) {
        __summaryMultiSelect_acceptInputValue(this.containerId);
    }
    if (this.resetCustomOnClick != null) {
	this.resetCustomOnClick();
    }
    return true;
}

function __summaryMultiSelect_close(e)
{
    srcEl = (e && e.target) ? e.target : e.srcElement;
    if (browserInfo.isOpera && !srcEl.parentNode && srcEl.tagName !== "HTML") {
        return false;
    }
    for (var i in multiSelectCollection) {
        if (multiSelectCollection[i].containerType == multiSelectCollection.CONTAINER_TYPE && multiSelectCollection[i].state) {
            var closeMultiSelect = true;
            do {
                if (srcEl == multiSelectCollection[i].containerObj) {
                    closeMultiSelect = false;
                    break;
                }
            } while (srcEl = srcEl.parentNode);
            if (closeMultiSelect) {
                __summaryMultiSelect_closeMultiSelect(i);
            }
        }
    }
}

function __summaryMultiSelect_closeAll()
{
    for (var i in multiSelectCollection) {
        if (multiSelectCollection[i].containerType == multiSelectCollection.CONTAINER_TYPE && multiSelectCollection[i].state) {
            __summaryMultiSelect_closeMultiSelect(i);
        }
    }
    __summaryMultiSelect_unRegisterEvents();
}

function __summaryMultiSelect_closeMultiSelect(i, withoutOnChange)
{
    if (multiSelectCollection[i].state) {
        __summaryMultiSelect_clearTimeout(i);
        multiSelectCollection[i].divObj.style.display = "none";
        multiSelectCollection[i].state = 0;
        if (!withoutOnChange) {
            __summaryMultiSelect_acceptInputValue(i);
        }
    }
    multiSelectCollection[i].divObj.innerHTML = "";
    __summaryMultiSelect_unRegisterEvents();
}

function __summaryMultiSelect_clearTimeout(i)
{
    if (multiSelectCollection[i].timer) {
        clearTimeout(multiSelectCollection[i].timer);
        multiSelectCollection[i].timer = null;
    }
}

function __summaryMultiSelect_getTop()
{
    var _element = this.containerObj;//this.displayObj;
    var y = 0;
    while (_element != null && _element.style.position !== 'absolute')
    {
        if (_element.nodeName !== 'BODY' && _element.nodeName !== 'IMG')
        {
            y += _element.offsetTop - _element.scrollTop;
        }
        else
        {
            y += _element.offsetTop;
        }
        _element = _element.offsetParent;
    }
    return y;
}

function __summaryMultiSelect_getLeft()
{
    var _element = this.containerObj;//this.displayObj;
    var x = 0;
    while (_element != null && _element.style.position !== 'absolute')
    {
        if (_element.nodeName !== 'BODY' && _element.nodeName !== 'IMG')
        {
            x += _element.offsetLeft - _element.scrollLeft;
        }
        else
        {
            x += _element.offsetLeft;
        }
        _element = _element.offsetParent;
    }

    return x;
}

function __summaryMultiSelect_addEvent(obj, evType, fn) {
    if (obj.addEventListener) {
        obj.addEventListener(evType, fn, false);
        return true;
    } else if (obj.attachEvent) {
        var r = obj.attachEvent("on" + evType, fn);
        return r;
    } else {
        alert("Handler could not be attached");
    }
}

function __summaryMultiSelect_removeEvent(obj, evType, fn) {
    if (obj.removeEventListener) {
        obj.removeEventListener(evType, fn, false);
        return true;
    } else if (obj.detachEvent) {
        var r = obj.detachEvent("on" + evType, fn);
        return r;
    } else {
        alert("Handler could not be detached");
    }
}

function __summaryMultiSelect_registerEvents() {
    __summaryMultiSelect_addEvent(window, 'unload', __summaryMultiSelect_unRegisterEvents);
    __summaryMultiSelect_addEvent(document, 'click', __summaryMultiSelect_close);
    //__summaryMultiSelect_addEvent(window, 'scroll', __summaryMultiSelect_closeAll);
    __summaryMultiSelect_addEvent(window, 'resize', __summaryMultiSelect_closeAll);
}

function __summaryMultiSelect_unRegisterEvents() {
    __summaryMultiSelect_removeEvent(document, 'click', __summaryMultiSelect_close);
    //__summaryMultiSelect_removeEvent(window, 'scroll', __summaryMultiSelect_closeAll);
    __summaryMultiSelect_removeEvent(window, 'resize', __summaryMultiSelect_closeAll);
    __summaryMultiSelect_removeEvent(window, 'unload', __summaryMultiSelect_unRegisterEvents);
}

function __summaryMultiSelect_createEvent(i, eventType) {
    if (browserInfo.isIe) {
      return multiSelectCollection[i].inputObj.fireEvent(eventType);
    } else {
// todo implement for other browsers
   var eventT = document.createEvent("UIEvents");
   eventT.initEvent("change", true, true);
   return multiSelectCollection[i].inputObj.dispatchEvent(eventT);
}
}

function __summaryMultiSelect_acceptInputValue(i) {
    if (multiSelectCollection[i].oldValue != multiSelectCollection[i].inputObj.value) {
        multiSelectCollection[i].oldValue = multiSelectCollection[i].inputObj.value;
        __summaryMultiSelect_createEvent(i, 'onChange');
    }
}

function __summaryMultiSelect_setSelectedHint(value) {
    if (!this.inputObj.value) {
        this.containerObj.getElementsByTagName("input")[0].title = multiSelectCollection.LOOKUP_HINT;
    } else {
        this.containerObj.getElementsByTagName("input")[0].title = value;
    }
}

function __summaryMultiSelect_initRadioButtonTree(containerId) {
	jQuery("div[id='" + containerId + "'] #multiSelectTable " +
		"input[type='checkbox']:checked").parent().parent().each(function (i) {
        var checkedParentCheckboxes = jQuery(this).attr("customvalue");
        jQuery("input[type='radio'][parent='" + checkedParentCheckboxes +"']").parent().parent().show();
	});
	jQuery("div[id='" + containerId + "'] " +
		"#multiSelectTable input[type='checkbox']:not(:checked)").parent().parent().each(function (i) {
        var notCheckedParentCheckboxes = jQuery(this).attr("customvalue");
        jQuery("input[type='radio'][parent='" + notCheckedParentCheckboxes +"']").parent().parent().hide();
	});
}

function __summaryMultiSelect_clearChildRadioButtons(parentId, entities) {
	for (var i = 0; i < entities.length; i++) {
		var entity = entities[i];
		if (entity.parent == parentId && entity.radio === 'Y') {
			entity.selected = false;
		}
	}
}

function __summaryMultiSelect_checkChildRadioButtons(parentId, entities) {
	for (var i = 0; i < entities.length; i++) {
		var entity = entities[i];
		if (entity.parent == parentId && entity.radio === 'Y' && entity.selected == true) {
			return true;
		}
	}
	return false;
};var infoQtip = {
    position: {
        my: 'left center',
        at: 'center right',
        adjust: {
            x: 10
        }
    },
    content: 'Info tip',
    style: {
        tip: true,
        classes: 'qtip-light qtip-shadow'
    },
    hide: {
        fixed: true,
        delay: 300
    }
};

function initQTip(target, hint, position, width, noTip) {
    var $target = jQuery(target);
    var $hint = jQuery(hint);
    position = typeof position === 'undefined' ? { my: 'top center', at: 'bottom center'} : position;
    width = typeof width === 'undefined' ? { min: 50, max: 280 } : width;
    var options = {
        position: position,
        content: $hint.html(),
        style: { classes: 'qtip-light qtip-shadow', width: width },
        hide: {
            fixed: true,
            delay: 300
        }
    };
    if (!!noTip) {
        options.style.tip = 'center';
    }
    $target.qtip(options);
    $target.mouseleave(function() {
        if ($target.hasMouseLeave) {
            return;
        } else {
            $target.hasMouseLeave = true;
            jQuery('.menu-item').click(function() {
                jQuery('[data-hasqtip]').qtip('hide');
            });
        }
    });
}

function initQTipWithStyles(target, hint, position, width,cssClasses) {
    var $target = jQuery(target);
    var $hint = jQuery(hint);
    position = typeof position === 'undefined' ? { my: 'top center', at: 'bottom center'} : position;
    width = typeof width === 'undefined' ? { min: 50, max: 1000 } : width;
    $target.qtip({
        position: position,
        content: $hint.html(),
        style: { classes: 'qtip-light qtip-shadow '+cssClasses, width: width },
        hide: {
            fixed: true,
            delay: 300
        }
    });
}

function initQTipOnclick(target, hint, position, width) {
    var $target = jQuery(target);
    var $hint = jQuery(hint);
    position = typeof position === 'undefined' ? { my: 'top center', at: 'bottom center'} : position;
    width = typeof width === 'undefined' ? { min: 50, max: 280 } : width;
    $target.qtip({
        position: position,
        content: $hint.html(),
        style: { classes: 'qtip-light qtip-shadow', width: width },
        show: {
            event: 'click'
        },
        hide: {
            fixed: true,
            delay: 300
        }
    });
    $target.mouseleave(function() {
        if ($target.hasMouseLeave) {
            return;
        } else {
            $target.hasMouseLeave = true;
            jQuery('.menu-item').click(function() {
                jQuery('[data-hasqtip]').qtip('hide');
            });
        }
    });
}

function initQTipByValue(target, value, position, width) {
    jQuery(target).data('tip', value);
    initQTipByData(target, position, width);
}

function initQTipByData(target, position, width) {
    var $target = jQuery(target);
    position = typeof position === 'undefined' ? {
        my: 'left center',
        at: 'right center',
        adjust: {
            x: 25,
            y: -5
        }
    } : position;
    width = typeof width === 'undefined' ? {
        min: 50,
        max: 280
    } : width;
    var statusAsterisk = jQuery.extend({}, infoQtip, {
        position: position,
        style: { classes: 'qtip-light qtip-shadow', width: width }
    });

    statusAsterisk.content = $target.data('tip');

    $target.qtip(statusAsterisk);
};var TWO_COLUMN = TWO_COLUMN || {};

TWO_COLUMN.initialize = function () {
    TWO_COLUMN._defineEmptyRowChangeEvent();
};

TWO_COLUMN._defineEmptyRowChangeEvent = function () {
    var $collapseStateEl = jQuery(".tc-collapse-row-state");
    $collapseStateEl.click(function () {
        if ($collapseStateEl.data("stateName") === "collapsed") {
            TWO_COLUMN.showEmptyRow();
            $collapseStateEl.find('.group-buttons-text').text('Hide empty fields');
            $collapseStateEl.find('.group-buttons-icon i').attr('class', 'icon-eye-close');
        } else {
            TWO_COLUMN.hideEmptyRow();
            $collapseStateEl.find('.group-buttons-text').text('Show empty fields');
            $collapseStateEl.find('.group-buttons-icon i').attr('class', 'icon-eye-open');
        }
    });
};

TWO_COLUMN.hideEmptyRow = function () {
    var $collapseStateEl = jQuery(".tc-collapse-row-state");
    $collapseStateEl.data("stateName", "collapsed");
    jQuery(".tc-property-table > tbody > tr > td").each(function () {
        var $td = jQuery(this);
        if ($td.html().trim() === '') {
            $td.parent().css("background-color", "#FEE5E5").hide();
        }
    });
};

TWO_COLUMN.showEmptyRow = function () {
    var $collapseStateEl = jQuery(".tc-collapse-row-state");
    $collapseStateEl.data("stateName", "expanded");
    jQuery(".tc-property-table tbody > tr > td").each(function () {
        var $td = jQuery(this);
        if ($td.html().trim() === '') {
            $td.parent().show();
        }
    });
};

TWO_COLUMN.changeEmptyRowState = function () {
    jQuery(".tc-property-table tbody > tr > td").each(function () {
        var $td = jQuery(this);
        if ($td.html().trim() === '') {
            $td.parent().show();
        }
    });
};

TWO_COLUMN.defineTbodyLight = function (tableClass) {
    jQuery("." + tableClass + " tbody").each(function () {
        var $this = jQuery(this);
        $this.hover(function () {
            $this.css("background-color", "#F5F5F5");
        }, function () {
            $this.css("background-color", "");
        });
    });
};/************************************************************************
*************************************************************************
@Name :       	jNotify - jQuery Plugin
@Revison :    	2.1
@Date : 		01/2011
@Author:     	ALPIXEL - (www.myjqueryplugins.com - www.alpixel.fr)
@Support:    	FF, IE7, IE8, MAC Firefox, MAC Safari
@License :		Open Source - MIT License : http://www.opensource.org/licenses/mit-license.php

**************************************************************************
*************************************************************************/
(function($){

	$.jNotify = {
		defaults: {
			/** VARS - OPTIONS **/
			autoHide : true,				// Notify box auto-close after 'TimeShown' ms ?
			clickOverlay : false,			// if 'clickOverlay' = false, close the notice box on the overlay click ?
			MinWidth : 200,					// min-width CSS property
			TimeShown : 1500, 				// Box shown during 'TimeShown' ms
			ShowTimeEffect : 200, 			// duration of the Show Effect
			HideTimeEffect : 200, 			// duration of the Hide effect
			LongTrip : 15,					// in pixel, length of the move effect when show and hide
			HorizontalPosition : 'right', 	// left, center, right
			VerticalPosition : 'bottom',	 // top, center, bottom
			ShowOverlay : true,				// show overlay behind the notice ?
			ColorOverlay : '#000',			// color of the overlay
			OpacityOverlay : 0.3,			// opacity of the overlay

			/** METHODS - OPTIONS **/
			onClosed : null,
			onCompleted : null
		},

		/*****************/
		/** Init Method **/
		/*****************/
		init:function(msg, options, id) {
			opts = $.extend({}, $.jNotify.defaults, options);

			/** Box **/
			if($("#"+id).length == 0)
				$Div = $.jNotify._construct(id, msg);

			// Width of the Brower
			WidthDoc = parseInt($(window).width());
			HeightDoc = parseInt($(window).height());

			// Scroll Position
			ScrollTop = parseInt($(window).scrollTop());
			ScrollLeft = parseInt($(window).scrollLeft());

			// Position of the jNotify Box
			posTop = $.jNotify.vPos(opts.VerticalPosition);
			posLeft = $.jNotify.hPos(opts.HorizontalPosition);

			// Show the jNotify Box
			if(opts.ShowOverlay && $("#jOverlay").length == 0)
				$.jNotify._showOverlay($Div);

			$.jNotify._show(msg);
		},

		/*******************/
		/** Construct DOM **/
		/*******************/
		_construct:function(id, msg) {
			$Div =
			$('<div id="'+id+'"/>')
			.css({opacity : 0,minWidth : opts.MinWidth})
			.html(msg)
			.appendTo('body');
			return $Div;
		},

		/**********************/
		/** Postions Methods **/
		/**********************/
		vPos:function(pos) {
			switch(pos) {
				case 'top':
					var vPos = ScrollTop + parseInt($Div.outerHeight(true)/2);
					break;
				case 'center':
					var vPos = ScrollTop + (HeightDoc/2) - (parseInt($Div.outerHeight(true))/2);
					break;
				case 'bottom':
					var vPos = ScrollTop + HeightDoc - parseInt($Div.outerHeight(true));
					break;
			}
			return vPos;
		},

		hPos:function(pos) {
			switch(pos) {
				case 'left':
					var hPos = ScrollLeft;
					break;
				case 'center':
					var hPos = ScrollLeft + (WidthDoc/2) - (parseInt($Div.outerWidth(true))/2);
					break;
				case 'right':
					var hPos = ScrollLeft + WidthDoc - parseInt($Div.outerWidth(true));
					break;
			}
			return hPos;
		},

		/*********************/
		/** Show Div Method **/
		/*********************/
		_show:function(msg) {
			$Div
			.css({
				top: posTop,
				left : posLeft
			});
			switch (opts.VerticalPosition) {
				case 'top':
					$Div.animate({
						top: posTop + opts.LongTrip,
						opacity:1
					},opts.ShowTimeEffect,function(){
						if(opts.onCompleted) opts.onCompleted();
					});
					if(opts.autoHide)
						$.jNotify._close();
					else
						$Div.css('cursor','pointer').click(function(e){
							$.jNotify._close();
						});
					break;
				case 'center':
					$Div.animate({
						opacity:1
					},opts.ShowTimeEffect,function(){
						if(opts.onCompleted) opts.onCompleted();
					});
					if(opts.autoHide)
						$.jNotify._close();
					else
						$Div.css('cursor','pointer').click(function(e){
							$.jNotify._close();
						});
					break;
				case 'bottom' :
					$Div.animate({
						top: posTop - opts.LongTrip,
						opacity:1
					},opts.ShowTimeEffect,function(){
						if(opts.onCompleted) opts.onCompleted();
					});
					if(opts.autoHide)
						$.jNotify._close();
					else
						$Div.css('cursor','pointer').click(function(e){
							$.jNotify._close();
						});
					break;
			}
		},

		_showOverlay:function(el){
			var overlay =
			$('<div id="jOverlay" />')
			.css({
				backgroundColor : opts.ColorOverlay,
				opacity: opts.OpacityOverlay
			})
			.appendTo('body')
			.show();

			if(opts.clickOverlay)
			overlay.click(function(e){
				e.preventDefault();
				opts.TimeShown = 0;
				$.jNotify._close();
			});
		},


		_close:function(){
				switch (opts.VerticalPosition) {
					case 'top':
						if(!opts.autoHide)
							opts.TimeShown = 0;
						$Div.stop(true, true).delay(opts.TimeShown).animate({
							top: posTop-opts.LongTrip,
							opacity:0
						},opts.HideTimeEffect,function(){
							$(this).remove();
							if(opts.ShowOverlay && $("#jOverlay").length > 0)
								$("#jOverlay").remove();
								if(opts.onClosed) opts.onClosed();
						});
						break;
					case 'center':
						if(!opts.autoHide)
							opts.TimeShown = 0;
						$Div.stop(true, true).delay(opts.TimeShown).animate({
							opacity:0
						},opts.HideTimeEffect,function(){
							$(this).remove();
							if(opts.ShowOverlay && $("#jOverlay").length > 0)
								$("#jOverlay").remove();
								if(opts.onClosed) opts.onClosed();
						});
						break;
					case 'bottom' :
						if(!opts.autoHide)
							opts.TimeShown = 0;
						$Div.stop(true, true).delay(opts.TimeShown).animate({
							top: posTop+opts.LongTrip,
							opacity:0
						},opts.HideTimeEffect,function(){
							$(this).remove();
							if(opts.ShowOverlay && $("#jOverlay").length > 0)
								$("#jOverlay").remove();
								if(opts.onClosed) opts.onClosed();
						});
						break;
				}
		},

		_isReadable:function(id){
			if($('#'+id).length > 0)
				return false;
			else
				return true;
		}
	};

	/** Init method **/
	jNotify = function(msg,options) {
		if($.jNotify._isReadable('jNotify'))
			$.jNotify.init(msg,options,'jNotify');
	};

	jSuccess = function(msg,options) {
		if($.jNotify._isReadable('jSuccess'))
			$.jNotify.init(msg,options,'jSuccess');
	};

	jError = function(msg,options) {
		if($.jNotify._isReadable('jError'))
			$.jNotify.init(msg,options,'jError');
	};
})(jQuery);/************************************************************************
*************************************************************************
@Name :       	jNotify - jQuery Plugin
@Revison :    	2.1
@Date : 		18/01/2011
@Author:     	 ALPIXEL (www.myjqueryplugins.com - www.alpixel.fr)
@Support:    	 FF, IE7, IE8, MAC Firefox, MAC Safari
@License :		 Open Source - MIT License : http://www.opensource.org/licenses/mit-license.php

**************************************************************************
*************************************************************************/
