// Simplified a lot by Maksim Golunko, 2013
// Dropped error handling and passing all results to await function, added possibility to set tasks in front of current queue

/*
 API Reference

 queue([parallelism])
 Constructs a new queue with the specified parallelism. If parallelism is not specified, the queue has infinite parallelism.
 Otherwise, parallelism is a positive integer. For example, if parallelism is 1, then all tasks will be run in series.
 If parallelism is 3, then at most three tasks will be allowed to proceed concurrently; this is useful,
 for example, when loading resources in a web browser.

 queue.defer(method[, arguments…])
 Adds the specified method to the queue, with any optional arguments.
 The method is called with the optional arguments and a final callback argument, which should be called when the task has finished.

 queue.await(callback)
 Sets the callback to be notified when all deferred tasks have finished.
 If await is used, each result is passed as a separate argument; if awaitAll is used, the entire array of results is passed as a single argument.

 Callbacks
 The callbacks follow the Node.js convention where the first argument is an optional error object,
 and the second is used to pass on the result of an operation. Queue.js does not directly support
 asynchronous functions that return multiple results; however, you can homogenize such functions by
 wrapping them and converting multiple results into a single object or array.
 */

function Queue(parallelism) {
    var queue = {},
        active = 0,
        remaining = 0,
        head = null,
        tail = null,
        await = jQuery.noop;

    if (arguments.length < 1) {
        parallelism = Infinity;
    }

    queue.isEmpty = function () {
        return head === null;
    };

    queue.first = function () {
        var node = arguments;
        if (head) {
            node.next = head;
            head = node;
        } else {
            head = tail = node;
        }
        ++remaining;
        pop();
        return queue;
    };

    queue.last = function () {
        var node = arguments;
        if (tail) {
            tail.next = node, tail = tail.next;
        } else {
            head = tail = node;
        }
        ++remaining;
        pop();
        return queue;
    };

    queue.await = function (f) {
        await = f;
        if (!remaining) {
            notify();
        }
        return queue;
    };

    queue.clearQueue = function () {
        remaining = 0;
        head = tail = null;
        notify();
        return queue;
    };

    function pop() {
        if (head && active < parallelism) {
            var node = head,
                f = node[0],
                a = Array.prototype.slice.call(node, 1);
            if (head === tail) {
                head = tail = null;
            } else {
                head = head.next;
            }
            ++active;
            a.push(function () {
                --active;
                if (--remaining) {
                    pop();
                } else {
                    notify();
                }
            });
            f.apply(null, a);
        }
    }

    function notify() {
        await();
    }

    return queue;
};
var calendarCollection = [];

var isIe10 = jQuery.browser.msie && (jQuery.browser.version == 10);

// For calculate correct height of calendar. Constants correct for current font 10.
var calendarHeight;
var columnHeight = 148;
var rowHeight = 20;
var headerHeight = 40;
var timeHeight = 29;

function createComponentCalendar(containerId)
{
    return (calendarCollection[containerId] = new classCalendar(containerId));
}

function deleteComponentCalendar(containerId) {
    delete calendarCollection[containerId];
}

function getComponentCalendar(containerId) {
    return calendarCollection[containerId];
}

function classCalendar(containerId)
{
    /* CONSTANTS is used for customization */
    var CLNDR_IMG_PATH = "images/";
    var CLNDR_IMG_BUTTON = "Calendar_icon.png";
    var CLNDR_IMG_BUTTON_DISABLED = "Calendar_icon_disabled.png";
    var CLNDR_IMG_CLOSE = "calendarClose.gif";

    /* SERVICE PROPERTIES */
    this.containerId = containerId;
    this.containerObj = null;
    this.state = 0; // 0 - closed (default); 1 - opened
    this.divOffsetX = 0; // left offset for Div with calendar for displaying in absolute div
    this.divOffsetY = 0; // top offset for Div with calendar for displaying in absolute div

    /* FORM FILED PROPERTIES */
    this.name = '';
    this.oldValue = ''; // contains the initial value of the field.
    this.id = '';

    /* DATE PROPERTIES */
    this.monthArray = new Array('January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December', 'Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec');
    this.dayNamesArray = new Array('Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Su', 'Mo', 'Tu', 'We', 'Th', 'Fr', 'Sa');
    this.datesArray = new Array(31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31);
    this.DEFAULT_DATE_FORMAT = "MMM d, yyyy";
    this.DEFAULT_TIME_FORMAT = "hh:mm:ssa";

    /* CSS FILE */
    this.cssFile = "css/calendar.css" + (appVersion ? "?curr_v=" + appVersion : "");

    /* IMAGE PROPERTIES */
    this.calendImg = CLNDR_IMG_PATH + CLNDR_IMG_BUTTON;
    this.calendImgDisabled = CLNDR_IMG_PATH + CLNDR_IMG_BUTTON_DISABLED;
    this.calendCloseImg = CLNDR_IMG_PATH + CLNDR_IMG_CLOSE;

    /* PARAMETERS OF DISPLAYING CALENDAR */
    this.daysOrder = ''; // H - horizontal (default); V - vertical
    this.startDay = 'M'; // S - sunday (default); M - monday

    this.changingDates = ''; // A - arrows (default); I - input fields
    this.holidaysSelection = ''; // N - disabled (default); Y - enabled
    this.pastDaysSelection = ''; // Y - enabled (default); N - disabled
    this.pastMonthDaysDisplay = ''; // N - no (default); Y - yes
    this.withTime = ''; // N - no (default); Y - yes
    this.withIntervals = ''; // N - no (default); Y - yes
    this.weekIntervalSelection = ''; // N - no (default); Y - yes
    this.width = null; // value of width of input field
    this.readonly = ''; // Y - yes (default); N - no
    this.closeOnSelection = ''; // N - no (default); Y - yes
    this.dateFormat = ''; // The format string consists of the following abbreviations:
    /*Field        | Full Form          | Short Form
     -------------|--------------------|-----------------------
     Year         | yyyy (4 digits)    | yy (2 digits), y (2 or 4 digits)
     Month        | MMM (name or abbr.)| MM (2 digits), M (1 or 2 digits)
     | NNN (abbr.)        |
     Day of Month | dd (2 digits)      | d (1 or 2 digits)
     Day of Week  | EE (name)          | E (abbr)
     Hour (1-12)  | hh (2 digits)      | h (1 or 2 digits)
     Hour (0-23)  | HH (2 digits)      | H (1 or 2 digits)
     Hour (0-11)  | KK (2 digits)      | K (1 or 2 digits)
     Hour (1-24)  | kk (2 digits)      | k (1 or 2 digits)
     Minute       | mm (2 digits)      | m (1 or 2 digits)
     Second       | ss (2 digits)      | s (1 or 2 digits)
     AM/PM        | a                  |
     NOTE THE DIFFERENCE BETWEEN MM and mm! Month=MM, not mm!
     Examples:
     "MMM d, y" matches: January 01, 2000
     Dec 1, 1900
     Nov 20, 00
     "M/d/yy"   matches: 01/20/00
     9/2/00
     "MMM dd, yyyy hh:mm:ssa" matches: "January 01, 2000 12:30:45AM"
     */
    this.showCalendarImg = true;
    /*  Parameter for disable specified date.
        This parameter should by array of entities:
        {
            startDate: <Date to disable>
            hint: <Text for hint>
        }
    */
    this.disabledDates = [];


    /* METHODS */
    this.init = __calendar_init;
    this.render = __calendar_render;
    this.show = __calendar_show;
    this.setMonth = __calendar_setMonth;
    this.setYear = __calendar_setYear;
    this.setChoosenDate = __calendar_setChoosenDate;
    this.formatDate = __calendar_formatDate;
    this.getDateFromFormat = __calendar_getDateFromFormat;
    this.getTop = __calendar_getTop;
    this.getLeft = __calendar_getLeft;
    this.setDisabled = __calendar_setDisabled;
    this.setInputValue = __calendar_setInputValue;
    this.hideCalendarDiv = __calendar_hideCalendarDiv;
    this.setDivOffset = __calendar_setDivOffset;
    this.setParam = __calendar_update_param;
    this.getDisabledDateConfig = __calendar_disable_date_config;
    this._initial_params = {};

    /* EVENTS */
    this.onclick = null;
}

function __calendar_update_param(paramName, paramValue) {
    this._initial_params[paramName] = paramValue;
    this.init(this._initial_params);
}

function __calendar_init(params)
{
    this.containerObj = document.getElementById(this.containerId);
    this.name = params.name;
    this.value = (params.value) ? params.value : '';
    this.oldValue = this.value ;
    this.popupId = (params.popupId) ? params.popupId : '';
    this.id = (params.id) ? params.id : params.name;
    this.daysOrder = (params.daysOrder) ? params.daysOrder : 'H';
    params.startDay = 'M';
    this.startDay = (params.startDay) ? params.startDay : 'S';
    this.changingDates = (params.changingDates) ? params.changingDates : 'A';
    this.pastDaysSelection = (params.pastDaysSelection) ? params.pastDaysSelection : 'Y';
    this.pastMonthDaysDisplay = (params.pastMonthDaysDisplay) ? params.pastMonthDaysDisplay : 'N';
    this.holidaysSelection = (params.holidaysSelection) ? params.holidaysSelection : 'N';
    this.withTime = (params.withTime) ? params.withTime : 'N';
    this.withIntervals = (params.withIntervals) ? params.withIntervals : 'N';
    this.weekIntervalSelection = (params.weekIntervalSelection) ? params.weekIntervalSelection : 'N';
    this.width = (params.inputWidth) ? params.inputWidth : null;
    this.calendarWidth = (params.calendarWidth) ? params.calendarWidth : null;
    this.disabled = (params.disabled) ? params.disabled : 'N';
    this.placeholder = (params.placeholder) ? params.placeholder : null;
    this.readonly = (params.readonly) ? params.readonly : 'Y';
    this.closeOnSelection = (params.closeOnSelection) ? params.closeOnSelection : 'N';
    this.dateFormat = (params.dateFormat) ? params.dateFormat : this.DEFAULT_DATE_FORMAT;
    this.maxlength = (params.maxlength) ? params.maxlength : null;
    if (!params.dateFormat && this.withTime === 'Y') {
        this.dateFormat += ' ' + this.DEFAULT_TIME_FORMAT;
    }
    this.paramDate = new Date();
    this.choosenDate = new Date();
    if (this.withIntervals === 'Y') {
        this.startIntervalDate = new Date();
        this.endIntervalDate = new Date();
        this.waitForEndInterval = false;
        this.startEndDateDelimiter = (params.startEndDateDelimiter) ? params.startEndDateDelimiter : ' - ';
    }
    this.showCalendarImg = (typeof params.showCalendarImg !== "undefined") ? params.showCalendarImg : true;
    this.historyIconId = params.historyIconId;
    this.historyIconHintId = params.historyIconHintId;
    this.validPeriodStart = (params.validPeriodStart) ? params.validPeriodStart : null;
    var endDate = new Date();
    endDate.setFullYear(endDate.getFullYear() + 10);
    this.validPeriodEnd = (params.validPeriodEnd) ? params.validPeriodEnd : endDate;
    this.disabledDates = params.disabledDates || [];
    this.globalHintForDisabledDates = params.globalHintForDisabledDates || '';
    this._initial_params = params;
}

function __calendar_render()
{
    if (this.name === '')
    {
        alert('ERROR: NAME property is not defined!');
        this.containerObj.style.display = 'none';
        return false;
    }

    var html = '';
    var style = ((this.width) ? ' style="width:' + this.width + 'px;' : '') + '"';
    html += '<input type="text" autocomplete="off" name="' + this.name +
            '" id="' + this.id + '"' + style + ((this.readonly === 'Y' || this.disabled === 'Y') ? ' readonly=""' : '')
            + ' value="' + this.value + '"' + ((this.disabled === 'Y') ? 'disabled="true"' : '');
    if (this.disabled !== 'Y') {
        html += 'onclick="calendarCollection[\'' + this.containerId + '\'].show()"';
    }
    if (!!this.placeholder) {
        html += ' placeholder="' + this.placeholder.replace(/"/g, '\\"') + '"';
    }
    html += ((this.maxlength == null) ? '' : ' maxlength="' + this.maxlength + '"') + '>&nbsp;';
    if (this.showCalendarImg) {
        html += '<i id="' + this.id + '_img" class = "icon-calendar ' + ((this.disabled == 'Y') ? 'disabled-icon' : '') + '"  align="absmiddle" border="0" onclick=\'if (calendarCollection["' + this.containerId + '"].state) __calendar_closeCalendar("' + this.containerId + '"); else __calendar_show.call(calendarCollection["' + this.containerId + '"],event);return false\' style="cursor: ' + ((this.disabled == 'Y') ? 'default' : 'pointer') + '; ' + (isIe10 ? 'position: relative; ' : '') + '"></i>';

    }
    html += '<div style="display:none;position:absolute;" class="calendarLayout" style="width:' + this.calendarWidth + '"></div>';
    if (this.historyIconId != null && this.historyIconHintId != null) {
        html += '<i class="icon-book control-icon" style="margin-left: 5px;" id="' + this.historyIconId + '"></i>';
        html += '<div id="' + this.historyIconHintId + '" style="display: none;"></div>';
    }

    this.containerObj.className = "calendar input-group-wrapper";
    this.containerObj.innerHTML = html;
    
    this.inputObj = this.containerObj.getElementsByTagName("INPUT")[0];
    this.divObj = this.containerObj.getElementsByTagName("DIV")[0];
    
    var calendarObj = this;
    this.inputObj.addEventListener("change", function(event) {
        if (this.value) {
            var time = calendarObj.getDateFromFormat(this.value, calendarObj.dateFormat);
            
            var validStartTime = null;
            if (calendarObj.validPeriodStart) {
                validStartTime = calendarObj.getDateFromFormat(
                    calendarObj.formatDate(calendarObj.validPeriodStart, calendarObj.dateFormat), calendarObj.dateFormat);
            }
            
            var validEndTime = null;
            if (calendarObj.validPeriodEnd) {
                validEndTime = calendarObj.getDateFromFormat(
                    calendarObj.formatDate(calendarObj.validPeriodEnd, calendarObj.dateFormat), calendarObj.dateFormat);
            }

            var isCustomDisabled = !!calendarObj.getDisabledDateConfig((new Date(time)).setHours(0,0,0,0));

            if (time === 0 || (validStartTime !== null && time < validStartTime)
                    || (validEndTime !== null && time > validEndTime) || isCustomDisabled) {
                event.stopImmediatePropagation();
                this.value = calendarObj.oldValue;
            } else {
                calendarObj.oldValue = this.value;
            }
        }
    });
}

function __calendar_setDisabled(disabled) {
    var img = document.getElementById(this.id + '_img');
    var textField = document.getElementById(this.id);
    if (disabled) {
        this.disabled = 'Y';
        if (img) {
            img.src = this.calendImgDisabled;
            img.style.cursor = 'default';
            jQuery(img).addClass('disabled-icon');
        }
    } else {
        this.disabled = 'N';
        if (img) {
            img.src = this.calendImg;
            img.style.cursor = 'pointer';
            img.style.display = 'inline';
            jQuery(img).removeClass('disabled-icon');
        }
    }
    if (textField) {
        var field = jQuery("[id='" + this.id + "']");
        if (disabled) {
            field.prop("disabled", true);
        } else {
            field.prop("disabled", false);
        }
        if (this.width) {
            field.css("width", this.width);
        }
    }
}
// when calendar appears, everything disables
function __calendar_show()
{
    if (this.disabled === 'Y') {
        return;
    }
    if (!this.state) {
        if (this.inputObj.value !== '') {
            if (this.withIntervals === 'Y') {
                var inputValue = this.inputObj.value.split(this.startEndDateDelimiter);
                var startTimeFromInput = this.getDateFromFormat(inputValue[0], this.dateFormat);
                var endTimeFromInput = this.getDateFromFormat(inputValue[1], this.dateFormat);
                if (startTimeFromInput == 0 || endTimeFromInput == 0) {
                    alert('The date in input field is in wrong format. Please correct the date value.\nDate should be in format: ' + this.dateFormat + this.startEndDateDelimiter + this.dateFormat);
                    return false;
                }
                this.paramDate.setTime(endTimeFromInput);
                this.choosenDate.setTime(endTimeFromInput);
                this.startIntervalDate.setTime(startTimeFromInput);
                this.endIntervalDate.setTime(endTimeFromInput);
                this.waitForEndInterval = false;
            } else {
                var timeFromInput = this.getDateFromFormat(this.inputObj.value, this.dateFormat);
                if (timeFromInput == 0) {
                    this.paramDate = new Date();
                    this.choosenDate = new Date();
                } else {
                    this.paramDate.setTime(timeFromInput);
                    this.choosenDate.setTime(timeFromInput);
                }
            }
        } else {
            this.paramDate = new Date();
            this.choosenDate = new Date();
        }
    }
    var paramYear = this.paramDate.getFullYear();
    if (((paramYear % 4 === 0) && (paramYear % 100 !== 0)) || paramYear % 400 === 0) {
        this.datesArray[1] = 29; // Leap Year
    } else {
        this.datesArray[1] = 28;
    }
    var paramMonth = this.paramDate.getMonth();
    var paramDay = this.paramDate.getDate();
    var paramHours = this.paramDate.getHours();
    var paramMinutes = this.paramDate.getMinutes();
    var paramSeconds = this.paramDate.getSeconds();
    var choosenYear = this.choosenDate.getFullYear();
    var choosenMonth = this.choosenDate.getMonth();
    var choosenDay = this.choosenDate.getDate();
    var presentYear = new Date().getFullYear();
    var presentMonth = new Date().getMonth();
    var presentDay = new Date().getDate();

    if (this.withIntervals === 'Y') {
        var choosenStartIntervalYear = this.startIntervalDate.getFullYear();
        var choosenStartIntervalMonth = this.startIntervalDate.getMonth();
        var choosenStartIntervalDay = this.startIntervalDate.getDate();

        var choosenEndIntervalYear = this.endIntervalDate.getFullYear();
        var choosenEndIntervalMonth = this.endIntervalDate.getMonth();
        var choosenEndIntervalDay = this.endIntervalDate.getDate();
    }

    var firstDate = new Date(paramYear, paramMonth, 1);
    var paramStartDay = firstDate.getDay();


    var lastDate = new Date(paramYear, paramMonth, this.datesArray[paramMonth]);
    var paramEndDay = lastDate.getDay();


    var firstDayOfWeek = (this.startDay === 'M') ? 1 : 0;
    if (firstDayOfWeek) {
        paramStartDay = (paramStartDay == 0) ? 6 : paramStartDay - 1;
        paramEndDay = (paramEndDay == 0) ? 6 : paramEndDay - 1;
    }

    if (this.weekIntervalSelection) {
        this.startEndWeeks = new Array();
    }

    //Building html of Calendar
    var html = '<table cellpadding="0" cellspacing="0" class="mainTable" width="100%"><tr><td class="head-calendar" style="padding: 0;">';

    if (this.changingDates === 'I')
    {
        html += '<table class="navigationLayout" cellspacing="0"><tr><td>' +
                '<select name="month" onchange=\'__calendar_setMonth.call(calendarCollection["' + this.containerId + '"],this.selectedIndex);return false;\'>';
        for (var i = 0; i < 12; i++) {
            html += '<option value="' + i + '"' + ((i == paramMonth) ? ' selected' : '') + '>' + this.monthArray[i] + '</option>';
        }
        html += '</select>' +
                '</td><td width="70">' +
                '<select name="year" onchange=\'__calendar_setYear.call(calendarCollection["' + this.containerId + '"],this.value);return false;\'>';
        for (var i = 1970; i < 2100; i++) {
            html += '<option value="' + i + '"' + ((i == paramYear) ? ' selected' : '') + '>' + i + '</option>';
        }
        html += '</select>' +
                '</td></tr></table>';
    } else {
        html += '<table class="navigationLayout" cellspacing="0"><tr style="line-height: 19px;height: 19px;">' +
                '<td class="arrowsWrapper left">' +
                '<a href="#" class="year" onclick=\'__calendar_setYear.call(calendarCollection["' + this.containerId + '"],"-1");return false;\'><i data-calendar="true" class="icon-double-angle-left"></i></a>&nbsp;&nbsp;' +
                '</td>' +
                '<td class="arrowsWrapper left">' +
                '<a href="#" class="month " onclick=\'__calendar_setMonth.call(calendarCollection["' + this.containerId + '"],"-1");return false;\'><i data-calendar="true" class="icon-angle-left"></i></a>' +
                '</td>' +
                '<td class="center">' +
                '<a' + ((this.withIntervals === 'Y') ? ' class="intervalSelect" href="#" onclick=\'__setMonthInterval.call(calendarCollection["' + this.containerId + '"],' + paramMonth + ');return false;\'' : '') + '>' + this.monthArray[paramMonth] + '</a>' +
                ' &nbsp;' +
                '<a' + ((this.withIntervals === 'Y') ? ' class="intervalSelect" href="#" onclick=\'__setYearInterval.call(calendarCollection["' + this.containerId + '"],' + paramYear + ');return false;\'' : '') + '>' + paramYear + '</a>' +
                '</td><td class="arrowsWrapper right">' +
                '<a href="#" class="month " onclick=\'__calendar_setMonth.call(calendarCollection["' + this.containerId + '"],"+1");return false;\'><i data-calendar="true" class="icon-angle-right"></i></a>&nbsp;&nbsp;' +
                '</td>' +
                '</td><td class="arrowsWrapper right">' +
                '<a href="#" class="year " onclick=\'__calendar_setYear.call(calendarCollection["' + this.containerId + '"],"+1");return false;\'><i data-calendar="true" class="icon-double-angle-right"></i></a>' +
                '</td>' +
                '</tr></table>';
    }

    html += '</td></tr><tr><td class="no-padding-calendar">';

    html += '<table class="daysLayout" cellpadding="0" cellspacing="0"><tr class="first-row">';
    if (this.daysOrder === 'V')
    {
        //Displaying days upright
        var numberOfColumns = (this.datesArray[paramMonth] + paramStartDay + 6 - paramEndDay) / 7;
        calendarHeight = columnHeight;
        for (var i = 0; i < 7; i++) {
            html += '<td class="dayName' + (((i + firstDayOfWeek + 1) % 7 === 0 || (i + firstDayOfWeek) % 7 === 0) ? ' holiday' : '') + '"><a>' + this.dayNamesArray[(i + firstDayOfWeek) % 7 + 7] + '</a></td>';
            for (var j = 0; j < numberOfColumns; j++) {
                var day = j * 7 + i - paramStartDay + 1;
                var isChoosenDay = (day === choosenDay && paramMonth === choosenMonth && paramYear === choosenYear);
                var isHoliday = ((day + paramStartDay + firstDayOfWeek) % 7 === 0 || (day + paramStartDay + firstDayOfWeek - 1) % 7 === 0);
                var pastDay = ((day < choosenDay && paramMonth === choosenMonth && paramYear === choosenYear) || (paramMonth < choosenMonth && paramYear == choosenYear) || (paramYear < choosenYear));
                if (this.withIntervals === 'Y' && this.weekIntervalSelection === 'Y') {
                    if (this.holidaysSelection === 'N' && !isHoliday || this.holidaysSelection === 'Y') {
                        if (!this.startEndWeeks[j]) {
                            this.startEndWeeks[j] = new Array();
                        }
                        if (!this.startEndWeeks[j].start) {
                            this.startEndWeeks[j].start = day;
                        }
                        this.startEndWeeks[j].end = day;
                    }
                }

                var disabledDateConfig = __calendar_disable_date_config.call(this, new Date(paramYear, paramMonth, day, 0, 0, 0, 0).getTime());
                var isDateDisabled = !!disabledDateConfig || __calendar_disable_date_selection.call(this, {day: day, month: paramMonth, year: paramYear}, isHoliday, pastDay);
                var hintForDisabledDate = (!!disabledDateConfig && !!disabledDateConfig.hint) ? ' title ="' + disabledDateConfig.hint + '" ' : '';
                if (isDateDisabled && hintForDisabledDate === '' && !!this.globalHintForDisabledDates) {
                    hintForDisabledDate = ' title ="' + this.globalHintForDisabledDates + '" ';
                }

                if (day < 1 || day > this.datesArray[paramMonth]) {
                    if (this.pastMonthDaysDisplay === 'Y') {
                        var tempDate = new Date();
                        tempDate.setTime(this.paramDate.getTime());
                        tempDate.setDate(day);
                        var dayToShow = tempDate.getDate();
                        html += '<td class="';
                        if (this.withIntervals === 'Y' && this.waitForEndInterval && (dayToShow == choosenDay && tempDate.getMonth() == choosenMonth && paramYear == tempDate.getFullYear())) {
                            html += 'startSelectInterval';
                        } else if (this.withIntervals === 'Y' && __isInIntervalValue(dayToShow, tempDate.getMonth(), tempDate.getFullYear(), choosenStartIntervalDay, choosenStartIntervalMonth, choosenStartIntervalYear, choosenEndIntervalDay, choosenEndIntervalMonth, choosenEndIntervalYear) && !this.waitForEndInterval || this.withIntervals === 'N' && __isInIntervalValue(dayToShow, tempDate.getMonth(), tempDate.getFullYear(), choosenDay, choosenMonth, choosenYear, choosenDay, choosenMonth, choosenYear)) {
                            html += 'pastMonthInervalDay';
                        } else {
                            html += 'pastMonthDay';
                        }
                        if (isDateDisabled) {
                            html += ' disabledDay';
                        }
                        html += '"><a' + (isDateDisabled ? hintForDisabledDate : ' href="#" onclick=\'__calendar_setChoosenDate.call(calendarCollection["' + this.containerId + '"],' + day + ');return false\'') + '>';
                        html += dayToShow;
                        html += '</a></td>';
                    } else {
                        html += '<td>&nbsp;</td>';
                    }
                } else {
                    html += '<td class="';
                    if (this.withIntervals === 'Y') {
                        if (this.waitForEndInterval) {
                            if (isChoosenDay) {
                                html += 'startSelectInterval';
                            } else if (isHoliday) {
                                html += 'holiday';
                            }
                        } else {
                            var isInInterval = __isInIntervalValue(day, paramMonth, paramYear, choosenStartIntervalDay, choosenStartIntervalMonth, choosenStartIntervalYear, choosenEndIntervalDay, choosenEndIntervalMonth, choosenEndIntervalYear);
                            if (isInInterval && isHoliday) {
                                html += 'intervalHoliday';
                            } else if (isInInterval) {
                                html += 'intervalDay';
                            } else if (isHoliday) {
                                html += 'holiday';
                            }
                        }
                    } else {
                        if (isChoosenDay) {
                            html += 'selectedDay';
                        } else {
                            if (isHoliday && pastDay) {
                                html += 'pastHoliday';
                            } else {
                                if (isHoliday) {
                                    html += 'holiday';
                                }
                                if (pastDay) {
                                    html += 'pastDay';
                                }
                            }
                        }
                    }
                    if (isDateDisabled) {
                        html += ' disabledDay';
                    }
                    html += '">';
                    html += '<a' + (isDateDisabled ? hintForDisabledDate : ' href="#" onclick=\'__calendar_setChoosenDate.call(calendarCollection["' + this.containerId + '"],' + day + ');return false\'') + '>';
                    html += day;
                    html += '</a>';
                    html += '</td>';
                }
            }
            html += '</tr>';
            if (i !== 6) {
                html += '<tr>';
            }
        }
        if (this.withIntervals === 'Y' && this.weekIntervalSelection === 'Y')
        {
            html += '<tr>';
            html += '<td class="dayName">&nbsp;</td>';
            for (var i = 0; i < numberOfColumns; i++) {
                html += '<td>' + ((this.startEndWeeks[i]) ? '<a href="#" onclick=\'__setWeekInterval.call(calendarCollection["' + this.containerId + '"],' + i + ');return false;\'><b>^</b></a>' : '&nbsp;') + '</td>';
            }
            html += '</tr>';
        }
    }
    else
    {
        //Displaying days across
        var numberOfRows = (this.datesArray[paramMonth] + paramStartDay + 6 - paramEndDay) / 7;


        calendarHeight = numberOfRows * rowHeight + headerHeight;
        for (var i = 0; i < 7; i++) {
            var tdDayName;
            if (i === 0) {
                tdDayName = '<td class="dayName first';
            }
            else
            {
                if (i === 6) {
                    tdDayName = '<td class="dayName last';
                }
                else {
                    tdDayName = '<td class="dayName';
                }
            }

            html += tdDayName + (((i + firstDayOfWeek + 1) % 7 === 0 || (i + firstDayOfWeek) % 7 === 0) ? ' holiday' : '') + '"><a>' + this.dayNamesArray[(i + firstDayOfWeek) % 7 + 7] + '</a></td>';
        }
        if (this.withIntervals === 'Y' && this.weekIntervalSelection === 'Y') {
            html += '<td class="dayName">&nbsp;</td>';
        }
        html += '</tr><tr>';
        for (var i = 0; i < numberOfRows; i++) {
            for (var j = 0; j < 7; j++) {
                var day = i * 7 + j - paramStartDay + 1;
                var isChoosenDay = (day === choosenDay && paramMonth === choosenMonth && paramYear === choosenYear);
                var isPresentDay = (day === presentDay && paramMonth === presentMonth && paramYear === presentYear);
                var isHoliday = ((day + paramStartDay + firstDayOfWeek) % 7 === 0 || (day + paramStartDay + firstDayOfWeek - 1) % 7 === 0);
                var pastDay = ((day < choosenDay && paramMonth === choosenMonth && paramYear === choosenYear) || (paramMonth < choosenMonth && paramYear == choosenYear) || (paramYear < choosenYear));
                if (this.withIntervals === 'Y' && this.weekIntervalSelection === 'Y') {
                    if (this.holidaysSelection === 'N' && !isHoliday || this.holidaysSelection === 'Y') {
                        if (!this.startEndWeeks[i]) {
                            this.startEndWeeks[i] = new Array();
                        }
                        if (!this.startEndWeeks[i].start) {
                            this.startEndWeeks[i].start = day;
                        }
                        this.startEndWeeks[i].end = day;
                    }
                }

                var disabledDateConfig = __calendar_disable_date_config.call(this, new Date(paramYear, paramMonth, day, 0, 0, 0, 0).getTime());
                var isDateDisabled = !!disabledDateConfig || __calendar_disable_date_selection.call(this, {day: day, month: paramMonth, year: paramYear}, isHoliday, pastDay);
                var hintForDisabledDate = (!!disabledDateConfig && !!disabledDateConfig.hint) ? ' title ="' + disabledDateConfig.hint + '" ' : '';
                if (isDateDisabled && hintForDisabledDate === '' && !!this.globalHintForDisabledDates) {
                    hintForDisabledDate = ' title ="' + this.globalHintForDisabledDates + '" ';
                }

                if (day < 1 || day > this.datesArray[paramMonth]) {
                    if (this.pastMonthDaysDisplay === 'Y') {
                        var tempDate = new Date();
                        tempDate.setTime(this.paramDate.getTime());
                        tempDate.setDate(day);
                        var dayToShow = tempDate.getDate();
                        var todayMonth = tempDate.getMonth();
                        var todayYear = tempDate.getFullYear();
                        var isPresentDayOthMonth = (dayToShow == presentDay && todayMonth == presentMonth && todayYear == presentYear);
                        html += '<td class="';
                        if (this.withIntervals === 'Y' && this.waitForEndInterval && (dayToShow == choosenDay && tempDate.getMonth() == choosenMonth && paramYear == tempDate.getFullYear())) {
                            html += 'startSelectInterval';
                        } else if (this.withIntervals === 'Y' && __isInIntervalValue(dayToShow, tempDate.getMonth(), tempDate.getFullYear(), choosenStartIntervalDay, choosenStartIntervalMonth, choosenStartIntervalYear, choosenEndIntervalDay, choosenEndIntervalMonth, choosenEndIntervalYear) && !this.waitForEndInterval || this.withIntervals == 'N' && __isInIntervalValue(dayToShow, tempDate.getMonth(), tempDate.getFullYear(), choosenDay, choosenMonth, choosenYear, choosenDay, choosenMonth, choosenYear)) {
                            html += "pastMonthInervalDay";
                        } else {
                            html += "pastMonthDay";
                            if (isPresentDayOthMonth) {
                                html += " presentDay";
                            }
                        }
                        if (isDateDisabled) {
                            html += ' disabledDay';
                        }
                        html += '"><a' + (isDateDisabled ? hintForDisabledDate : ' href="#" onclick=\'__calendar_setChoosenDate.call(calendarCollection["' + this.containerId + '"],' + day + ');return false\'') + '>';
                        html += dayToShow;
                        html += '</a></td>';
                    } else {
                        html += '<td>&nbsp;</td>';
                    }
                } else {
                    html += '<td class="';
                    if (this.withIntervals === 'Y') {
                        if (this.waitForEndInterval) {
                            if (isChoosenDay) {
                                html += 'startSelectInterval';
                            } else if (isHoliday) {
                                html += 'holiday';
                            }
                        } else {
                            var isInInterval = __isInIntervalValue(day, paramMonth, paramYear, choosenStartIntervalDay, choosenStartIntervalMonth, choosenStartIntervalYear, choosenEndIntervalDay, choosenEndIntervalMonth, choosenEndIntervalYear);
                            if (isInInterval && isHoliday) {
                                html += 'intervalHoliday';
                            } else if (isInInterval) {
                                html += 'intervalDay';
                            } else if (isHoliday) {
                                html += 'holiday';
                            }
                        }
                    } else {
                        if (isChoosenDay) {
                            html += 'selectedDay';
                        } else {
                            if (isHoliday && pastDay) {
                                html += 'pastHoliday';
                            } else {
                                if (isHoliday) {
                                    html += 'holiday';
                                }
                                if (pastDay) {
                                    html += 'pastDay';
                                }
                            }
                        }
                    }
                    if (isPresentDay) {
                        html += ' presentDay';
                    }
                    if (isDateDisabled) {
                        html += ' disabledDay';
                    }
                    html += '">';
                    html += '<a' + (isDateDisabled ? hintForDisabledDate : ' href="#" onclick=\'__calendar_setChoosenDate.call(calendarCollection["' + this.containerId + '"],' + day + ');return false\'') + '>';
                    html += day;
                    html += '</a>';
                    html += '</td>';
                }
                if (this.withIntervals === 'Y' && this.weekIntervalSelection === 'Y' && j === 6) {
                    html += '<td>' + ((this.startEndWeeks[i]) ? '<a href="#" onclick=\'__setWeekInterval.call(calendarCollection["' + this.containerId + '"],' + i + ');return false;\'><b>&lt;</b></a>' : '&nbsp;') + '</td>';
                }
            }
            html += '</tr>';
            if (i !== numberOfRows - 1) {
                html += '<tr>';
            }
            else
            {


                var date = this.formatDate(new Date(), "d ") + this.formatDate(new Date(), "MMM").substring(0, 3) + this.formatDate(new Date(), ", y");

                var day = parseInt(this.formatDate(new Date(), "M")) - 1;

                var strOnClickDay = '__calendar_setChoosenDate.call(calendarCollection["' + this.containerId + '"], "' + this.formatDate(new Date(), "d") + '");';
                var strOnClickMonth = '__calendar_setMonth.call(calendarCollection["' + this.containerId + '"], "' + day + '");';
                var strOnClickYear = '__calendar_setYear.call(calendarCollection["' + this.containerId + '"], "' + this.formatDate(new Date(), "y") + '");';

                date = '<a href="#" id="today" onclick=\'' + strOnClickYear + strOnClickMonth + strOnClickDay + "return false;" + '\'>' + "Today: " + date + '</a>';

                html += '<tr><td colspan="7" class="today">' + date + '</td></tr>';
            }
        }
    }
    html += '</table>';
    html += '</td></tr>';
    if (this.withTime === 'Y') {
        calendarHeight += timeHeight;
        html += '<tr><td class="timeLayout">';
        html += '<select name="hours" onchange=\'__calendar_setHours.call(calendarCollection["' + this.containerId + '"],this.value);return false;\'>';
        for (var i = 0; i < 24; i++) {
            html += '<option value="' + i + '"' + ((paramHours === i) ? ' selected' : '') + '>' + __LZ(i) + '</option>';
        }
        html += '</select>';
        html += '&nbsp;<b>:</b>&nbsp;';
        html += '<select name="minutes" onchange=\'__calendar_setMinutes.call(calendarCollection["' + this.containerId + '"],this.value);return false;\'>';
        for (var i = 0; i < 60; i++) {
            html += '<option value="' + i + '"' + ((paramMinutes === i) ? ' selected' : '') + '>' + __LZ(i) + '</option>';
        }
        html += '</select>';
        html += '&nbsp;<b>:</b>&nbsp;';
        html += '<select name="seconds" onchange=\'__calendar_setSeconds.call(calendarCollection["' + this.containerId + '"],this.value);return false;\'>';
        for (var i = 0; i < 60; i++) {
            html += '<option value="' + i + '"' + ((paramSeconds === i) ? ' selected' : '') + '>' + __LZ(i) + '</option>';
        }
        html += '</select>';
        html += '</td></tr>';
    }
    html += '</table>';

    this.divObj.innerHTML = html;

    if (!this.state) {
        //Hide other calendars
        __calendar_closeAll();
        this.divObj.style.visibility = "hidden";
        this.divObj.style.display = "block";

        this.divObj.style.visibility = "visible";
        this.divObj.style.zIndex = 6;

        var jCalendarDiv = jQuery(this.divObj),
                divHeight = jCalendarDiv.height(),
                jCalendarParent = jCalendarDiv.parent(),
                parentHeigth = jCalendarDiv.parent().height(),
                jWindow = jQuery(window),
                spaceTop = jCalendarParent.offset().top - jWindow.scrollTop() - 5,
                spaceBottom = jWindow.height() + jWindow.scrollTop() - jCalendarParent.offset().top - parentHeigth - 5;

        if (spaceBottom < divHeight && spaceTop > spaceBottom) {
            jCalendarDiv.css("top", -divHeight - 5);
        } else {
            jCalendarDiv.css("top", parentHeigth + 5);
        }
        this.state = 1;
        jQuery(document).on('click', __calendar_close);
        jQuery(window).on('resize', __calendar_closeAll);
    }
}

function __calendar_close(e)
{
    srcEl = (e) ? e.target : window.event.srcElement;
    for (var i in calendarCollection) {
        if (calendarCollection[i].state) {
            var closeClndr = true;
            do {
                if (srcEl == calendarCollection[i].containerObj ||
                        (srcEl.className && srcEl.className.indexOf('navigationLayout') !== -1) ||
                        // workaround for IE; IE returns null as parent for detached DOM element
                                (srcEl.getAttribute && srcEl.getAttribute('data-calendar'))) {
                    closeClndr = false;
                    break;
                }
            } while (srcEl = srcEl.parentNode);
            if (closeClndr) {
                jQuery(document).off('click', __calendar_close);
                jQuery(window).off('resize', __calendar_closeAll);
                jQuery(window).off('scroll', __calendar_closeAll);
                calendarCollection[i].paramDate.setTime(calendarCollection[i].choosenDate.getTime());
                calendarCollection[i].divObj.style.display = "none";
                calendarCollection[i].state = 0;
            }
        }
    }
}

function __calendar_closeAll()
{
    for (var i in calendarCollection) {
        if (calendarCollection[i].state) {
            calendarCollection[i].paramDate.setTime(calendarCollection[i].choosenDate.getTime());
            calendarCollection[i].divObj.style.display = "none";
            calendarCollection[i].state = 0;
        }
    }
    jQuery(document).off('click', __calendar_close);
}

function __calendar_closeCalendar(i)
{
    if (calendarCollection[i].state) {
        calendarCollection[i].paramDate.setTime(calendarCollection[i].choosenDate.getTime());
        calendarCollection[i].divObj.style.display = "none";
        calendarCollection[i].state = 0;
    }
    jQuery(document).off('click', __calendar_close);
}

function __calendar_setChoosenDate(_dayNum)
{

    if (this.withIntervals === 'Y') {
        if (this.waitForEndInterval) {
            this.endIntervalDate.setFullYear(this.paramDate.getFullYear(), this.paramDate.getMonth(), _dayNum);
            this.endIntervalDate.setHours(this.paramDate.getHours(), this.paramDate.getMinutes(), this.paramDate.getSeconds());
            if (this.endIntervalDate < this.startIntervalDate) {
                var tempDate = this.endIntervalDate.getTime();
                this.endIntervalDate.setTime(this.startIntervalDate.getTime());
                this.startIntervalDate.setTime(tempDate);
            }
            this.waitForEndInterval = false;
            this.choosenDate.setTime(this.endIntervalDate.getTime());
            this.setInputValue(this.formatDate(this.startIntervalDate, this.dateFormat) + this.startEndDateDelimiter + this.formatDate(this.endIntervalDate, this.dateFormat));
            if (this.closeOnSelection === 'Y') {
                this.divObj.style.display = "none";
                this.state = 0;
            } else {
                this.show();
            }
        } else {
            this.startIntervalDate.setFullYear(this.paramDate.getFullYear(), this.paramDate.getMonth(), _dayNum);
            this.startIntervalDate.setHours(this.paramDate.getHours(), this.paramDate.getMinutes(), this.paramDate.getSeconds());
            this.choosenDate.setTime(this.startIntervalDate.getTime());
            this.waitForEndInterval = true;
            this.show();
        }
    } else {
        this.choosenDate.setFullYear(this.paramDate.getFullYear(), this.paramDate.getMonth(), _dayNum);
        this.choosenDate.setHours(this.paramDate.getHours(), this.paramDate.getMinutes(), this.paramDate.getSeconds());
        this.setInputValue(this.formatDate(this.choosenDate, this.dateFormat));

        if (this.closeOnSelection === 'Y') {
            this.divObj.style.display = "none";
            this.state = 0;
        } else {
            this.show();
        }
    }
    __calendar_closeAll();
}

function __calendar_setMonth(_toSet)
{
    switch (_toSet)
    {
        case "-1":
            if (this.paramDate.getMonth() === 0)
            {
                this.setYear("-1");
                this.setMonth("11");
                return true;
            }
            else
            {
                this.paramDate.setMonth(this.paramDate.getMonth() - 1, 1);
            }
            break;
        case "+1":
            if (this.paramDate.getMonth() === 11)
            {
                this.setYear("+1");
                this.setMonth("0");
                return true;
            }
            else
            {
                this.paramDate.setMonth(this.paramDate.getMonth() + 1, 1);
            }
            break;
        default:
            var toSet = parseInt(_toSet);
            if (isNaN(toSet) || toSet < 0 || toSet > 11) {
                return false;
            }
            this.paramDate.setMonth(toSet, 1);
    }
    this.show();
    return true;
}

function __calendar_setYear(_toSet)
{
    switch (_toSet)
    {
        case "-1":
            this.paramDate.setFullYear(this.paramDate.getFullYear() - 1);
            break;
        case "+1":
            this.paramDate.setFullYear(this.paramDate.getFullYear() + 1);
            break;
        default:
            var toSet = parseInt(_toSet);
            if (isNaN(toSet)) {
                return false;
            }
            this.paramDate.setFullYear(toSet);
    }
    this.show();
    return true;
}

function __calendar_setHours(_toSet)
{
    var toSet = parseInt(_toSet);
    if (isNaN(toSet)) {
        return false;
    }
    this.paramDate.setHours(toSet);
}

function __calendar_setMinutes(_toSet)
{
    var toSet = parseInt(_toSet);
    if (isNaN(toSet)) {
        return false;
    }
    this.paramDate.setMinutes(toSet);
}

function __calendar_setSeconds(_toSet)
{
    var toSet = parseInt(_toSet);
    if (isNaN(toSet)) {
        return false;
    }
    this.paramDate.setSeconds(toSet);
}

function __isInIntervalValue(day, month, year, dayStart, monthStart, yearStart, dayEnd, monthEnd, yearEnd)
{
    return ((day >= dayStart && month === monthStart && year === yearStart || month > monthStart && year === yearStart || year > yearStart) && (day <= dayEnd && month === monthEnd && year === yearEnd || month < monthEnd && year === yearEnd || year < yearEnd));
}

function __setWeekInterval(_toSet)
{
    this.startIntervalDate.setFullYear(this.paramDate.getFullYear(), this.paramDate.getMonth(), this.startEndWeeks[_toSet].start);
    this.startIntervalDate.setHours(this.paramDate.getHours(), this.paramDate.getMinutes(), this.paramDate.getSeconds());
    this.endIntervalDate.setFullYear(this.paramDate.getFullYear(), this.paramDate.getMonth(), this.startEndWeeks[_toSet].end);
    this.endIntervalDate.setHours(this.paramDate.getHours(), this.paramDate.getMinutes(), this.paramDate.getSeconds());
    this.setInputValue(this.formatDate(this.startIntervalDate, this.dateFormat) + this.startEndDateDelimiter + this.formatDate(this.endIntervalDate, this.dateFormat));
    if (this.closeOnSelection === 'Y') {
        this.divObj.style.display = "none";
        this.state = 0;
    } else {
        this.show();
    }
}

function __setMonthInterval(_toSet)
{
    this.startIntervalDate.setFullYear(this.paramDate.getFullYear(), _toSet, 1);
    this.startIntervalDate.setHours(this.paramDate.getHours(), this.paramDate.getMinutes(), this.paramDate.getSeconds());
    this.endIntervalDate.setFullYear(this.paramDate.getFullYear(), _toSet, this.datesArray[_toSet]);
    this.endIntervalDate.setHours(this.paramDate.getHours(), this.paramDate.getMinutes(), this.paramDate.getSeconds());
    this.setInputValue(this.formatDate(this.startIntervalDate, this.dateFormat) + this.startEndDateDelimiter + this.formatDate(this.endIntervalDate, this.dateFormat));
    if (this.closeOnSelection === 'Y') {
        this.divObj.style.display = "none";
        this.state = 0;
    } else {
        this.show();
    }
}

function __setYearInterval(_toSet)
{
    this.startIntervalDate.setFullYear(_toSet, 0, 1);
    this.startIntervalDate.setHours(this.paramDate.getHours(), this.paramDate.getMinutes(), this.paramDate.getSeconds());
    this.endIntervalDate.setFullYear(_toSet, 11, this.datesArray[11]);
    this.endIntervalDate.setHours(this.paramDate.getHours(), this.paramDate.getMinutes(), this.paramDate.getSeconds());
    this.setInputValue(this.formatDate(this.startIntervalDate, this.dateFormat) + this.startEndDateDelimiter + this.formatDate(this.endIntervalDate, this.dateFormat));
    if (this.closeOnSelection === 'Y') {
        this.divObj.style.display = "none";
        this.state = 0;
    } else {
        this.show();
    }
}
/*
 function __calendar_getTop()
 {
 var obj = this.inputObj;
 var top = obj.offsetTop;
 while (obj.tagName !== "BODY")
 {
 obj = obj.offsetParent;
 top += obj.offsetTop;
 }
 return top - this.divOffsetY;
 }
 function __calendar_getLeft()
 {
 var obj = this.inputObj;
 var left = obj.offsetLeft;
 while (obj.tagName !== "BODY")
 {
 obj = obj.offsetParent;
 left += obj.offsetLeft;
 }
 return left - this.divOffsetX;
 }
 */
function __calendar_getTop() {
    var _element = this.inputObj;
    var y = 0;
    while (_element !== null && _element.style.position !== 'absolute') {
        if (_element.nodeName !== 'BODY' && _element.nodeName !== 'IMG') {
            y += _element.offsetTop - _element.scrollTop;
        } else {
            y += _element.offsetTop;
        }
        _element = _element.offsetParent;
    }
    return y - this.divOffsetY;
}

function __calendar_getLeft() {
    var _element = this.inputObj;
    var x = 0;
    while (_element !== null && _element.style.position !== 'absolute') {
        if (_element.nodeName !== 'BODY' && _element.nodeName !== 'IMG') {
            x += _element.offsetLeft - _element.scrollLeft;
        } else {
            x += _element.offsetLeft;
        }
        _element = _element.offsetParent;
    }
    return x - this.divOffsetX;
}

function __LZ(x) {
    return (x < 0 || x > 9 ? "" : "0") + x;
}

function __calendar_formatDate(date, format) {
    format = format + "";
    var result = "";
    var i_format = 0;
    var c = "";
    var token = "";
    var y = date.getYear() + "";
    var M = date.getMonth() + 1;
    var d = date.getDate();
    var E = date.getDay();
    var H = date.getHours();
    var m = date.getMinutes();
    var s = date.getSeconds();
    var yyyy, yy, MMM, MM, dd, hh, h, mm, ss, ampm, HH, H, KK, K, kk, k;
    var value = new Object();
    if (y.length < 4) {
        y = "" + (y - 0 + 1900);
    }
    value["y"] = "" + y;
    value["yyyy"] = y;
    value["yy"] = y.substring(2, 4);
    value["M"] = M;
    value["MM"] = __LZ(M);
    value["MMM"] = this.monthArray[M - 1];
    value["NNN"] = this.monthArray[M + 11];
    value["d"] = d;
    value["dd"] = __LZ(d);
    value["E"] = this.dayNamesArray[E + 7];
    value["EE"] = this.dayNamesArray[E];
    value["H"] = H;
    value["HH"] = __LZ(H);
    if (H === 0) {
        value["h"] = 12;
    } else if (H > 12) {
        value["h"] = H - 12;
    } else {
        value["h"] = H;
    }
    value["hh"] = __LZ(value["h"]);
    if (H > 11) {
        value["K"] = H - 12;
    } else {
        value["K"] = H;
    }
    value["k"] = H + 1;
    value["KK"] = __LZ(value["K"]);
    value["kk"] = __LZ(value["k"]);
    if (H > 11) {
        value["a"] = "PM";
    } else {
        value["a"] = "AM";
    }
    value["m"] = m;
    value["mm"] = __LZ(m);
    value["s"] = s;
    value["ss"] = __LZ(s);
    while (i_format < format.length) {
        c = format.charAt(i_format);
        token = "";
        while ((format.charAt(i_format) == c) && (i_format < format.length)) {
            token += format.charAt(i_format++);
        }
        if (value[token] != null) {
            result = result + value[token];
        } else {
            result = result + token;
        }
    }
    return result;
}

function __calendar_getDateFromFormat(val, format) {
    function _getInt(str, i, minlength, maxlength) {
        for (var x = maxlength; x >= minlength; x--) {
            var token = str.substring(i, i + x);
            if (token.length < minlength) {
                return null;
            }
            if (_isInteger(token)) {
                return token;
            }
        }
        return null;
    }
    function _isInteger(val) {
        var digits = "1234567890";
        for (var i = 0; i < val.length; i++) {
            if (digits.indexOf(val.charAt(i)) === -1) {
                return false;
            }
        }
        return true;
    }
    val = val + "";
    format = format + "";
    var i_val = 0;
    var i_format = 0;
    var c = "";
    var token = "";
    var token2 = "";
    var x, y;
    var now = new Date();
    var year = now.getYear();
    var month = now.getMonth() + 1;
    var date = 1;
    var hh = now.getHours();
    var mm = now.getMinutes();
    var ss = now.getSeconds();
    var ampm = "";
    while (i_format < format.length) {
        c = format.charAt(i_format);
        token = "";
        while ((format.charAt(i_format) == c) && (i_format < format.length)) {
            token += format.charAt(i_format++);
        }
        if (token === "yyyy" || token === "yy" || token === "y") {
            if (token === "yyyy") {
                x = 4;
                y = 4;
            }
            if (token === "yy") {
                x = 2;
                y = 2;
            }
            if (token === "y") {
                x = 2;
                y = 4;
            }
            year = _getInt(val, i_val, x, y);
            if (year == null) {
                return 0;
            }
            i_val += year.length;
            if (year.length === 2) {
                if (year > 70) {
                    year = 1900 + (year - 0);
                } else {
                    year = 2000 + (year - 0);
                }
            }
        } else if (token === "MMM" || token === "NNN") {
            month = 0;
            for (var i = 0; i < this.monthArray.length; i++) {
                var month_name = this.monthArray[i];
                if (val.substring(i_val, i_val + month_name.length).toLowerCase() === month_name.toLowerCase()) {
                    if (token === "MMM" || (token === "NNN" && i > 11)) {
                        month = i + 1;
                        if (month > 12) {
                            month -= 12;
                        }
                        i_val += month_name.length;
                        break;
                    }
                }
            }
            if ((month < 1) || (month > 12)) {
                return 0;
            }
        } else if (token === "EE" || token === "E") {
            for (var i = 0; i < DAY_NAMES.length; i++) {
                var day_name = DAY_NAMES[i];
                if (val.substring(i_val, i_val + day_name.length).toLowerCase() === day_name.toLowerCase()) {
                    i_val += day_name.length;
                    break;
                }
            }
        } else if (token === "MM" || token === "M") {
            month = _getInt(val, i_val, token.length, 2);
            if (month == null || (month < 1) || (month > 12)) {
                return 0;
            }
            i_val += month.length;
        } else if (token === "dd" || token === "d") {
            date = _getInt(val, i_val, token.length, 2);
            if (date == null || (date < 1) || (date > 31)) {
                return 0;
            }
            i_val += date.length;
        } else if (token === "hh" || token === "h") {
            hh = _getInt(val, i_val, token.length, 2);
            if (hh == null || (hh < 1) || (hh > 12)) {
                return 0;
            }
            i_val += hh.length;
        } else if (token === "HH" || token === "H") {
            hh = _getInt(val, i_val, token.length, 2);
            if (hh == null || (hh < 0) || (hh > 23)) {
                return 0;
            }
            i_val += hh.length;
        } else if (token === "KK" || token === "K") {
            hh = _getInt(val, i_val, token.length, 2);
            if (hh == null || (hh < 0) || (hh > 11)) {
                return 0;
            }
            i_val += hh.length;
        } else if (token === "kk" || token === "k") {
            hh = _getInt(val, i_val, token.length, 2);
            if (hh == null || (hh < 1) || (hh > 24)) {
                return 0;
            }
            i_val += hh.length;
            hh--;
        } else if (token === "mm" || token === "m") {
            mm = _getInt(val, i_val, token.length, 2);
            if (mm == null || (mm < 0) || (mm > 59)) {
                return 0;
            }
            i_val += mm.length;
        } else if (token === "ss" || token === "s") {
            ss = _getInt(val, i_val, token.length, 2);
            if (ss == null || (ss < 0) || (ss > 59)) {
                return 0;
            }
            i_val += ss.length;
        } else if (token === "a") {
            if (val.substring(i_val, i_val + 2).toLowerCase() === "am") {
                ampm = "AM";
            } else if (val.substring(i_val, i_val + 2).toLowerCase() === "pm") {
                ampm = "PM";
            } else {
                return 0;
            }
            i_val += 2;
        } else {
            if (val.substring(i_val, i_val + token.length) != token) {
                return 0;
            } else {
                i_val += token.length;
            }
        }
    }
    if (i_val != val.length) {
        return 0;
    }
    if (month === 2) {
        if (((year % 4 === 0) && (year % 100 !== 0)) || (year % 400 === 0)) {
            if (date > 29) {
                return 0;
            }
        } else {
            if (date > 28) {
                return 0;
            }
        }
    }
    if ((month === 4) || (month === 6) || (month === 9) || (month === 11)) {
        if (date > 30) {
            return 0;
        }
    }
    if (hh < 12 && ampm === "PM") {
        hh = hh - 0 + 12;
    } else if (hh > 11 && ampm === "AM") {
        hh -= 12;
    }
    var newdate = new Date(year, month - 1, date, hh, mm, ss);
    return newdate.getTime();
}

function __calendar_setInputValue(param, nonFireEvent) {
    this.inputObj.value = param;
    if (typeof (fireEvent) !== "boolean" && nonFireEvent != true) {
        if (jQuery.browser.msie && (jQuery.browser.version === "7.0" || jQuery.browser.version === "8.0")) {
            this.inputObj.fireEvent("onchange");
        } else {
            var evt = document.createEvent('HTMLEvents');
            evt.initEvent('change', true, true);
            this.inputObj.dispatchEvent(evt);
        }
    }
}

function __calendar_hideCalendarDiv() {
    this.divObj.style.display = "none";
    this.state = 0;
}
/* This function sets offset for calendars div when it plased on absolute div on page. */
function __calendar_setDivOffset(offX, offY) {
    this.divOffsetX = offX;
    this.divOffsetY = offY;
}

function __calendar_disable_date_selection(date, isHoliday, pastDay) {
    var currentDate = new Date(date.year, date.month, date.day, 0, 0, 0, 0);
    var isDisabled = false;
    isDisabled = isDisabled || (this.validPeriodEnd != null && currentDate > this.validPeriodEnd);
    isDisabled = isDisabled || (this.validPeriodStart != null && currentDate < this.validPeriodStart);
    isDisabled = isDisabled || (isHoliday && this.holidaysSelection === 'N' || pastDay && this.pastDaysSelection === 'N');
    return isDisabled;
}

function __calendar_disable_date_config(time) {
    if (!this.disabledDates || this.disabledDates.length === 0) {
        return;
    }
    return this.disabledDates.find(function (value) {
        return value.startDate.getTime() === time;
    });
}
;var Settings = {
    popupName: null,
    fillSettingsContainerFunction: null,
    applySettingsFunction: null,
    cancelFunction: null,
    showDialog: function (popup, applyFunctionName, cancelFunctionName, onDoneCb) {
        var $that = jQuery(this);
        this.popupName = popup;
        var $target = jQuery("#" + this.popupName);
        this.applySettingsFunction = (applyFunctionName == null) ? applySettings : applyFunctionName;
        this.cancelFunction = (cancelFunctionName == null) ? cancelDialog : cancelFunctionName;
        showLoaderImg({container: jQuery('#workloadBody')});
        jQuery.ajax({
            url: jQuery.param.querystring($that.data("url"), $that.data("urlParams")),
            method: "GET"
        }).done(function (data) {
                jQuery("#settings-popup").html(data);
                hideLoaderImg();

                onDoneCb && onDoneCb(this);

                $target.createWin();
            });
    },
    applySettings: function () {
        if (checkWidthRange()) {
            removeAttrDisabled();
            document.forms['resplanViewForm'].submit();
            jQuery("#" + popupName).closeWin();
            lockBody();
        }
    },
    cancelDialog: function () {
        var checkedColumns = document.getElementById('checkedColumns').value.split(",");
        jQuery("input[type=checkbox][id^=column_]").prop("checked", false);
        jQuery("input[type=text][id^=inputWidth_]").prop("disabled", true);
        jQuery("span[id^=pixels_]").css("color", "gray");
        for (var i = 0; i < checkedColumns.length; i++) {
            var columnId = checkedColumns[i];
            jQuery("#column_" + columnId).prop("checked", true);
            disableTextInput(columnId);
        }
        jQuery("#" + popupName).closeWin();
    },
    disableTextInput: function (columnId) {
        var element = document.getElementById('inputWidth_' + columnId);
        if (element.disabled) {
            document.getElementById('pixels_' + columnId).style.color = '716DBF';
            element.disabled = false;
        } else {
            document.getElementById('pixels_' + columnId).style.color = 'gray';
            element.disabled = true;
        }
    },
    allowOnlyNumbers: function (event) {
        var code = event.keyCode ? event.keyCode : event.charCode;
        if (code === 8 || event.keyCode === 37 || (event.keyCode === 39)
            || event.keyCode === 36 || event.keyCode === 35 || event.keyCode === 39 || event.keyCode === 46) {
            return true;
        } else
        if (code < 95) {
                if (code < 48 || code > 57) {
                    return false;
                }
        } else {
            return false;
        }
    },
    lockBody: function () {
        showPageLocker({disableInputs: true});
    },
    removeAttrDisabled: function () {
        jQuery('input:text:disabled[id^=inputWidth_]').prop("disabled", false);
        jQuery('input:checkbox:disabled[id^=column_]').prop("disabled", false);
    },
    showPopup: function (that, onDoneCb) {
        var $that = jQuery(that);
        var $settingsPopup = jQuery("#settings-popup");
        if ($settingsPopup.length === 0) {
            $settingsPopup = jQuery("<div id='settings-popup' class='settings_popup'></div>");
            jQuery('body').append($settingsPopup);
        }
        showLoaderImg({container: jQuery('#workloadBody')});
        jQuery.ajax({
            url: jQuery.param.querystring($that.data("url"), $that.data("urlParams")),
            method: "GET"
        }).done(function (data) {
                $settingsPopup.html(data);
                hideLoaderImg();

                onDoneCb && onDoneCb(that);

                $settingsPopup.createWin({esc:true});
        });
    },
    cancelPopup: function () {
        jQuery("#settings-popup").closeWin();
    },
    checkWidthRange: function () {
        var activeWidthInputs = jQuery("input:text:not(:disabled)[id^=inputWidth_]");
        var widthValue = 0;
        for (var i = 0; i < activeWidthInputs.length; i++) {
            widthValue = activeWidthInputs[i].value;
            if (widthValue == null || widthValue < 1) {
                alert(emptyWidthMsg);
                activeWidthInputs[i].focus();
                return false;
            } else if (widthValue > 999) {
                alert(tooBigWidthMsg);
                activeWidthInputs[i].focus();
                return false;
            }
        }
        return true;
    },
    switchSelect: function () {
        var $this = jQuery('#settingsSelectCheckbox'),
            $label = jQuery('label[for=settingsSelectCheckbox]'),
            checked = $this.is(':checked');

        checked ? selectAll() : deselectAll();
        $label.attr('title', checked ? 'Deselect all' : 'Select all');
    }
};

function DefaultResultObj(defaultResultDivName, popupDivId, txtSearch, txtSearchBody) {
    this.defaultResultDivName = defaultResultDivName || "searchResult";
    this.searchResult = defaultResultDivName || "searchResult";
    this.popupDivId = popupDivId || "search-popup";
    this.txtSearch = txtSearch || searchCriteriaMessage;
    this.txtSearchBody = txtSearchBody || txtSearchBodyDefault;
}
DefaultResultObj.prototype.unicSearchDivNumber = 0;

var locker = null;
var popupName = null;
var hiddenElement = null;
var visibleElement = null;
var resultContainer = null;
var cancelFunction = null;
var checkBillingType = false;
var searchCriteriaMessage = "";
var txtSearchBodyDefault = "";
var noEntries = "<tr><td class='search_null_result' align='center'><br><br>No Entries</td></tr>";

var defaultResultDivName = null;
var resultObj = null;

var searchResultDivName = "searchResult";
var txtWaitSearch = "<tr><td colspan='3' align='center' class='search_null_result'><br/><br/>Please wait...</td></tr>";


function checkEventState(ev) {
    var event = ev || window.event;
    return (ev == null) || (event.keyCode === 13);
}

//must that DWR return div ID as first item. Before callback specify resultObj!
function setSearchResults(searchResults) {
    if (resultObj == null) {
        alert("Error setting results");
    }
    var searchResultSize = trim(searchResults[1]).length;
    if (searchResultSize === 0) {
        searchResults[1] = noEntries;
    }
    var divID = searchResults[0];
    if (jQuery("[id=" + divID + "]").length !== 0) {
        jQuery("[id=" + divID + "]")[0].innerHTML = resultObj.txtSearchBody.replace("#RESULT#", searchResults[1]);
    }
}

function showDialog(lockerName, hiddenElementName, visibleElementName, resultContainerName, cancelFunctionName) {
    /*if ((resultContainerName instanceof DefaultResultObj) || resultContainerName.__proto__ == DefaultResultObj) {*/
    	//
        locker = lockerName;
        popupName = resultContainerName.popupDivId;
        hiddenElement = hiddenElementName;
        visibleElement = visibleElementName;
        resultObj = resultContainerName;
        resultContainer = document.getElementById(resultContainerName.searchResult);
        cancelFunction = (cancelFunctionName == null) ? cancelDialog : cancelFunctionName;
        jQuery("#" + popupName).createWin();
        jQuery("#" + popupName).find('input[type="text"]').eq(0).focus();
   /* } else {
        alert("Object "+resultContainerName+" not instanceof DefaultResultObj");
    }*/
}

function makeUnicDivSearchElement(txtSearch) {
    resultObj.searchResult = resultObj.defaultResultDivName + "-" + resultObj.unicSearchDivNumber++;
    jQuery(resultContainer).attr("id", resultObj.searchResult);
    resultContainer.innerHTML = resultObj.txtSearchBody.replace("#RESULT#", txtSearch);
    return resultObj.searchResult;
}

function cancelDialog() {
    var txtSearch = resultObj.txtSearch;
    makeUnicDivSearchElement(txtSearch);
    jQuery("#" + popupName).find("input:text").val("");
    jQuery("#" + popupName).closeWin();
}

function prCh(personId, personName) {
    jQuery("[name='" + hiddenElement + "']").val(personId).trigger('change');
    jQuery("[name='" + visibleElement + "']").val(personName);

    var flagElProgram = (document.getElementById('programName') !== null);
    var flagElCustomer = (document.getElementById('customerName') !== null);

    if (flagElProgram  && flagElCustomer ) {
        if (visibleElement === "filter.programName") {
            /*
             * You should include LookupDwrUtil.js
             * Here personId is ID of program
             * You should create setCustomer(data) function in your js-file
             */
            if (personId != "") {
                LookupDwrUtil.getCustomerByProgramId(personId, setCustomer);
            }
        } else {
            /*
             * You should create clearProgram() function in your js-file
             */
            clearProgram();
        }
    }
    //TODO: CHECK IT
    if (checkBillingType) {
        checkBillingType();
    }
    /*
    little hack to check if popup is salesManager
     */
    if (hiddenElement.indexOf("salesManager") !== -1 && jQuery("select[name='bean.profitCenterId']").length > 0) {
        LookupDwrUtil.getDefaultProfitCenterBySalesManagerId(personId, selectAcordingProfitCenter);
    }
    cancelFunction();
}

var elementNamesForAllValue = ['refereeName', 'curatorName', 'unitName', 'filter.customerName', 'customerName', 'performerName',
                               'costObjectName', 'projectPopupName', 'pastProjectName', 'traineeName', 'filter.programName', 'reporterName',
                               'filter.requestorName', 'filter.reporterName'];

function clearField(hiddenElementName, visibleElementName) {
	var isAllValue = false;
	for (var i=0; i<elementNamesForAllValue.length && !isAllValue; i++) {
		if (visibleElementName == elementNamesForAllValue[i]) {
			isAllValue = true;
		}
	}
    if (!isAllValue) {
        jQuery("[name='" + visibleElementName + "']").val("");
    } else if (visibleElementName === "refereeName" || visibleElementName === "curatorName") {
        jQuery("[name='" + visibleElementName + "']").val("(None)");
    } else {
        jQuery("[name='" + visibleElementName + "']").val("All");
    }
    jQuery("[name='" + hiddenElementName + "']").val("").trigger('change');
};var displaySortImages = function () {
    jQuery("td.reportHeader a").each(function () {
        jQuery(this).parent().width(jQuery(this).width() + 1);
    });
    jQuery("td.sortLinkstyle img").each(function () {
        jQuery(this).css("visibility", "visible");
    });

};


var showRightSortArrows = function () {

    if (jQuery('link[href*="summary.css"]').length > 0) //it is a new summary page with thin sorting arrows
    {
        jQuery("td.sortLinkstyle a img").each(function () {
            if (jQuery(this).attr("src") === "images/up.gif") {
                jQuery(this).attr("src", "images/upThin.gif");
            }
            if (jQuery(this).attr("src") === "images/down.gif") {
                jQuery(this).attr("src", "images/downThin.gif");
            }
        });
    }
};


jQuery(document).ready(function () {
    showRightSortArrows();
    displaySortImages();
});







//////////////////////////////////////////////////////////////////////////////
