    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    /**
     * Threshold value of pixels to the bottom of the widget's
     * library when new portion of badges should be requested.
     */
    var PIXELS_TO_THE_BOTTOM = 150;
    var SEARCH_LENGTH = 20;

    var BADGE_BEFORE_POPUP_CLS = 'badge-before-popup';
    var ACTIVE_BADGE_CLS = 'active-badge';

    /**
     * For connecting different parts of application with common events
     * @type {exports}
     */
    var core = __webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js");
    var App = __webpack_require__(/*! com/epam/heroes/portal/core/App */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/App.js");
    var config = __webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js");
    var moment = __webpack_require__(/*! moment */ "./src/main/resources/static-site/js/libs/vendor/moment/moment.js");
    var featuresConfig = __webpack_require__(/*! FeatureConfig */ "./src/main/resources/static-site/featuresConfig.js");

    var Utils = __webpack_require__(/*! com/epam/heroes/portal/core/util/Utils */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/util/Utils.js");

    var BadgeSections = __webpack_require__(/*! com/epam/heroes/widget/givebadge/model/BadgeSections */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/BadgeSections.js");

    var BadgeAssignPopupModel = __webpack_require__(/*! com/epam/heroes/widget/givebadge/model/BadgeAssignPopupModel */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/BadgeAssignPopupModel.js");
    var BadgeAssignPopup = __webpack_require__(/*! com/epam/heroes/widget/givebadge/view/BadgeAssignPopup */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/view/BadgeAssignPopup.js");
    var AssignEntryCollection = __webpack_require__(/*! com/epam/heroes/widget/givebadge/model/AssignEntryCollection */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/AssignEntryCollection.js");

    var OverlayPopup = __webpack_require__(/*! com/epam/heroes/portal/core/view/OverlayPopup */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/OverlayPopup.js");
    var Genetics = __webpack_require__(/*! com/epam/heroes/core/util/backbone.genetics */ "./src/main/resources/static-site/js/com/epam/heroes/core/util/backbone.genetics.js");

    __webpack_require__(/*! template/com/epam/heroes/widget/givebadge/BadgeLibrary.soy */ "./src/main/soy/com/epam/heroes/widget/givebadge/BadgeLibrary.soy");

    var SEARCH_DELAY = 600;

    var BadgeLibraryView = OverlayPopup.extend({

        animationDuration: 500,

        id: 'popupBody',

        className: 'popup-slider badge-library bl-inner',

        template: com.epam.heroes.widget.givebadge.template.BadgeLibrary,

        events: {
            'click .body:not(.edit-assign-comment-mode) .badge:not(.disabled)': 'placeBadgeAssignPopup',
            'mouseenter .badge': 'showBadgeNameTooltip',
            'mouseleave .badge': 'removeBadgeNameTooltip',
            'click .voting-banner': 'openVotingPage',
            'input .search-badge-input': 'delayedQuickSearch',
            'click .search-badge:not(.open):not(.clear)': 'openSearchBadge',
            'click .clear': 'clearSearch',
            'click .give-one-more-badge': 'reopenBadgeLibrary'
        },

        /**
         * @override
         */
        initialize: function initialize(options) {
            OverlayPopup.prototype.initialize.apply(this, arguments);

            if (options && _.isNumber(options.animationDuration)) {
                this.animationDuration = options.animationDuration;
            }

            _.bindAll(this, 'windowResize', 'showMoreBadges', 'beforeRemove', 'remove');

            var badgeSections = this.model.getBadgeSections();

            this.listenTo(badgeSections, 'reset', this.onChangeBadgeSections);
            this.listenTo(App, 'open-badge-library', this.open);
            this.listenTo(App, 'badge:assign assignment:updated', this.hide);
            this.listenTo(App, 'badgeCount:change', this.restoreBadge);
            this.listenTo(App, 'badge:assign', this.restoreBadge);
            this.listenTo(App, 'give-badge-page:badge assignment info', this.singlePageBadgeAssignmentResults);

            this.throttledShowMoreBadges = _.throttle(this.showMoreBadges, 500);
            this.throttleResizeHandler = _.throttle(this.windowResize, 100);

            $(window).on('resize.' + this.getNamespace(), this.throttleResizeHandler);
        },

        /**
         * @override
         */
        restoreBadge: function restoreBadge() {
            this.model.getBadgeSections().restore();
            this.render();
        },

        reopenBadgeLibrary: function reopenBadgeLibrary() {
            var _this = this;

            this.model.unset('assignmentResultsInfo').unset('badgeToAssignId').unset('receivers');

            this.removeChildView('BadgeAssignPopup');
            $.when(this.fetchBadges()).done(function () {
                return _this.render();
            });
        },

        singlePageBadgeAssignmentResults: function singlePageBadgeAssignmentResults(params) {
            var assignmentResultsInfo = {
                imageUrl: params.imageUrl,
                badgeName: params.badgeName,
                isApprovable: params.isApprovable
            };
            this.model.set('assignmentResultsInfo', assignmentResultsInfo);
            this.render();
        },

        getModel: function getModel() {
            var model = this.model;
            this.badgeSections = model.getBadgeSections().toDerivedJSON();
            var badgeSections = this.badgeSections;
            var singlePage = this.model.isSinglePage();
            if (this.model.isSinglePage()) {
                this.animationDuration = 0;
            }

            if (badgeSections.length) {
                _.forEach(badgeSections, function (badgeSections) {
                    _.forEach(badgeSections.badges, function (badge) {
                        if (badge.image) {
                            badge.image.imageUrl = Utils.getBadgeResolution(badge.image.imageUrl);
                        }
                        var currentDate = moment().format("MMMM Do YYYY");
                        badge.renewalDays = Math.round(moment.duration(moment(badge.renewalDate, "MMMM Do YYYY").diff(moment(currentDate, "MMMM Do YYYY"))).asDays());
                    });
                });
            }

            return {
                sectionsNameEnabled: core.Application.features.getFeatureStateByName('features_sections_name_enabled'),
                badgeSections: badgeSections,
                singlePage: singlePage,
                assignmentResultsInfo: this.model.get('assignmentResultsInfo') || false,
                featuresConfig: featuresConfig
            };
        },

        hideBadges: function hideBadges(badgeSections) {
            var _this2 = this;

            var $searchBadgeInput = this.$(".search-badge-input");
            var searchValue = $searchBadgeInput.val();
            this.searchValue = searchValue;

            if (this.searchValue === "") {
                this.showAllBadges(badgeSections);
            } else if (!this.searchValue) {
                return;
            }

            if (this.childViews.BadgeAssignPopup) {
                this._removeOldPopup();
                $(".active-badge").removeClass("active-badge").animate();
            }

            var regV = new RegExp(this.searchValue.trim(), "ig");
            var sectionIsOpen = false;

            _.forEach(badgeSections, function (badgeSection) {
                sectionIsOpen = false;
                _.forEach(badgeSection.badges, function (badge) {
                    var $activeBadge = _this2.$('[data-badge-id=' + badge.id + ']');
                    var shouldShow = badge.name.match(regV) || badge.description.match(regV) || _this2.doesMatchBadgeTags(badge, regV);
                    if (shouldShow && shouldShow.length > 0) {
                        $activeBadge.show();
                        sectionIsOpen = true;
                    } else {
                        $activeBadge.hide();
                    }
                });
                var $badgeSection = $('.bls-title:contains(' + badgeSection.name + ')').closest('.badge-list-section');
                if (sectionIsOpen) {
                    $badgeSection.show();
                } else {
                    $badgeSection.hide();
                }
            });
        },

        doesMatchBadgeTags: function doesMatchBadgeTags(badge, regex) {
            var matches = null;
            _.forEach(badge.tags, function (tag) {
                if (tag.match(regex)) {
                    matches = tag.match(regex);
                }
            });
            return matches;
        },

        showAllBadges: function showAllBadges(badgeSections) {
            var _this3 = this;

            _.forEach(badgeSections, function (badgeSection) {
                _.forEach(badgeSection.badges, function (badge) {
                    var $activeBadge = _this3.$('[data-badge-id=' + badge.id + ']');
                    $activeBadge.show();
                });
                var $badgeSection = $('.bls-title:contains(' + badgeSection.name + ')').closest('.badge-list-section');
                $badgeSection.show();
            });
        },

        delayedQuickSearch: function delayedQuickSearch() {
            var _this = this;
            if (_this.quickSearchTimerId) {
                clearTimeout(_this.quickSearchTimerId);
            }
            _this.quickSearchTimerId = setTimeout(function () {
                _this.hideBadges(_this.badgeSections);
            }, SEARCH_DELAY);
        },

        openSearchBadge: function openSearchBadge(e) {
            var _this = this;
            var $search = _this.$('.search-badge');

            var callback = function callback(e) {
                var $target = $(e.target);

                if (!$target.parents('.badge').length && ($target.parents('header').length || $target.is('header')) && !$target.parents().andSelf().is($search) && !$target.hasClass('open')) {
                    _this.$('.search-badge').removeClass('open');
                    $search.parent().removeClass('invisibility');
                    _this.clearSearch();
                    $(document).off('click', callback);
                }
            };

            e.stopPropagation();
            $search.addClass('open');
            $search.parent().addClass('invisibility');
            this.$('.clear').css("visibility", "visible");
            $(document).on('click.' + this.name, callback);
        },

        clearSearch: function clearSearch() {
            var _this = this;
            var $search = _this.$('.search-badge-input');

            _this.$('.clear').css("visibility", "hidden");
            _this.$('.search-badge').removeClass("open");
            _this.$('.search-badge').parent().removeClass('invisibility');

            if (_this.quickSearchTimerId) {
                clearTimeout(_this.quickSearchTimerId);
            }
            if ($search.val()) {
                _this.searchValue = '';
                $search.val('');
            }

            _this.showAllBadges(_this.badgeSections);
            if (this.childViews.BadgeAssignPopup) {
                this._removeOldPopup();
            }
        },

        /**
         * @override
         */
        afterRender: function afterRender() {
            OverlayPopup.prototype.afterRender.apply(this);
            if (Utils.isTouchDevice()) {
                this.$el.addClass('touch-device');
            }
            this.checkFeatures();

            if (this.model.isSinglePage()) {
                $('.popup-slider').addClass('single-page');
            }
        },

        checkFeatures: function checkFeatures() {
            var genetic = new Genetics();
            var votingFeatureName = this.model.isSinglePage() ? 'features_voting_standalone_enabled' : 'features_voting_enabled';
            if (!core.Application.features.getFeatureStateByName(votingFeatureName)) {
                genetic.genes['features_voting'] = false;
            }

            var suggestBadgeFeatureName = this.model.isSinglePage() ? 'features_suggest_new_badge_standalone' : 'features_suggest_new_badge';
            if (!core.Application.features.getFeatureStateByName(suggestBadgeFeatureName)) {
                genetic.genes['features_suggest_new_badge'] = false;
            }
            genetic.bind(this);

            if (this.model.isSinglePage()) {
                $('.ask-support-button').css('display', 'none');
            }
        },

        /**
         * @override
         */
        beforeRemove: function beforeRemove() {
            OverlayPopup.prototype.beforeRemove.apply(this);

            $(window).off('resize.' + this.getNamespace());
            this.hideOverlay();
        },

        /**
         * @override
         */
        beforeOpen: function beforeOpen(params) {
            this.model.set(params);
            this.$el.show(); //just remove display now. Library is still hidden
            return this.fetchBadges();
        },

        /**
         * @override
         */
        afterOpen: function afterOpen() {
            var badgeToAssignId = this.model.get('badgeToAssignId');
            var badgeToAssign = this.model.getBadgeSections().getBadge(badgeToAssignId);
            var receivers = this.model.get('receivers');
            var assignOptions = {
                badgeToAssign: badgeToAssign
            };
            if (receivers) {
                //Only if defined! Not to override default field
                assignOptions.receivers = receivers;
            }

            if (badgeToAssign) {
                this._sendOpenBadgeStatistics(badgeToAssign.get('id'));
                var $activeBadge = this.$('[data-badge-id=' + badgeToAssignId + ']');
                $activeBadge.addClass(ACTIVE_BADGE_CLS);

                var viewOptions = {
                    $badgeToAssign: $activeBadge,
                    createNewPopup: true
                };

                return this.checkEmployeeToAssign(assignOptions, viewOptions);
            }
        },

        /**
         * @override
         */
        afterHide: function afterHide() {
            this.$el.hide(0);
            this.$el.empty();

            // To make the library create new badge-assign-popup and other child views
            // instead of using the older
            this.removeChildView('BadgeAssignPopup');

            this.model.unset('badgeToAssignId').unset('receivers');

            return this;
        },

        getNamespace: function getNamespace() {
            return this.name;
        },

        windowResize: function windowResize() {
            var $badge = this.$('.active-badge');
            var $popup = this.$('.badge-info-popup');

            if ($badge.length) {
                this.positionPopupArrow($popup, $badge);
            }
        },

        isInProgress: function isInProgress() {
            return this._inProgress === true;
        },

        setInProgress: function setInProgress() {
            this._inProgress = true;
        },

        setDone: function setDone() {
            this._inProgress = false;
        },

        onAppendEnd: function onAppendEnd() {
            // Rendering search results is finished
            this.setDone();
        },

        showMoreBadges: function showMoreBadges() {
            var _this = this;
            var model = this.model;
            var badgeSections = model.getBadgeSections();
            var currentBadgesCount = badgeSections.totalBadges();

            if (currentBadgesCount < badgeSections.totalCount && !this.isInProgress() && this.isToLoadMoreItems()) {

                var newBadgeSections = new BadgeSections(),
                    fetchOptions = this.getFetchOptions({
                    data: {
                        start: currentBadgesCount
                    },
                    success: function success() {
                        badgeSections.totalCount = newBadgeSections.totalCount;
                        badgeSections.merge(newBadgeSections);
                        _this.showBadges(newBadgeSections);
                    }
                });
                newBadgeSections.fetch(fetchOptions);
            }
        },

        showBadges: function showBadges(badgeSections) {
            // Rendering search results is in progress so no more results should be loaded
            this.setInProgress();

            var $content = this.$('.content');

            _.each(badgeSections.toDerivedJSON(), function (badgeSection) {
                var sectionId = badgeSection.id;
                var $section = $('#section-' + sectionId);
                var viewModel = badgeSection;

                if (!$section.length) {
                    var section = com.epam.heroes.widget.givebadge.template.BadgeSection(viewModel);
                    $content.append(section);
                } else {
                    var badges = com.epam.heroes.widget.givebadge.template.Badges(_.pick(viewModel, 'badges'));
                    $section.find('.badge-list').append(badges);
                }
            });

            this.onAppendEnd();
        },

        /**
         * Opens popup with badge name
         *
         * @param {Event} e
         */
        showBadgeNameTooltip: function showBadgeNameTooltip(e) {
            // For handling first click event on IOS
            if (navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPod/i)) {
                return;
            }
            e && e.preventDefault();

            var $badge = $(e.currentTarget);
            // As parent element you should take element in relative to which position of $badge counts
            var $parentElement = this.$el;
            Utils.calculateTooltipPosition($badge, $parentElement);
        },

        /**
         * Removes popup for current badge
         *
         * @param {Event} e
         */
        removeBadgeNameTooltip: function removeBadgeNameTooltip(e) {
            e.preventDefault();
            $(e.currentTarget).find('.badge-name-tooltip').removeClass('tooltip-position-left tooltip-position-right tooltip-position-top');
        },

        /**
         * Callback on "badge sections reset" event
         * Renders view and store model
         */
        onChangeBadgeSections: function onChangeBadgeSections() {
            this.model.storePermanentData();
            this.render();
        },

        /**
         * Checks whether there is less than PIXELS_TO_THE_BOTTOM to the end of the widget's library.
         *
         * @returns {Boolean}
         */
        isToLoadMoreItems: function isToLoadMoreItems() {
            var $libraryWrapper = this.$el;
            var $scrollableContent = this.$('.scrollable-content');
            return $scrollableContent.height() - $libraryWrapper.height() - $libraryWrapper.scrollTop() < PIXELS_TO_THE_BOTTOM;
        },

        /**
         * Sends request to fetch badges if they are not loaded
         * or shows them in other case
         *
         * @param {Object} [fetchOptions]
         * @returns {XMLHttpRequest|undefined}
         */
        fetchBadges: function fetchBadges(fetchOptions) {
            var model = this.model;
            var badgeSections = model.getBadgeSections();
            var request;

            if (badgeSections.totalBadges()) {
                this.showBadges(badgeSections);
            } else {
                var options = this.getFetchOptions(fetchOptions);
                request = badgeSections.fetch(options);
            }

            return request;
        },

        /**
         * Invokes model restoring
         */
        clearAssignForm: function clearAssignForm() {
            this.model.clear();
            this.model.restore({ silent: true });
        },

        /**
         * Set the place of popup and sets badge to its model
         * sets badge item as active
         *
         * @param {Event} [e]
         */
        placeBadgeAssignPopup: function placeBadgeAssignPopup(e) {
            e && e.preventDefault();

            var _this = this;
            var $badge = $(e.currentTarget);
            var badgeId = $badge.attr('data-badge-id');

            this._sendOpenBadgeStatistics(badgeId);

            var $oldBadge = _this.$('.' + ACTIVE_BADGE_CLS);

            if (!$badge.hasClass('limited')) {

                $oldBadge.removeClass(ACTIVE_BADGE_CLS);
                _this.$('.' + BADGE_BEFORE_POPUP_CLS).removeClass(BADGE_BEFORE_POPUP_CLS);

                var popup = this.getBadgeAssignPopup();
                var popupModelData = popup && popup.model.attributes || {};

                // Second click on the badge just hides popup
                if ($badge[0] === $oldBadge[0]) {
                    popup.hide();
                    return;
                }
                $badge.addClass(ACTIVE_BADGE_CLS);

                //Compares old one badge and new one placed in one row
                var isInSameRow = $oldBadge.length && $badge.offset().top === $oldBadge.offset().top;
                var viewOptions = {
                    $badgeToAssign: $badge,
                    $oldBadge: $oldBadge,
                    keepPopupPlace: isInSameRow
                };
                var assignOptions = this._collectAssignOptions(popupModelData, badgeId);
                assignOptions.singlePage = this.model.isSinglePage();

                var showPopupWithParams = this.checkEmployeeToAssign.bind(_this, assignOptions, viewOptions);
                var showAfterRemovingOldPopup = !isInSameRow && popup;

                $.when(showAfterRemovingOldPopup && this._hidePopup('BadgeAssignPopup')).done(showPopupWithParams);
            }
        },

        _sendOpenBadgeStatistics: function _sendOpenBadgeStatistics(badgeId) {
            $.ajax({
                url: config.api.stats.openBadge.url(badgeId),
                contentType: 'application/json',
                type: 'POST',
                blockUI: false
            });
        },

        /**
         * Collects assign data from old popup model and selected badge and receivers
         * to set them to new badge-assign-popup
         *
         * @param {Object} oldAssignData
         * @param {String} badgeId
         * @returns {Object}
         * @private
         */
        _collectAssignOptions: function _collectAssignOptions(oldAssignData, badgeId) {
            var sectionSettings = this.model.getBadgeSectionSettings(badgeId);

            var receivers = this.model.get('receivers') || oldAssignData.receivers;
            var badgeSections = this.model.getBadgeSections();

            var badgeToAssign = badgeSections.getBadge(badgeId);
            var previousBadge = oldAssignData.badgeToAssign;

            var badgeBelongsOtherScope = previousBadge && (this._isBadgeExceptional(badgeToAssign) || this._isBadgeExceptional(previousBadge));

            var removePreviousGroupAssignment = !!(receivers && receivers.length > 1 && sectionSettings);

            var assignOptions = badgeBelongsOtherScope || removePreviousGroupAssignment ? {} : oldAssignData;

            _.extend(assignOptions, {
                badgeToAssign: badgeToAssign,
                programSettings: sectionSettings,
                editAssignCommentMode: false
            });

            if (badgeBelongsOtherScope || removePreviousGroupAssignment || !receivers) {
                //Only if defined! Not to override default field
                delete assignOptions.receivers;
            } else {
                assignOptions.receivers = receivers;
            }
            return assignOptions;
        },

        /**
         * Opens widget's assignment popup.
         * Is used for outer views to open badge assign functionality
         *
         * @param {Object} assignOptions
         * @param {Array} [assignOptions.receivers]
         * @param {Object} [assignOptions.badgeToAssign]
         *
         * @param {Object} viewOptions
         * @param {jQuery} viewOptions.$badgeToAssign
         * @param {jQuery} [viewOptions.$oldBadge]
         * @param {Boolean} [viewOptions.keepPopupPlace]
         * @param {Boolean} [viewOptions.createNewPopup]
         */
        showBadgeAssignPopup: function showBadgeAssignPopup(assignOptions, viewOptions) {
            var isBudgetEmpty = void 0;
            var badgeId = assignOptions.badgeToAssign.id;
            var sectionSettings = this.model.getBadgeSectionSettings(badgeId);

            this._removeOldPopup();

            if (sectionSettings) {
                assignOptions.programSettings ? assignOptions : assignOptions.programSettings = sectionSettings;
                isBudgetEmpty = assignOptions.programSettings.remainingPointCount;
            } else {
                isBudgetEmpty = true;
            }

            var newPopup = this.createBadgeAssignPopup(assignOptions);
            if (!viewOptions.$badgeToAssign.hasClass('limited') && isBudgetEmpty) {
                newPopup.openPopupAfter(this._defineBadgeBeforePopup(viewOptions.$badgeToAssign), !viewOptions.keepPopupPlace);
            }

            if (!viewOptions.keepPopupPlace) {
                this.scrollToElement(newPopup.$el);

                viewOptions.$badgeToAssign.hasClass('limited') && viewOptions.$badgeToAssign.removeClass('active-badge');
                !isBudgetEmpty && viewOptions.$badgeToAssign.removeClass('active-badge');
            }

            // Position popup's arrow
            this.positionPopupArrow(newPopup.$el, viewOptions.$badgeToAssign, viewOptions.$oldBadge);
        },

        /**
         * Paste employee, who's profile tab you are checking, as a receiver
         * In case of telesope assignment checks right receivers
         */
        filterAssignmentEmployees: function filterAssignmentEmployees(assignOptions, viewOptions) {
            var receivers = this.model.get('receivers');
            var newReceivers = receivers.toDerivedJSON();
            var badge = assignOptions.badgeToAssign;
            var restrictToAccount = badge.get('accounts').length;
            var singleAssignment = badge.get('approvable') && !badge.get('settings').allowMultipleAssignment;
            var userId = this.model.getUser().id;
            var ajaxArray = [];
            var _this = this;

            if (!restrictToAccount) {
                assignOptions = _.extend(assignOptions, {
                    receivers: new AssignEntryCollection(newReceivers)
                });

                return this.showBadgeAssignPopup(assignOptions, viewOptions);
            }

            _.forEach(receivers.models, function (receiver) {
                var searchStr = receiver.get('firstName') && receiver.get('lastName') ? receiver.get('firstName') + ' ' + receiver.get('lastName') : receiver.get('name');
                var id = receiver.get('id');

                ajaxArray.push($.ajax({
                    url: config.api.search.url,
                    data: {
                        search: searchStr,
                        exclEmpl: userId,
                        badgeId: badge.get('id'),
                        length: SEARCH_LENGTH
                    },
                    success: function success(response) {
                        var employee = _.find(response.content, function (item) {
                            return item.result.id === id;
                        });

                        if (!employee) {
                            newReceivers = _.filter(newReceivers, function (employee) {
                                return employee.id !== id;
                            });
                        }
                    }
                }));
            });

            $.when.apply($, ajaxArray).then(function () {
                if (singleAssignment && newReceivers.length) {
                    newReceivers = [newReceivers.pop()];
                }

                assignOptions = _.extend(assignOptions, {
                    receivers: new AssignEntryCollection(newReceivers)
                });
                return _this.showBadgeAssignPopup(assignOptions, viewOptions);
            });
        },

        checkEmployeeToAssign: function checkEmployeeToAssign(assignOptions, viewOptions) {
            var employeeToAssign = this.getCurrentEmployee() && this.getCurrentEmployee().toJSON();
            var receivers = this.model.get('receivers');

            if (!employeeToAssign) {
                return this.showBadgeAssignPopup(assignOptions, viewOptions);
            }

            if (receivers && receivers.length) {
                return this.filterAssignmentEmployees(assignOptions, viewOptions);
            } else {
                this.model.set({
                    receivers: new AssignEntryCollection(employeeToAssign)
                });
                return this.filterAssignmentEmployees(assignOptions, viewOptions);
            }
        },

        /**
         * @private
         */
        _removeOldPopup: function _removeOldPopup() {
            var oldPopup = this.getBadgeAssignPopup();

            if (oldPopup) {
                this.removeChildView('BadgeAssignPopup');
            }
        },

        /**
         * Returns jQuery element of badge to set popup after
         *
         * @param {jQuery} $badgeToAssign
         * @returns {jQuery}
         * @private
         */
        _defineBadgeBeforePopup: function _defineBadgeBeforePopup($badgeToAssign) {
            var $badgeBeforePopup = this.$('.' + BADGE_BEFORE_POPUP_CLS);
            $badgeBeforePopup = $badgeBeforePopup.length ? $badgeBeforePopup : this.findLastBadgeInRow($badgeToAssign);
            return $badgeBeforePopup;
        },

        /**
         * Finds last badge in the row where specified badge is located.
         *
         * @param $badge
         * @returns Last badge in the row
         */
        findLastBadgeInRow: function findLastBadgeInRow($badge) {
            var $lastBadgeInRow = $badge;
            var badgeTopPosition = $badge.position().top;

            $badge.nextAll().filter(function () {
                return $(this).css("display") !== "none";
            }).each(function () {
                var minTopValue = $(this).position().top - 1;
                var maxTopValue = $(this).position().top + 1;

                if (badgeTopPosition >= minTopValue && badgeTopPosition <= maxTopValue) {
                    $lastBadgeInRow = $(this);
                }
            });

            return $lastBadgeInRow;
        },

        /**
         * Scrolls to the popup if necessary.
         *
         * @param {jQuery} $element
         */
        scrollToElement: function scrollToElement($element) {
            var top;
            if (Utils.isMobile()) {
                var libraryWrapper = this.$el.find('#badgeLibraryWrapper');
                var badge = this.$el.find('.active-badge');
                top = $element.position().top - badge.outerHeight();
                libraryWrapper.animate({ scrollTop: top });
            } else {
                var badge = this.$el.find('.active-badge');
                top = badge.position() && badge.position().top + // checking badge.position() is added for tests
                badge.outerHeight() - this.$el.find('header').outerHeight();
                this.$el.animate({ scrollTop: top });
            }
        },

        /**
         * Positions popup's arrow under the specified badge.
         *
         * @param {jQuery} $popup
         * @param {jQuery} $badge
         * @param {jQuery} $oldBadge
         */
        positionPopupArrow: function positionPopupArrow($popup, $badge, $oldBadge) {
            var $arrow = $popup.find('.badge-pointer');

            Utils.positionArrow($arrow, $badge, $oldBadge);
        },

        getBadgeAssignPopup: function getBadgeAssignPopup() {
            return this.childViews['BadgeAssignPopup'];
        },

        /**
         * Removes popup from dom and from childViews
         *
         * @param {String} popupName
         * @returns {jQuery.Deferred.promise} [promise] for closing popup animation
         */
        _hidePopup: function _hidePopup(popupName) {
            var popupView = this.childViews[popupName];

            if (popupView) {
                // Hack! Cause .$el is spoiled during reposition and re-render of the popup
                popupView.$el = this.$(popupView.getSelector());
                return popupView.hide(function () {
                    popupView.remove();
                }).promise();
            }
        },

        /**
         * Checks if badgeModel is exceptional (approvable or account-badgeModel)
         *
         * @param badgeModel
         * @returns {Boolean}
         * @private
         */
        _isBadgeExceptional: function _isBadgeExceptional(badgeModel) {
            var badge = badgeModel.attributes || badgeModel;
            return !!(badge && (badge.accounts && badge.accounts.length || badge.approvable || badge.externalProgram));
        },

        /**
         * @param {Object} [assignOptions]
         * @returns {Backbone.View}
         */
        createBadgeAssignPopup: function createBadgeAssignPopup(assignOptions) {
            var user = this.model.getUser();
            var userToken = this.model.getUserToken();
            var popupName = 'BadgeAssignPopup';
            var model = this.model;

            // We want to keep user's data, but change view to newer one
            var modelParams = _.extend(assignOptions, {
                userToken: userToken,
                user: user
            });

            return this.addChildView(new BadgeAssignPopup({
                name: popupName,
                model: new BadgeAssignPopupModel(_.extend(modelParams, {
                    isSinglePage: model.isSinglePage()
                }))
            }));
        },

        getCurrentEmployee: function getCurrentEmployee() {
            var currentEmployee = this.model.get('currentEmployee');
            var currentEmployeeId = currentEmployee && currentEmployee.get('id');
            var userId = this.model.getUser().get('id');

            if (currentEmployeeId && currentEmployeeId !== userId) {
                return currentEmployee;
            }
        },

        /**
         * @param options
         */
        getFetchOptions: function getFetchOptions(options) {
            options = options || {};
            var _this = this,
                model = this.model,
                userToken = model.getUserToken(),
                completeCallback = options.complete,
                beforeSendCallback = options.beforeSend,
                fetchOptions = {
                blockUI: this.model.isSinglePage(),
                data: {
                    //TODO: Init length when bug in Backend will be fixed (badges are loaded in bad sequence)
                    //length: BADGES_TO_LOAD
                },
                beforeSend: function beforeSend() {
                    _this.setInProgress();
                    if (beforeSendCallback) {
                        beforeSendCallback();
                    }
                },
                complete: function complete() {
                    _this.setDone();
                    if (completeCallback) {
                        completeCallback();
                    }
                }
            };
            _.merge(fetchOptions, options);
            if (userToken) {
                _.merge(fetchOptions, {
                    headers: {
                        Authorization: 'Token ' + userToken
                    }
                });
            }

            return fetchOptions;
        },

        openVotingPage: function openVotingPage(e) {
            e.preventDefault();

            if (this.parent.name === "VotingPage") {
                this.hide();
            }
            this.getRouter().navigateVotingPage();
        }
    });

    return BadgeLibraryView;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js"), __webpack_require__(/*! moment */ "./src/main/resources/static-site/js/libs/vendor/moment/moment.js")))

/***/ }),

/***/ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/view/NotifyColleaguesView.js":
/*!*********************************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/view/NotifyColleaguesView.js ***!
  \*********************************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
/* WEBPACK VAR INJECTION */(function($) {var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var core = __webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js");
    var config = __webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js");

    var App = __webpack_require__(/*! com/epam/heroes/portal/core/App */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/App.js");
    var Utils = __webpack_require__(/*! com/epam/heroes/portal/core/util/Utils */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/util/Utils.js");

    var featuresConfig = __webpack_require__(/*! FeatureConfig */ "./src/main/resources/static-site/featuresConfig.js");

    __webpack_require__(/*! template/com/epam/heroes/widget/givebadge/SearchFilter.soy */ "./src/main/soy/com/epam/heroes/widget/givebadge/SearchFilter.soy");
    __webpack_require__(/*! template/com/epam/heroes/widget/givebadge/NotifyColleagues.soy */ "./src/main/soy/com/epam/heroes/widget/givebadge/NotifyColleagues.soy");

    __webpack_require__(/*! select2v4 */ "./src/main/resources/static-site/js/libs/vendor/select2/dist/js/select2.full.js");
    __webpack_require__(/*! perfect-scrollbar.jquery */ "./src/main/resources/static-site/js/libs/vendor/perfect-scrollbar/js/perfect-scrollbar.jquery.js");

    var COUNT_OF_ROWS_FOUND_EMPLOYEES_WHO_WILL_BE_NOTIFIED = 3;

    var templates = com.epam.heroes.widget.givebadge.template;

    var NotifyColleaguesView = core.BaseView.extend({

        template: templates.NotifyColleagues,

        events: {
            'click #add-rms-btn': 'onAddRMs',
            'click #clear-cc': 'onClearCC',
            'click .remove-chosen': 'removeRMFromField'
        },

        /**
         * @override
         */
        initialize: function initialize() {
            core.BaseView.prototype.initialize.apply(this, arguments);

            _.bindAll(this, '_parseResponse', '_error');
        },

        /**
         * @override
         */
        afterRender: function afterRender() {
            var model = this.model;

            var $notifyColleagues = this._initSelect2();
            this._restoreInformedEmployees($notifyColleagues);

            this.listenTo(App, 'receiversEdition', this.toggleAddRMsButton);
            this.listenTo(model, 'change:informedEmployees', this.handleInformedEmployees);

            if (!featuresConfig.featureAddRmBadgeAssignPopupEnabled) {
                var $addRMsButton = this.$('#add-rms-btn');
                $addRMsButton.remove();
            }
        },

        /**
         * @override
         * @returns {Object}
         */
        getModel: function getModel() {
            var model = this.model;
            return {
                isAddRMsDisabled: !model.isReceiversNumberValid()
            };
        },

        /**
         * Listener for clear CC field button
         *
         * @param {Event} event
         */
        onClearCC: function onClearCC(event) {
            event.preventDefault();
            this.model.trigger('clearCC');

            this.clearCCField();
            this.removeInformedEmployees();
            this.toggleAddRMsButton();
        },

        /**
         * Clears input from all selected items
         */
        clearCCField: function clearCCField() {
            var $notifyColleagues = this.$('select[name="notifyColleagues"]');
            $notifyColleagues.select2('val', '');
            $notifyColleagues.empty();
            this.$('.select2-selection__choice').remove();
        },

        /**
         * Remove all informed employees from array after cleaning input
         */
        removeInformedEmployees: function removeInformedEmployees() {
            var informedEmployees = this.model.getInformedEmployees();
            informedEmployees.length = 0;
            this.model.trigger('change:informedEmployees');
        },

        /**
         * Listener for AddRMs button click
         *
         * @param {Event} event
         */
        onAddRMs: function onAddRMs(event) {
            event.preventDefault();

            var receivers = this.model.getReceivers();
            var receiversIds = _.map(receivers.models, function (receiver) {
                return receiver.id;
            });

            var ajaxOptions = {
                url: config.api.employees.rms.url,
                type: 'POST',
                data: JSON.stringify(receiversIds),
                contentType: 'application/json',
                context: this,
                error: this._error,
                success: function success(response) {
                    this.addRMs(response.content);
                }
            };

            $.ajax(ajaxOptions);
        },

        /**
         * Displays errors
         *
         * @param {XMLHttpRequest} xhr
         * @private
         */
        _error: function _error(xhr) {
            this.displayErrors(xhr);
        },

        /**
         * Add rms main function
         *
         * @param {Array} unparsedRMs
         */
        addRMs: function addRMs(unparsedRMs) {
            var parsedRMs = this.parseRMs(unparsedRMs);
            this.addRMsToInformedEmployees(parsedRMs);
            this.addRMsToField(parsedRMs);
            this.$('#add-rms-btn').hide();
        },

        /**
         * Parse json objects according to require objects for select2
         *
         * @param {Array} unparsedRMS
         * @returns {Array}
         */
        parseRMs: function parseRMs(unparsedRMS) {
            var rms = this._putIntoArray(unparsedRMS);
            var parsedRMS = _.map(rms, function (rm) {
                var text = rm.firstName && rm.lastName ? rm.firstName + ' ' + rm.lastName : rm.name;

                return _.assign({}, rm, { // _.assign left some unused attributes in object
                    text: text,
                    isRM: true
                });
            });
            return parsedRMS;
        },

        /**
         * Push rm objects to informedEmployees array.
         *
         * @param {Array} rms
         */
        addRMsToInformedEmployees: function addRMsToInformedEmployees(rms) {
            var informedEmployees = this.model.getInformedEmployees();

            _.forEach(rms, function (rm) {
                var matchEmployeeIndex = _.findIndex(informedEmployees, function (informedEmployee) {
                    return informedEmployee.id === rm.id;
                });
                if (matchEmployeeIndex === -1) {
                    informedEmployees.push(rm);
                } else {
                    informedEmployees.splice(matchEmployeeIndex, 1, rm);
                }
            });
            this.model.trigger('change:informedEmployees');
        },

        /**
         *  Add resource managers to field to already selected people,
         *  if they haven't already added
         *
         * @param {Array} rms
         */
        addRMsToField: function addRMsToField(rms) {
            var rmsToAdd = rms;
            var $alreadyAddedEmployeesList = this.$('.select2-selection__choice');
            var $chosenEmployeesList = this.$('ul.select2-selection__rendered');

            //exclude already added RM
            if ($alreadyAddedEmployeesList.length > 0) {
                _.forEach($alreadyAddedEmployeesList, function (alreadyAddedEmployee) {
                    var alreadyAddedEmployeeName = { name: $(alreadyAddedEmployee).contents()[1].textContent };
                    if (_.some(rmsToAdd, alreadyAddedEmployeeName)) {
                        $(alreadyAddedEmployee).remove();
                    }
                });
            }

            _.forEach(rmsToAdd, function (rmToAdd) {
                var chosenEmployee = templates.ChosenEmployeeItem({
                    userName: rmToAdd.name
                });
                $chosenEmployeesList.prepend(chosenEmployee);
            });

            $chosenEmployeesList.find('input.select2-search__field').width('auto').attr('placeholder', '');
        },

        /**
         *  Removes resource managers from field of already selected people.
         *
         * @param {Event} e
         */
        removeRMFromField: function removeRMFromField(e) {
            var informedEmployees = this.model.getInformedEmployees();
            var $itemToDelete = $(e.currentTarget).closest('.select2-selection__choice');
            var $containter = this.$('.select2-container.select2-container--default.select2-container--open').not('.select2');
            var itemToDeleteName = $itemToDelete.find('.rm-name').text();

            var employeeToDelete = _.remove(informedEmployees, function (employee) {
                return employee.name === itemToDeleteName;
            });
            this.$('option[value="' + employeeToDelete[0].id + '"]').remove();
            $itemToDelete.remove();

            $containter.css("visibility", "hidden"); //dirty hack for select2 in mobile version

            this.toggleAddRMsButton();
            this.model.trigger('change:informedEmployees');
        },

        /**
         * Syncronizes Clear Button state and
         * employees to notify according to model.
         *
         */
        handleInformedEmployees: function handleInformedEmployees() {
            var _this = this;
            setTimeout(function () {
                _this.syncDisplayedInformedEmployees();
                _this.toggleClearButton();
            }, 0);
        },

        /**
         * Finds RMs in model and add them to input.
         * This function is needed because select2
         * rewrites selected items list in input field
         *
         */
        syncDisplayedInformedEmployees: function syncDisplayedInformedEmployees() {
            var informedEmployees = this.model.getInformedEmployees();
            var rms = _.filter(informedEmployees, _.matches({ 'isRM': true }));
            this.addRMsToField(rms);
        },

        /**
         * Checks whether to display clear button for select2.
         *
         */
        toggleClearButton: function toggleClearButton() {
            var informedEmployees = this.model.getInformedEmployees();
            var $clearCCButton = this.$('#clear-cc');
            $clearCCButton.toggle(informedEmployees.length > 0);
        },

        /**
         * Checks whether to display clear button for select2.
         *
         */
        toggleAddRMsButton: function toggleAddRMsButton() {
            var isReceiversNumberValid = this.model.isReceiversNumberValid();
            var $addRMsButton = this.$('#add-rms-btn');
            $addRMsButton.toggle(isReceiversNumberValid);
        },

        /**
         * Inits select2 for notifyColleagues input
         *
         * @returns {JQuery}
         * @private
         */
        _initSelect2: function _initSelect2() {
            var _this2 = this;

            var $notifyColleagues = this.$('select[name="notifyColleagues"]');

            var informedEmployees = this.model.getInformedEmployees();

            var options = {
                placeholder: 'Send notification to',
                multiple: true,
                cache: true,
                allowClear: false,
                width: '100%',
                dropdownParent: this.$el,
                minimumInputLength: 2,
                templateResult: this._formatDropdownItem.bind(this),
                ajax: {
                    url: config.api.search.url,
                    dataType: 'json',
                    delay: 100,
                    data: function data(_data) {
                        return {
                            length: COUNT_OF_ROWS_FOUND_EMPLOYEES_WHO_WILL_BE_NOTIFIED,
                            search: _data.term && _data.term.trim() || '',
                            type: 'EMPLOYEE',
                            compact: true,
                            exclEmpl: []
                        };
                    },
                    processResults: this._parseResponse,
                    params: {
                        blockUI: false
                    }
                },
                templateSelection: function templateSelection(data) {
                    return data.name;
                }
            };

            $notifyColleagues.select2(options).on('select2:select select2:unselect', function (e) {
                _this2._handleInputChanging(e, informedEmployees);
            });

            this.$('.select2-selection--multiple').perfectScrollbar();
            return $notifyColleagues;
        },

        /**
         * Makes matched text bold
         *
         * @param {String} inputText
         * @param {String} search
         * @returns {String}
         * @private
         */
        _makeMatchBold: function _makeMatchBold(inputText, search) {
            search = search.trim().replace(/[^\w\s]/ig, '');
            var keywords = search.trim().replace(/\s+/ig, '|');
            return inputText.replace(new RegExp('(' + keywords + ')', 'ig'), '<strong>$1</strong>');
        },

        /**
         * Returns template for one menu item
         *
         * @param {Object} result
         * @private
         */
        _formatDropdownItem: function _formatDropdownItem(result) {
            var $containter = this.$('.select2-container.select2-container--default.select2-container--open').not('.select2');

            if (result.id) {
                // if we have successful response
                $containter.css("visibility", "visible"); //dirty hack for select2 in mobile version
                return $(com.epam.heroes.widget.givebadge.template.EmployeeItem({
                    item: result
                }));
            } else {
                $containter.css("visibility", "hidden"); //dirty hack for select2 in mobile version
            }
        },

        /**
         * Handles changes of notifyColleagues input
         *
         * @param {Event} e
         * @param {Array} informedEmployees
         * @private
         */
        _handleInputChanging: function _handleInputChanging(e, informedEmployees) {
            if (e.params.data.selected) {
                this._addItemIntoField(e.params.data, informedEmployees);
            } else {
                this._removeItemFromInput(e.params.data, informedEmployees);
            }
            this.model.trigger('change:informedEmployees');
        },

        /**
         * Adding item to input field.
         *
         * @param {Object} itemToAdd
         * @param {Array} informedEmployees
         * @private
         */
        _addItemIntoField: function _addItemIntoField(itemToAdd, informedEmployees) {
            var _this3 = this;

            var _Utils = Utils;
            var employeesToAdd = this._putIntoArray(itemToAdd);

            _.forEach(employeesToAdd, function (employeeToAdd) {
                if (employeeToAdd.noResponse && _Utils.isEmail(employeeToAdd.name)) {
                    employeeToAdd.email = employeeToAdd.name;
                }

                // This event is triggered twice ->
                // to prevent double parsing
                var isEmployeeParsed = Boolean($(_this3).find('img').length);

                var $addedEmployee = _this3.$('.select2-selection__choice').filter(function () {
                    return $(this).contents()[1] === employeeToAdd.name && !isEmployeeParsed;
                });

                var employeePhoto = employeeToAdd.photo;
                var employeeToInformImage = templates.EmployeeToInformItem({
                    imgSrc: employeePhoto,
                    userName: employeeToAdd.name
                });
                $addedEmployee.html(employeeToInformImage);

                if (!_.some(informedEmployees, employeeToAdd)) {
                    informedEmployees.push(employeeToAdd);
                }
            });
        },
        /**
         * Removing item from selected area according to logic.
         *
         * @param {Object} itemToDelete
         * @param {Array} informedEmployees
         * @private
         */
        _removeItemFromInput: function _removeItemFromInput(itemToDelete, informedEmployees) {
            var itemToDeleteId = itemToDelete.id;
            var isReceiversNumberValid = this.model.isReceiversNumberValid();
            if (itemToDelete.isRM && isReceiversNumberValid) {
                this.$('#add-rms-btn').show();
            }
            _.remove(informedEmployees, function (employee) {
                return employee.id === itemToDeleteId;
            });
            this.$('option[value="' + itemToDeleteId + '"]').remove();
        },

        /**
         * Parses response
         *
         * @param {Object} response
         * @param {Object} request
         * @returns {{results: Array}}
         * @private
         */
        _parseResponse: function _parseResponse(response, request) {
            var _this = this;
            if (response.content.length) {
                return {
                    results: _.map(response.content, function (searchItem) {
                        var employee = response.refs[searchItem.result.url];
                        var name = employee.firstName && employee.lastName ? employee.firstName + ' ' + employee.lastName : employee.name;
                        var firstName;
                        var lastName;
                        if (employee.firstName && employee.lastName) {
                            firstName = _this._makeMatchBold(employee.firstName, request.term);
                            lastName = _this._makeMatchBold(employee.lastName, request.term);
                        }
                        return {
                            id: employee.id,
                            firstName: firstName,
                            lastName: lastName,
                            name: name,
                            position: employee.position,
                            email: employee.email,
                            photo: employee.photo
                        };
                    })
                };
            } else {
                var _Utils = Utils;
                return {
                    results: _Utils.isEmail(request.term) ? [{
                        noResponse: true,
                        name: request.term,
                        id: request.term + '_' + request.term
                    }] : []
                };
            }
        },

        /**
         * Serializes elem into array
         *
         * @param {Object|Array} elem
         * @returns {Array} array of elems
         * @private
         */
        _putIntoArray: function _putIntoArray(elem) {
            return _.isArray(elem) ? elem : [elem];
        },

        /**
         * Puts information about employees into select2 and clears informedEmployees
         *
         * @param {JQuery} $notifyColleagues
         * @private
         */
        _restoreInformedEmployees: function _restoreInformedEmployees($notifyColleagues) {
            var informedEmployees = this.model.getInformedEmployees();
            if (informedEmployees.length) {
                var select2data = $notifyColleagues.select2('data');
                select2data.push.apply(select2data, informedEmployees);
                this.addRMsToField(informedEmployees);
                $notifyColleagues.select2('data', select2data, true);
            }
        }
    });

    return NotifyColleaguesView;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js")))

/***/ }),

