/***/ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/model/EmployeeProfileItem.js":
/*!****************************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/portal/core/model/EmployeeProfileItem.js ***!
  \****************************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {
    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var config = __webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js");

    var EmployeeOptionModel = __webpack_require__(/*! com/epam/heroes/portal/core/model/EmployeeOptionModel */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/model/EmployeeOptionModel.js");
    var Badge = __webpack_require__(/*! com/epam/heroes/core/model/Badge */ "./src/main/resources/static-site/js/com/epam/heroes/core/model/Badge.js");

    var EmployeeProfileItem = EmployeeOptionModel.extend({

        urlRoot: config.api.employee.badges.achievements,

        defaults: function defaults() {
            return {
                badge: new Badge(),
                employeeBadgeSetLevel: null,
                levels: []
            };
        },

        /**
         * For employeeBadge employeeId = assigner id
         *
         * @override
         */
        initialize: function initialize(options) {
            options = options || {};
            options.employeeId = options.employeeId ? options.employeeId : options.assigner && options.assigner.id;

            EmployeeOptionModel.prototype.initialize.apply(this, arguments);

            if (options.badge) {
                this.set('badge', new Badge(options.badge));
            }
        },

        /**
         * @override
         * @param {{content}} response
         * @returns {Object}
         */
        parse: function parse(response) {
            var data = response.content;
            if (data) {
                var badge = data.baseBadge ? data.baseBadge : data.badge;
                badge = response.refs[badge.url];

                return {
                    levels: data.levels,
                    badge: new Badge(badge, { parse: true }),
                    badgeSetId: data.badgeSetId,
                    badgeWeight: badge.weight
                };
            }
            return response;
        },

        /**
         * @override
         *
         * To convert to JSON inner model too
         *
         * @returns {Object}
         */
        toDerivedJSON: function toDerivedJSON() {
            var json = EmployeeOptionModel.prototype.toDerivedJSON.apply(this, arguments);
            json.image = this.getImage();
            json.badge = json.badge instanceof Badge ? json.badge.toDerivedJSON() : json.badge;
            return json;
        },

        getEmployeeBadgeSetLevel: function getEmployeeBadgeSetLevel() {
            return this.get('employeeBadgeSetLevel');
        },

        getBadge: function getBadge() {
            return this.get('badge');
        },

        getImage: function getImage() {
            var img = this.deepGet('badge', 'image');
            return img;
        },

        getBadgeWeight: function getBadgeWeight() {
            return this.get('badgeWeight');
        },

        /**
         * Checks if passed item is badgeSet-entity
         *
         * @returns {Boolean}
         */
        isBadgeSet: function isBadgeSet() {
            var employeeBadgeSetLevel = this.getEmployeeBadgeSetLevel();
            return Boolean(employeeBadgeSetLevel);
        },

        /**
         * Extends badgeSets model by adding data from employeeBadges collection:
         *  defines the level of set for current employee by counting his badges
         *
         * @override
         * @param commentsData
         * @param refs
         */
        extendModelWithEmployeeItemsData: function extendModelWithEmployeeItemsData(commentsData, refs) {
            var item = this.get('item');
            var collected = commentsData.length;

            item.set({ collected: collected }).initCurrentLevel();

            if (collected) {
                var levels = item.get('levels');
                var nextLevelIndex = item.get('nextLevelIndex');
                var badge = refs[commentsData[0].badge.url];

                //Acquired level is defined only after initCurrentLevel() completed
                var currentLevel = item.get('currentLevel');
                var acquiredLevel = item.get('acquiredLevel');
                nextLevelIndex = Math.min(acquiredLevel, levels.length - 1);

                item.set({
                    collected: collected,
                    badge: new Badge(badge),
                    badgeWeight: badge.weight,
                    employeeBadgeSetLevel: _.merge(item.get('employeeBadgeSetLevel'), {
                        name: currentLevel.name,
                        description: currentLevel.description,
                        nextLevelAmount: levels[nextLevelIndex].amount,
                        image: currentLevel.image,
                        acquiredLevel: acquiredLevel
                    })
                });
            }
        }
    });

    return EmployeeProfileItem;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));

/***/ }),

