/***/ (function(module, exports, __webpack_require__) {

"use strict";
var __WEBPACK_AMD_DEFINE_ARRAY__, __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_ARRAY__ = [__webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js"), __webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js")], __WEBPACK_AMD_DEFINE_RESULT__ = (function (core, config) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var Badge = core.BaseModel.extend({

        urlRoot: config.api.badges.url,

        /**
         * @override
         * @param {Object} [options]
         */
        initialize: function initialize(options) {
            if (options && options.badgeMultipleAssignmentSettings) {
                this.set('settings', options.badgeMultipleAssignmentSettings);
            }
        },

        /**
         * @override
         */
        parse: function parse(response) {
            response = core.BaseModel.prototype.parse.apply(this, arguments);

            // convert different responses to one form
            if (response.badgeMultipleAssignmentSettings) {
                response.settings = response.badgeMultipleAssignmentSettings;
            }
            return response;
        },

        getName: function getName() {
            return this.get('name');
        },

        getDescription: function getDescription() {
            return this.get('description');
        },

        getImage: function getImage() {
            return this.get('image');
        },

        /**
         * @returns {Array}
         */
        getAccounts: function getAccounts() {
            return this.get('accounts');
        },

        /**
         * Inits url with badge id
         *
         * @param id
         * @returns {Badge}
         */
        setUrl: function setUrl(id) {
            this.url = config.api.badges.ids.url(id);
            return this;
        },

        /**
         * @returns {{
         *      allowMultipleAssignment: Boolean,
         *      allowSelfAssignment: Boolean
         *      requiresIndividualContribution: Boolean
         *      requiresLink: Boolean
         *      requiresProject: Boolean
         *      requiresSummary: Boolean
         *    }}
         * @private
         */
        getSettings: function getSettings() {
            var _this = this;

            return _.assign({}, _this.get('settings'), _this.get('optionalSettings'));
        },

        /**
         * @returns {boolean}
         */
        isMultipleAssignmentAvailable: function isMultipleAssignmentAvailable() {
            return this.getSettings().allowMultipleAssignment;
        },

        /**
         * @returns {boolean}
         */
        isSelfAssignmentAvailable: function isSelfAssignmentAvailable() {
            return this.getSettings().allowSelfAssignment;
        },

        /**
         * @returns {boolean}
         */
        isIndividualContributionRequired: function isIndividualContributionRequired() {
            return this.getSettings().requiresIndividualContribution;
        },

        /**
         * @returns {boolean}
         */
        isLinkRequired: function isLinkRequired() {
            return this.getSettings().requiresLink;
        },

        /**
         * @returns {boolean}
         */
        isProjectRequired: function isProjectRequired() {
            return this.getSettings().requiresProject;
        },

        /**
         * @returns {boolean}
         */
        isSummaryRequired: function isSummaryRequired() {
            return this.getSettings().requiresSummary;
        },

        /**
         * Validates whether badge can be assigned for that receivers number
         * according badge settings
         *
         * @param receiversNumber
         * @returns {boolean}
         */
        isReceiversNumberValid: function isReceiversNumberValid(receiversNumber) {
            if (!this.isMultipleAssignmentAvailable()) {
                return receiversNumber === 1;
            } else if (this.isIndividualContributionRequired()) {
                return receiversNumber >= 2;
            } else {
                return receiversNumber > 0;
            }
        },

        /**
         * Approvable badge is special badge that restrict assignment
         * for several persons
         *
         * @returns {boolean}
         */
        isApprovable: function isApprovable() {
            return this.get('approvable');
        },

        isLimited: function isLimited() {
            return this.get('limited');
        },

        /**
         * Checks badge's name
         * if its't Gold or Silver ER badge - badge can be given only for people
         * in the same account
         * @returns {boolean}
         */
        isBadgeOnlyForAccount: function isBadgeOnlyForAccount() {
            var badgeName = this.get('name');
            return !(badgeName === 'Gold Team Recognition' || badgeName === 'Silver Team Recognition');
        }
    });

    return Badge;
}).apply(exports, __WEBPACK_AMD_DEFINE_ARRAY__),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));

