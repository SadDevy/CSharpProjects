/***/ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/OverlayPopup.js":
/*!********************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/OverlayPopup.js ***!
  \********************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
/* WEBPACK VAR INJECTION */(function($) {var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var BaseView = __webpack_require__(/*! com/epam/heroes/portal/core/view/BaseView */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/BaseView.js");
    var OverlayView = __webpack_require__(/*! com/epam/heroes/portal/core/view/OverlayView */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/OverlayView.js");

    //constants
    var resolutions = __webpack_require__(/*! com/epam/heroes/portal/core/resolutions */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/resolutions.js");

    __webpack_require__(/*! template/com/epam/heroes/portal/core/OverlayPopup.soy */ "./src/main/soy/com/epam/heroes/portal/core/OverlayPopup.soy");

    var OverlayPopup = BaseView.extend({

        /**
         * Flag that notifies
         * whether its necessary to set popup height after every resize
         * if yes - sets callback on resize event
         *
         * This is used for those popups that have scrollable body and not scrollable
         * block. Scrollable block requires height property to be set!
         */
        adjustHeightOnResize: false,

        /**
         * Duration for show/hide animation
         */
        animationDuration: 400,

        /**
         * Returns html-header to set it in popup
         * @abstract
         */
        headerTemplate: function headerTemplate() {},

        /**
         * Returns html-body to set it in popup
         * @abstract
         */
        bodyTemplate: function bodyTemplate() {},

        /**
         * Returns html-footer to set it in popup
         * Override it with function to make popup create footer template with it
         * @abstract
         */
        footerTemplate: null,

        /**
         * Returns object of soy-params for using in header-template
         * @abstract
         */
        getHeaderModel: function getHeaderModel() {},

        /**
         * Returns object of soy-params for using in body-template
         * @abstract
         */
        getBodyModel: function getBodyModel() {},

        /**
         * Returns object of soy-params for using in body-template
         * @abstract
         */
        getFooterModel: function getFooterModel() {},

        /**
         * @override
         * @returns {Object}
         */
        getModel: function getModel() {
            return {
                hasFooter: Boolean(this.footerTemplate)
            };
        },

        /**
         * @returns {String} rendered header template to html
         */
        getHeader: function getHeader() {
            return this.headerTemplate(this.getHeaderModel());
        },

        /**
         * @returns {String} rendered body template to html
         */
        getBody: function getBody() {
            return this.bodyTemplate(this.getBodyModel());
        },

        /**
         * @returns {String} rendered footer template to html
         */
        getFooter: function getFooter() {
            return this.footerTemplate ? this.footerTemplate(this.getFooterModel()) : '';
        },

        template: com.epam.heroes.template.portal.OverlayPopup,

        className: 'popup-slider',

        events: {
            'click .header-button.close': 'hide'
        },

        /**
         * @override
         * @param {Object} [options]
         * @param {boolean} [options.adjustHeightOnResize]
         */
        initialize: function initialize(options) {
            BaseView.prototype.initialize.apply(this, arguments);

            _.bindAll(this, 'popupScrollableContentHeight', 'relocateBlock', 'setHeightInitOnResize', 'hide', 'open', 'afterHide', 'remove');

            this.zIndex = arguments.length && arguments[0].zIndex;
            this.adjustHeightOnResize = options && options.adjustHeightOnResize;
        },

        /**
         * @override
         *
         * Appends popup to document body (popup is not a usual child view)
         * Replaces mock html elements with appropriate rendered templates
         * Sets resizing callback for explicit setting height property
         *
         * @returns {OverlayPopup}
         */
        afterRender: function afterRender() {
            this.$el.appendTo('body');

            this.$('.header-content').replaceWith(this.getHeader());
            this.$('.body-content').replaceWith(this.getBody());
            this.$('.footer-content').replaceWith(this.getFooter());

            /**
             * During the rendering dom-tree of popup changes,
             * but method delegateEvents doesn't invokes after that,
             * that why it's necessary to call with force
             */
            this.delegateEvents();
            this.popupScrollableContentHeight();
            if (this.adjustHeightOnResize) {
                this.setHeightInitOnResize();
            }
            return this;
        },

        /**
         * Hides overlay and destroys event callbacks
         *
         * @returns {OverlayPopup}
         */
        beforeRemove: function beforeRemove() {
            BaseView.prototype.beforeRemove.apply(this, arguments);
            this.hideOverlay();
            $(window).off('.' + this.name);

            return this;
        },

        /**
         * Use it for fetching data before open popup
         *
         * @abstract
         */
        beforeOpen: function beforeOpen() {
            return true;
        },

        /**
         * Opens popup if it's closed,
         * executes callback on animation end
         * and returns the promise
         *
         * @param {Object} [params]
         * @returns {jQuery.Deferred.promise|*}
         */
        open: function open(params) {
            var _this = this;
            var isAlreadyOpen = Boolean(this.overlay);
            var promise;
            var isSinglePage = this.model && this.model.attributes && this.model.get('singlePage');

            if (!isAlreadyOpen && isSinglePage) {
                $.when(this.beforeOpen(params)).done(function () {
                    return _this.render();
                }).done(function () {
                    return _this.render();
                }).done(this.afterOpen.bind(this, params));
            }

            if (!isAlreadyOpen && !isSinglePage) {
                promise = $.when(this.beforeOpen(params)).done(function () {
                    return _this.render().showOverlay().animateShow().promise();
                }).done(this.afterOpen.bind(this, params));

                return promise;
            }
        },

        /**
         * Is invoked after popup opened
         *
         * @abstract
         */
        afterOpen: function afterOpen() {
            return this;
        },

        /**
         * @param {Event} e
         * @returns {OverlayPopup}
         */
        hide: function hide(e) {
            e && e.preventDefault();
            var isSinglePage = this.model && this.model.attributes && this.model.get('singlePage');

            if (!isSinglePage) {
                this.hideOverlay().animateHide(this.afterHide);
            }
            return this;
        },

        /**
         * Is invoked after popup closed
         * @returns {OverlayPopup}
         */
        afterHide: function afterHide() {
            this.remove();
            return this;
        },

        /**
         * Creates overlay
         *
         * @returns {OverlayPopup}
         */
        showOverlay: function showOverlay() {
            if (!this.overlay) {
                this.overlay = new OverlayView({ animationDuration: this.animationDuration, zIndex: this.zIndex });
                this.listenTo(this.overlay, 'click', this.hide);
            }
            this.overlay.render();
            return this;
        },

        /**
         * Hides overlay
         * and resets to null field afterwards
         *
         * @returns {OverlayPopup}
         */
        hideOverlay: function hideOverlay() {
            var _this = this;

            if (this.overlay) {
                this.stopListening(this.overlay);

                this.overlay.remove().done(function () {
                    _this.overlay = null;
                });
            }
            return this;
        },

        /**
         * Animation to show popup
         *
         * @param {Function} [callback]
         * @returns {jQuery.Deferred.promise}
         */
        animateShow: function animateShow(callback) {
            return this.$el.animate({
                right: 0,
                opacity: 1
            }, this.animationDuration, callback).promise();
        },

        /**
         * Animation to hide popup
         *
         * @param {Function} [callback]
         * @returns {jQuery.Deferred.promise}
         */
        animateHide: function animateHide(callback) {
            this.$el.animate({
                right: '-100%',
                opacity: 0
            }, this.animationDuration, callback);
            return this;
        },

        /**
         * @override
         * @returns {OverlayPopup}
         */
        remove: function remove() {
            BaseView.prototype.remove.apply(this, arguments);
            this.$el.remove();
            return this;
        },

        /**
         * Defines height of unscrollable part of popup
         * and invokes unscrollable block relocation
         */
        popupScrollableContentHeight: function popupScrollableContentHeight() {
            this.relocateBlock();

            var popup = this.$el;
            var head = popup.find('header');
            var $unscrollableSection = popup.find('.unscrollable-section');
            var $scrollableContent = popup.find('.scrollable-content');
            var $footer = popup.find('footer');
            var upperUnscrollablePartHeight = head.outerHeight(true) + $footer.outerHeight(true) + $unscrollableSection.outerHeight(true);

            $scrollableContent.css({ 'max-height': popup.height() - upperUnscrollablePartHeight });
            return this;
        },

        /**
         * Inits callback function to onresize event
         * that defines height of scrollable section
         */
        setHeightInitOnResize: function setHeightInitOnResize() {
            $(window).on('resize.' + this.name, _.throttle(this.popupScrollableContentHeight, 350));
        },

        /**
         * Relocates unscrollable block in case of mobile version
         */
        relocateBlock: function relocateBlock() {
            var $jumpingBlock = this.$('.jumping-block');

            if (!$jumpingBlock) {
                return;
            }

            if ($('body').outerWidth() > resolutions.mobile) {
                if ($jumpingBlock.length) {
                    $jumpingBlock.prependTo(this.$('.body')).addClass('unscrollable-section');
                }
            } else {
                // Mobile

                $jumpingBlock.prependTo(this.$('.scrollable-content')).removeClass('unscrollable-section');
            }
        }
    });

    return OverlayPopup;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js")))

/***/ }),

/***/ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/OverlayView.js":
/*!*******************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/OverlayView.js ***!
  \*******************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
/* WEBPACK VAR INJECTION */(function($) {var __WEBPACK_AMD_DEFINE_ARRAY__, __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_ARRAY__ = [__webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js")], __WEBPACK_AMD_DEFINE_RESULT__ = (function (core) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var OverlayView = core.BaseView.extend({
        id: 'overlay',

        className: 'popup-overlay',

        animateDuration: 400,

        events: {
            'click': 'click'
        },

        initialize: function initialize(options) {
            options = options || {};
            this.zIndex = options.zIndex;
            var animate = options.animate;
            this.animate = animate != null ? animate : true;
            this.animateDuration = _.isNumber(options.animationDuration) ? options.animationDuration : this.animateDuration;

            core.BaseView.prototype.initialize.apply(this, arguments);
        },

        _enableScroll: function _enableScroll() {
            $('html').removeClass('underpopup-content');
        },

        _disableScroll: function _disableScroll() {
            $('html').addClass('underpopup-content');
        },

        /**
         * If there is one popup -> enable body's scroll and removes element
         * else -> just removes
         *
         */
        _removeCallback: function _removeCallback(args) {
            core.BaseView.prototype.remove.apply(this, args);
            this._enableScroll();
            this.$el.remove();
        },

        render: function render() {
            core.BaseView.prototype.render.apply(this, arguments);
            this.$el[0].style.zIndex = this.zIndex;
            var $el = this.$el;
            if (this.animate) {
                $el.hide().appendTo('body').fadeIn(this.animateDuration);
            } else {
                $el.appendTo('body');
            }
            this._disableScroll();

            return this;
        },

        /**
         * If animatable -> fades out overlay and removes
         * else -> just removes
         *
         * @returns {jQuery.Deferred.promise|*}
         */
        remove: function remove() {
            var $el = this.$el;
            var args = arguments;
            //Native fade out promise is always pending =(
            var $dfd = $.Deferred();
            var _this;

            if (this.animate) {
                _this = this;
                $el.fadeOut(this.animateDuration, function () {
                    _this._removeCallback(args);
                    $dfd.resolve();
                });

                return $dfd.promise();
            } else {
                this._removeCallback(args);
            }
        },

        click: function click() {
            this.trigger('click');
        }
    });

    return OverlayView;
}).apply(exports, __WEBPACK_AMD_DEFINE_ARRAY__),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
