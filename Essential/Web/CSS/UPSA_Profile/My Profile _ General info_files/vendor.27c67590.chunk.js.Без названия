        },

        /**
         * @override
         */
        initialize: function initialize() {
            core.BaseView.prototype.initialize.apply(this, arguments);

            if (core.Application.features.getFeatureStateByName('features_givabadge_specify_project')) {
                this.addChildView(new ProjectSearchView({
                    name: 'ProjectSearch',
                    model: this.model
                }), '.project-search-wrapper');
            }

            this.listenTo(this.model, 'change:pointedEmployeeId', this.render);
        },

        /**
         * @override
         * @returns {Object}
         */
        getModel: function getModel() {
            var model = this.model;
            var comment = model.getCurrentComment();
            var assignData = _.pick(model.attributes, ['summary', 'link']);
            var badgeAssignOptions = model.getBadge().getSettings();
            var project = this.model.get('project') && this.model.get('project').getName();
            var featurePetrofacUiStylesEnabled = featuresConfig.featurePetrofacUiStylesEnabled;
            var featuresProjectIconEnabled = core.Application.features.getFeatureStateByName('features_givabadge_specify_project');
            var featuresLinkButtonEnabled = core.Application.features.getFeatureStateByName('features_badge_library_link_button_enabled');

            return _.extend(assignData, {
                isIndividualFormRequired: !this.model.isGroupItemPointed(),
                arrowLeftOffset: this.model.get('arrowLeftOffset') || 0,
                comment: comment ? comment : '',
                commentFieldPlaceholder: this.createCommentFieldPlaceholder(),
                project: project,
                featurePetrofacUiStylesEnabled: featurePetrofacUiStylesEnabled,
                featuresProjectIconEnabled: featuresProjectIconEnabled,
                featuresLinkButtonEnabled: featuresLinkButtonEnabled
            }, badgeAssignOptions);
        },

        /**
         * @override
         */
        afterRender: function afterRender() {
            var _this = this;
            var model = this.model;
            var featuresProjectIconEnabled = core.Application.features.getFeatureStateByName('features_givabadge_specify_project');
            var $commentField = this.$('textarea[name="comment"]');
            var $summaryField = this.$('input[name="summary"]');
            this.$('textarea').perfectScrollbar();
            //Initialize tooltips
            this.$('[data-toggle="tooltip"]').tooltip();
            $commentField.one('keyup paste', function () {
                return _this.handleFirstCommentKey(model);
            });

            if (model.get('receivers').length) {
                $commentField.focus();
                $summaryField && $summaryField.focus();
            }

            if (!this.model.isGroupItemPointed()) {
                $('.row.comment-wrapper').addClass('no-bottom-offset');
            } else {
                $('.row.comment-wrapper.no-bottom-offset').removeClass('no-bottom-offset');
            }
        },

        /**
         * Returns special placeholder for individual contributions field
         *
         * @param {string} employeeName
         * @returns {string}
         * @private
         */
        _getIndividualContributionPlaceholder: function _getIndividualContributionPlaceholder(employeeName) {
            return 'Add individual comment for ' + employeeName;
        },

        /**
         * If current field is group comment -> returns placeholder for it,
         * else -> creates and returns special placeholder for individual contribution comment
         *
         * @returns {String}
         */
        createCommentFieldPlaceholder: function createCommentFieldPlaceholder() {
            var isApprovable = this.model.getBadge().get('approvable');
            if (isApprovable) {
                return this.APPROVABLE_COMMENT_PLACEHOLDER;
            }
            if (this.model.isGroupItemPointed()) {
                return this.GROUP_COMMENT_PLACEHOLDER;
            }

            var pointedEmployeeId = this.model.getPointedEmployeeId();
            var pointedEmployee = this.model.getReceivers().get(pointedEmployeeId);

            return pointedEmployee.getFirstName() ? this._getIndividualContributionPlaceholder(pointedEmployee.getFirstName()) : this._getIndividualContributionPlaceholder(pointedEmployee.getName());
        },

        /**
         * Callback on comment-changing:
         * Sets comment to model
         *
         * @param {Event} event
         */
        changeComment: function changeComment(event) {
            event.preventDefault();

            var newComment = $(event.currentTarget).val().trim();
            this.model.setEmployeeComment(newComment);
        },

        /**
         * Pastes the data first, and call changeComment() after that
         * setTimeout(, 0) sets action to the end of js commands queue
         *
         * @param {Event} event
         */
        _pasteCommentInTextarea: function _pasteCommentInTextarea(event) {
            setTimeout(this.changeComment.bind(this, event), 0);
        },

        /**
         * Invokes changeInputField at the end of js commands queue
         *
         * @param {Event} event
         * @private
         */
        _pasteInInputField: function _pasteInInputField(event) {
            setTimeout(this.changeInputField.bind(this, event), 0);
        },

        /**
         * Saves input field data to the model
         *
         * @param {Event} event
         */
        changeInputField: function changeInputField(event) {
            event.preventDefault();

            var $field = $(event.currentTarget);
            var fieldValue = $field.val().trim();
            var fieldName = $field.attr('name');

            this.model.set(fieldName, fieldValue);
        },

        handleFirstCommentKey: function handleFirstCommentKey(model) {
            if (!this.COMMENT_TYPE_SENT) {
                this.COMMENT_TYPE_SENT = true;
                $.ajax({
                    url: config.api.stats.typeComment.url(model.getBadge().get('id')),
                    contentType: 'application/json',
                    type: 'POST',
                    blockUI: false
                });
            }
        },

        /**
         * Returns arrow - employee pointer jQuery element
         *
         * @returns {jQuery}
         */
        get$EmployeePointer: function get$EmployeePointer() {
            return this.$('.comment-pointer');
        },

        /**
         * Adds class 'focused' to make arrow be same colour as summary field
         */
        onFocusEqualArrowColourField: function onFocusEqualArrowColourField() {
            this.get$EmployeePointer().addClass('focused');
        },

        /**
         * Removes class 'focused' to make arrow be same colour as summary field
         */
        onBlurEqualArrowColourField: function onBlurEqualArrowColourField() {
            this.get$EmployeePointer().removeClass('focused');
        }
    });

    return AssignFormView;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js")))

/***/ }),

