/***/ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/AssignEntryModel.js":
/*!******************************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/AssignEntryModel.js ***!
  \******************************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
var __WEBPACK_AMD_DEFINE_ARRAY__, __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_ARRAY__ = [__webpack_require__(/*! com/epam/heroes/core/model/Employee */ "./src/main/resources/static-site/js/com/epam/heroes/core/model/Employee.js")], __WEBPACK_AMD_DEFINE_RESULT__ = (function (EmployeeModel) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");

    var AssignEntryModel = EmployeeModel.extend({

        MIN_SYMBOLS_IN_INDIVIDUAL_CONTRIBUTION_COMMENT: 1,
        MAX_SYMBOLS_IN_INDIVIDUAL_CONTRIBUTION_COMMENT: 3000,

        defaultAssignSettings: function defaultAssignSettings() {
            return {
                isAssignDataFilled: false,
                isAssignDataPristine: true,
                individualContribution: ''
            };
        },

        defaults: function defaults() {
            var superDefaults = EmployeeModel.prototype.defaults();

            return _.extend(superDefaults, this.defaultAssignSettings());
        },

        /**
         * @override
         * @param {Object} [options]
         * @returns {AssignEntryModel}
         */
        initialize: function initialize(options) {
            EmployeeModel.prototype.initialize.apply(this, arguments);
            return this.set(_.extend(this.defaultAssignSettings(), options));
        },

        /**
         * Saves comment in model and updates flags according its value
         *
         * @param {String} comment
         * @returns {AssignEntryModel}
         */
        setIndividualContributionComment: function setIndividualContributionComment(comment) {
            this.set('individualContribution', comment).setAssignFormFilled(comment.length > 0 && this.isIndividualContributionCommentValid());

            if (comment.length > 0) {
                this.unsetAssignFormPristine();
            }
            return this;
        },

        /**
         * @returns {String}
         */
        getIndividualContributionComment: function getIndividualContributionComment() {
            return this.get('individualContribution');
        },

        /**
         * Checks whether individual contribution is defined and has valid length
         *
         * @returns {boolean}
         */
        isIndividualContributionCommentValid: function isIndividualContributionCommentValid() {
            var comment = this.getIndividualContributionComment();

            return this.MIN_SYMBOLS_IN_INDIVIDUAL_CONTRIBUTION_COMMENT <= comment.length && comment.length <= this.MAX_SYMBOLS_IN_INDIVIDUAL_CONTRIBUTION_COMMENT;
        },

        isAssignDataFilled: function isAssignDataFilled() {
            return this.get('isAssignDataFilled');
        },

        isAssignDataPristine: function isAssignDataPristine() {
            return this.get('isAssignDataPristine');
        },

        setAssignFormFilled: function setAssignFormFilled(value) {
            value = _.isBoolean(value) ? value : true;
            return this.set('isAssignDataFilled', value);
        },

        unsetAssignFormFilled: function unsetAssignFormFilled() {
            return this.set('isAssignDataFilled', false);
        },

        setAssignFormPristine: function setAssignFormPristine(value) {
            value = _.isBoolean(value) ? value : true;
            return this.set('isAssignDataPristine', value);
        },

        unsetAssignFormPristine: function unsetAssignFormPristine() {
            return this.set('isAssignDataPristine', false);
        }
    });

    return AssignEntryModel;
}).apply(exports, __WEBPACK_AMD_DEFINE_ARRAY__),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));

/***/ }),

/***/ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/BadgeAssignPopupModel.js":
/*!***********************************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/BadgeAssignPopupModel.js ***!
  \***********************************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
/* WEBPACK VAR INJECTION */(function(Backbone) {var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var OverlayPopupModel = __webpack_require__(/*! com/epam/heroes/portal/core/model/OverlayPopupModel */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/model/OverlayPopupModel.js");

    var ProjectsCollection = __webpack_require__(/*! com/epam/heroes/widget/givebadge/model/ProjectsCollection */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/ProjectsCollection.js");
    var GroupCollection = __webpack_require__(/*! com/epam/heroes/core/model/GroupCollection */ "./src/main/resources/static-site/js/com/epam/heroes/core/model/GroupCollection.js");
    var AssignEntryCollection = __webpack_require__(/*! com/epam/heroes/widget/givebadge/model/AssignEntryCollection */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/AssignEntryCollection.js");

    var Group = __webpack_require__(/*! com/epam/heroes/core/model/Group */ "./src/main/resources/static-site/js/com/epam/heroes/core/model/Group.js");
    var Badge = __webpack_require__(/*! com/epam/heroes/core/model/Badge */ "./src/main/resources/static-site/js/com/epam/heroes/core/model/Badge.js");
    var EmployeeBadge = __webpack_require__(/*! com/epam/heroes/portal/core/model/EmployeeBadge */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/model/EmployeeBadge.js");

    var BadgeAssignPopupModel = OverlayPopupModel.extend({

        MIN_SYMBOLS_IN_COMMENT: 3,
        MAX_SYMBOLS_IN_COMMENT: 3000,
        GROUP_ASSIGN_ENTRY_ID: 'groupAssignEntryId',

        /**
         * Simple mapping of fields in badge settings to names of inputs on the form
         */
        REQUIRES_TO_FIELDS: {
            requiresLink: 'link',
            requiresProject: 'project',
            requiresSummary: 'summary',
            requiresComment: 'comment'
        },

        /**
         * @override
         */
        defaults: function defaults() {
            return {
                //Only one group can be chosen at one moment
                //by default we can create here only private(=personal) group
                group: new Group({ personal: true }),
                receivers: new AssignEntryCollection(),

                foundEmployees: new AssignEntryCollection(),
                foundGroups: new GroupCollection(),
                foundProjects: new ProjectsCollection(),

                comment: '',
                pointedEmployeeId: this.GROUP_ASSIGN_ENTRY_ID,
                groupControlId: this.GROUP_ASSIGN_ENTRY_ID,

                isBudgetExceeded: false,

                groupActionsDisabled: false,

                informedEmployees: [],
                notifyColleagues: false,

                isReceiversNumberValid: false
            };
        },

        /**
         * @override
         * @param {Object} [options]
         * @param {Group} [options.group]
         * @param {EmployeeBadge|Object} [options.badgeToEdit]
         * @param {Badge|Object} [options.badgeToAssign]
         * @param {AssignEntryCollection} [options.receivers]
         * @param {boolean} [options.editAssignCommentMode]
         */
        initialize: function initialize(options) {
            options = options || {};
            OverlayPopupModel.prototype.initialize.apply(this, arguments);

            if (options.badgeToEdit) {
                this.setDataForEditAssignData(options);
            } else {
                this.setDataForBadgeAssign(options);
            }

            if (options.group) {
                this.set({ potentialGroupName: options.group.getName() });
            }

            this.setIsReceiversNumberValid();
            this.listenTo(this.getReceivers(), 'add remove reset', this.onChangeReceivers);
        },

        /**
         * @returns {boolean}
         */
        isReceiversNumberValid: function isReceiversNumberValid() {
            var badge = this.getBadge();
            var receivers = this.getReceivers();
            var isValidAccordingBadgeSettings = badge.isReceiversNumberValid(receivers.length);
            var userId = this.getUser().id;
            var isUserSelectedByHimself = Boolean(receivers.findWhere({ id: userId }));
            var isGroupComplete = this.isGroupDefined() ? this.getGroup().hasValidEmployeesNumber() : true;

            return (isUserSelectedByHimself && badge.isSelfAssignmentAvailable() || !isUserSelectedByHimself) && isValidAccordingBadgeSettings && isGroupComplete;
        },

        /**
         * Removes employee from group list and receivers list
         *
         * @param {String} id
         * @returns {Object} employeeToRemove
         */
        removeReceiver: function removeReceiver(id) {
            var receivers = this.getReceivers();
            var employeeToRemove = receivers.get(id);
            var group = this.getGroup();

            group.removeReceiver(employeeToRemove);
            receivers.remove(employeeToRemove); //this action triggers event -> it should follow previous actions!
            return employeeToRemove;
        },

        /**
         * @param {boolean} [value]
         * @returns {BadgeAssignPopupModel}
         */
        setIsReceiversNumberValid: function setIsReceiversNumberValid(value) {
            value = _.isBoolean(value) ? value : this.isReceiversNumberValid();
            return this.set({ isReceiversNumberValid: value });
        },

        /**
         * On changing receivers collection:
         * toggles Give A Badge Btn and
         * changes pointed employee index
         * if correspond employee doesn't exist in receivers collection
         */
        onChangeReceivers: function onChangeReceivers() {
            this.setIsReceiversNumberValid();

            var receivers = this.getReceivers();
            var pointedEmployeeId = this.getPointedEmployeeId();
            var pointedEmployee = receivers.get(pointedEmployeeId);
            var needResetPointedIndex = !pointedEmployee || !pointedEmployeeId;

            if (needResetPointedIndex) {
                // pointed employee is not more available or was not available
                this.setGroupItemPointedId();
            }
        },

        /**
         * Sets assign data to model
         * and filters receivers for special badge
         *
         * @param {Object} options
         * @param {Badge} options.badgeToAssign
         * @param {AssignEntryCollection} [options.receivers]
         * @returns {BadgeAssignPopupModel}
         */
        setDataForBadgeAssign: function setDataForBadgeAssign(options) {
            this.set(options);
            this.setBadgeToAssign(options.badgeToAssign);

            var remainedReceivers = this.getReceivers();
            if (remainedReceivers.length) {
                this.getGroup().set({ employees: remainedReceivers });
            }

            return this;
        },

        /**
         * Sets badges and mode according badge settings
         *
         * @param {Badge|Object} badge
         * @returns {BadgeAssignPopupModel}
         */
        setBadgeToAssign: function setBadgeToAssign(badge) {
            badge = badge instanceof Backbone.Model ? badge : new Badge(badge);
            this.set('badgeToAssign', badge);
            return this;
        },

        /**
         * Sets badges and mode according employeeBadge settings
         *
         * @param {Badge|Object} employeeBadge
         * @returns {BadgeAssignPopupModel}
         */
        setBadgeToEdit: function setBadgeToEdit(employeeBadge) {
            employeeBadge = employeeBadge instanceof EmployeeBadge ? employeeBadge : new EmployeeBadge(employeeBadge);

            this.set('badgeToAssign', employeeBadge.get('badge'));
            this.set('badgeToEdit', employeeBadge);
            return this;
        },

        /**
         * Sets badges and mode according badge settings
         *
         * @returns {BadgeAssignPopupModel}
         */
        unsetBadgeToAssign: function unsetBadgeToAssign() {
            this.unset('badgeToAssign');
            return this;
        },

        /**
         * Sets data for edit assignment options to model
         *
         * @param {Object} options
         * @returns {BadgeAssignPopupModel}
         */
        setDataForEditAssignData: function setDataForEditAssignData(options) {
            options.editAssignCommentMode = true;
            this.set(options);
            this.setBadgeToEdit(options.badgeToEdit);

            return this;
        },

        /**
         * @override
         */
        clearReceivers: function clearReceivers() {
            this.getReceivers().reset();
            this.resetGroup();
        },

        /**
         * Invokes for particular field correspond validator
         *
         * @param {String} fieldName
         * @param {String} value
         * @returns {boolean}
         */
        isFieldValid: function isFieldValid(fieldName, value) {
            var defaultValidator = function defaultValidator() {
                return value && value.length > 0;
            };

            var validatorsMap = {
                'link': 'isLinkValid',
                'comment': 'isGroupCommentValid',
                'project': 'isProjectValid'
            };

            var validatorName = validatorsMap[fieldName];
            var validator = this[validatorName] || defaultValidator;

            return validator.apply(this);
        },

        /**
         * @returns {Boolean}
         */
        isProjectValid: function isProjectValid() {
            var project = this.get('project');
            return Boolean(project);
        },

        /**
         * If group comment is valid
         *
         * @returns {boolean}
         */
        isGroupCommentValid: function isGroupCommentValid() {
            var comment = this.getComment();
            comment = comment && comment.trim();

            return comment;
        },

        /**
         * @returns {boolean}
         */
        isLinkValid: function isLinkValid() {
            // This crazy regex is equal to ER regex
            var regexp = /^(?:(?:https?|ftp|file):\/\/)(?:\S+(?::\S*)?@)?(?:(?!10(?:\.\d{1,3}){3})(?!127(?:\.\d{1,3}){3})(?!169\.254(?:\.\d{1,3}){2})(?!192\.168(?:\.\d{1,3}){2})(?!172\.(?:1[6-9]|2\d|3[0-1])(?:\.\d{1,3}){2})(?:[1-9]\d?|1\d\d|2[01]\d|22[0-3])(?:\.(?:1?\d{1,2}|2[0-4]\d|25[0-5])){2}(?:\.(?:[1-9]\d?|1\d\d|2[0-4]\d|25[0-4]))|(?:(?:[a-z\u00a1-\uffff0-9]+-?)*[a-z\u00a1-\uffff0-9]+)(?:\.(?:[a-z\u00a1-\uffff0-9]+-?)*[a-z\u00a1-\uffff0-9]+)*(?:\.(?:[a-z\u00a1-\uffff]{2,})))(?::\d{2,5})?(?:\/[^\s]*)?$/i;
            var link = this.get('link');
            return regexp.test(link);
        },

        /**
         * Adds new comment to the array of comments in model
         *
         * @param {String} commentText
         * @param {String} [employeeId]
         */
        setEmployeeComment: function setEmployeeComment(commentText, employeeId) {
            employeeId = employeeId || this.get('pointedEmployeeId');
            commentText = commentText && commentText.trim();

            if (this.isGroupItemPointed()) {
                this.set({ comment: commentText });
            } else {
                var assignEntry = this.getReceivers().get(employeeId);
                assignEntry.setIndividualContributionComment(commentText);
                this.trigger('change:assignEntry', assignEntry);
            }
        },

        /**
         * @param {String} employeeId
         * @returns {BadgeAssignPopupModel}
         */
        setPointedEmployeeId: function setPointedEmployeeId(employeeId) {
            return this.set({ pointedEmployeeId: employeeId });
        },

        /**
         * Sets groupId as id of pointed employee
         *
         * @returns {BadgeAssignPopupModel}
         */
        setGroupItemPointedId: function setGroupItemPointedId() {
            return this.setPointedEmployeeId(this.GROUP_ASSIGN_ENTRY_ID);
        },

        /**
         * @param {ProjectModel} project
         * @returns {BadgeAssignPopupModel}
         */
        setProject: function setProject(project) {
            return this.set('project', project);
        },

        /**
         * If current pointed item is group item
         *
         * @returns {boolean}
         */
        isGroupItemPointed: function isGroupItemPointed() {
            return this.get('pointedEmployeeId') === this.GROUP_ASSIGN_ENTRY_ID;
        },

        /*** Getters ***/

        /**
         * For assignment with query params badgeToAssign is Collection
         * perhaps for ability to give several badges at the same time,
         * which is not implemented yet
         *
         * @returns {Backbone.Model}
         */
        getBadge: function getBadge() {
            var badgeToAssign = this.get('badgeToAssign');

            if (badgeToAssign instanceof Backbone.Collection) {
                return badgeToAssign.models && badgeToAssign.models[0];
            }
            return badgeToAssign;
        },

        /**
         * For edit-assign-mode
         *
         * @returns {EmployeeBadge}
         */
        getBadgeToEdit: function getBadgeToEdit() {
            return this.get('badgeToEdit');
        },

        getFoundEmployees: function getFoundEmployees() {
            return this.get('foundEmployees');
        },

        getFoundGroups: function getFoundGroups() {
            return this.get('foundGroups');
        },

        /**
         * @returns {Group}
         */
        getGroup: function getGroup() {
            return this.get('group');
        },

        /**
         * @returns {ProjectModel}
         */
        getProject: function getProject() {
            return this.get('project');
        },

        /**
         * @returns {AssignEntryCollection}
         */
        getReceivers: function getReceivers() {
            return this.get('receivers');
        },

        /**
         * @returns {ProjectsCollection}
         */
        getFoundProjects: function getFoundProjects() {
            return this.get('foundProjects');
        },

        /**
         * It's a name that is shown in input field for editing,
         * but it can not be saved as group name, so it's potential
         *
         * @returns {String}
         */
        getPotentialGroupName: function getPotentialGroupName() {
            return this.get('potentialGroupName');
        },

        /**
         * By default returns group comment id
         *
         * @param {Number} [employeeId]
         * @returns {String}
         */
        getComment: function getComment(employeeId) {
            if (employeeId && employeeId !== this.GROUP_ASSIGN_ENTRY_ID) {
                return this.getReceivers().get(employeeId).getIndividualContributionComment();
            }
            return this.get('comment');
        },

        /**
         * Returns comment for employee that is now editing
         *
         * @returns {String}
         */
        getCurrentComment: function getCurrentComment() {
            var comment = this.get('comment');
            if (!this.isGroupItemPointed() && this.isIndividualContributionEnabled()) {
                comment = this.getComment(this.getPointedEmployeeId());
            }
            return comment;
        },

        /**
         * @returns {String}
         */
        getPointedEmployeeId: function getPointedEmployeeId() {
            return this.get('pointedEmployeeId');
        },

        /**
         * @returns {Array} of strings that are employees mails
         */
        getInformedEmployees: function getInformedEmployees() {
            return this.get('informedEmployees');
        },

        getPointsSteps: function getPointsSteps() {
            return this.get('programSettings') && this.get('programSettings').pointsSteps;
        },

        getSliders: function getSliders() {
            return this.get('programSettings') && this.get('programSettings').sliders;
        },

        getSliderByOrder: function getSliderByOrder(sliderOrder) {
            return _.findWhere(this.getSliders(), { order: sliderOrder });
        },

        getRemainigPoints: function getRemainigPoints() {
            return this.get('programSettings') && this.get('programSettings').remainingPointCount;
        },

        /*** Flags ***/

        /**
         * @returns {boolean}
         */
        canSaveGroup: function canSaveGroup() {
            return this.get('canSaveGroup');
        },

        /**
         * @returns {boolean}
         */
        canCreateNewGroup: function canCreateNewGroup() {
            return this.get('createNew');
        },

        /**
         * @returns {boolean}
         */
        isIndividualContributionEnabled: function isIndividualContributionEnabled() {
            return this.getBadge().isMultipleAssignmentAvailable();
        },

        /**
         * @returns {boolean}
         */
        isMultipleAssignmentAvailable: function isMultipleAssignmentAvailable() {
            return this.getBadge().isMultipleAssignmentAvailable();
        },

        /**
         * @returns {boolean}
         */
        isIndividualContributionRequired: function isIndividualContributionRequired() {
            return this.getBadge().isIndividualContributionRequired();
        },

        /**
         * @returns {boolean}
         */
        isSelfAssignmentAvailable: function isSelfAssignmentAvailable() {
            return this.getBadge().isSelfAssignmentAvailable();
        },

        /**
         * @returns {boolean}
         */
        isProjectRequired: function isProjectRequired() {
            return this.getBadge().isProjectRequired();
        },

        isApprovable: function isApprovable() {
            return this.getBadge().isApprovable();
        },

        /**
         * @returns {boolean}
         */
        isEditAssignCommentMode: function isEditAssignCommentMode() {
            return Boolean(this.getBadgeToEdit());
        },

        /**
         * Public group is not editable!
         * Only private can be edited
         *
         * @returns {boolean}
         */
        areGroupActionsDisabled: function areGroupActionsDisabled() {
            return !this.getGroup().isPrivate();
        },

        /**
         * Is used for restriction to choose group
         * if any employee or group is chosen
         * or multiple assignment is restricted by badge settings
         *
         * @returns {boolean}
         */
        isGroupSearchDisabled: function isGroupSearchDisabled() {
            if (this.get('badgeToAssign').get('externalProgram') && this.isMultipleAssignmentAvailable()) {
                return false;
            }
            if (this.get('programSettings')) {
                return true;
            }

            return !this.isMultipleAssignmentAvailable() && !this.isGroupDefined() && this.getReceivers().length > 0 || this.get('badgeToAssign').getAccounts().length > 0;
        },

        /**
         * @returns {boolean}
         */
        isNotifyColleaguesToggled: function isNotifyColleaguesToggled() {
            return this.get('notifyColleagues');
        },

        /*** end of getters ***/

        /**
         * Adds receiver to list of receivers and potential group
         *
         * @param {String} id
         */
        addEmployeeToReward: function addEmployeeToReward(id) {
            var userId = this.getUser().id;
            if (userId === id && !this.isSelfAssignmentAvailable()) {
                return;
            }
            var employeeToReward = this.getFoundEmployees().get(id);
            var group = this.getGroup();
            var receivers = this.getReceivers();
            var alreadyAdded = receivers.findWhere(receivers, { id: id });

            if (!alreadyAdded) {
                group.unshiftEmployee(employeeToReward);
                receivers.unshift(employeeToReward); // Adding the same this to a collection more than once is a no-op.
            }
        },

        /**
         * Adds group to reward in model
         *
         * @param {string} id
         */
        addGroupToReward: function addGroupToReward(id) {
            var userId = this.getUser().id;
            var newGroup = this.getFoundGroups().get(id);
            var refs = newGroup.collection.refs;
            var receivers = this.getReceivers();

            newGroup.getEmployees().each(function (empl) {
                var url = empl.get('url');
                var emplAttributes = _.extend(empl.attributes, refs[url]);

                if (userId !== emplAttributes.id) {
                    receivers.add(emplAttributes, { silent: true });
                }
            });

            this.set({ group: newGroup });
            receivers.trigger('add', receivers);
        },

        /**
         * Checks by name if it's assignment to group
         *
         * @returns {boolean}
         */
        isGroupDefined: function isGroupDefined() {
            var group = this.getGroup();
            return Boolean(group && group.getName());
        },

        /**
         * Removes group data from model
         *
         * @returns {Group}
         */
        resetGroup: function resetGroup() {
            var cleanGroup = new Group({ personal: true });

            this.set({
                group: cleanGroup,
                groupActionsDisabled: false,
                potentialGroupName: ''
            });

            return cleanGroup;
        },

        /**
         * If individual comment is required ->
         *      returns assignments with not filled individual contribution
         * else ->
         *      empty array
         *
         * @returns {Array} of AssignmentEntry items
         *
         */
        defineEmployeesToHighlight: function defineEmployeesToHighlight() {
            var employeesToHighlight = [];

            if (this.isIndividualContributionRequired()) {
                employeesToHighlight = this.getReceivers().filter(function (assignment) {
                    return !assignment.isIndividualContributionCommentValid();
                });
            }

            this.set({ employeesToHighlight: new AssignEntryCollection(employeesToHighlight) });
            return employeesToHighlight;
        },

        /**
         * @returns {Array} names of inputs that need to be highlighted
         */
        getFieldsToHighlight: function getFieldsToHighlight() {
            var badge = this.getBadge();
            var settings = badge.getSettings();
            var _this = this;

            var requiredSettings = _.filter(_.keys(settings), function (key) {
                return settings[key];
            });

            var fieldsToHighlight = _.map(requiredSettings, function (key) {
                return _this.REQUIRES_TO_FIELDS[key];
            });
            fieldsToHighlight.push('comment');

            if (this.get('selectPointsField') && !this.get('pointsChosen')) {
                fieldsToHighlight.push('select');
            }

            fieldsToHighlight = _.filter(fieldsToHighlight, function (field) {
                var value = _this.get(field);
                return field && !_this.isFieldValid(field, value);
            });

            return fieldsToHighlight;
        },

        isSinglePage: function isSinglePage() {
            return this.get('singlePage');
        }
    });

    return BadgeAssignPopupModel;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! backbone */ "./src/main/resources/static-site/js/libs/vendor/backbone/backbone.js")))

/***/ }),

/***/ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/BadgeLibraryModel.js":
/*!*******************************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/BadgeLibraryModel.js ***!
  \*******************************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var core = __webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js");
    var config = __webpack_require__(/*! com/epam/heroes/widget/givebadge/config */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/config.js");
    var BadgeSections = __webpack_require__(/*! com/epam/heroes/widget/givebadge/model/BadgeSections */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/BadgeSections.js");

    var BadgeLibraryModel = core.BaseModel.extend({

        defaults: function defaults() {
            return {
                badgeSections: new BadgeSections(),
                searchFilter: config.BADGE_CATALOGUE_SEARCH_FILTER,
                singlePage: false
            };
        },

        getBadgeSections: function getBadgeSections() {
            return this.get('badgeSections');
        },

        getBadgeSectionSettings: function getBadgeSectionSettings(badgeId) {
            var sectionSettings;

            _.forEach(this.get('badgeSections').models, function (model) {
                if (model.get('badges')._byId[badgeId]) {
                    sectionSettings = model.get('settings');
                }
            });

            return sectionSettings;
        },

        isSinglePage: function isSinglePage() {
            return this.get('singlePage');
        },

        getUserToken: function getUserToken() {
            return this.get('userToken');
        },

        getUser: function getUser() {
            return this.get('user');
        },

        /**
         * Stores data that shouldn't be redefined
         * @returns {BadgeLibraryModel}
         */
        storePermanentData: function storePermanentData() {
            return this.store(['badgeSections', 'user', 'userToken']);
        }
    });

    return BadgeLibraryModel;
}).call(exports, __webpack_require__, exports, module),
