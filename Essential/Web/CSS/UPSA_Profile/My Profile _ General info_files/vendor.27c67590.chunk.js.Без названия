var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function () {
    var versionRegEx = '([\\d+.]+\\d*)';
    var featuresConfig = __webpack_require__(/*! FeatureConfig */ "./src/main/resources/static-site/featuresConfig.js");

    function getVersion() {
        var regEx = this.browserName + versionRegEx;
        var matches = new RegExp(regEx, 'i').exec(navigator.userAgent);
        return matches && matches.length > 1 && matches[1];
    }

    var BrowserValidator = {
        browsers: {
            IE: {
                browserName: 'msie ',
                firstAvailableVersion: 9,
                getVersion: getVersion
            },
            Firefox: {
                browserName: 'firefox\/',
                firstAvailableVersion: 4,
                getVersion: getVersion
            },
            /*Opera1: {
             browserName: 'Opera\/',
             firstAvailableVersion: Infinity,
             getVersion: getVersion
             },
             Opera2: {
             browserName: 'OPR\/',
             firstAvailableVersion: Infinity,
             getVersion: getVersion
             },*/
            Chrome: {
                browserName: 'chrome\/',
                firstAvailableVersion: 26,
                getVersion: getVersion
            },
            Safari: {
                browserName: 'safari\/',
                firstAvailableVersion: '5.1.0',
                getVersion: function getVersion() {
                    var regEx = 'Version\/' + versionRegEx;
                    var matches = new RegExp(regEx, 'i').exec(navigator.userAgent);
                    return matches && matches.length > 1 && matches[1];
                }
            }
        },
        isOldBrowserVersion: function isOldBrowserVersion() {
            if (featuresConfig.featuresReactUuiEnabled && /MSIE \d|Trident.*rv:/.test(window.navigator.userAgent)) {
                return true;
            }

            function compareVersions(version1, version2) {
                version1 = (version1 + '').split('.');
                version2 = (version2 + '').split('.');
                var minLength = Math.min(version1.length, version2.length);
                for (var i = 0; i < minLength; i++) {
                    var diff = version1[i] - version2[i];
                    if (diff) {
                        return diff;
                    }
                }
                return 0;
            }

            for (var browser in this.browsers) {
                if (this.browsers.hasOwnProperty(browser)) {
                    browser = this.browsers[browser];
                    var version = browser.getVersion();
                    if (version) {
                        return compareVersions(version, browser.firstAvailableVersion) < 0;
                    }
                }
            }
            return false;
        }
    };

    return BrowserValidator;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));

/***/ }),

