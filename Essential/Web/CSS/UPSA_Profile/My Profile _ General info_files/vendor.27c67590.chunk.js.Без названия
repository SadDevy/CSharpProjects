    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var config = __webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js");
    var core = __webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js");
    var messages = __webpack_require__(/*! com/epam/heroes/nls/messages */ "./src/main/resources/static-site/js/com/epam/heroes/nls/messages.js");

    var App = __webpack_require__(/*! com/epam/heroes/portal/core/App */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/App.js");
    var Utils = __webpack_require__(/*! com/epam/heroes/portal/core/util/Utils */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/util/Utils.js");

    var BaseView = __webpack_require__(/*! com/epam/heroes/portal/core/view/BaseView */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/BaseView.js");
    var AssignFormView = __webpack_require__(/*! com/epam/heroes/widget/givebadge/view/AssignFormView */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/view/AssignFormView.js");
    var SearchFilterView = __webpack_require__(/*! com/epam/heroes/widget/givebadge/view/SearchFilterView */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/view/SearchFilterView.js");
    var PeopleToRewardView = __webpack_require__(/*! com/epam/heroes/widget/givebadge/view/PeopleToRewardView */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/view/PeopleToRewardView.js");
    var NotifyColleaguesView = __webpack_require__(/*! com/epam/heroes/widget/givebadge/view/NotifyColleaguesView */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/view/NotifyColleaguesView.js");
    var PointsToAssignView = __webpack_require__(/*! com/epam/heroes/widget/givebadge/view/PointsToAssignView */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/view/PointsToAssignView.js");

    var featuresConfig = __webpack_require__(/*! FeatureConfig */ "./src/main/resources/static-site/featuresConfig.js");

    __webpack_require__(/*! template/com/epam/heroes/widget/givebadge/BadgeAssignPopup.soy */ "./src/main/soy/com/epam/heroes/widget/givebadge/BadgeAssignPopup.soy");

    var silentOption = { silent: true };

    var MAX_DESCRIPTION_LINK_LENGTH = 200;
    var MAX_CUTTED_DESCRIPTION_LINK_LENGTH = 35;

    var BadgeAssignPopup = BaseView.extend({

        animationDuration: 400,

        template: com.epam.heroes.widget.givebadge.template.BadgeAssignPopup,

        className: 'badge-assignment badge-info-popup',

        events: {
            'click .cancel': 'close',
            'click #assign-btn': 'onSubmitAssignment',
            'change input#notify-colleagues-checkbox': 'toggleNotifyColleagues',
            'focus .highlight-mandatory': 'unHighlight',
            'keyup input[name="project"]': 'clearProjectIfNecessary',
            'click .remove': 'clearProjectIfNecessary'
        },

        /**
         * @override
         */
        initialize: function initialize() {
            BaseView.prototype.initialize.apply(this, arguments);

            _.bindAll(this, 'windowResize', '_beforeSend', '_error', '_complete', '_assignBadgeSuccess', '_updateCommentSuccess', 'remove', 'afterOpen');

            var model = this.model,
                namespace = this.name;
            model.isPreventSearchAutofocus = Utils.isMobile() || model.get('receivers').length;

            this.search = this.addChildView(new SearchFilterView({
                name: 'SearchFilter',
                model: model
            }), '.search-filter-container');

            this.peopleToReward = this.addChildView(new PeopleToRewardView({
                name: 'PeopleToReward',
                model: model
            }), '#people-to-reward');

            this.addChildView(new AssignFormView({
                name: 'AssignForm',
                model: model
            }), '.comment-wrapper');

            this.addChildView(new PointsToAssignView({
                name: 'PointToAssign',
                model: model
            }), '.points-wrapper');

            this.addChildView(new NotifyColleaguesView({
                name: 'NotifyColleagues',
                model: model
            }), '.notify-colleagues-view');

            this.listenTo(App, 'receiversEdition', this.unHighlightAllFields);
            this.listenTo(model, 'change:pointedEmployeeId', this.definePointedElementAndSetArrow);
            this.listenTo(model, 'change:isReceiversNumberValid', this.updateAssignButtonsState);
            this.listenTo(model, 'change:isBudgetExceeded', this.updateAssignButtonsState);
            this.listenTo(this.peopleToReward, 'reposition:employee-items', this.onRepositionEmployees);

            this.throttleResizeHandler = _.throttle(this.windowResize, 500);

            $(window).on('resize.' + namespace, this.throttleResizeHandler);
        },

        /**
         * @override
         * @returns {Object}
         */
        getModel: function getModel() {
            var model = this.model;
            var badgeToAssign = model.getBadge().toDerivedJSON();
            var featuresProjectIconEnabled = core.Application.features.getFeatureStateByName('features_givabadge_specify_project');
            var featuresLinkButtonEnabled = core.Application.features.getFeatureStateByName('features_badge_library_link_button_enabled');

            badgeToAssign = badgeToAssign.badge || badgeToAssign; //if badge instanceof EmployeeBadge -> badge model is inner field
            if (badgeToAssign.descriptionLink) {
                badgeToAssign.cuttedDescriptionLink = Utils.cutExtraLongString(badgeToAssign.descriptionLink, MAX_CUTTED_DESCRIPTION_LINK_LENGTH, '...');
                badgeToAssign.maxCuttedDescriptionLink = Utils.cutExtraLongString(badgeToAssign.descriptionLink, MAX_DESCRIPTION_LINK_LENGTH, '...');
            }

            return {
                editAssignCommentMode: model.isEditAssignCommentMode(),
                badge: badgeToAssign,
                notifyColleagues: model.isNotifyColleaguesToggled(),
                isAssignmentDisabled: !model.isReceiversNumberValid() || this.model.get('isBudgetExceeded'),
                featuresProjectIconEnabled: featuresProjectIconEnabled,
                featuresLinkButtonEnabled: featuresLinkButtonEnabled,
                featuresConfig: featuresConfig
            };
        },

        afterRender: function afterRender() {
            this.$('[data-toggle="tooltip"]').tooltip();
        },

        /**
         * Hides popup and returns promise it
         *
         * @returns {jQuery.Deferred.Promise}
         */
        hide: function hide() {
            this.$el.removeClass('visible');
            return this.$el.slideUp(this.animationDuration).promise();
        },

        /**
         * Sets badge to model
         *
         * @param {Object} badge
         */
        initBadge: function initBadge(badge) {
            this.model.setBadgeToAssign(badge);
            return this;
        },

        /**
         * Sets popup after provided $badge and renders it
         *
         * @param {jQuery} $elBefore
         * @param {boolean} [animated]
         * @returns {jQuery.Deferred.promise|undefined}
         */
        openPopupAfter: function openPopupAfter($elBefore, animated) {
            var promise;
            $elBefore.after(this.$el);
            this.render();

            if (animated) {
                promise = this.$el.slideDown(this.animationDuration, this.afterOpen).promise();
            } else {
                this.$el.css({
                    height: 'auto',
                    display: 'block'
                });
                this.afterOpen();
            }

            return promise;
        },

        /**
         * Opens after open animation completes
         */
        afterOpen: function afterOpen() {
            this.$el.addClass('visible');
            _.forEach(this.childViews, function (childView) {
                childView.afterRender();
            });
            $('#next-page').prop('disabled', false).removeClass('disabled');
        },

        /**
         * @override
         * Switches off resize events
         */
        beforeRemove: function beforeRemove() {
            BaseView.prototype.beforeRemove.apply(this);
            var namespace = this.name;
            $(window).off('resize.' + namespace, this.throttleResizeHandler);
        },

        /**
         * Callback on window resize.
         * Switches view of peopleToReward view
         */
        windowResize: function windowResize() {
            this.peopleToReward.switchView();
        },

        /**
         * Triggers event and removes popup
         * @param e
         */
        close: function close(e) {
            if (e) {
                e.preventDefault();
            }
            this.trigger('close');
            this.remove();
        },

        /**
         * @override
         */
        remove: function remove() {
            BaseView.prototype.remove.apply(this, arguments);
            this.$el.remove();
        },

        /**
         * Callback on repositioning employee items
         * Just invokes arrow-pointer positioning
         */
        onRepositionEmployees: function onRepositionEmployees() {
            this.definePointedElementAndSetArrow(this.model.getPointedEmployeeId());
        },

        /**
         * overloading:
         *   enableAssignButton(model, isReceiversNumberValid) - model event callback
         *   enableAssignButton(isReceiversNumberValid)
         *
         * @param {Backbone.Model} [model]
         * @param {boolean} isReceiversNumberValid
         */
        enableAssignButton: function enableAssignButton(model, isReceiversNumberValid) {
            // overloading
            isReceiversNumberValid = model instanceof Backbone.Model ? isReceiversNumberValid : model;

            isReceiversNumberValid = _.isBoolean(isReceiversNumberValid) ? isReceiversNumberValid : true;

            var isBadgeReadyToAssign = isReceiversNumberValid && !this.model.get('isBudgetExceeded');

            this.$('#assign-btn').prop('disabled', !isBadgeReadyToAssign);
        },

        updateAssignButtonsState: function updateAssignButtonsState() {
            var isBadgeReadyToAssign = this.model.get('isReceiversNumberValid') && !this.model.get('isBudgetExceeded');

            this.$('#assign-btn').prop('disabled', !isBadgeReadyToAssign);
        },

        /**
         * Returns jQuery-element that corresponds employee with passed id
         *
         * @param {String} employeeId
         * @returns {jQuery}
         */
        get$EmployeeElement: function get$EmployeeElement(employeeId) {
            if (employeeId !== this.model.GROUP_ASSIGN_ENTRY_ID) {
                return this.$('.employee-item[data-employee-id=' + employeeId + ']');
            } else {
                return this.$('.employee-item.group-item');
            }
        },

        /**
         * Returns arrow - employee pointer jQuery element
         *
         * @returns {jQuery}
         */
        get$EmployeePointer: function get$EmployeePointer() {
            return this.$('.comment-pointer');
        },

        /**
         * @returns {boolean}
         * @private
         */
        _isGroupItemEnabled: function _isGroupItemEnabled() {
            return this.$('.group-item:visible').length > 0;
        },

        /**
         * Sets pointer to the selected employee comment to edit
         * if group comment is chosen, but group controls are not available ->
         * set arrow under first available employee
         *
         * @param {BadgeAssignPopupModel} [model]
         * @param {String} pointedEmployeeId
         */
        definePointedElementAndSetArrow: function definePointedElementAndSetArrow(model, pointedEmployeeId) {
            // overloading
            pointedEmployeeId = model instanceof Backbone.Model ? pointedEmployeeId : model;
            model = this.model;

            var groupControlsAreEnable = this._isGroupItemEnabled();

            if (this.model.isMultipleAssignmentAvailable()) {
                // Set pointer to first available employee if '.group-item' is not enabled
                if (model.isGroupItemPointed() && !groupControlsAreEnable) {
                    var firstEnableEmployee = model.getReceivers().at(0);
                    pointedEmployeeId = firstEnableEmployee && firstEnableEmployee.id;
                }

                var $employeeToPoint = this.get$EmployeeElement(pointedEmployeeId);

                // If pointed employee is not available -> set pointer under first receiver
                if (!$employeeToPoint.is(':visible')) {
                    var firstEmployee = model.getReceivers().at(0);
                    var firstEmployeeId = firstEmployee && firstEmployee.id;
                    $employeeToPoint = this.get$EmployeeElement(firstEmployeeId);
                }
                this.positionEmployeePointer($employeeToPoint);
            } else {
                this.get$EmployeePointer().hide();
            }
        },

        /**
         * Places arrow that is pointer to curtain employee to point to current employee
         *
         * @param {jQuery} [$employeeToPoint]
         * @returns {number}
         */
        positionEmployeePointer: function positionEmployeePointer($employeeToPoint) {
            var $arrow = this.get$EmployeePointer();
            var $elementToPoint = $employeeToPoint.find('.img');

            var arrowLeftOffset = Utils.positionArrow($arrow, $elementToPoint, _.bind(this._getEmployeeLeftOffsetWithRespectToCommonContainer, this));

            this.model.set({ arrowLeftOffset: arrowLeftOffset });
            return arrowLeftOffset;
        },

        /**
         * This returns left offset with respect to curtain wrapper
         * or negative value if $employeeItem is out of wrapper
         *
         * @param {jQuery} $employeeItem
         * @returns {number}
         * @private
         */
        _getEmployeeLeftOffsetWithRespectToCommonContainer: function _getEmployeeLeftOffsetWithRespectToCommonContainer($employeeItem) {
            var leftOffsetWithRespectToCommonContainer = -1;

            if ($employeeItem && $employeeItem.length) {
                var $employeeItemsWrapper = this.$('#people-to-reward');
                var $closestToCommonWrapperParent = this._getClosestToCommonWrapperParent($employeeItem, $employeeItemsWrapper);
                var parentLeftOffset = $closestToCommonWrapperParent.position().left;
                var itemLeftOffset = $employeeItem.parent().position().left;

                leftOffsetWithRespectToCommonContainer = itemLeftOffset >= 0 ? parentLeftOffset + itemLeftOffset : -1;
            }

            return leftOffsetWithRespectToCommonContainer;
        },

        /**
         * Searches for the most closest parent element to curtain wrapper
         * is used to get right left offset with respect to the wrapper
         *
         * @param {jQuery} $element
         * @param {jQuery} $commonWrapper
         * @returns {jQuery}
         */
        _getClosestToCommonWrapperParent: function _getClosestToCommonWrapperParent($element, $commonWrapper) {
            var $parent = $element.parent();

            while ($parent.length && $parent[0] !== $commonWrapper[0] || $parent[0] === this.$el[0]) {
                $element = $element.parent();
                $parent = $element.parent();
            }

            return $element;
        },

        /**
         * Updates employeeBadge if popup is opened for comment editing
         * assigns badge if popup is opened for badge assignment
         *
         * @param {Event} event
         */
        onSubmitAssignment: function onSubmitAssignment(event) {
            event.preventDefault();

            var model = this.model;
            var notValidFields = model.getFieldsToHighlight();
            var employeesWithNotFilledComment = model.defineEmployeesToHighlight();
            var employeeWithNotValidFields = employeesWithNotFilledComment.length > 0 ? employeesWithNotFilledComment[0].get('id') : notValidFields.length > 0 ? model.GROUP_ASSIGN_ENTRY_ID : null;

            if (employeeWithNotValidFields) {
                App.trigger('found-employees-with-not-valid-fields', employeeWithNotValidFields);
                model.setPointedEmployeeId(employeeWithNotValidFields);
                this.highlightInputs(notValidFields);
            } else {
                var editAssignCommentMode = model.isEditAssignCommentMode();
                var employeeBadge = model.getBadgeToEdit();

                // If the widget is opened in "read-only" mode and employeeBadge is set
                // it means that user wants to edit comment on a previously given badge
                if (editAssignCommentMode && employeeBadge) {
                    this._updateComment(employeeBadge);
                } else {
                    // Otherwise just assign badge to the selected employees
                    this._assignBadge();
                }
            }
        },

        /**
         * @param {Array} fieldsToHighlight - names of inputs to highlight
         */
        highlightInputs: function highlightInputs(fieldsToHighlight) {
            var _this = this;
            _.forEach(fieldsToHighlight, function (field) {
                _this.$('[name=' + field + ']').closest('.element-to-highlight').addClass('highlight-mandatory');
            });
        },

        /**
         * Removes warning class from field
         *
         * @param {Event} event
         */
        unHighlight: function unHighlight(event) {
            $(event.target).closest('.highlight-mandatory').removeClass('highlight-mandatory');
        },

        /**
         * Removes warning class from every field when person is removed
         *
         * @param {Event} event
         */
        unHighlightAllFields: function unHighlightAllFields() {
            $('.highlight-mandatory').removeClass('highlight-mandatory');
        },

        /**
         * Clears project from model if the value in input is not equal to the current project name
         * @param {Event} event
         */
        clearProjectIfNecessary: function clearProjectIfNecessary(event) {
            var $projectInput = $(event.target);
            var model = this.model;
            var project = model.getProject();

            if (project && $projectInput.value !== project.get('name')) {
                model.unset('project');
            }
        },

        /**
         * Creates group entries from badges receivers
         *
         * @returns {Array}
         * @private
         */
        _getGroupEntries: function _getGroupEntries() {
            var model = this.model;
            var comment = model.getComment();
            var groupName = model.getPotentialGroupName();
            var badgeToAssignId = model.getBadge().id;
            var receivers = model.getReceivers();
            var selectedGroup = model.getGroup();
            var selectedGroupId = selectedGroup && selectedGroup.get('id');
            var sendNotificationTo = this._getInformedEmployeesEmails();
            var project = model.get('project');
            var link = model.get('link');
            var summary = model.get('summary');
            var accountsRefs = model.getBadge().getAccounts().map(function (account) {
                return account.id;
            });

            return receivers.map(function (employeeToReward) {

                if (employeeToReward.getExternalId()) {
                    return {
                        toExternalEmployeeId: employeeToReward.getExternalId(),
                        badge: {
                            id: badgeToAssignId
                        },
                        groupInfo: groupName,
                        group: selectedGroupId && { id: selectedGroupId },
                        summary: summary,
                        comment: comment,
                        projectName: project && project.getName(),
                        link: link,
                        individualContribution: employeeToReward.getIndividualContributionComment(),
                        sendNotification: true,
                        ccRecipients: sendNotificationTo,
                        accounts: accountsRefs
                    };
                }
                return {
                    toEmployee: {
                        id: employeeToReward.id
                    },
                    badge: {
                        id: badgeToAssignId
                    },
                    groupInfo: groupName,
                    group: selectedGroupId && { id: selectedGroupId },
                    summary: summary,
                    comment: comment,
                    projectName: project && project.getName(),
                    link: link,
                    individualContribution: employeeToReward.getIndividualContributionComment(),
                    sendNotification: true,
                    ccRecipients: sendNotificationTo,
                    accounts: accountsRefs
                };
            });
        },

        /**
         *
         * @returns {{toEmployee: {id: *}, badge: {id: *}, comment: *, sendNotification: boolean}}
         * @private
         */
        _getSingleEntry: function _getSingleEntry() {
            var model = this.model;
            var badgeToAssignId = model.getBadge().id;
            var programPoints = model.get('programSettings') && model.get('programSettings').chosenPointStep;
            var employeeToReward = model.getReceivers().at(0);
            var sendNotificationTo = this._getInformedEmployeesEmails();
            var project = model.get('project');
            var link = model.get('link');
            var summary = model.get('summary');
            var accountsRefs = model.getBadge().getAccounts().map(function (account) {
                return account.id;
            });

            if (employeeToReward.getExternalId()) {
                return {
                    toExternalEmployeeId: employeeToReward.getExternalId(),
                    badge: {
                        id: badgeToAssignId
                    },
                    summary: summary,
                    comment: model.getComment(),
                    projectName: project && project.getName(),
                    link: link,
                    sendNotification: true,
                    ccRecipients: sendNotificationTo,
                    accounts: accountsRefs,
                    programPoints: programPoints
                };
            }
            return {
                toEmployee: {
                    id: employeeToReward.id
                },
                badge: {
                    id: badgeToAssignId
                },
                summary: summary,
                comment: model.getComment(),
                projectName: project && project.getName(),
                link: link,
                sendNotification: true,
                ccRecipients: sendNotificationTo,
                accounts: accountsRefs,
                programPoints: programPoints
            };
        },
        /**
         * Extracts employees emails in appropriate format
         *
         * @returns {Array}
         * @private
         */
        _getInformedEmployeesEmails: function _getInformedEmployeesEmails() {
            if (this.model.isNotifyColleaguesToggled()) {
                return _.map(this.model.getInformedEmployees(), function (emp) {
                    return { email: emp.email };
                });
            }
            return [];
        },
        /**
         * Returns data for badge assignment: single entry for approvable badge
         * group entries in the other case
         *
         * @returns {{singleEntries: Array} | {groupEntries: Array}}
         * @private
         */
        _getDataToAssign: function _getDataToAssign() {
            var isSingleAssignment = !this.model.isMultipleAssignmentAvailable() || this.model.getReceivers().length === 1;
            var data;

            if (isSingleAssignment) {
                var singleEntry = this._getSingleEntry();
                data = { singleEntries: [singleEntry] };
            } else {
                var groupEntries = this._getGroupEntries();
                data = { groupEntries: [{ groupEntry: groupEntries }] };
            }
            return data;
        },

        /**
         * Abort previous request
         * disabled but to prevent giving badge twice
         *
         * @param {XMLHttpRequest} xhr
         * @private
         */
        _beforeSend: function _beforeSend(xhr) {
            // abort subsequent request if we are making one
            if (this._xhr) {
                xhr.abort();
                this._xhr = null;
                return;
            }
            this._xhr = xhr;
            this.enableAssignButton(false);
        },

        /**
         * Displays errors
         *
         * @private
         */
        _error: function _error() {
            this.displayErrors(this._xhr);
        },

        /**
         * Enables give-a-badge button
         *
         * @private
         */
        _complete: function _complete() {
            this._xhr = null;
            this.enableAssignButton();
        },

        /**
         * Sends request for badge assign
         *
         * @private
         */
        _assignBadge: function _assignBadge() {
            var model = this.model;
            var user = model.getUser();
            var userToken = model.getUserToken();
            var receivers = model.getReceivers();
            if (!model.deepGet('badgeToAssign', 'settings').allowSelfAssignment) {
                receivers.remove(user);
            }

            var data = this._getDataToAssign();
            var ajaxOptions = {
                url: config.api.badges.assign.url,
                type: 'POST',
                data: JSON.stringify(data),
                contentType: 'application/json; charset=utf-8',
                beforeSend: this._beforeSend,
                success: this._assignBadgeSuccess,
                error: this._error,
                complete: this._complete
            };

            if (userToken) {
                _.extend(ajaxOptions, {
                    headers: {
                        Authorization: 'Token ' + userToken
                    }
                });
            }

            $.ajax(ajaxOptions);
        },

        /**
         * On success assignment -> notify user
         *
         * @param response
         * @private
         */
        _assignBadgeSuccess: function _assignBadgeSuccess(response) {
            // Retrieve first of possibly many employee badges
            // that were assigned and check its status
            var assignedEmployeeBadge = response.content[0],
                badgeToAssign = this.model.getBadge(),
                message = assignedEmployeeBadge.status === 'PENDING' ? messages.BadgeAssignment.notifications.badgeSubmitted : messages.BadgeAssignment.notifications.badgeGiven,
                options = {};
            var isApprovable = response.refs[assignedEmployeeBadge.badge.url].approvable;

            options.imageUrl = response.refs[assignedEmployeeBadge.badge.url].image.imageUrl;
            options.badgeName = response.refs[assignedEmployeeBadge.badge.url].name;
            options.isApprovable = isApprovable;

            if (badgeToAssign.memento.limited) {
                App.trigger('badgeCount:change');
            }
            message = message.replace('{badgeName}', badgeToAssign.get('name'));

            App.trigger('badge:assign');

            if (!this.model.isSinglePage()) {
                this.notify({
                    body: message,
                    cssClass: 'success',
                    timeout: 10000
                });
            } else {
                App.trigger('give-badge-page:badge assignment info', options);
            }
            this.close();
        },

        /**
         * Sends request to save comment for employee badge
         *
         * @param {EmployeeBadge} employeeBadge
         * @private
         */
        _updateComment: function _updateComment(employeeBadge) {
            var comment = this.model.getComment();
            var project = this.model.get('project');
            var link = this.model.get('link');
            var toEmployeeId = this.model.getReceivers().at(0).id;
            var options = {
                beforeSend: this._beforeSend,
                success: this._updateCommentSuccess,
                error: this._error,
                complete: this._complete
            };

            employeeBadge.setEmployeeId(toEmployeeId).set({ comment: comment,
                project: project,
                projectName: project && project.get('name'),
                link: link
            }, silentOption).save(null, options);
        },

        /**
         * On success comment update
         *
         * @param {Object} employeeBadge
         * @private
         */
        _updateCommentSuccess: function _updateCommentSuccess(employeeBadge) {
            var badge = employeeBadge.get('badge');
            var message = messages.BadgeAssignment.notifications.badgeUpdated.replace('{badgeName}', badge.get('name'));
            this.notify({
                body: message,
                cssClass: 'success'
            });
            App.trigger('assignment:updated');
            this.close();
        },

        /**
         * Shows or hides NotifyColleaguesView
         *
         * @param {Event} e
         * @See NotifyColleaguesView
         *
         */
        toggleNotifyColleagues: function toggleNotifyColleagues(e) {
            e.preventDefault();
            var state = this.$(e.currentTarget).prop('checked');
            this.childViews['NotifyColleagues'].$el.toggle(state);
            this.model.set({ notifyColleagues: state });
        }
    });

    return BadgeAssignPopup;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js"), __webpack_require__(/*! backbone */ "./src/main/resources/static-site/js/libs/vendor/backbone/backbone.js")))

/***/ }),

