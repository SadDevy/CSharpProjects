    var App = __webpack_require__(/*! com/epam/heroes/portal/core/App */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/App.js");

    var resolutions = __webpack_require__(/*! com/epam/heroes/portal/core/resolutions */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/resolutions.js");
    var messages = __webpack_require__(/*! com/epam/heroes/nls/messages */ "./src/main/resources/static-site/js/com/epam/heroes/nls/messages.js");

    var EmployeeRoundAnimationService = __webpack_require__(/*! com/epam/heroes/widget/givebadge/service/EmployeeRoundAnimationService */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/service/EmployeeRoundAnimationService.js");
    var BaseView = __webpack_require__(/*! com/epam/heroes/portal/core/view/BaseView */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/BaseView.js");

    var GA = __webpack_require__(/*! com/epam/heroes/core/util/GA */ "./src/main/resources/static-site/js/com/epam/heroes/core/util/GA.js");

    __webpack_require__(/*! com/epam/heroes/portal/core/util/Utils */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/util/Utils.js");
    __webpack_require__(/*! template/com/epam/heroes/widget/givebadge/PeopleToReward.soy */ "./src/main/soy/com/epam/heroes/widget/givebadge/PeopleToReward.soy");
    __webpack_require__(/*! jquery.sly */ "./src/main/resources/static-site/js/libs/vendor/sly/dist/sly.js");

    /***    constants    ***/
    var PEOPLE_IN_ROW_DESKTOP = 4;
    var PEOPLE_IN_ROW_MOBILE = 4;
    var PEOPLE_IN_ROW_MDMOBILE = 3;
    var PEOPLE_IN_ROW_XSMOBILE = 2;

    var ESC_KEY_CODE = 27;
    var ENTER_KEY_CODE = 13;

    /**
     * Enum
     *
     * Defines target of saving group
     * Values are string (not number), cause it makes prettier debugging
     * and process of bug searching + messages is easier to make unique
     * (in case if smbd decides to add new value somewhere in the middle
     * of inheritance chain)
     *
     * @type {{
     *   ADD_MEMBER: string,
     *   REMOVE_MEMBER: string,
     *   CREATE_NEW_GROUP: string,
     *   UPDATE_NAME: string
     *  }}
     */
    var SAVE_GROUP_TARGET = {
        ADD_MEMBER: 'add member',
        REMOVE_MEMBER: 'remove member',
        CREATE_NEW_GROUP: 'create new group',
        UPDATE_NAME: 'update group name'
    };

    var GROUP_CONTAINER_CLASS = 'group-members';

    /**
     * Triggers events: 'reposition:employee-items' -> command to reset pointer to current employee-item
     */
    var PeopleToRewardView = BaseView.extend({

        POINTED_EMPLOYEE_CLASS: 'pointed',

        ERROR_EMPLOYEE_ITEM_CLASS: 'error-highlighted',

        template: com.epam.heroes.widget.givebadge.template.PeopleToReward,

        events: {
            'click .employee-item': 'switchEmployeeItemToEdit',
            'click .scroll-frame.employee-item': 'googleAnalyticsTracker',
            'click .scroll-frame .remove': 'removeReceiver',
            'click .clear': 'onClearReceivers',
            'click .save-as-group-btn': 'onSaveAsGroupBtn',

            'click .edit-group-name-btn': 'onEditGroupName',
            'focus input[name="groupName"]': 'onEditGroupName',
            'blur input[name="groupName"]': 'onBlurGroupNameInput',

            'click .remove-group-name-btn': 'onExitGroupNameEditing',
            'click .save-group-name-btn': 'onSaveGroupName',

            'mousedown .save-group-name-btn': '_preventDefaultEvent',
            'mousedown .remove-group-btn': '_preventDefaultEvent',
            'mousedown .remove-group-name-btn': '_preventDefaultEvent',

            'click .remove-group-btn': 'onRemoveGroupBtn',

            'keyup input[name="groupName"]': 'onChangeGroupName'
        },

        EDIT_GROUP_MODES: {
            NO_GROUP: {
                value: 'no group',
                templateOptions: function templateOptions() {
                    return {
                        groupActionsDisabled: false,
                        showSaveAsGroupBtn: false,
                        showEditGroupBtn: false,
                        showGroupNameInput: false,
                        showRemoveGroupBtn: false,
                        showGroupAsPresent: false,
                        showSaveGroupNameBtn: false,
                        showGroupControl: false
                    };
                }
            },
            POTENTIAL_GROUP: {
                value: 'potential group',
                templateOptions: function templateOptions() {
                    return {
                        showGroupControl: true,
                        groupActionsDisabled: false,
                        showSaveAsGroupBtn: true,
                        showEditGroupBtn: false,
                        showGroupNameInput: false,
                        showRemoveGroupBtn: false,
                        showGroupAsPresent: false,
                        showSaveGroupNameBtn: false
                    };
                }
            },
            NO_EDITABLE_GROUP: {
                value: 'no editable group',
                templateOptions: function templateOptions() {
                    return {
                        showGroupControl: true,
                        showSaveAsGroupBtn: false,
                        showEditGroupBtn: false,
                        showGroupNameInput: true,
                        showRemoveGroupBtn: false,
                        showGroupAsPresent: true,
                        showSaveGroupNameBtn: false,
                        groupActionsDisabled: true
                    };
                }
            },
            EDITABLE_GROUP: {
                value: 'editable group',
                templateOptions: function templateOptions() {
                    return {
                        showGroupControl: true,
                        groupActionsDisabled: false,
                        showSaveAsGroupBtn: false,
                        showEditGroupBtn: true,
                        showGroupNameInput: true,
                        showRemoveGroupBtn: true,
                        showGroupAsPresent: true,
                        showSaveGroupNameBtn: false
                    };
                }
            },
            EDIT_GROUP_PROCESS: {
                value: 'edit group process',
                templateOptions: function templateOptions() {
                    return {
                        showGroupControl: true,
                        groupActionsDisabled: false,
                        showSaveAsGroupBtn: false,
                        showEditGroupBtn: false,
                        showGroupNameInput: true,
                        showRemoveGroupBtn: true,
                        showGroupAsPresent: true,
                        showSaveGroupNameBtn: true
                    };
                }
            }
        },

        initialize: function initialize() {
            core.BaseView.prototype.initialize.apply(this, arguments);
            _.bindAll(this, 'onChangeGroupName', 'removeGroup', 'onResizeWindow', '_onScrollEmployeeContainer');

            this.model.set({ editGroupMode: this.EDIT_GROUP_MODES.NO_GROUP });

            this.initListeners();
            this.animationService = new EmployeeRoundAnimationService({ view: this });

            //Init submodels' listeners
            this.onChangeGroup();
            this.onChangeReceivers();

            $(window).on('resize' + '.' + this.getNamespace(), _.throttle(this.onResizeWindow, 30));
        },

        /**
         * Inits listeners
         */
        initListeners: function initListeners() {
            var model = this.model;

            this.listenTo(model, 'restore store', this.render);
            this.listenTo(model, 'change:assignEntry', this.onChangeAssignEntry);
            this.listenTo(model, 'change:group', this.onChangeGroup);
            this.listenTo(model, 'change:receivers', this.onChangeReceivers);
            this.listenTo(model, 'change:editGroupMode', this.onChangeGroupMode);
            this.listenTo(model, 'change:employeesToHighlight', this.onChangeHighlightedEmployees);
            this.listenTo(model, 'change:pointedEmployeeId', this.onChangePointedReceiverId);
            this.listenTo(App, 'found-employees-with-not-valid-fields', this.slideToFirstEmployeeWithNoValidFields);
        },

        /**
         * @override
         */
        beforeRender: function beforeRender() {
            this.destroySly();
        },

        /**
         * @override
         * @returns {Object}
         */
        getModel: function getModel() {
            var params = _.pick(this.model.attributes, ['editAssignCommentMode', 'receivers', 'potentialGroupName', 'canSaveGroup', 'editGroupNameProcess', 'hasCachedData', 'message', 'user', 'showGroupAsPresent', 'editGroupMode', 'pointedEmployeeId', 'groupControlId']);
            params.showEmployeeOverlay = this.model.isIndividualContributionRequired();
            params.isGroupControlPointed = this.model.isGroupItemPointed();
            params.receivers = params.receivers.toDerivedJSON();
            params.userId = params.user.id;
            params.numberEmployees = params.receivers.length;

            return _.extend(params, params.editGroupMode.templateOptions.apply(this));
        },

        /**
         * @override
         */
        afterRender: function afterRender() {
            this.switchView();

            var currentMode = this.model.get('editGroupMode').value;

            if (currentMode === this.EDIT_GROUP_MODES.EDIT_GROUP_PROCESS.value) {
                this.$('input[name="groupName"]').endFocus();
            }

            this.$('input[name="groupName"]:not(:focus)').autoResizeInput();
            this.trigger('reposition:employee-items');
        },

        /**
         * Used for binding events
         *
         * @returns {String}
         */
        getNamespace: function getNamespace() {
            return this.name;
        },

        /**
         * @override
         */
        beforeRemove: function beforeRemove() {
            this.destroySly();

            $(window).off('resize' + '.' + this.getNamespace());
        },

        /**
         * Fixes position of svg frame and
         * switches views mobile <-> desktop
         */
        onResizeWindow: function onResizeWindow() {
            var _this = this;

            setTimeout(function () {
                _this.animationService.updateSvgCircles();
                _this.switchView();
            }, 0);
        },

        /**
         * @param {Event} event
         * @private
         */
        _preventDefaultEvent: function _preventDefaultEvent(event) {
            event.preventDefault();
        },

        /**
         * Changes group mode according to state of model
         */
        changeGroupEditMode: function changeGroupEditMode() {
            var group = this.model.getGroup();
            var previousMode = this.model.get('editGroupMode');
            var mode = previousMode;

            if (!group.hasValidEmployeesNumber() && group.isNew()) {
                mode = this.EDIT_GROUP_MODES.NO_GROUP;
            } else if (group.isNew()) {
                mode = this.EDIT_GROUP_MODES.POTENTIAL_GROUP;
            } else if (!group.isPrivate()) {
                mode = this.EDIT_GROUP_MODES.NO_EDITABLE_GROUP;
            } else {
                mode = this.EDIT_GROUP_MODES.EDITABLE_GROUP;
            }

            this.model.set('editGroupMode', mode);
        },

        /**
         * Changes view according to mode
         */
        onChangeGroupMode: function onChangeGroupMode() {
            var currentModeValue = this.model.get('editGroupMode').value;

            // To prevent re-rendering in case of switching between these two modes
            if (currentModeValue === this.EDIT_GROUP_MODES.EDIT_GROUP_PROCESS.value) {
                this.$('.edit-group-name-btn').addClass('hidden');
                this.$('label[for="groupName"]').removeClass('hidden');
                this.$('.save-group-name-btn').removeClass('hidden');
                this.$('.remove-group-name-btn').removeClass('hidden');
                this.$('input[name="groupName"]').endFocus();
                this.toggleSaveGroupNameBtn();
            } else if (currentModeValue === this.EDIT_GROUP_MODES.POTENTIAL_GROUP.value) {
                this.model.set('potentialGroupName', '');
                this.render();
            } else {
                this.render();
            }
        },

        /**
         * Sets listeners to the new group
         * and sets flags of enabling editing
         */
        onChangeGroup: function onChangeGroup() {
            var newGroup = this.model.getGroup();
            var isNewGroupDefined = newGroup && newGroup.getName(); //if name is absent -> it's default empty group
            var hasCachedData = this.model.get('hasCachedData');

            this.listenTo(newGroup, 'remove:employee', this.saveGroupAndShowResultAnimation.bind(this, SAVE_GROUP_TARGET.REMOVE_MEMBER));

            this.listenTo(newGroup, 'add:employee', this.saveGroupAndShowResultAnimation.bind(this, SAVE_GROUP_TARGET.ADD_MEMBER));

            this.model.set({
                potentialGroupName: newGroup.getName()
            });

            // if there was some cached data, but a new group is added -> don't allow restore data
            if (hasCachedData && isNewGroupDefined) {
                this.model.set({ hasCachedData: false });
            }

            this.changeGroupEditMode();
            this.render();
        },

        /**
         * Returns jQuery-element that corresponds employee with passed id
         *
         * @param {String} employeeId
         * @returns {jQuery}
         */
        get$EmployeeElement: function get$EmployeeElement(employeeId) {
            return this.$('.employee-item[data-employee-id=' + employeeId + ']');
        },

        /**
         * Triggers event to switch position of pointer to employee,
         * highlights clicked employee
         *
         * @param {Event} event
         */
        switchEmployeeItemToEdit: function switchEmployeeItemToEdit(event) {
            if (this.model.isIndividualContributionEnabled()) {
                this.$('.' + this.POINTED_EMPLOYEE_CLASS).removeClass(this.POINTED_EMPLOYEE_CLASS);

                var $employeeToPoint = $(event.currentTarget).addClass(this.POINTED_EMPLOYEE_CLASS);

                if (!this.isAddedJustOneReceiver()) {
                    var employeeId = $employeeToPoint.attr('data-employee-id');
                    this.model.setPointedEmployeeId(employeeId);
                    this.highlightEmployee(employeeId);
                }
            }
        },

        /**
         * Checkes is added just one receiver and there is no active groupes
         *
         * @returns {boolean}
         */
        isAddedJustOneReceiver: function isAddedJustOneReceiver() {
            // compare with 2, because in DOM already exist '.employee-item .group-item'
            return $('.employee-item').length == 2 && $('.group-item').filter(':hidden').length;
        },

        /**
         * Updates employee-item view according to filled assign data
         *
         * @param {AssignEntryModel} assignEntry
         */
        onChangeAssignEntry: function onChangeAssignEntry(assignEntry) {
            var $employeeItem = this.get$EmployeeElement(assignEntry.id).toggleClass('filled', assignEntry.isAssignDataFilled());

            if (!assignEntry.isAssignDataPristine()) {
                //only before first editing
                $employeeItem.removeClass('overlaid');
            }
        },

        /**
         * Sets listeners to the new receivers collection
         */
        onChangeReceivers: function onChangeReceivers() {
            var receivers = this.model.getReceivers();

            this.listenTo(receivers, 'reset', this.render);
            this.listenTo(receivers, 'add', this.onAddReceiver);

            var pointedReceiver = receivers.get(this.model.getPointedEmployeeId());
            if (pointedReceiver) {
                this.listenTo(pointedReceiver, 'change:individualContribution', this.onChangePointedReceiverIndividualContribution);
            }
        },

        /**
         * Sets hasCachedData to false
         * and invokes changing mode of group editing
         */
        onAddReceiver: function onAddReceiver() {
            this.model.set('hasCachedData', false);
            this.changeGroupEditMode();
            this.render();
            $('#add-rms-btn').show();
        },

        /**
         * Removes error highlighting if comment becomes valid
         *
         * @param {AssignEntryModel} pointedReceiver
         */
        onChangePointedReceiverIndividualContribution: function onChangePointedReceiverIndividualContribution(pointedReceiver) {
            if (pointedReceiver.isIndividualContributionCommentValid()) {
                this.get$EmployeeElement(pointedReceiver.id).removeClass(this.ERROR_EMPLOYEE_ITEM_CLASS);

                var highlightedEmployees = this.model.get('employeesToHighlight');
                if (highlightedEmployees && highlightedEmployees.length) {
                    highlightedEmployees.remove({ id: pointedReceiver.id });
                }
            }
        },

        /**
         * Shows input for group name
         * switches view to editGroupNameProcess
         *
         * @param {Event} event
         */
        onSaveAsGroupBtn: function onSaveAsGroupBtn(event) {
            event && event.preventDefault();
            $(event.currentTarget).hide();

            this.model.set({ editGroupMode: this.EDIT_GROUP_MODES.EDIT_GROUP_PROCESS });

            var $groupTitle = this.$('#group-title');
            $groupTitle.show();
        },

        /**
         * Shows input for group name
         * switches view to editGroupNameProcess
         *
         * @param {Event} event
         */
        onEditGroupName: function onEditGroupName(event) {
            event && event.preventDefault();

            this.model.set({ editGroupMode: this.EDIT_GROUP_MODES.EDIT_GROUP_PROCESS });
        },

        /**
         * Removes group name from model and re-renders view
         */
        onExitGroupNameEditing: function onExitGroupNameEditing() {
            // Do not preventDefault behaviour of event! It triggers click event afterwards
            var group = this.model.getGroup();
            var originalGroupName = group.getName();

            this.model.set({
                potentialGroupName: originalGroupName
            });
            this.$('input[name="groupName"]').val(originalGroupName);

            this.changeGroupEditMode();
        },

        /**
         * Saves new group name and
         * exits from EDIT_GROUP_PROCESS_MODE
         *
         * @param {Event} e
         */
        onBlurGroupNameInput: function onBlurGroupNameInput(e) {
            var savedGroupName = this.model.getGroup().getName();
            var groupNameChanged = savedGroupName !== this.model.getPotentialGroupName();

            if (groupNameChanged) {
                // To allow on click event to be triggered before blur
                setTimeout(this.onSaveGroupName.bind(this, e), 1);
            } else {
                this.changeGroupEditMode();
            }
        },

        /**
         * Sends request to save group
         *
         * @param {Backbone.Model} group
         * @param {Object} [options]
         * @returns {XmlHttpRequest}
         * @private
         */
        _saveGroup: function _saveGroup(group, options) {
            if (this._xhr) {
                // Prevent double click on button
                return;
            }
            options = options || {};
            var model = this.model;
            var _this = this;
            var groupName = model.getPotentialGroupName();
            var employees = group.getEmployees().map(function (empl) {
                return {
                    id: empl.id,
                    url: empl.url
                };
            });
            var saveOptions = {
                beforeSend: function beforeSend(jqXHR) {
                    _this._xhr = jqXHR;
                },
                success: function success() {
                    _this.changeGroupEditMode();
                    if (_.isFunction(options.afterSaveCallback)) {
                        options.afterSaveCallback();
                    }
                    group.trigger('saved');
                },
                error: function error(model, response) {
                    options.onErrorCallback();
                    _this.displayErrors(response, true);
                },
                complete: function complete() {
                    _this._xhr = null;
                }
            };
            var userToken = model.getUserToken();
            if (userToken) {
                _.extend(saveOptions, {
                    headers: {
                        Authorization: 'Token ' + userToken
                    }
                });
            }
            return group.save({
                name: groupName,
                employees: employees,
                personal: true
            }, saveOptions);
        },

        destroySly: function destroySly() {
            if (this.slyScroller) {
                this.slyScroller.destroy();
                this.slyScroller = null;
            }
        },

        /**
         * Removes receiver from model.
         * If number of receivers is exactly 1 -> removes the hole group
         *
         * @param {Event} event
         */
        removeReceiver: function removeReceiver(event) {
            if (event) {
                event.preventDefault();
                event.stopPropagation();
            }

            var id = $(event.currentTarget).closest('.employee-item').data().employeeId;
            var _this = this;
            var isGroupDefined = this.model.isGroupDefined();
            var groupMembersCount = this.model.getGroup().getEmployees().length;

            if (!isGroupDefined) {
                this.model.removeReceiver({ id: id });
                this.model.setPointedEmployeeId('groupAssignEntryId');
                this.changeGroupEditMode();
                this.render();
            } else if (groupMembersCount > 1) {
                this.model.removeReceiver({ id: id });
            } else {
                //group is defined and groupMembersCount == 1
                this.confirmRemoveGroup({
                    yesCallback: function yesCallback() {
                        $.when(_this.animateRemoveGroup()).done(function () {
                            _this.removeGroup();
                            _this.model.removeReceiver({ id: id });
                        });
                    }
                });
            }

            App.trigger("receiversEdition");
        },

        /**
         * @param {Event} [event]
         */
        onRemoveGroupBtn: function onRemoveGroupBtn(event) {
            event && event.preventDefault();
            this.confirmRemoveGroup();
        },

        /**
         * Shows confirmation dialog on group removing
         *
         * @param {Object} [options]
         */
        confirmRemoveGroup: function confirmRemoveGroup(options) {
            options = options || {};
            var _this = this;

            var defaultYesCallback = function defaultYesCallback() {
                $.when(_this.animateRemoveGroup()).done(function () {
                    _this.removeGroup();
                });
            };

            if (this._xhr) {
                // Prevent double click on button
                return;
            }

            var dialogOptions = messages.PeopleToRewardView.confirmRemoveGroup(this.model.getGroup().getName());
            _.extend(dialogOptions, {
                yesCallback: options.yesCallback || defaultYesCallback,
                noCallback: options.noCallback
            });

            this.confirm(dialogOptions);
        },

        /**
         * Clears all receivers and set flag that data is stored
         *
         * @param {Event} e
         */
        onClearReceivers: function onClearReceivers(e) {
            e.preventDefault();
            e.stopPropagation();
            var yesCallback = _.bind(function () {
                this.model.set({ editGroupMode: this.EDIT_GROUP_MODES.NO_GROUP });
                this.model.clearReceivers();

                App.trigger("receiversEdition");
            }, this);

            var dialogOptions = messages.PeopleToRewardView.confirmClearReceivers;

            this.confirm(_.extend(dialogOptions, {
                yesCallback: yesCallback
            }));
        },

        /**
         * Is callback on action to group name
         * ESC -> exit editing mode
         * ENTER -> save group
         * DEFAULT -> sets new group name to model
         *
         * @param {Event} e
         */
        onChangeGroupName: function onChangeGroupName(e) {
            e.preventDefault();

            var model = this.model;
            var $input = $(e.currentTarget);

            //instead of switch cause switch is slower than if-else
            if (e.which === ESC_KEY_CODE) {

                this.onExitGroupNameEditing(e);
            } else if (e.which === ENTER_KEY_CODE) {

                var saveGroupTarget = this.model.isGroupDefined() ? SAVE_GROUP_TARGET.UPDATE_NAME : SAVE_GROUP_TARGET.CREATE_NEW_GROUP;

                this.saveGroupAndShowResultAnimation(saveGroupTarget);
                //default
            } else {
                model.set({ potentialGroupName: $input.val().trim() });
                this.toggleSaveGroupNameBtn();
            }
        },

        /**
         * Enables/disables save-group-name-btn
         */
        toggleSaveGroupNameBtn: function toggleSaveGroupNameBtn() {
            if (this.canSaveGroup()) {
                this.$('.save-group-name-btn').removeProp('disabled');
            } else {
                this.$('.save-group-name-btn').prop('disabled', true);
            }
        },

        /**
         * On click 'check' button to save group with that name
         * Saves group if it's possible
         *
         * @param {Event} [e]
         */
        onSaveGroupName: function onSaveGroupName(e) {
            e && e.preventDefault();

            var saveGroupTarget = this.model.isGroupDefined() ? SAVE_GROUP_TARGET.UPDATE_NAME : SAVE_GROUP_TARGET.CREATE_NEW_GROUP;

            this.saveGroupAndShowResultAnimation(saveGroupTarget);
        },

        /**
         * Saves group if it's possible
         * Ends editGroupNameProcess
         * Defines after save actions
         *
         * @param {String} saveGroupTarget
         * @param [datum]
         */
        saveGroupAndShowResultAnimation: function saveGroupAndShowResultAnimation(saveGroupTarget, datum) {
            var _this = this;
            var model = this.model;
            var group = model.getGroup();
            var currentGroupName = group.getName();

            var enableCreateNewGroup = this.canSaveGroup();
            // Group with 1 employee can be updated, but not created!!!
            var enableUpdateGroup = group.id && this.canSaveGroup(1);
            var groupMemberSelector = this._getGroupMembersSelector();
            var afterSaveCallback;

            //Init save callbacks depending on target
            var onErrorCallback = function onErrorCallback() {
                _this.model.restore();
                _this.model.set({ potentialGroupName: currentGroupName });
                _this.changeGroupEditMode();
                _this.render();
            };

            if (saveGroupTarget === SAVE_GROUP_TARGET.ADD_MEMBER) {
                var addEmployee = datum;

                afterSaveCallback = function afterSaveCallback() {
                    // To update scroll bar (Sly plugin)
                    _this.render();
                    _this.$(groupMemberSelector + ' svg').remove();
                    _this.animateAddEmployee(addEmployee);
                };
            } else if (saveGroupTarget === SAVE_GROUP_TARGET.REMOVE_MEMBER) {
                afterSaveCallback = function afterSaveCallback() {
                    var removedEmployee = datum;

                    $.when(_this.animateRemoveEmployee(removedEmployee)).done(function () {
                        // To update scroll bar (Sly plugin)
                        _this.render();
                    });
                };
            } else if (saveGroupTarget === SAVE_GROUP_TARGET.UPDATE_NAME) {
                afterSaveCallback = function afterSaveCallback() {
                    _this.animateSaveGroup();
                };
            } else if (saveGroupTarget === SAVE_GROUP_TARGET.CREATE_NEW_GROUP) {
                afterSaveCallback = function afterSaveCallback() {
                    _this.animateSaveGroup();
                };
            }

            // Save group
            if (group.isPrivate() && (enableCreateNewGroup || enableUpdateGroup)) {

                this.model.store({ silent: true }); //enable to revert changes on error

                this._saveGroup(group, {
                    afterSaveCallback: afterSaveCallback,
                    onErrorCallback: onErrorCallback
                });
            }
        },

        /**
         * Returns selector for group members
         *
         * @param {String} [additionalSelectData]
         * @returns {String}
         * @private
         */
        _getGroupMembersSelector: function _getGroupMembersSelector(additionalSelectData) {
            return '.' + GROUP_CONTAINER_CLASS + ' .employee-item' + (additionalSelectData || '') + ' .img';
        },

        /**
         * On click 'trash' button to remove current group
         *
         * @param {Event} [e]
         */
        removeGroup: function removeGroup(e) {
            e && e.preventDefault();

            if (this._xhr) {
                // Prevent double click on button
                return;
            }

            var _this = this;
            var model = this.model;
            var group = model.getGroup();
            var destroyOptions = {
                beforeSend: function beforeSend(jqXHR) {
                    _this._xhr = jqXHR;
                },
                success: function success() {
                    group.trigger('deleted');
                    var receivers = _this.model.getReceivers();
                    var newGroup = _this.model.resetGroup();
                    newGroup.set('employees', receivers);

                    _this.changeGroupEditMode();
                },
                error: function error(model, response) {
                    _this.displayErrors(response, true);
                },
                complete: function complete() {
                    _this._xhr = null;
                }
            };
            var userToken = model.getUserToken();
            if (userToken) {
                _.extend(destroyOptions, {
                    headers: {
                        Authorization: 'Token ' + userToken
                    }
                });
            }

            group.destroy(destroyOptions);
        },

        /**
         * Checks if group is valid for save
         * allows to change restriction for minPeopleInGroup
         *
         * @param {Number} [minPeopleInGroup]
         * @returns {*|boolean}
         */
        canSaveGroup: function canSaveGroup(minPeopleInGroup) {
            var group = this.model.getGroup();
            var groupName = this.model.getPotentialGroupName();
            var isNameValid = group.isGroupNameValid(groupName);
            var isEmployeeNumberValid = minPeopleInGroup ? group.getEmployees().length >= minPeopleInGroup : group.hasValidEmployeesNumber();

            return group && isNameValid && isEmployeeNumberValid;
        },

        /**
         * Recalculates width for scroll container of receivers
         */
        switchView: function switchView() {
            if (!this.$el.closest('body').length) {
                return;
            }

            var $resultsContainer = this.$('#resultz-container');
            var $results = $resultsContainer.find('.search-results');
            var $resultsChildren = $results.children();
            var bodyWidth = $('body').outerWidth();

            var isMobile = bodyWidth <= resolutions.mobile;
            var isMdMobile = bodyWidth <= resolutions.mdMobile;
            var isXsMobile = bodyWidth <= resolutions.xsMobile;

            var numberOfVisibleEmployeeItems = isXsMobile ? PEOPLE_IN_ROW_XSMOBILE : isMdMobile ? PEOPLE_IN_ROW_MDMOBILE : isMobile ? PEOPLE_IN_ROW_MOBILE : PEOPLE_IN_ROW_DESKTOP;

            if ($resultsChildren.length) {
                $resultsContainer.show();
            } else {
                $resultsContainer.hide();
            }

            $resultsContainer.width('auto');
            this.reCalcElWidth(numberOfVisibleEmployeeItems);

            var fullNumberOfEmployeeItems = $resultsChildren.length;
            this.sliderManage(fullNumberOfEmployeeItems > numberOfVisibleEmployeeItems);
        },

        /**
         * As group control influence on dimensions of receivers container
         * it's necessary to take it into account
         *
         * @param {Object} options
         * @param {Number} options.numberOfEmployeeItems
         * @param {Number} options.containerWidth
         * @returns {{ itemWidth: Number, employeeItemsContainerWidth: Number}}
         * @private
         */
        _getLayoutDimensionParamsInAccountWithGroupControls: function _getLayoutDimensionParamsInAccountWithGroupControls(options) {
            var bodyWidth = $('body').outerWidth();
            var isMobile = bodyWidth <= resolutions.mobile;
            var numberOfEmployeeItems = options.numberOfEmployeeItems;
            var availableWidth = options.containerWidth;
            var numberOfAllScrollItems = numberOfEmployeeItems + 1; // one is for .group-controls

            var spaceAmongGroupIconAndEmployeeContainer = this.$('.group-actions-wrapper').width();
            var availableWidthForEmployeeItems = isMobile ? availableWidth : availableWidth - spaceAmongGroupIconAndEmployeeContainer;

            var itemWidth = Math.ceil(availableWidthForEmployeeItems / numberOfAllScrollItems);

            return {
                itemWidth: itemWidth,
                employeeItemsContainerWidth: itemWidth * numberOfEmployeeItems
            };
        },

        /**
         * Sets width of every element of scrolling explicitly
         * according to count of elements that is allowed to show
         *
         * @param {Number} numberOfVisibleEmployeeItems
         */
        reCalcElWidth: function reCalcElWidth(numberOfVisibleEmployeeItems) {
            var $resultsContainer = this.$('#resultz-container');
            var containerBorderWidth = $resultsContainer.css('border-width');
            containerBorderWidth = containerBorderWidth && containerBorderWidth.replace('px', '') || 0;

            var params = this._getLayoutDimensionParamsInAccountWithGroupControls({
                numberOfEmployeeItems: numberOfVisibleEmployeeItems,
                containerWidth: this.$el.width() - containerBorderWidth * 2
            });

            $resultsContainer.css({ width: params.employeeItemsContainerWidth });

            // Set width to all .scroll-frame elements (employees-items + group-item)
            var $elems = this.$('.scroll-frame');
            $elems.each(function () {
                $(this).css({ width: params.itemWidth });
            });
        },

        /**
         *
         * @param reloadSlider
         */
        sliderManage: function sliderManage(reloadSlider) {
            var slyScroller = this.slyScroller;
            var $sliderControls = this.$('.slider-controls');

            if (reloadSlider) {
                if (slyScroller) {
                    slyScroller.reload();
                } else {
                    this.toggleSly();
                    $sliderControls.show();
                }
            } else {
                if (slyScroller) {
                    this.toggleSly();
                }
                $sliderControls.hide();
            }
        },

        /**
         * Slides sly to employee in question
         * @param employee {AssignEntryModel}
         */
        slideToEmployee: function slideToEmployee(employee) {
            var receivers = this.model.get('receivers');
            var index = _.indexOf(receivers.models, employee);
            this.$('#resultz-container').sly('toCenter', index);
        },

        /**
         * Slides sly to employee in question
         * @param employee {AssignEntryModel}
         */
        slideToFirstEmployeeWithNoValidFields: function slideToFirstEmployeeWithNoValidFields(employeeId) {
            var receivers = this.model.get('receivers');
            var index = _.findIndex(receivers.models, { id: employeeId });
            if (this.slyScroller) {
                this.slyScroller.toCenter(index);
            }
        },

        toggleSly: function toggleSly() {
            var $frame = this.$('#resultz-container');
            var $sliderControls = this.$('.slider-controls');
            var $prevPage = $sliderControls.find('#prev-page');
            var $nextPage = $sliderControls.find('#next-page');
            var $pagesBar = $sliderControls.find('#pages-bar');

            if (this.slyScroller) {
                this.slyScroller.destroy();
                this.slyScroller = null;
            } else {
                this.slyScroller = new Sly($frame, {
                    horizontal: true,
                    itemNav: 'basic',
                    smart: 1,

                    activateOn: 'click',
                    mouseDragging: 1,
                    touchDragging: 1,
                    releaseSwing: 1,
                    scrollBy: 1,
                    activatePageOn: 'click',
                    speed: 350,
                    elasticBounds: 1,
                    easing: 'swing',
                    dragHandle: 1,
                    dynamicHandle: 1,

                    // Buttons
                    prevPage: $prevPage,
                    nextPage: $nextPage,
                    pagesBar: $pagesBar,
                    pageBuilder: // Page item generator.
                    function pageBuilder(index) {
                        var el = '<span class="page">' + (index + 1) + '</span>';

                        if (index === this.pages.length - 1) {
                            el += '<span class="page total-pages">/' + this.pages.length + '</span>';
                        }
                        return el;
                    }
                }).init();

                this.slyScroller.on('moveEnd', this._onScrollEmployeeContainer);
            }
        },

        /**
         * Callback on changing highlightedEmployees collection:
         *  rounds employees with red circle
         *
         * @param {BadgeAssignPopupModel} model
         * @param {core.BaseCollection} highlightedEmployees
         */
        onChangeHighlightedEmployees: function onChangeHighlightedEmployees(model, highlightedEmployees) {
            this.highlightErrorEmployees(highlightedEmployees);
        },

        /**
         * Reinits listener
         *
         * @param {BadgeAssignPopupModel} model
         * @param {string} pointedEmployeeId
         */
        onChangePointedReceiverId: function onChangePointedReceiverId(model, pointedEmployeeId) {
            var pointedReceiver = this.model.getReceivers().get(pointedEmployeeId);
            if (pointedReceiver) {
                this.listenTo(pointedReceiver, 'change:individualContribution', this.onChangePointedReceiverIndividualContribution);
            }
        },

        /**
         * @param {core.BaseCollection} employees - names of inputs to highlight
         * @param {Number} [animationDuration]
         */
        highlightErrorEmployees: function highlightErrorEmployees(employees, animationDuration) {
            if (!employees || !employees.length) {
                return;
            }

            var $emp;
            var _this = this;

            employees.forEach(function (emp) {
                $emp = _this.get$EmployeeElement(emp.id);
                $emp.addClass(_this.ERROR_EMPLOYEE_ITEM_CLASS);
            });

            this.animationService.animateLoadEmployees('.' + this.ERROR_EMPLOYEE_ITEM_CLASS, animationDuration);

            var firstEmp = employees.first();
            if (firstEmp) {
                this.slideToEmployee(firstEmp);
            }
        },

        /**
         * Callback on scrolling end
         * Triggers event to notify about scrolling end
         *
         * @private
         */
        _onScrollEmployeeContainer: function _onScrollEmployeeContainer() {
            this.trigger('reposition:employee-items');
        },

        /**
         * @returns {jQuery.Deferred.promise}
         */
        animateRemoveGroup: function animateRemoveGroup() {},

        /**
         * @returns {jQuery.Deferred.promise}
         */
        animateSaveGroup: function animateSaveGroup() {
            return this.animationService.animateGroupControl();
        },

        /**
         * @param {Object} removedEmployee
         * @returns {jQuery.Deferred.promise}
         */
        animateRemoveEmployee: function animateRemoveEmployee(removedEmployee) {
            return this.animationService.animateGroupControl();
        },

        /**
         * Paints a circle around the photo of a given employee
         * @param {string} employeeId
         */
        highlightEmployee: function highlightEmployee(employeeId) {
            this.animationService.animateLoadSingleEmployee(employeeId);

            var highlightedEmployees = this.model.get('employeesToHighlight');
            this.highlightErrorEmployees(highlightedEmployees, 0);
        },

        /**
         * @param {string} employeeId
         * @returns {jQuery.Deferred.promise}
         */
        animateAddEmployee: function animateAddEmployee(employeeId) {
            return this.animationService.animateGroupControl();
        },

        googleAnalyticsTracker: function googleAnalyticsTracker() {
            GA.event('Give a badge library', "Click on user's photo to give individual comment");
        }

    });

    return PeopleToRewardView;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js")))

