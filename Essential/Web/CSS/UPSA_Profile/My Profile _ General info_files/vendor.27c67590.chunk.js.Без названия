    /**
     * Id of the overlay element.
     */
    var OVERLAY_ID = 'block-ui-overlay';

    var Semaphore = {
        count: 0,

        acquire: function acquire() {
            this.count++;
            if (this.count == 1) {
                var $overlay = this._getOverlay();
                if (!$overlay.length) {
                    var overlayHtml = com.epam.heroes.template.core.Overlay({ id: OVERLAY_ID });
                    $('body').append(overlayHtml);
                    this._disableUserInput();
                }
            }
        },

        release: function release() {
            this.count--;
            if (this.count == 0) {
                var $overlay = this._getOverlay();
                if ($overlay.length) {
                    $overlay.remove();
                    this._enableUserInput();
                }
            }
        },

        // Private fields and methods
        _activeElement: null,

        _namespace: 'BlockUIOverlay',

        _enableUserInput: function _enableUserInput() {
            $(document).off('keydown.' + this._namespace, this._preventKeydown);
        },

        _disableUserInput: function _disableUserInput() {
            $(document).on('keydown.' + this._namespace, this._preventKeydown);
        },

        _preventKeydown: function _preventKeydown(e) {
            return false;
        },

        _getOverlay: function _getOverlay() {
            return $('#' + OVERLAY_ID);
        }
    };

    return Semaphore;
}).apply(exports, __WEBPACK_AMD_DEFINE_ARRAY__),
