/***/ "./src/main/resources/static-site/js/com/epam/heroes/core/util/HeroesCookies.js":
/*!**************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/core/util/HeroesCookies.js ***!
  \**************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function () {

    var HeroesCookies = {
        get: function get(key) {
            return decodeURIComponent(document.cookie.replace(new RegExp('(?:(?:^|.*;)\\s*' + encodeURIComponent(key).replace(/[\-\.\+\*]/g, '\\$&') + '\\s*\\=\\s*([^;]*).*$)|^.*$'), '$1')) || null;
        },

        set: function set(key, value, options) {
            if (!key || /^(?:expires|max\-age|path|domain|secure)$/i.test(key)) {
                return false;
            }
            var end = options.end,
                path = options.path,
                domain = options.domain,
                secure = options.secure,
                expires = '';
            if (end) {
                switch (end.constructor) {
                    case Number:
                        expires = end === Infinity ? '; expires=Fri, 31 Dec 9999 23:59:59 GMT' : '; max-age=' + end;
                        break;
                    case String:
                        expires = '; expires=' + end;
                        break;
                    case Date:
                        expires = '; expires=' + end.toUTCString();
                        break;
                }
            }
            document.cookie = encodeURIComponent(key) + '=' + value + expires + (domain ? '; domain=' + domain : '') + (path ? '; path=' + path : '') + (secure ? '; secure' : '');
            return true;
        },

        remove: function remove(key, options) {
            var path = options.path,
                domain = options.domain;
            if (!key || !this.isPresent(key)) {
                return false;
            }
            document.cookie = encodeURIComponent(key) + '=; expires=Thu, 01 Jan 1970 00:00:00 GMT' + (domain ? '; domain=' + domain : '') + (path ? '; path=' + path : '');
            return true;
        },

        isPresent: function isPresent(sKey) {
            return new RegExp('(?:^|;\\s*)' + encodeURIComponent(sKey).replace(/[\-\.\+\*]/g, '\\$&') + '\\s*\\=').test(document.cookie);
        }
    };

    return HeroesCookies;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));

