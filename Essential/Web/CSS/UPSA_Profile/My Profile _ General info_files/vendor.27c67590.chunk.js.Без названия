/***/ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/AutoCompeteSearchView.js":
/*!*****************************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/AutoCompeteSearchView.js ***!
  \*****************************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
/* WEBPACK VAR INJECTION */(function($) {var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    // Core modules
    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var core = __webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js");

    // Plugins
    __webpack_require__(/*! jquery-ui/autocomplete */ "./src/main/resources/static-site/js/libs/vendor/jquery-ui/ui/autocomplete.js");

    var AutoCompleteSearchView = core.BaseView.extend({

        //Constants
        MAX_SEARCH_RESULTS: 6,

        SEARCH_DELAY: 300,

        resultContainerSelector: '.search-results',

        /**
         * View template
         * @abstract
         */
        template: function template() {},

        /**
         * Result item template
         * @abstract
         */
        searchItemTemplate: function searchItemTemplate() {},

        /**
         * For extending default set of options
         * @abstract
         */
        autoCompleteOptions: {},

        /**
         * @override
         * @param {Object} options are used for extending autocomplete options
         */
        initialize: function initialize(options) {

            _.bindAll(this, 'onSelectItem', '_onSuccessAutocompleteResponse', 'fetchSearchData', '_renderSearchItem', 'responseHandler');

            if (options) {
                _.extend(this.autoCompleteOptions, options);
            }
        },

        /**
         * @param {Object} item
         * @returns {Object} soy params for item template
         */
        getItemModel: function getItemModel(item) {
            return {
                item: item
            };
        },

        /**
         * @override
         */
        afterRender: function afterRender() {
            this._configureAutoComplete();
        },

        /**
         * Adds converted search item to the list of found items
         *
         * @param {HTMLElement} ul
         * @param {Object} item
         * @returns {jQuery}
         * @private
         */
        _renderSearchItem: function _renderSearchItem(ul, item) {
            $(ul).addClass('search-results-menu');
            var itemEl = this.searchItemTemplate(this.getItemModel(item));
            return $(itemEl).appendTo(ul);
        },

        /**
         * @abstract
         * @returns {jQuery}
         */
        get$SearchInput: function get$SearchInput() {
            return this.$('input');
        },

        /**
         * @abstract
         * @returns {core.BaseCollection}
         */
        getResultCollection: function getResultCollection() {
            return new core.BaseCollection();
        },

        /**
         * @abstract
         * @param {core.BaseModel} itemModel
         */
        setSelectedItemToModel: function setSelectedItemToModel(itemModel) {
            this.model.set({ selectedItem: itemModel });
        },

        /**
         * Sets view methods as automplete callbacks
         * and also configurates delay, selector of found items container
         *
         * @private
         */
        _configureAutoComplete: function _configureAutoComplete() {
            var $searchInput = this.get$SearchInput();
            if (!$searchInput.length) {
                return;
            }

            $searchInput.autocomplete(_.extend({
                delay: this.SEARCH_DELAY,
                source: this.fetchSearchData,
                appendTo: this.resultContainerSelector,
                select: this.onSelectItem,
                response: this.responseHandler
            }, this.autoCompleteOptions)).data('ui-autocomplete')._renderItem = this._renderSearchItem;
        },

        /**
         * Loads data from server via fetching collection of found items
         *
         * @param {Object} request
         * @param {Function} proxyResponse
         * @private
         */
        fetchSearchData: function fetchSearchData(request, proxyResponse) {
            var searchStr = request.term.trim();

            this.getResultCollection().fetch(this.getAjaxRequestOptions(searchStr, proxyResponse));
        },

        /**
         * @param {String} searchStr
         * @param {Function} proxyResponse
         * @returns {{data: Object, beforeSend: Function, blockUI: boolean, success: Function, complete: Function}}
         */
        getAjaxRequestOptions: function getAjaxRequestOptions(searchStr, proxyResponse) {
            var _this = this;
            return {
                data: {
                    search: searchStr,
                    length: this.MAX_SEARCH_RESULTS
                },
                beforeSend: function beforeSend(jqXHR) {
                    // Abort previous pending search request
                    _this._xhr && _this._xhr.abort();
                    _this._xhr = jqXHR;
                },
                blockUI: false,
                success: this._onSuccessAutocompleteResponse.bind(this, searchStr, proxyResponse),
                complete: function complete() {
                    _this._xhr = null;
                }
            };
        },

        /**
         * Parses achieved data to the convenient format
         * and passes it to the autocomplete
         *
         * @param {String} searchStr
         * @param {Function} proxyResponse
         * @param {core.BaseCollection} itemsCollection
         * @private
         */
        _onSuccessAutocompleteResponse: function _onSuccessAutocompleteResponse(searchStr, proxyResponse, itemsCollection) {
            proxyResponse(itemsCollection.map(function (itemModel) {
                return {
                    id: itemModel.id,
                    name: itemModel.get('name'),
                    description: itemModel.get('description')
                };
            }));
        },

        /**
         * Adds item to the model
         *
         * @param {Event} event
         * @param {Object} ui
         */
        onSelectItem: function onSelectItem(event, ui) {
            event.preventDefault();

            var selectedItemModel = this.getResultCollection().get(ui.item.id);
            this.setSelectedItemToModel(selectedItemModel);
        },

        /**
         * Will be called on ajax response
         * @abstract
         */
        responseHandler: function responseHandler() {}
    });

    return AutoCompleteSearchView;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js")))

/***/ }),

