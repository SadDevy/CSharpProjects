/***/ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/view/PointsSliderView.js":
/*!*****************************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/view/PointsSliderView.js ***!
  \*****************************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
/* WEBPACK VAR INJECTION */(function($) {var __WEBPACK_AMD_DEFINE_RESULT__;

var _typeof = typeof Symbol === "function" && typeof Symbol.iterator === "symbol" ? function (obj) { return typeof obj; } : function (obj) { return obj && typeof Symbol === "function" && obj.constructor === Symbol && obj !== Symbol.prototype ? "symbol" : typeof obj; };

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var App = __webpack_require__(/*! com/epam/heroes/portal/core/App */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/App.js");
    var config = __webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js");
    var BaseView = __webpack_require__(/*! com/epam/heroes/portal/core/view/BaseView */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/BaseView.js");
    __webpack_require__(/*! template/com/epam/heroes/widget/givebadge/PointsToAssign.soy */ "./src/main/soy/com/epam/heroes/widget/givebadge/PointsToAssign.soy");

    var templates = com.epam.heroes.widget.givebadge.template;

    var PATH_TO_DROPDOWN_ICON = config.staticSiteUrl + '/images/portal/icons/arrow-collapsed.svg';

    var ACTIVE_COLOR = '#37B9D7';
    var DEFAULT_COLOR = '#DCDDE4';

    var OBSOLETE_ACTIVE_COLOR = '#7E7D7D';
    var OBSOLETE_DEFAULT_COLOR = '#5B5A59';

    var PointsSliderView = BaseView.extend({

        template: templates.PointsSlider,

        events: {
            'click .dropdown': 'toggleSliders',
            'click .dot-wrapper ': 'onSliderChange'
        },

        initialize: function initialize() {
            _.bindAll(this, 'positionDots', 'paintActiveSliderBone');

            this.listenTo(App, 'point-chooser:changed', this.onPointChooserChange);

            if (this.model.get('programSettings')) {
                this.model.getSliders().expanded = true;
                this.calculatePoints();
            }
        },

        calculatePoints: function calculatePoints() {
            var steps = this.model.getPointsSteps();
            var maxStep = this.getMaxValue(steps);
            var sliders = this.model.getSliders();
            var slidersWeight = 0;
            var smallestPointsAmount = 0;

            _.forEach(sliders, function (slider) {
                slidersWeight += +slider.weight;
            });
            smallestPointsAmount = Math.round(maxStep / slidersWeight * 100) / 100;

            _.map(sliders, function (slider) {
                var dotsNumber = slider.dots.length - 1;
                slider.maxPointsAmount = slider.weight * smallestPointsAmount;
                slider.dotPoints = Math.round(slider.maxPointsAmount / dotsNumber * 100) / 100;
            });
        },

        getModel: function getModel() {
            var sliders = this.model.getSliders();

            return {
                pathToDropdownIcon: PATH_TO_DROPDOWN_ICON,
                sliders: sliders
            };
        },

        afterRender: function afterRender() {
            var _this2 = this;

            $('.slider-item').each(function (index, el) {
                _this2.positionDots(el);
                _this2.paintActiveSliderBone(el);
            });
        },

        positionDots: function positionDots(slider, lastActiveDot) {
            lastActiveDot = lastActiveDot || this.calculateLastActiveSliderDot(slider);
            var sliderData = this.model.getSliderByOrder($(slider).data('slider'));

            var dotsInterval = $(slider).find('.slider-bone').width() / (sliderData.dots.length - 1);

            $(slider).find('.dot-wrapper').each(function (index, dot) {
                var order = $(dot).data('dot');

                if (order <= lastActiveDot) {
                    $(dot).addClass('active');
                }

                if (order == sliderData.dots.length) {
                    $(dot).addClass('last-dot-wrapper');
                    $(dot).css({
                        'right': '-2px',
                        'width': dotsInterval / 2 + 2 + 'px'
                    });
                } else {
                    $(dot).css({
                        'left': (order - 1) * dotsInterval - dotsInterval / 2,
                        'width': dotsInterval + 'px'
                    });
                }

                if (order == '1') {
                    $(dot).addClass('first-dot-wrapper');
                    $(dot).css({
                        'left': '-2px',
                        'width': dotsInterval / 2 + 2 + 'px'
                    });
                }
            });

            if (this.model.getSliders().obsoleteMode) {
                $('.sliders-wrapper').addClass('obsolete');
            }
        },

        paintActiveSliderBone: function paintActiveSliderBone($slider) {
            var lastActiveDot = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : this.calculateLastActiveSliderDot($slider);

            var gradientProperty = void 0,
                colorsBoundary = void 0;
            var $sliderBone = $($slider).closest('.slider-item').find('.slider-bone');

            var dotsInterval = $sliderBone.width() / (this.model.getSliderByOrder($($slider).data('slider')).dots.length - 1);

            var $dotWrapper = $($slider).find('.dot-wrapper[data-dot =' + lastActiveDot + ' ]');

            lastActiveDot === 1 ? colorsBoundary = '0' : colorsBoundary = $dotWrapper.position().left + dotsInterval / 2;

            var coveredPercentage = Math.round(colorsBoundary / $sliderBone.width() * 1000) / 10;

            if (this.model.getSliders().obsoleteMode) {
                gradientProperty = 'linear-gradient(90deg, ' + OBSOLETE_ACTIVE_COLOR + ' ' + coveredPercentage + '%,' + OBSOLETE_DEFAULT_COLOR + ' ' + coveredPercentage + '%)';
            } else {
                gradientProperty = 'linear-gradient(90deg, ' + ACTIVE_COLOR + ' ' + coveredPercentage + '%,' + DEFAULT_COLOR + ' ' + coveredPercentage + '%)';
            }

            $sliderBone.css({
                'background-image': gradientProperty,
                'background-color': 'unset'
            });
        },

        calculateLastActiveSliderDot: function calculateLastActiveSliderDot(slider) {
            var lastActiveDot = void 0;

            if (this.isElement(slider)) {
                slider = this.model.getSliderByOrder($(slider).data('slider'));
            }

            slider.lastActiveDot ? lastActiveDot = slider.lastActiveDot : slider.actualPointsAmount ? lastActiveDot = Math.round(slider.actualPointsAmount / slider.dotPoints) + 1 : lastActiveDot = 1;

            return lastActiveDot;
        },

        //Returns true if it is a DOM element
        isElement: function isElement(o) {
            return (typeof HTMLElement === 'undefined' ? 'undefined' : _typeof(HTMLElement)) === "object" ? o instanceof HTMLElement : //DOM2
            o && (typeof o === 'undefined' ? 'undefined' : _typeof(o)) === "object" && o !== null && o.nodeType === 1 && typeof o.nodeName === "string";
        },

        toggleSliders: function toggleSliders() {
            var isExpanded = this.model.getSliders().expanded;
            this.model.getSliders().expanded = !isExpanded;

            $('.sliders-wrapper').slideToggle();

            $('.slider-container').toggleClass('expanded');
            this.model.getSliders().expanded ? $('.dropdown-expander').text("Hide sliders") : $('.dropdown-expander').text("Show sliders");

            if (this.model.getSliders().obsoleteMode) {
                this.switchToObsoleteMode();
            }
        },

        onSliderChange: function onSliderChange(e) {
            $(e.currentTarget).hasClass('active') ? this.deactivateDotsAfter($(e.currentTarget)) : this.activateDotsTill($(e.currentTarget));

            this.calculateSliderPointsAmount($(e.currentTarget));
        },

        deactivateDotsAfter: function deactivateDotsAfter($el) {
            var dotsOrderNumber = $el.data('dot');
            var slider = this.model.getSliderByOrder($el.closest('.slider-item').data('slider'));
            slider.lastActiveDot = dotsOrderNumber;

            var dotsToActivate = $el.closest('.slider-item').find('.dot-wrapper').filter(function (index, el) {
                return $(el).data('dot') > dotsOrderNumber;
            });
            dotsToActivate.removeClass('active');

            this.model.getSliders().obsoleteMode ? this.exitSlidersObsoleteMode() : this.paintActiveSliderBone($el.closest('.slider-item'), dotsOrderNumber);
        },

        activateDotsTill: function activateDotsTill($el) {
            var dotsOrderNumber = $el.data('dot');
            var slider = this.model.getSliderByOrder($el.closest('.slider-item').data('slider'));
            slider.lastActiveDot = dotsOrderNumber;

            var dotsToActivate = $el.closest('.slider-item').find('.dot-wrapper').filter(function (index, el) {
                return $(el).data('dot') <= dotsOrderNumber;
            });
            dotsToActivate.addClass('active');

            this.model.getSliders().obsoleteMode ? this.exitSlidersObsoleteMode() : this.paintActiveSliderBone($el.closest('.slider-item'), dotsOrderNumber);
        },

        exitSlidersObsoleteMode: function exitSlidersObsoleteMode() {
            var _this = this;
            this.model.getSliders().obsoleteMode = false;

            $('.sliders-wrapper').removeClass('obsolete');

            $('.slider-item').each(function (index, slider) {
                _this.paintActiveSliderBone(slider);
            });
        },

        onPointChooserChange: function onPointChooserChange() {
            if (this.model.getSliders().expanded) {
                this.toggleSliders();
            }
            this.switchToObsoleteMode();
        },

        switchToObsoleteMode: function switchToObsoleteMode() {
            var _this3 = this;

            this.model.getSliders().obsoleteMode = true;

            this.model.set({
                isBudgetExceeded: false
            });

            if (this.model.getSliders().expanded) {
                $('.sliders-wrapper').addClass('obsolete');

                $('.slider-item').each(function (index, slider) {
                    _this3.paintActiveSliderBone(slider);
                });
            }
        },

        calculateSliderPointsAmount: function calculateSliderPointsAmount($el) {
            var dotOrderNumber = $el.data('dot');
            var sliderOrderNumber = +$el.closest('.slider-item').data('slider');

            var slider = _.findWhere(this.model.getSliders(), { order: sliderOrderNumber });
            slider.actualPointsAmount = Math.round(slider.dotPoints * (dotOrderNumber - 1) * 100) / 100;

            App.trigger('change:slider');
        },

        getMaxValue: function getMaxValue(arr) {
            return Math.max.apply(this, arr);
        }
    });

    return PointsSliderView;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js")))

