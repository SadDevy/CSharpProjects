!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var App = __webpack_require__(/*! com/epam/heroes/portal/core/App */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/App.js");
    var BaseView = __webpack_require__(/*! com/epam/heroes/portal/core/view/BaseView */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/BaseView.js");
    __webpack_require__(/*! template/com/epam/heroes/widget/givebadge/PointsToAssign.soy */ "./src/main/soy/com/epam/heroes/widget/givebadge/PointsToAssign.soy");

    var templates = com.epam.heroes.widget.givebadge.template;

    var PointsChooserView = BaseView.extend({

        template: templates.PointsChooser,

        afterRender: function afterRender() {
            this.initSelect();
        },

        initSelect: function initSelect() {
            var _this = this;

            var steps = void 0,
                data = [],
                $selectPoints = this.$('select.select-steps');

            var chosenPointStep = this.model.get('programSettings').chosenPointStep;

            steps = this.model.getPointsSteps() || '';

            if (steps) {
                _.forEach(steps, function (step, index) {
                    data.push({
                        id: index,
                        text: step,
                        disabled: step > _this.model.get('programSettings').remainingPointCount
                    });
                });
            }

            $selectPoints.select2({
                allowClear: false,
                multiple: false,
                data: data,
                containerCssClass: '.points-select-wrap',
                placeholder: 'Select points number',
                minimumResultsForSearch: Infinity,
                dropdownParent: this.$('.points-select-dropdown'),
                width: '100%'
            });

            $selectPoints.on('select2:select', function (e) {
                var data = e.params.data;
                _this.model.get('programSettings').chosenPointStep = data.text;

                App.trigger('point-chooser:changed');
            });

            this.parent.changePointStep(this.parent.getIndexOfStep(chosenPointStep || 0));
        }
    });

    return PointsChooserView;
