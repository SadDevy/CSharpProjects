    var ProjectSearchView = AutoCompleteSearchView.extend({

        resultContainerSelector: '#project-search-result',

        /**
         * View template
         * @override
         */
        template: com.epam.heroes.widget.givebadge.template.ProjectSearch,

        /**
         * Result item template
         * @override
         */
        searchItemTemplate: com.epam.heroes.widget.givebadge.template.ProjectSearchItem,

        events: {
            'click .remove': 'resetSearch',
            'focus input[name="project"]': 'triggerAutocomplete'
        },

        /**
         * @override
         */
        initialize: function initialize() {
            var model = this.model;
            var receivers = model.getReceivers();

            var viewArguments = this.hasProjectDefaultValue() ? { minLength: 0 } : null;
            AutoCompleteSearchView.prototype.initialize.apply(this, viewArguments);

            this.listenTo(model, 'change:project', this.render);
            this.listenTo(receivers, 'add remove reset', this.toggleSearchForRequireProject);
            this.listenTo(receivers, 'add', this.handleAddingPeople);
        },

        /**
         * @override
         */
        getModel: function getModel() {
            var project = this.model.getProject();
            return {
                project: project && project.getName() || ''
            };
        },

        /**
         * @override
         */
        afterRender: function afterRender() {
            AutoCompleteSearchView.prototype.afterRender.apply(this, arguments);
            this.toggleSearchForRequireProject();
            this.toggleSearch();
        },

        /**
         * @override
         * @returns {core.BaseCollection}
         */
        getResultCollection: function getResultCollection() {
            return this.model.getFoundProjects();
        },

        /**
         * @param {core.BaseModel} projectModel
         * @returns {BadgeAssignPopupModel}
         */
        setSelectedItemToModel: function setSelectedItemToModel(projectModel) {
            return this.model.setProject(projectModel);
        },

        /**
         * @returns {jQuery}
         */
        get$SearchInput: function get$SearchInput() {
            return this.$('input[name="project"]');
        },

        /**
         * @override
         */
        getAjaxRequestOptions: function getAjaxRequestOptions() {
            var ajaxOptions = AutoCompleteSearchView.prototype.getAjaxRequestOptions.apply(this, arguments);
            var model = this.model;
            var badge = model.getBadge();
            var badgeAccountIds = _.map(badge.getAccounts(), 'id');
            if (badgeAccountIds.length === 0 && badge.get('account')) {
                badgeAccountIds = badge.get('account').id;
            }
            var receiversIds = model.getReceivers().map('id');
            _.extend(ajaxOptions.data, { length: 4, accountId: badgeAccountIds, badgeId: badge.get('id'), empl: receiversIds });
            return ajaxOptions;
        },

        /**
         * Adds item to the model
         *
         * @param {Event} event
         * @param {Object} ui
         */
        onSelectItem: function onSelectItem(event, ui) {
            event.preventDefault();
            var selectedItemModel = this.getResultCollection().get(ui.item.id);
            this.setSelectedItemToModel(selectedItemModel);
        },

        /**
         * Resets chosen project
         */
        resetSearch: function resetSearch(e) {
            if (e && e.preventDefault) {
                e.preventDefault();
            }

            this.get$SearchInput().val('');
            this.model.unset('project');
        },

        /**
         * Disables input field when project is chosen
         * and shows remove button
         */
        toggleSearch: function toggleSearch() {
            var $search = this.get$SearchInput();
            var val = this.model.get('project') ? true : false;
            $search.prop('disabled', val);
        },

        /**
         * Return true if project
         * is approvable
         * has required project field
         * and not for multiple assignment.
         * In other case - false
         */
        hasProjectDefaultValue: function hasProjectDefaultValue() {
            return this.model.isApprovable() && this.model.isProjectRequired() && !this.model.isMultipleAssignmentAvailable();
        },

        /**
         * Checks if project is required for badge
         * and receivers chosen - enable project field
         * else - disable project field
         */
        toggleSearchForRequireProject: function toggleSearchForRequireProject() {
            if (this.hasProjectDefaultValue()) {
                var isReceiversEmpty = !this.model.getReceivers().length;
                var $search = this.get$SearchInput();
                if (isReceiversEmpty) {
                    $search.val('');
                }
                $search.prop('disabled', isReceiversEmpty);
                $search.toggleClass('project-required', isReceiversEmpty);
            }
        },

        /**
         * Focus project search input
         * for person's projects
         * if project is required for badge
         */
        handleAddingPeople: function handleAddingPeople() {
            var $search = this.get$SearchInput();

            if (this.isGroupLinkedToProject()) {
                $('#project-icon').prop('checked', false);
                $search.focus();
            }

            if (this.hasProjectDefaultValue()) {
                $search.focus();
            }
        },

        /**
         * Trigger autocomplete ajax request
         * for person's projects
         * if project is required for badge
         */
        triggerAutocomplete: function triggerAutocomplete() {
            var _this = this;
            if (this.isGroupLinkedToProject()) {
                $.ajax({
                    url: this.model.get('group').get('source').url,
                    success: function success(resp) {
                        _this.initiateAutocompleteRequest(resp.content.name);
                    }
                });
            }
            if (this.hasProjectDefaultValue()) {
                this.initiateAutocompleteRequest(" ");
            }
        },

        /**
         * Initiating for ajax request for input autocomplete with incoming search param.
         *
         * @param search
         */
        initiateAutocompleteRequest: function initiateAutocompleteRequest(search) {
            var $search = this.get$SearchInput();
            $search.autocomplete("search", search);
        },

        /**
         * Checks whether selected group is linked to to project.
         *
         * @returns {boolean}
         */
        isGroupLinkedToProject: function isGroupLinkedToProject() {
            return this.model.get('group').get('sourceType') === 'PROJECT';
        },

        /**
         * Choose project automatically
         * if project is required for badge
         * and there is only one project for this person
         */
        responseHandler: function responseHandler() {
            if (this.hasProjectDefaultValue() && this.getResultCollection().length === 1 || this.isGroupLinkedToProject()) {
                var selectedItemModel = this.getResultCollection().first();
                this.setSelectedItemToModel(selectedItemModel);
            }
        }
    });

    return ProjectSearchView;
