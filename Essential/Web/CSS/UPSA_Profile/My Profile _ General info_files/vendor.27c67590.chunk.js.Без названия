 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var core = __webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js");
    var config = __webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js");
    var EmployeeCollection = __webpack_require__(/*! com/epam/heroes/core/model/EmployeeCollection */ "./src/main/resources/static-site/js/com/epam/heroes/core/model/EmployeeCollection.js");

    var Group = core.BaseModel.extend({

        MIN_NUMBER_EMPLOYEES_FOR_SAVE: 2,
        MIN_GROUP_NAME_LENGTH: 2,

        urlRoot: config.api.groups.url,

        defaults: function defaults() {
            return {
                employees: new EmployeeCollection()
            };
        },

        /**
         * @override
         * @param {Object} [options]
         */
        initialize: function initialize(options) {
            if (options && !(options.employees instanceof Backbone.Collection)) {
                this.set({ employees: new EmployeeCollection(options.employees) });
            }
        },

        /**
         * @override
         * @param {Object} response
         */
        parse: function parse(response) {
            response = core.BaseModel.prototype.parse.apply(this, arguments);
            response.employees = new EmployeeCollection(response.employees);
            return response;
        },

        /**
         * @returns {Array}
         */
        getEmployees: function getEmployees() {
            return this.get('employees');
        },

        /**
         * @returns {String}
         */
        getName: function getName() {
            return this.get('name');
        },

        /**
         * Private group is a personal group that is created by current
         * employee and is can be used/edited only by him
         *
         * @returns {Boolean}
         */
        isPrivate: function isPrivate() {
            return this.get('personal');
        },

        getOriginalTeam: function getOriginalTeam() {
            return this.get('originalTeam');
        },

        isFull: function isFull() {
            return this.getOriginalTeam().length === this.getEmployees().length;
        },

        isReduced: function isReduced() {
            return this.getOriginalTeam().length > this.getEmployees().length;
        },

        getSourceType: function getSourceType() {
            return this.get('sourceType');
        },

        getSource: function getSource() {
            return this.get('source');
        },

        getGroupDescription: function getGroupDescription() {
            if (this.get('personal')) {
                return 'Private group';
            }
            if (this.get('accessPermission') === 'ADMIN') {
                return 'Public group (admins only)';
            }
            return 'Public group (all company)';
        },

        /**
         * Removes employee from list of group employees
         * and triggers event
         *
         * @param {Backbone.Model|String} employee
         * @returns {Backbone.Model} removed employee's model
         */
        removeReceiver: function removeReceiver(employee) {
            var employeeId = employee;

            if (employee instanceof Backbone.Model) {
                employeeId = employee.get('id');
            }

            var removedEmployee = this.getEmployees().remove({ id: employeeId });
            this.trigger('remove:employee', removedEmployee);

            return removedEmployee;
        },

        /**
         * Sets new model to the beginning of employees array
         *
         * @param {core#BaseModel|Object} employee
         */
        unshiftEmployee: function unshiftEmployee(employee) {
            this.getEmployees().unshift(employee);
            this.trigger('add:employee', employee);
        },

        /**
         * Checks whether group can be saved
         *
         * @returns {Boolean}
         */
        hasValidEmployeesNumber: function hasValidEmployeesNumber() {
            return this.getEmployees().length >= this.MIN_NUMBER_EMPLOYEES_FOR_SAVE;
        },

        /**
         * Checks whether name is valid as group name
         *
         * @param {String} groupName
         * @returns {Boolean}
         */
        isGroupNameValid: function isGroupNameValid(groupName) {
            return groupName && groupName.length >= this.MIN_GROUP_NAME_LENGTH;
        }
    });

    return Group;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
