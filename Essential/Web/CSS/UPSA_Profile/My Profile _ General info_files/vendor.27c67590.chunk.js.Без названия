!(__WEBPACK_AMD_DEFINE_ARRAY__ = [__webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js"), __webpack_require__(/*! com/epam/heroes/portal/core/view/OverlayView */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/OverlayView.js"), __webpack_require__(/*! template/com/epam/heroes/portal/core/Confirmation.soy */ "./src/main/soy/com/epam/heroes/portal/core/Confirmation.soy")], __WEBPACK_AMD_DEFINE_RESULT__ = (function (core, OverlayView) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    // Confirmation view default options
    var defaultOptions = {
        cssClass: 'warning',
        buttons: {
            yes: {
                text: 'Yes',
                cssClass: 'yep'
            },
            no: {
                text: 'No',
                cssClass: 'nope'
            }
        }
    };

    var Confirmation = core.BaseView.extend({
        template: com.epam.heroes.template.portal.Confirmation,

        className: 'confirm-popup',

        events: {
            'click .close': 'close'
        },

        initialize: function initialize() {
            core.BaseView.prototype.initialize.apply(this, arguments);
            _.bindAll(this, 'attachButtonHandler');
            var model = this.model;

            for (var key in defaultOptions.buttons) {
                model.buttons[key] = Object.assign({}, defaultOptions.buttons[key], model.buttons[key]);
            }

            this.model = model instanceof core.BaseModel ? model : new core.BaseModel(model);
        },

        afterRender: function afterRender() {
            core.BaseView.prototype.afterRender.apply(this, arguments);
            var cssClass = this.model.get('cssClass') || defaultOptions.cssClass;
            this.$el.addClass(cssClass);
            _.each(this.model.get('buttons'), this.attachButtonHandler);
        },

        open: function open() {
            var $overlay = $('.' + OverlayView.prototype.className);
            if (!$overlay.length) {
                this.overlay = new OverlayView({ animate: false }).render();
            } else if ($(".assignment-details-popup")) {
                this.overlay = new OverlayView({ animate: false, zIndex: 7 }).render();
            }
            this.render();
            this.$el.appendTo('body');

            return this;
        },

        close: function close(e) {
            e.preventDefault();
            this.overlay && this.overlay.remove();
            this.remove();
        },

        remove: function remove() {
            core.BaseView.prototype.remove.apply(this, arguments);
            this.$el.remove();
        },

        attachButtonHandler: function attachButtonHandler(button) {
            this.$('.' + button.cssClass).on('click', _.bind(function (e) {
                e.preventDefault();
                if (button.callback) {
                    button.callback();
                }
                this.close(e);
            }, this));
        }
    });

    return Confirmation;
}).apply(exports, __WEBPACK_AMD_DEFINE_ARRAY__),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
