/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var core = __webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js");

    /**
     * @abstract
     */
    var EmployeeOptionModel = core.BaseModel.extend({

        /**
         * @override Backbone.Model: initialize
         * @param {Object} [options]
         * @returns {EmployeeOptionModel}
         */
        initialize: function initialize(options) {
            core.BaseModel.prototype.initialize.apply(this, arguments);

            //If model is created by user, employeeId should be passed to init-options,
            //if model is created by parent collection, employeeId should be set to collection field
            var employeeId = options && options.employeeId;
            employeeId = employeeId || this.collection && this.collection.employeeId;

            if (employeeId) {
                this.setEmployeeId(employeeId);
            }
            return this;
        },

        getEmployeeId: function getEmployeeId() {
            this.get('employeeId');
        },

        /**
         * Sets employee's id to model,
         * and binds url and url (id-dependent options) with this id
         *
         * @param {String} employeeId
         * @returns {EmployeeOptionModel}
         */
        setEmployeeId: function setEmployeeId(employeeId) {
            this.set('employeeId', employeeId);
            this.bindAttributes(['url', 'urlRoot'], employeeId);
            return this;
        },

        /**
         * Binds methods that are dependent on transmitted params with these params
         *
         * @param {Array} methodsNameArray - names of methods
         * @param {*} params
         */
        bindAttributes: function bindAttributes(methodsNameArray, params) {
            var _this = this;
            methodsNameArray = _.isArray(methodsNameArray) && methodsNameArray || [methodsNameArray];

            _.forEach(methodsNameArray, function (methodName) {
                var attr = _this[methodName];

                if (_.isFunction(attr)) {
                    _this[methodName] = attr.bind(_this, params);
                }
            });
        }
    });

    return EmployeeOptionModel;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));

