    var AssignFormView = core.BaseView.extend({

        animationDuration: 400,

        template: com.epam.heroes.widget.givebadge.template.AssignForm,

        className: 'badge-assignment badge-info-popup',

        POINTED_EMPLOYEE_CLASS: 'pointed',

        GROUP_COMMENT_PLACEHOLDER: 'Your colleague did something great? Tell about it!',

        APPROVABLE_COMMENT_PLACEHOLDER: 'Please specify:\n\u2022\xA0background information in order to provide some context\n\u2022\xA0detailed description of what exactly happened\n\u2022\xA0results/positive effect of the achievement',

