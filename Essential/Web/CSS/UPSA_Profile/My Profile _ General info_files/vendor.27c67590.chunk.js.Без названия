 */
!(__WEBPACK_AMD_DEFINE_ARRAY__ = [__webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js"), __webpack_require__(/*! com/epam/heroes/portal/core/App */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/App.js"), __webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js"), __webpack_require__(/*! com/epam/heroes/portal/core/view/Confirmation */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/Confirmation.js"), __webpack_require__(/*! com/epam/heroes/nls/httpErrors */ "./src/main/resources/static-site/js/com/epam/heroes/nls/httpErrors.js"), __webpack_require__(/*! com/epam/heroes/nls/constraints */ "./src/main/resources/static-site/js/com/epam/heroes/nls/constraints.js")], __WEBPACK_AMD_DEFINE_RESULT__ = (function (core, App, config, Confirmation, httpErrors, constraints) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var BaseView = core.BaseView.extend({

        confirm: function confirm(options) {
            var yesCallback = options.yesCallback,
                noCallback = options.noCallback,
                buttons = options.buttons || {};

            if (yesCallback && typeof yesCallback === 'function') {
                var yes = buttons.yes ? Object.assign(buttons.yes, { callback: yesCallback }) : { callback: yesCallback };
                buttons.yes = yes;
            }
            if (noCallback && typeof noCallback === 'function') {
                var no = buttons.no ? Object.assign(buttons.no, { callback: noCallback }) : { callback: noCallback };
                buttons.no = no;
            }
            App.confirm(_.extend(_.omit(options, 'yesCallback', 'noCallback'), { buttons: buttons }));
        },

        notify: function notify(options) {
            App.notify(options);
        },

        showErrors: function showErrors(params) {
            var errors = params.errors,
                errorsWithPath = params.errorsWithPath,
                notShownErrors = this.showFieldErrors(errorsWithPath).concat(errors);
            _.each(notShownErrors, function (error) {
                App.notify({
                    body: error.message,
                    cssClass: 'error'
                });
            });
        },

        /**
         * Triggers event to open badge library
         *
         * @param {Object} [options]
         * @param {Object|Backbone.Model} [options.badgeToAssign]
         * @param {Backbone.Collection} [options.receivers]
         */
        triggerOpenBadgeLibrary: function triggerOpenBadgeLibrary(options) {
            $.ajax({
                url: config.api.stats.openSections,
                contentType: 'application/json',
                type: 'POST',
                blockUI: false
            });
            App.trigger('open-badge-library', options);
        },

        triggerOpenMovie: function triggerOpenMovie(options) {
            App.trigger('open-movie', options);
        },

        triggerCloseMovie: function triggerCloseMovie(options) {
            App.trigger('close-movie', options);
        }
    });

    return BaseView;
}).apply(exports, __WEBPACK_AMD_DEFINE_ARRAY__),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
