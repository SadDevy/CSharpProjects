/***/ "./src/main/resources/static-site/js/com/epam/heroes/core/model/FeaturesCollection.js":
/*!********************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/core/model/FeaturesCollection.js ***!
  \********************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var core = __webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js");
    var config = __webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js");
    var FeatureModel = __webpack_require__(/*! com/epam/heroes/core/model/FeatureModel */ "./src/main/resources/static-site/js/com/epam/heroes/core/model/FeatureModel.js");
    var featuresConfig = __webpack_require__(/*! FeatureConfig */ "./src/main/resources/static-site/featuresConfig.js");
    var FeaturesCollection = core.BaseCollection.extend({
        model: FeatureModel,
        url: config.api.features.url,
        parse: function parse(data) {
            this.addGlobalMenuScript(data);
            this.addGoogleAnalyticScript();

            var features = _.filter(data.content.FEATURE, function (features) {
                return features.featureProperties !== true;
            });
            for (var property in data.content.PROPERTY) {
                features.push(data.content.PROPERTY[property]);
            }

            return features;
        },

        addGlobalMenuScript: function addGlobalMenuScript(data) {
            if (data.content.PROPERTY && data.content.PROPERTY.length) {
                var VERSION = new Date().getTime();
                var globalMenuProperty = _.findWhere(data.content.PROPERTY, { name: 'links_global_menu_script' });
                var globalMenuLink = globalMenuProperty.value.replace('#version', VERSION);
                var menuScript = document.createElement('script');

                menuScript.setAttribute('type', 'text/javascript');
                menuScript.setAttribute('src', globalMenuLink);
                menuScript.setAttribute('async', 'true');
                document.body.appendChild(menuScript);
            }
        },

        addGoogleAnalyticScript: function addGoogleAnalyticScript() {
            var analyticScript = document.createElement('script');
            analyticScript.setAttribute('type', 'text/javascript');
            analyticScript.setAttribute('async', 'true');
            if (featuresConfig.featurePetrofacUiStylesEnabled) {
                window.ga = function () {};
                var linkedScript = document.createElement('script');
                linkedScript.setAttribute('src', 'https://www.googletagmanager.com/gtag/js?id=UA-152602576-3');
                linkedScript.setAttribute('async', 'true');
                document.head.appendChild(linkedScript);
                analyticScript.text = "window.dataLayer = window.dataLayer || [];function gtag(){dataLayer.push(arguments);}gtag('js', new Date());gtag('config', 'UA-152602576-3');";
            } else {
                analyticScript.text = "(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');ga('create', 'UA-155540-80', 'auto');ga('send', 'pageview');";
            }
            document.head.appendChild(analyticScript);
        },

        getFeatureStateByName: function getFeatureStateByName(name) {
            var features = this.models;
            if (features.length > 0) {
                var feature = _.findWhere(features, { id: name });
                return feature ? feature.get('enabled') : false;
            }
        },

        getPropertyValueByName: function getPropertyValueByName(name) {
            var features = this.models;
            if (features.length > 0) {
                var feature = _.findWhere(features, { id: name });
                return feature ? feature.get('value') : null;
            }
        },

        getFeatureByName: function getFeatureByName(name) {
            var features = this.models;
            if (features.length > 0) {
                var feature = _.findWhere(features, { id: name });
                return feature;
            }
        }
    });

    return FeaturesCollection;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));

/***/ }),

/***/ "./src/main/resources/static-site/js/com/epam/heroes/core/model/Group.js":
