/***/ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/BadgeSections.js":
/*!***************************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/model/BadgeSections.js ***!
  \***************************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var core = __webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js");
    var config = __webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js");

    var Badge = __webpack_require__(/*! com/epam/heroes/core/model/Badge */ "./src/main/resources/static-site/js/com/epam/heroes/core/model/Badge.js");
    var DateFormats = __webpack_require__(/*! com/epam/heroes/core/util/DateFormats */ "./src/main/resources/static-site/js/com/epam/heroes/core/util/DateFormats.js");

    var BadgeSections = core.BaseCollection.extend({
        model: core.BaseModel,

        url: config.api.badges.sections.url,

        /**
         * @override
         */
        initialize: function initialize() {
            core.BaseCollection.prototype.initialize.apply(this, arguments);
            _.bindAll(this, 'mergeSection');
        },

        /**
         * @override
         * @returns {core#BaseCollection}
         */
        reset: function reset(models) {
            this._cachedBadges = {};
            var needParse = !(models[0] instanceof this.model);
            return core.BaseCollection.prototype.reset.call(this, models, { parse: needParse });
        },

        /**
         * @param {Object} response
         * @returns {Array}
         */
        parse: function parse(response) {
            var badgeSections = core.BaseCollection.prototype.parse.apply(this, arguments);
            this._parseBadgesWithRefs(badgeSections, response.refs)._cacheBadgeSections(badgeSections);

            return badgeSections;
        },

        /*** Here is caching magic ***/

        _cachedBadges: {},

        /**
         * Returns badges of the passed badge section
         *
         * @param badgeSection
         * @returns {core#BaseCollection}
         * @private
         */
        _getBadges: function _getBadges(badgeSection) {
            return badgeSection.badges || badgeSection.get('badges');
        },

        /**
         * Caches passed badges in private field
         *
         * @param {core#BaseCollection} badges
         * @private
         */
        _cacheBadges: function _cacheBadges(badges) {
            var _badges = this._cachedBadges;
            badges.each(function (badge) {
                badge.collection.get('pointsSteps') ? badge.pointsSteps = badge.collection.get('pointsSteps') : badge;
                _badges[badge.id] = badge;
            });
        },

        /**
         * Adds passed badges to the particular badge section and
         * caches them at the same time
         *
         * @param {core#BaseModel} existingBadgeSection
         * @param  {core#BaseCollection} badges
         * @private
         */
        _addAndCacheAbsentBadges: function _addAndCacheAbsentBadges(existingBadgeSection, badges) {
            var _badges = this._cachedBadges;
            var existingBadges = this._getBadges(existingBadgeSection);
            var badgeId;

            badges.each(function (badge) {
                badgeId = badge.id;
                if (!_badges[badgeId]) {
                    _badges[badgeId] = badge;
                    existingBadges.push(badge);
                }
            });
        },

        /**
         * Caches every badge of every badge section to the private store
         *
         * @param {Array} badgeSections
         * @private
         */
        _cacheBadgeSections: function _cacheBadgeSections(badgeSections) {
            var badges;
            _.each(badgeSections, function (badgeSection) {
                badges = this._getBadges(badgeSection);
                badgeSection.pointsSteps ? badges.pointsSteps = badgeSection.pointsSteps : badges;
                this._cacheBadges(badges);
            }, this);
        },

        /*** End of caching magic ***/

        /**
         * Transfers refs from all section to every ref
         *
         * @param {Array} badgeSections
         * @param {Object} [refs]
         * @returns {BadgeSections}
         * @private
         */
        _parseBadgesWithRefs: function _parseBadgesWithRefs(badgeSections, refs) {
            var _this = this;

            if (badgeSections.length) {
                _.forEach(badgeSections, function (badgeSection) {
                    badgeSection.badges = new core.BaseCollection(_.map(badgeSection.badges, function (badge) {
                        return _this.parseBadge(badge, refs);
                    }));
                });
            }

            return this;
        },

        /**
         * Converts renewal date to 'MMMM Do YYYY' format
         * and converts Array of groups names to one string
         *
         * @param  {Object} badge
         * @param  {Object} [refs]
         * @returns {Badge}
         */
        parseBadge: function parseBadge(badge, refs) {
            badge.refs = refs;
            var renewalDate = badge.renewalDate;
            var groupsNames = badge.groupsNames;

            badge.groupsNames = groupsNames.join(', ') + (groupsNames.length > 0 ? ' group' : '') + (groupsNames.length > 1 ? 's' : '');

            badge.renewalDate = renewalDate && DateFormats.moment.toMoment(renewalDate).format('MMMM Do YYYY') || null;

            return new Badge(badge);
        },

        restore: function restore() {
            core.BaseCollection.prototype.restore.apply(this, arguments);
            this._cacheBadgeSections(this.models);

            return this;
        },

        merge: function merge(badgeSections) {
            var newRefs = badgeSections.getRefs && badgeSections.getRefs();
            _.extend(this.getRefs(), newRefs);

            return badgeSections.length ? badgeSections.each(this.mergeSection) : this.mergeSection.call(this, badgeSections);
        },

        /**
         * Adds a new section to current list if it doesn't
         *
         * @param {Backbone.Model || Object} badgeSection
         * @param {Object} [addOptions]
         * @param {number} [addOptions.at] defines position to set to in badge section list
         */
        mergeSection: function mergeSection(badgeSection, addOptions) {
            var existingBadgeSection = this.get(badgeSection.id);
            var badges = this._getBadges(badgeSection);

            if (existingBadgeSection) {
                this._addAndCacheAbsentBadges(existingBadgeSection, badges);
            } else {
                this.add(badgeSection, addOptions);
                this._cacheBadges(badges);
            }
        },

        totalBadges: function totalBadges() {
            return _.size(this._cachedBadges);
        },

        getBadge: function getBadge(badgeId) {
            return this._cachedBadges[badgeId];
        },

        /**
         * @override
         */
        toDerivedJSON: function toDerivedJSON() {
            var badgeSections = core.BaseCollection.prototype.toDerivedJSON.apply(this, arguments);
            return _.map(badgeSections, function (badgeSection) {
                badgeSection.badges = badgeSection.badges.toDerivedJSON();
                return badgeSection;
            });
        }

    });

    return BadgeSections;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));

/***/ }),

