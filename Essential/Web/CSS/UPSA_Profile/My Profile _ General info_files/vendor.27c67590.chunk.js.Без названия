!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {
    // Libraries
    var Backbone = __webpack_require__(/*! backbone */ "./src/main/resources/static-site/js/libs/vendor/backbone/backbone.js");
    var $ = Backbone.$;
    __webpack_require__(/*! bootstrap/modal */ "./src/main/resources/static-site/js/libs/vendor/bootstrap-sass-official/assets/javascripts/bootstrap/modal.js");
    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");

    // Core modules
    var config = __webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js");
    var Utils = __webpack_require__(/*! com/epam/heroes/portal/core/util/Utils */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/util/Utils.js");

    var Mediator = __webpack_require__(/*! com/epam/heroes/core/Mediator */ "./src/main/resources/static-site/js/com/epam/heroes/core/Mediator.js");
    var Semaphore = __webpack_require__(/*! com/epam/heroes/core/util/Semaphore */ "./src/main/resources/static-site/js/com/epam/heroes/core/util/Semaphore.js");

    var httpErrors = __webpack_require__(/*! com/epam/heroes/nls/httpErrors */ "./src/main/resources/static-site/js/com/epam/heroes/nls/httpErrors.js");
    var constraints = __webpack_require__(/*! com/epam/heroes/nls/constraints */ "./src/main/resources/static-site/js/com/epam/heroes/nls/constraints.js");

    var featuresConfig = __webpack_require__(/*! FeatureConfig */ "./src/main/resources/static-site/featuresConfig.js");
    var messages = __webpack_require__(/*! com/epam/heroes/nls/messages */ "./src/main/resources/static-site/js/com/epam/heroes/nls/messages.js");

    /**
     * Number of pixels to the end of the page when more items should be loaded.
     */
    var PIXELS_TO_THE_PAGE_END = 700;

    /**
     * Checks whether error contains path to the DOM element containing incorrect value
     *
     * @param {Object} error Error
     * @param {String} error.path Path to the DOM element containing incorrect value
     */
    function isErrorWithPath(error) {
        return _.isString(error.path);
    }

    /**
     * Validates model attributes using the provided validation rule.
     * Returns error object if validation failed.
     *
     * @param {Object} attrs                       Model attributes
     * @param {Object} validationRule              Validation rule
     * @param {Function} validationRule.validate   Function that validates model attributes
     * @param {String} validationRule.message      Error message
     * @param {String} validationRule.path         Path to the DOM element containing incorrect value
     */
    function validateUsingRule(attrs, validationRule) {
        if (!validationRule.validate(attrs)) {
            return {
                message: validationRule.message,
                path: validationRule.path
            };
        }
    }

    var BaseModel = Backbone.Model.extend({

        /**
         * @abstract
         */
        defaults: function defaults() {},

        refs: {},

        set: function set(key) {
            if (key && key.refs) {
                this.refs = key.refs;
            }
            return Backbone.Model.prototype.set.apply(this, arguments);
        },

        /**
         * Sets attributes to the very last model of models chain, each of them is included in previous one
         * this.deepSet('innerModel1', 'innerModel2', objectToSet) =
         *      this.get('innerModel1').get('innerModel2').set(objectToSet)
         *
         *
         * @param {...String|Object} [innerModelName]
         * @returns {BaseModel}
         *
         * @throws 'TypeException: Parameter in chain is not an instance of Backbone.Model'
         */
        deepSet: function deepSet(innerModelName) {
            var attributes = arguments[arguments.length - 1];
            var modelNames = Array.prototype.slice.call(arguments, 0, arguments.length - 1);
            var currentModel = this;

            _.forEach(modelNames, function (modelName) {
                currentModel = currentModel.get(modelName);

                if (!(currentModel instanceof Backbone.Model)) {
                    throw new TypeError('TypeException: Parameter in chain is not an instance of Backbone.Model');
                }
            });

            currentModel.set(attributes);
            return this;
        },

        /**
         * Gets attribute from the very last model of models chain, each of them is included in previous one
         * this.deepGet('innerModel1', 'innerModel2', objectNameToGet) =
         *      this.get('innerModel1').get('innerModel2').get(objectNameToGet)
         *
         * @param {...String|Object} [innerModelName]
         * @returns {BaseModel}
         *
         * @throws 'TypeException: Parameter in chain is not an instance of Backbone.Model'
         */
        deepGet: function deepGet(innerModelName) {
            var attributeName = arguments[arguments.length - 1];
            var modelNames = Array.prototype.slice.call(arguments, 0, arguments.length - 1);
            var currentModel = this;

            _.forEach(modelNames, function (modelName) {
                currentModel = currentModel.get(modelName);

                if (!(currentModel instanceof Backbone.Model) && !(currentModel instanceof Backbone.Collection)) {

                    throw new TypeError('TypeException: Parameter in chain is not an instance of Backbone.Model/Backbone.Collection');
                }
            });

            return currentModel.get(attributeName);
        },

        fetch: function fetch(options) {
            if (options && options.data) {
                options.data = $.param(options.data, true);
            }

            return Backbone.Model.prototype.fetch.call(this, options);
        },

        parse: function parse(response) {
            if (response && response.refs) {
                this.refs = response.refs;
            }
            if (response && response.content && !response.id) {
                return response.content;
            }
            return response;
        },

        /**
         * <p>Validates model attributes using the validation rules defined in the model.
         * Returns errors if validation failed.
         *
         * <p>Validation rules are defined in the model in the following format:
         *      validationRules: [{
         *          // Single validation rule
         *          validate: function(attrs) {...},
         *          message: '...',
         *          path: '...' // Optional
         *      }, ...]
         *
         * @param {Object} attrs Model attributes
         */
        validate: function validate(attrs) {
            var validationErrors = _(this.validationRules).map(function (validationRule) {
                return validateUsingRule(attrs, validationRule);
            }).filter(_.isObject); // Filter out undefined values
            var errors = validationErrors.reject(isErrorWithPath).value();
            var errorsWithPath = validationErrors.filter(isErrorWithPath).value();
            if (errors.length || errorsWithPath.length) {
                var allErrors = {
                    errors: errors,
                    errorsWithPath: errorsWithPath
                };
                Application.showErrors(allErrors); // TODO Move logic that displays errors to the view
                return allErrors;
            } else {
                Application.clearFieldErrors();
            }
        },

        /**
         * Saves attributes with passed name (all by default)
         *
         * Overloading method:
         *  store(['field1', 'field2'], {silent: true}) - to store field1, field2 and not trigger event
         *  store({silent: true}) - to store all fields and not trigger event
         *  store() - to store all fields and trigger event
         *
         * @param {Array | {silent: Boolean} } [attributesNamesToStore] - array of names of fields to store
         * @param {{silent?: Boolean}} [options] - silent options defines if method should trigger event
         * @returns {BaseModel}
         */
        store: function store(attributesNamesToStore, options) {
            if (!_.isArray(attributesNamesToStore)) {
                options = attributesNamesToStore;
                attributesNamesToStore = Object.getOwnPropertyNames(this.attributes);
            }
            var silently = options && options.silent;
            var memento = this.memento = {},
                value;
            var attributes = this.attributes;

            _.forEach(attributesNamesToStore, function (attributeName) {
                value = attributes[attributeName];
                if (value != null) {
                    value = typeof value.clone === 'function' ? value.clone() : value;
                    memento[attributeName] = typeof value.store === 'function' ? value.store() : value;
                }
            });

            !silently && this.trigger('store');

            return this;
        },

        /**
         * @param {Object} [options] Options used when calling Backbone's "set" method on model
         * @param {Boolean} [options.silent] trigger otr not event
         *
         * @returns {core#BaseModel} - this
         */
        restore: function restore(options) {
            var storedAttributes = {},
                memento = this.memento,
                value;
            var silently = options && options.silent;

            for (var attribute in memento) {
                value = memento[attribute];
                if (value != null) {
                    storedAttributes[attribute] = typeof value.restore === 'function' ? value.restore(options) : value;
                }
            }

            this.set(storedAttributes, options);
            !silently && this.trigger('restore');

            return this;
        },

        destroy: function destroy(options) {
            // We must wait for the server to respond because model may not be
            // deleted thus shouldn't be removed from collection
            options = _.extend({
                wait: true
            }, options);
            return Backbone.Model.prototype.destroy.apply(this, arguments);
        },

        /**
         * Silently sets model attributes and calls fetch method.
         * @param data model attributes to set
         * @param params fetch params (success/error callbacks, etc.)
         * @returns {Object} XHR of sync-request (triggered by backbone)
         */
        load: function load(data, params) {
            this.set(data, {
                silent: true
            });
            return this.fetch(params || {});
        },

        toDerivedJSON: function toDerivedJSON() {
            var m = this.toJSON();
            m.refs = this.refs;
            return m;
        },

        getRefs: function getRefs() {
            return this.refs;
        },

        getRef: function getRef(url) {
            return this.refs[url];
        }
    });

    var BaseCollection = Backbone.Collection.extend({
        refs: {},

        fetch: function fetch(options) {
            var childOptions = options;
            options = _.extend({
                reset: true
            }, options);

            // Serialize query parameters if any specified
            var data = options.data;
            if (data) {
                options.data = $.param(data, true);
            }

            options.error = function (collection, response, options) {
                if (featuresConfig.featureEpamAutoLoginEnabled && response.status === 401 && window.location.pathname !== "/login") {
                    window.location.href = config.api.login.oauth.url;
                }
                if (typeof childOptions.error === "function") {
                    childOptions.error(collection, response, options);
                }
            };

            return Backbone.Collection.prototype.fetch.call(this, options);
        },

        parse: function parse(response) {
            if (response) {
                var refs = response.refs;
                if (refs) {
                    this.refs = refs;
                }
                var paging = response.paging;
                if (paging) {
                    this.totalCount = paging.totalCount;
                }

                var hasMoreResult = response.hasMoreResults;
                if (hasMoreResult) {
                    this.hasMoreResults = hasMoreResult;
                }

                return response.content ? response.content : response;
            }
            return;
        },

        /**
         * Overridden Backbone "sort" function that maintains backward compatibility with previous versions
         * which allowed passing a comparator as a method parameter.
         */
        sort: function sort(options) {
            var isSortFunction = _.isFunction(options);
            if (isSortFunction) {
                this.comparator = options; // Set comparator if it was passed as a function parameter
                options = {};
            }

            var sortedModels = Backbone.Collection.prototype.sort.call(this, options);

            if (isSortFunction) {
                this.comparator = null; // Unset comparator if it was passed as a function parameter
            }

            return sortedModels;
        },

        store: function store() {
            var models = this.models,
                memento = this.memento = [],
                model;
            for (var i in models) {
                model = models[i];
                if (model) {
                    memento[i] = typeof model.store === 'function' ? model.store() : model;
                }
            }
            return this.trigger('store');
        },

        /**
         * @param {Object} options Options used when calling Backbone's "reset" method on collection
         * @returns {Backbone.Collection} this
         */
        restore: function restore(options) {
            var storedModels = [],
                memento = this.memento,
                model;
            for (var i in memento) {
                model = memento[i];
                if (model) {
                    storedModels[i] = typeof model.restore === 'function' ? model.restore(options) : model;
                }
            }
            this.reset(storedModels, options);
            return this.trigger('restore');
        },

        toDerivedJSON: function toDerivedJSON() {
            return this.map(function (model) {
                return model.toDerivedJSON();
            });
        },

        getRefs: function getRefs() {
            return this.refs;
        },

        getRef: function getRef(url) {
            return this.refs[url];
        }
    });

    var UserTeamsCollection = BaseCollection.extend({
        url: config.api.user.teams.url,

        model: BaseModel
    });

    var UserModel = BaseModel.extend({

        urlRoot: config.api.user.url,

        modelName: 'UserModel',

        initialize: function initialize() {
            BaseModel.prototype.initialize.apply(this, arguments);

            if (featuresConfig.customersHasTeams) {
                this.set({
                    userTeams: new UserTeamsCollection()
                });
            }
        },

        getUserTeams: function getUserTeams() {
            return this.get('userTeams');
        },

        getActiveTeam: function getActiveTeam() {
            return this.get('team');
        },

        getSortedTeams: function getSortedTeams() {
            var userTeams = this.getUserTeams().toJSON();
            var activeTeam = this.getActiveTeam();

            var sortedTeams = _.filter(userTeams, function (team) {
                return team.id !== activeTeam.id;
            });
            sortedTeams.unshift(activeTeam);

            return sortedTeams;
        },

        fetchUserTeams: function fetchUserTeams() {
            this.getUserTeams().fetch();
        },

        /**
         * Returns user's full name.
         * @returns {String}
         */
        getName: function getName() {
            var name = this.get('firstName') && this.get('lastName') ? this.get('firstName') + ' ' + this.get('lastName') : this.get('name');

            return name;
        },

        /**
         * Returns history name that is used for saving in local storage
         *
         * @returns {string}
         */
        _getUserHistoryName: function _getUserHistoryName() {
            var currentUserId = this.id;
            return 'history.' + currentUserId;
        },

        /**
         * Gets user's history from local storage,
         * which is stored with key ' history.%userId% '
         *
         * If it doesn't exist, method creates it with exactly that name - ' history.%userId% '
         *
         * @return {Object|*}
         */
        getUserHistory: function getUserHistory() {
            if (Utils.isLocalStorageAvailable()) {
                var userHistoryName = this._getUserHistoryName();
                var userHistory = localStorage.getItem(userHistoryName);

                // Create history if it doesn't exist
                if (!userHistory) {
                    userHistory = JSON.stringify({});
                    localStorage.setItem(userHistoryName, userHistory);
                }

                return JSON.parse(userHistory);
            }
        },

        /**
         * Gets data from user's history from local storage
         * that is associated with that key
         *
         * @param {String} key
         * @return {Object|Boolean}
         */
        getFromUserHistory: function getFromUserHistory(key) {
            var userHistory = this.getUserHistory();
            return userHistory && userHistory[key];
        },

        /**
         * Saves pair key-value in user's data history
         * that is situated in the local storage
         *
         * @param {String} key
         * @param {*} value
         */
        saveInUserHistory: function saveInUserHistory(key, value) {
            if (Utils.isLocalStorageAvailable()) {
                var history = this.getUserHistory();
                history[key] = value;

                localStorage.setItem(this._getUserHistoryName(), JSON.stringify(history));
            }
        },

        /**
         * Checks if current user has among these specific permissions at least one.
         *
         * @param permissions
         *            permission array to check
         */
        hasAtLeastOnePermission: function hasAtLeastOnePermission(permissions) {
            var allowed = true;
            if (!permissions) {
                return allowed;
            }
            var userPermissions = this.uniquePermissions();
            allowed = _.some(permissions, function (permission) {
                return userPermissions[permission];
            });
            return allowed;
        },

        /**
         * Fetches of unique user permissions
         */
        uniquePermissions: function uniquePermissions() {
            var userPermissions = {};
            _.each(this.getRoles(), function (role) {
                _.each(role.permissions, function (permission) {
                    userPermissions[permission] = true;
                });
            });
            return userPermissions;
        },

        getRoles: function getRoles() {
            return this.get('roles');
        }
    });

    /**
     * Application context that is assumed to share across all application.
     */
    var Application = {

        getProp: function getProp(obj, desc) {
            var arr = desc.split('.');
            while (arr.length && obj != null) {
                obj = obj[arr.shift()];
            }
            return obj;
        },

        setProp: function setProp(obj, name, value) {
            var list = name.split('.'),
                root = obj,
                key;
            var leaf = list.pop();
            while (list.length) {
                key = list.shift();
                root = root[key] || (root[key] = {});
            }
            root[leaf] = value;
            return obj;
        },

        getUser: function getUser() {
            return this.userModel;
        },

        setUser: function setUser(userModel) {
            this.userModel = userModel;
        },

        getView: function getView() {
            return this.currentView;
        },

        setView: function setView(view) {
            this.currentView = view;
        },

        setPendingAction: function setPendingAction(action) {
            this.pendingAction = action;
        },

        getPendingAction: function getPendingAction() {
            return this.pendingAction;
        },

        execute: function execute(tasks, start, buffer, delay, taskHandler) {
            var self = this;
            var end = start + buffer;
            if (end > tasks.length) {
                end = tasks.length;
            }
            for (var i = start; i < end; i++) {
                taskHandler(tasks[i]);
            }
            if (end < tasks.length) {
                setTimeout(function () {
                    self.execute(tasks, end, buffer, delay, taskHandler);
                }, delay);
            }
        },

        /**
         * Show confirmation dialog.
         * @param {String} selector Dialog content selector
         * @param {Array} buttons
         */
        showDialog: function showDialog(selector, buttons) {
            var dialog = $(selector).modal();
            var modalFooter = dialog.find('.modal-footer').empty();

            _.each(_.keys(buttons), function (key) {
                var handler = buttons[key];

                var $btn = $('<button/>').attr('type', 'button').text(key).addClass(key === 'Ok' ? 'btn btn-info' : 'btn btn-default').on('click', handler).attr('data-dismiss', 'modal');

                modalFooter.append($btn);
            });
        },

        showErrorDialog: function showErrorDialog(errorMsg) {
            if (!this.errorDialog) {
                this.errorDialog = $('#dialog-error');
            }

            this.errorDialog.find('.modal-body').empty().append('<p>' + errorMsg + '</p>');
            this.errorDialog.modal('show');
        },

        clearFieldError: function clearFieldError($elem) {
            $elem.removeClass('error-field').closest('.form-group').removeClass('has-error').end().nextAll('.error-message').remove();
        },

        clearFieldErrors: function clearFieldErrors() {
            $('.error-field').removeClass('error-field').closest('.form-group').removeClass('has-error').end().nextAll('.error-message').remove();
        },

        /**
         * @returns {Array}
         */
        parseErrors: function parseErrors(resp) {
            var errors = [];
            if (resp === null || resp === void 0) {
                return errors;
            }
            if (typeof resp === 'string' || typeof resp.responseText === 'string') {
                resp = JSON.parse(resp.responseText || resp);
            }

            if (resp.errors) {
                errors = errors.concat(resp.errors);
            } else if (_.isArray(resp) || _.isObject(resp)) {
                // If it's an array then we
                // assume that it's an array
                // of errors
                errors = errors.concat(resp);
            } else {
                // Throw custom parse exception
                return [{
                    code: 'PARSE_ERROR',
                    message: 'Cannot parse response errors'
                }];
            }

            return _.filter(errors, function (error) {
                // If passed parameter is object with 'message' and 'code' or 'path'
                // fields then add it to errors array
                return error && typeof error.message === 'string' && (typeof error.code === 'string' || typeof error.path === 'string');
            });
        },

        getErrorField: function getErrorField() {
            return $('<span/>').addClass('error-message help-block');
        },

        fileUploadHandler: function fileUploadHandler(event, callback) {
            var files = event.target.files;

            var errorContainer = $('span#imageUploadError');

            if (!files || !files[0]) {
                return;
            }

            var file = files[0];

            var isFileValid = this.validateImageUpload(file, errorContainer);

            if (!isFileValid) {
                return;
            }

            var reader = new FileReader();

            reader.onload = function (e) {
                var imageBase64 = e.target.result;

                var _imageBase64$split = imageBase64.split(','),
                    _imageBase64$split2 = _slicedToArray(_imageBase64$split, 2),
                    type = _imageBase64$split2[0],
                    content = _imageBase64$split2[1];

                var image = { type: type, content: content };
                callback(image);
            };

            reader.readAsDataURL(file);
        },

        validateImageUpload: function validateImageUpload(file) {
            var maxSize = 512 * 1000;
            var allowedExt = '/png';
            var errorContainer = $('span#imageUploadError');

            errorContainer.html('');

            var errors = [];

            if (!file.type.endsWith(allowedExt)) {
                errors.push(messages.UploadImage.imageType);
            }
            if (file.size > maxSize) {
                errors.push(messages.UploadImage.imageSize);
            }

            if (errors.length) {
                errorContainer.html(errors.join('<br/>'));
                return false;
            }
            return true;
        },

        getImageSrc: function getImageSrc(image) {
            var url = image.content ? image.type ? image.type + ',' + image.content : 'data:image/png;base64,' + image.content : image.imageSourceUrl;
            return this.checkIsUrlEmpty(url);
        },

        checkIsUrlEmpty: function checkIsUrlEmpty(url) {
            return url || this.imagePlaceholderPath;
        },

        imagePlaceholderPath: '/static-site/images/admin/image-placeholder.png',

        uploadImagesFormat: ".png",

        showFieldError: function showFieldError($elem, errors) {
            var $errors = $(),
                $error = this.getErrorField();
            if (!_.isArray(errors)) {
                errors = [errors];
            }
            _.each(errors.reverse(), function (error) {
                if (error) {
                    $.merge($errors, $error.clone().text(error.message || error || ''));
                }
            });
            return $elem.addClass('error-field').after($errors).closest('.form-group').addClass('has-error');
        },

        showFieldErrors: function showFieldErrors(errors) {
            this.clearFieldErrors();
            var notShownErrors = [],
                $elements = $('[data-path]');

            _.each(_.groupBy(errors, function (error) {
                return error.path;
            }), function (errors, path) {
                var $elem = $elements.filter('[data-path="' + path + '"]');
                if ($elem.length) {
                    this.showFieldError($elem, errors);
                } else {
                    notShownErrors.push.apply(notShownErrors, errors);
                }
            }, this);

            // Focus on the first field that contains an error
            $elements.filter('.error-field').eq(0).focus();
            return notShownErrors;
        },

        /**
         * Function that show errors.
         *
         * @param {Object} params Object containing function parameters
         * @param {Object} params.errors Errors that do not have "path" property set
         * @param {Object} params.errorsWithPath Errors that have "path" property set
         */
        showErrors: function showErrors(params) {
            var errorMsg = '',
                errors = params.errors,
                errorsWithPath = params.errorsWithPath;
            var notShownErrors = this.showFieldErrors(errorsWithPath);

            _.each([].concat(_toConsumableArray(notShownErrors), _toConsumableArray(errors)), function (error) {
                errorMsg = errorMsg + error.message + '<br/>';
            });
            if (errorMsg !== '') {
                this.showErrorDialog(errorMsg);
            }
        },

        /**
         * Displays response errors.
         *
         * @param {Object} params Object containing function parameters
         * @param {Object} [params.view] View instance that called displayErrors method. Used in error messages lookup
         * @param {Object} [params.response] Server response
         * @param {Object} [params.showErrorsFunc] Function that actually shows errors
         */
        displayErrors: function displayErrors(params) {
            var _this = this,
                errorMsg = '',
                errors = [],
                errorsWithPath = [],
                view = params.view || null,
                response = params.response || null,
                key,
                paths,
                showErrorsFunc,
                constraint;
            try {
                _.each(this.parseErrors(response), function (error) {
                    if (error.path) {
                        paths = [error.path, error.code];
                        if (view) {
                            paths.unshift(view.name);
                        }
                        key = paths.join('.');
                        if (constraint = _this.getProp(constraints, key)) {
                            error.message = constraint;
                        }
                        errorsWithPath.push(error);
                    } else {
                        errors.push(error);
                    }
                });
            } catch (e) {
                key = response.status.toString();
                errorMsg = _this.getProp(httpErrors, key);
                // If response text is not in JSON format and we couldn't find the appropriate error message for the HTTP error status
                // then just show response status and text
                if (!errorMsg) {
                    errorMsg = 'Error ' + key + ' (' + response.statusText + ')';
                }
                errors.push({
                    code: 'ERROR',
                    message: errorMsg
                });
            } finally {
                showErrorsFunc = params.showErrorsFunc || this.showErrors;
                showErrorsFunc.call(this, {
                    errors: errors,
                    errorsWithPath: errorsWithPath
                });
            }
        },

        getCoords: function getCoords(node) {
            var $node = $(node),
                scrollTop = $(window).scrollTop(),
                scrollLeft = $(window).scrollLeft(),
                top = $node.offset().top - scrollTop,
                left = $node.offset().left - scrollLeft,
                height = $node.outerHeight(),
                width = $node.outerHeight();

            return {
                top: top,
                left: left,
                right: left + width,
                bottom: top + height,
                height: height,
                width: width
            };
        },

        isVisible: function isVisible(node, target, coords) {
            // Shift in pixels towards the center of the element.
            // 7 px has been chosen experimentally (due to Firefox's strange behavior)
            var shift = 7;
            coords = coords || this.getCoords(node);

            return node == $(document.elementFromPoint(coords.left + shift, coords.top + shift)).closest(target)[0] && node == $(document.elementFromPoint(coords.left + shift, coords.bottom - shift)).closest(target)[0] && node == $(document.elementFromPoint(coords.right - shift, coords.bottom - shift)).closest(target)[0] && node == $(document.elementFromPoint(coords.right - shift, coords.top + shift)).closest(target)[0];
        },

        /**
         * Generates a UUID compliant with RFC 4122.
         * @returns generated UUID
         */
        uuid: function uuid() {
            return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
                var r = Math.random() * 16 | 0,
                    v = c === 'x' ? r : r & 0x3 | 0x8;
                return v.toString(16);
            });
        },

        isBrowserSupported: function isBrowserSupported() {
            var isSupported = false,
                userAgent = navigator.userAgent,
                version;
            // check IE to be version between 8 and 10
            if (/MSIE (\d+\.\d+);/.test(userAgent)) {
                version = parseFloat(RegExp.$1);
                isSupported = version >= 8 && version <= 10;
            }
            // check IE 11 and higher
            if (/Trident[\/\s](\d+\.\d+).*rv:(\d+\.\d+)/.test(userAgent)) {
                var tridentVersion = parseFloat(RegExp.$1),
                    rvVersion = parseFloat(RegExp.$2);
                isSupported = tridentVersion >= 7 && rvVersion >= 11;
            }
            // check Firefox to be version 11 or higher
            if (/Firefox[\/\s](\d+\.\d+)/.test(userAgent)) {
                version = parseFloat(RegExp.$1);
                isSupported = version >= 11;
            }
            // Check Chrome to be version 11 or higher
            if (/Chrome[\/\s](\d+\.\d+)/.test(userAgent)) {
                // Support for Google TV
                version = parseFloat(RegExp.$1);
                isSupported = version >= 11;
            }
            // Check Safari to be version 5.1 or higher
            if (/Safari[\/\s](\d+\.\d+)/.test(userAgent)) {
                version = parseFloat(RegExp.$1);
                isSupported = version >= 5.1;
            }

            return isSupported;
        }
    };

    var BaseRouter = Backbone.Router.extend({
        renderView: function renderView(view) {
            var currentView = Application.getView();
            if (currentView) {
                currentView.remove();
            }
            Application.setView(view);
            if (view) {
                view.render();
            }
        },

        getUser: function getUser() {
            return Application.getUser();
        },

        getView: function getView() {
            return Application.getView();
        },

        /**
         * Invokes route-method with some default actions in application
         *  for routing to certain uri
         *
         *  default actions:
         *  - fetches user if it has been changed,
         *  - wraps route callback,
         *  - changes document title
         *
         * @param route
         */
        authRoute: function authRoute(route) {
            var _this = this;
            var title = route.title ? route.title : config.pageTitle;

            this.route(route.route, route.event, function () {
                // Update page title when route has been matched
                document.title = title;

                var callbackArgs = _.toArray(arguments);
                var callback = function callback() {
                    return _this._wrapRouterCallback(route.event, route.callback).apply(_this, callbackArgs);
                };

                var user = Application.getUser();
                if (user.isNew()) {
                    user.fetch({
                        success: function success() {
                            if (user.isNew()) {
                                Application.setPendingAction(callback);
                                _this.navigateLogin();
                            } else {
                                featuresConfig.customersHasTeams && user.fetchUserTeams();
                                _this.safeInvokeCallback(user, route, callback);
                            }
                        },
                        error: function error(model, response) {
                            switch (response.status) {
                                case 503:
                                    break;
                                case 401:
                                    if (featuresConfig.featureEpamAutoLoginEnabled && window.location.pathname !== "/login") {
                                        window.location.href = config.api.login.oauth.url;
                                    } else {
                                        Application.setPendingAction(callback);
                                        _this.navigateLogin();
                                    }
                                    break;
                                default:
                                    Application.setPendingAction(callback);
                                    _this.navigateLogin();
                                    break;
                            }
                        }
                    });
                } else {
                    _this.safeInvokeCallback(user, route, callback);
                }
            });
        },

        /**
         * If any available menu item exists, invoke callback,
         * else - display error-callback,
         * else - display error dialog
         *
         * @param user
         * @param {Object} route
         * @param callback
         */
        safeInvokeCallback: function safeInvokeCallback(user, route, callback) {
            if (user.hasAtLeastOnePermission(route.permissions)) {
                callback();
            } else {
                this.errorRoute();
            }
        },

        /**
         * Displays error message if user is not authorized
         */
        errorRoute: function errorRoute() {
            Application.displayErrors({
                response: {
                    code: 'NOT_AUTHORIZED',
                    message: 'You are not authorized to view this content'
                }
            });
        },

        /**
         * @see http://backbonejs.org/#Router-execute
         */
        execute: function execute(callback, args) {
            // If the last argument is NOT an object it means that query parameters were not specified
            // so we need to push an empty object representing query parameters to the arguments
            if (!_.isObject(args[args.length - 1])) {
                args.push({});
            }
            if (callback) {
                callback.apply(this, args);
            }
        },

        /**
         * @see http://backbonejs.org/#Router-navigate
         */
        navigate: function navigate(fragment, options) {
            var root = Backbone.history.root;
            // If fragment contains history root then remove everything before the root including itself
            var rootIndex = fragment.indexOf(root);
            if (rootIndex !== -1) {
                fragment = fragment.substring(rootIndex + root.length);
            }
            return Backbone.Router.prototype.navigate.call(this, fragment, options);
        },

        navigateLogin: function navigateLogin() {
            this.navigate('/login', {
                trigger: true
            });
        },

        navigateBrowserUnsupported: function navigateBrowserUnsupported() {
            this.navigate('/errors/unsupported', {
                trigger: true
            });
        },

        navigateApp: function navigateApp() {
            this.navigate('/', {
                trigger: true
            });
            var pendingAction = Application.getPendingAction();
            if (pendingAction) {
                pendingAction();
                Application.setPendingAction(null);
            } else {
                this.navigate('/', {
                    trigger: true
                });
            }
        },

        /**
         * Calls route callback if provided and publishes "BaseRouter:route" event.
         *
         * @param {String} name
         * @param {Function} callback
         * @returns {Function}
         * @private
         */
        _wrapRouterCallback: function _wrapRouterCallback(name, callback) {
            return function () {
                var callbackArgs = _.toArray(arguments);
                if (callback) {
                    callback.apply(this, callbackArgs);
                }
                this.publish('BaseRouter:route', name);
            };
        }
    });

    var BaseView = Backbone.View.extend({

        constructor: function constructor(options) {
            _.extend(this, _.pick(options || {}, ['name', 'router', 'dialog', 'navigate', 'filter', 'parent']));
            this.childViews = {};
            this.childViewContainer = {};
            Backbone.View.prototype.constructor.apply(this, arguments);
        },

        /**
         * Serializes form in JSON object.
         * @param {string} selector JQuery selector of the form to serialize
         * @param {object} fieldsToExclude Object containing "field name" : [true|false] pairs and specifying which
         * input fields should be excluded from the resulting JSON object. In false specified, then the value
         * of the input field with such name won't be included. Otherwise it will.
         */
        form2json: function form2json(selector, fieldsToExclude) {
            fieldsToExclude = fieldsToExclude || {};
            var json = {},
                form = $(selector),
                controls = form.serializeArray(),
                all = _.isEmpty(fieldsToExclude);

            var addToJSON = function addToJSON(name, value) {
                if (name && (all || Application.getProp(fieldsToExclude, name) !== false)) {
                    Application.setProp(json, name, value);
                }
            };

            _.each(controls, function (control) {
                addToJSON(control.name, control.value);
            });

            $('select', form).each(function (index, select) {
                var value = $(select).val();
                addToJSON(select.name, select.multiple ? value || [] : value);
            });
            $('input:checkbox', form).each(function (index, checkbox) {
                addToJSON(checkbox.name, checkbox.checked);
            });

            return json;
        },

        template: function template() {},

        getModel: function getModel() {
            return this.model ? this.model.toDerivedJSON() : {};
        },

        getName: function getName() {
            return this.name;
        },

        /**
         * Returns selector created from list of classNames
         * @see this.className
         * @see this.id
         * @returns {string}
         */
        getSelector: function getSelector() {
            var _this = this;
            var selector = '';

            //The order of the properties should be exactly that!
            var propertiesHandlers = [{
                name: 'tagName',
                handler: function handler(tagName) {
                    return tagName;
                }
            }, {
                name: 'id',
                handler: function handler(id) {
                    return '#' + id;
                }
            }, {
                name: 'className',
                handler: function handler(className) {
                    return '.' + className.split(' ').join('.');
                }
            }];

            _.forEach(propertiesHandlers, function (propertyHandler) {
                var property = _this._getStringProperty(propertyHandler.name);
                if (property) {
                    selector += propertyHandler.handler(property);
                }
            });

            return selector;
        },

        /**
         * If property is string, returns it
         * If property is function, returns function result
         * else returns empty string
         *
         * @param {String} propName
         * @returns {String}
         * @private
         */
        _getStringProperty: function _getStringProperty(propName) {
            var property = this[propName];

            if (typeof property === 'string') {
                return property;
            }

            if (_.isFunction(property)) {
                return property.apply(this);
            }

            return '';
        },

        getRouter: function getRouter() {
            return this.router;
        },

        setRouter: function setRouter(router) {
            this.router = router;
        },

        childViewInjection: {},

        /**
         *
         * @param {Backbone.View} view
         * @param {String|Element|jQuery} [container]
         * @returns {Backbone.View}
         */
        addChildView: function addChildView(view, container) {
            var viewName = view.getName();
            this.childViews[viewName] = view;
            view.parent = this;
            if (container) {
                this.childViewContainer[viewName] = container;
            }
            return view;
        },

        /**
         * Removes childView from view children list
         * and removes its entity
         *
         * @param childName
         */
        removeChildView: function removeChildView(childName) {
            var childView = this.childViews && this.childViews[childName];

            if (childView) {
                childView.remove();
                delete this.childViews[childName];
            }
        },

        getParent: function getParent() {
            return this.parent;
        },

        remove: function remove() {
            this.beforeRemove();
            _.each(this.childViews, function (view) {
                view.remove();
            });
            this.undelegateEvents();
            this.stopListening();
            this.$el.empty();
            return this;
        },

        /**
         * @returns {BaseView}
         */
        render: function render() {
            try {
                Semaphore.acquire();
                this.beforeRender();
                var m = $.extend(true, {
                    config: config
                }, this.getModel());
                var html = this.template(m);
                if (typeof html === 'string') {
                    this.$el.html(html);
                }
                this.afterRender();
                _.each(this.childViews, function (view, name) {
                    var el = this.childViewContainer[name],
                        isMethod;
                    if (el && view.render) {
                        isMethod = typeof el === 'string' && _.contains(['append', 'prepend', 'html', 'before', 'after'], el);
                        if (isMethod) {
                            this.$el[el](view.el);
                        } else {
                            view.setElement($(el));
                        }
                        view.render();
                    }
                }, this);
            } finally {
                Semaphore.release();
            }
            return this;
        },

        beforeRender: function beforeRender() {},

        /**
         * Invokes after rendering main content (except child views)
         * @abstract
         */
        afterRender: function afterRender() {},

        beforeRemove: function beforeRemove() {},

        getStack: function getStack(property) {
            var stack = [],
                value,
                obj = this;
            while (obj && property in obj) {
                if ((value = obj[property]) != null && !_.contains(stack, value)) {
                    stack.push(value);
                }
                obj = obj.constructor && obj.constructor.__super__;
            }

            return stack;
        },

        delegateEvents: function delegateEvents(events) {
            if (!(events || (events = this.getStack('events')))) {
                return this;
            }
            this.undelegateEvents();
            var ln = events.length;
            if (ln !== +ln) {
                events = [events];
                ln = 1;
            }
            var i = -1;
            while (++i < ln) {
                this.delegateEvent(events[i]);
            }

            return this;
        },

        delegateEvent: function delegateEvent(eventObj) {
            if (_.isFunction(eventObj)) {
                eventObj = eventObj.call(this);
            }
            if (eventObj) {
                for (var event in eventObj) {
                    var handler = eventObj[event];
                    if (typeof handler !== 'function' && typeof (handler = this[handler]) !== 'function') {
                        continue;
                    }
                    var selector = null;
                    var splitEvent = event.split(/\s+/);
                    if (splitEvent.length > 1) {
                        event = splitEvent.shift();
                        selector = splitEvent.join(' ');
                    }
                    this.$el.on(event + '.delegateEvents' + this.cid, selector, _.bind(handler, this));
                }
            }
        },

        clearFieldErrors: function clearFieldErrors() {
            return Application.clearFieldErrors();
        },

        showFieldErrors: function showFieldErrors(errors) {
            return Application.showFieldErrors(errors);
        },

        showErrors: function showErrors(params) {
            return Application.showErrors(params);
        },

        displayErrors: function displayErrors(response) {
            Application.displayErrors({
                view: this,
                response: response,
                showErrorsFunc: _.bind(this.showErrors, this)
            });
        },

        /**
         * Checks whether there is less than PIXELS_TO_THE_PAGE_END to the end of the page.
         * @returns {Boolean}
         */
        isToLoadMoreItems: function isToLoadMoreItems() {
            if ($('html').hasClass('underpopup-content')) {
                return false;
            }
            var $window = $(window),
                pageHeight = $(document).height(),
                windowHeight = $window.height(),
                scrollTop = $window.scrollTop();
            return pageHeight - windowHeight - scrollTop < PIXELS_TO_THE_PAGE_END;
        }
    });

    $(document).ajaxSend(function (e, jqXHR, ajaxOptions) {
        setTimeout(function () {
            if (ajaxOptions.blockUI !== false) {
                Semaphore.acquire();
            }
        }, 200);
    });

    $(document).ajaxComplete(function (e, jqXHR, ajaxOptions) {
        if (ajaxOptions.blockUI !== false) {
            Semaphore.release();
        }
    });

    // Add mediator functionality to core modules
    _.extend(BaseView.prototype, Mediator);
    _.extend(BaseModel.prototype, Mediator);
    _.extend(BaseCollection.prototype, Mediator);
    _.extend(BaseRouter.prototype, Mediator);

    return {
        Application: Application,
        UserModel: UserModel,
        BaseRouter: BaseRouter,
        BaseView: BaseView,
        BaseModel: BaseModel,
        BaseCollection: BaseCollection
    };
}).call(exports, __webpack_require__, exports, module),
