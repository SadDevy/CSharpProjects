    /**
     * Triggers events: 'reposition:employee-items' -> command to reset pointer to current employee-item
     */
    var EmployeeRoundAnimationService = function EmployeeRoundAnimationService(options) {
        this.initialize(options);
    };

    EmployeeRoundAnimationService.prototype = {

        animationDuration: 900,

        hideAnimationDuration: 700,

        /**
         * @typeof Backbone#View
         * @abstract
         */
        view: new Backbone.View(),

        /**
         * Returns selector for group members
         *
         * @param {String} [additionalSelectData]
         * @returns {String}
         *
         * @abstract
         */
        getEmployeeSelector: function getEmployeeSelector(additionalSelectData) {
            return ' .employee-item' + (additionalSelectData || '');
        },

        /**
         * Adds selectors data to get image of employee item
         *
         * @param {String} [selector]
         * @returns {string}
         * @throws {TypeError} if selector doesn't represent employee-item
         */
        getEmployeeImgSelector: function getEmployeeImgSelector(selector) {
            if (!$(selector).hasClass('employee-item') && !selector.match('employee-item')) {
                throw new TypeError('Selector does not represent employee item');
            }

            return selector + ' .img';
        },

        /**
         * Options to set/override native ones
         *
         * @param {Object} [options]
         * @returns {EmployeeRoundAnimationService}
         */
        initialize: function initialize(options) {
            _.extend(this, options);
            return this;
        },

        /**
         * Return group item jQuery element
         *
         * @returns {jQuery}
         */
        get$GroupControl: function get$GroupControl() {
            return this.view.$('.group-item');
        },

        /**
         * Returns selector for only one employee
         *
         * @param {String} employeeId
         * @returns {String}
         */
        getSingleEmployeeSelector: function getSingleEmployeeSelector(employeeId) {
            var selectorDataAttr = '[data-employee-id="' + employeeId + '"]';
            return this.getEmployeeSelector(selectorDataAttr);
        },

        getSingleEmployeeSelectorRelatedToCurrentComment: function getSingleEmployeeSelectorRelatedToCurrentComment(groupMemberSelector) {
            var newsId = this.view.$el.closest('.comment').attr('data-employee-badge-id');
            var selector = newsId ? ' [data-employee-badge-id="' + newsId + '"] ' + groupMemberSelector : groupMemberSelector;

            return selector;
        },

        /**
         * Returns selector for all employees except one
         *
         * @param {String} employeeId
         * @returns {String}
         */
        getAllEmployeesExceptOneSelector: function getAllEmployeesExceptOneSelector(employeeId) {
            var selectorDataAttr = ':not([data-employee-id="' + employeeId + '"])';
            return this.getEmployeeSelector(selectorDataAttr);
        },

        /**
         * Adds and removes animated class
         * to trigger css-animation
         */
        animateGroupControl: function animateGroupControl() {
            var animatedClass = 'animated';
            this.get$GroupControl().removeClass(animatedClass).addClass(animatedClass);
        },

        /**
         * Removes all svg elements and draw new on pointed employee
         *
         * @returns {jQuery.Deferred.promise}
         */
        updateSvgCircles: function updateSvgCircles() {
            var groupMemberSelector = this.getEmployeeSelector('.pointed');
            this.view.$(groupMemberSelector + ' svg').remove();
            var circleRadius = this.getProgressCircleRadius();
            return SvgRoundProgressBarDrawer.animateLoadProgressCircle(groupMemberSelector, circleRadius, 0);
        },

        /**
         * Defines radius for circle bar around group member
         *
         * @private
         * @returns {number}
         */
        getProgressCircleRadius: function getProgressCircleRadius() {
            var groupMemberSelector = this.getEmployeeSelector();
            return this.view.$(groupMemberSelector + ' img').outerWidth() / 2 + 5;
        },

        /**
         * @param {Number} [animationDuration]
         * @returns {jQuery.Deferred.promise}
         */
        animateUnloadAllEmployeesGroup: function animateUnloadAllEmployeesGroup(animationDuration) {
            animationDuration = _.isNumber(animationDuration) ? animationDuration : this.animationDuration;

            var groupMemberSelector = this.getEmployeeSelector();
            return SvgRoundProgressBarDrawer.animateUnloadProgressCircle(groupMemberSelector, this.getProgressCircleRadius(), animationDuration);
        },

        /**
         * @param {Number} [animationDuration]
         * @returns {jQuery.Deferred.promise}
         */
        animateLoadAllEmployees: function animateLoadAllEmployees(animationDuration) {
            animationDuration = _.isNumber(animationDuration) ? animationDuration : this.animationDuration;

            var groupMemberSelector = this.getEmployeeSelector();
            this.view.$(groupMemberSelector + ' svg').remove();
            var circleRadius = this.getProgressCircleRadius();
            return SvgRoundProgressBarDrawer.animateLoadProgressCircle(this.getEmployeeImgSelector(groupMemberSelector), circleRadius, animationDuration);
        },

        /**
         * @param {String} employeeId
         * @param {Number} [animationDuration]
         * @returns {jQuery.Deferred.promise}
         */
        animateUnloadSingleEmployee: function animateUnloadSingleEmployee(employeeId, animationDuration) {
            animationDuration = _.isNumber(animationDuration) ? animationDuration : this.animationDuration;

            var groupMemberSelector = this.getSingleEmployeeSelector(employeeId);
            var $employeeSvgWrapper = this.view.$(groupMemberSelector);
            var circleRadius = this.getProgressCircleRadius();
            var hideDuration = animationDuration === 0 ? 0 : this.hideAnimationDuration;

            return $.when(SvgRoundProgressBarDrawer.animateUnloadProgressCircle(this.getEmployeeImgSelector(groupMemberSelector), circleRadius, animationDuration)).done(function () {
                return $employeeSvgWrapper.animate({ opacity: 0 }, hideDuration, function () {
                    $employeeSvgWrapper.closest('.scroll-frame').remove();
                });
            });
        },

        /**
         * @param {String} employeeId
         * @param {Number} [animationDuration]
         * @returns {jQuery.Deferred.promise}
         */
        animateLoadSingleEmployee: function animateLoadSingleEmployee(employeeId, animationDuration) {
            animationDuration = _.isNumber(animationDuration) ? animationDuration : this.animationDuration;

            var $dfd = $.Deferred();

            this.view.$(this.getEmployeeSelector()).find('svg').remove();
            var circleRadius = this.getProgressCircleRadius();
            var groupMemberSelector = this.getSingleEmployeeSelector(employeeId);
            var singleMemberSelectorRelatedToComment = this.getSingleEmployeeSelectorRelatedToCurrentComment(groupMemberSelector);

            $.when(SvgRoundProgressBarDrawer.animateLoadProgressCircle(this.getEmployeeImgSelector(singleMemberSelectorRelatedToComment), circleRadius, animationDuration)).done(function () {
                $dfd.resolve();
            });

            return $dfd.promise();
        },

        /**
         * @param {String} employeesSelector
         * @param {Number} [animationDuration]
         * @returns {jQuery.Deferred.promise}
         */
        animateLoadEmployees: function animateLoadEmployees(employeesSelector, animationDuration) {
            animationDuration = _.isNumber(animationDuration) ? animationDuration : this.animationDuration;

            var $dfd = $.Deferred();
            var selectedEmployees = this.getEmployeeImgSelector(employeesSelector);

            this.view.$(selectedEmployees).find('svg').remove();
            var circleRadius = this.getProgressCircleRadius();

            $.when(SvgRoundProgressBarDrawer.animateLoadProgressCircle(selectedEmployees, circleRadius, animationDuration)).done(function () {
                $dfd.resolve();
            });

            return $dfd.promise();
        }

    };

    return EmployeeRoundAnimationService;
