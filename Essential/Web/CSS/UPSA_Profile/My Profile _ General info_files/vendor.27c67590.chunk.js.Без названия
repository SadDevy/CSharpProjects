!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    // Core modules
    var badgeAssignConfig = __webpack_require__(/*! com/epam/heroes/widget/givebadge/config */ "./src/main/resources/static-site/js/com/epam/heroes/widget/givebadge/config.js");
    var core = __webpack_require__(/*! com/epam/heroes/core/core */ "./src/main/resources/static-site/js/com/epam/heroes/core/core.js");

    // Parent view
    var AutoCompleteSearchView = __webpack_require__(/*! com/epam/heroes/portal/core/view/AutoCompeteSearchView */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/AutoCompeteSearchView.js");

