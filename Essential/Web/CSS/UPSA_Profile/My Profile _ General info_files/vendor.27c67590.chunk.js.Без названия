            PATTERN: 'YYYY-MM-DD HH:mm:ss Z',

            /**
             * Parses date with moment-js to defined pattern
             * or to default one
             *
             * @param {Number|String|Date} date
             * @param {String} [pattern]
             * @returns {String}
             */
            toMoment: function toMoment(date, pattern) {
                pattern = pattern || (typeof date !== 'number' ? DateFormats.moment.PATTERN : null);
                return moment(date, pattern);
            },

            /**
             * Returns how much time was left since target date to current moment
             *
             * @param {Number|Date|String} date
             * @param {String} [pattern]
             * @returns {String}
             */
            toDateFromNow: function toDateFromNow(date, pattern) {
                return DateFormats.moment.toMoment(date, pattern).fromNow();
            }

        }
    };

    return DateFormats;
