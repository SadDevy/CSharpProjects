 */
!(__WEBPACK_AMD_DEFINE_ARRAY__ = [__webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js")], __WEBPACK_AMD_DEFINE_RESULT__ = (function (config) {

    var badgeLibraryConfigs = {

        /**
         * All available receivers types
         * @type {{EMPLOYEE: string, GROUP: string}}
         */
        RECEIVER_TYPES: {
            EMPLOYEE: 'EMPLOYEE',
            GROUP: 'GROUP'
        },

        BADGE_CATALOGUE_SEARCH_FILTER: '',

        GROUP_ICON: config.staticSiteUrl + '/images/portal/a-group.jpg',

        /**
         * Maximum number of search results.
         */
        MAX_SEARCH_RESULTS_PER_GROUP: 6,

        /**
         * Goes through all configuration types for different badges
         * and if badge confirms "typeCriteria" of some type -> returns its configuration for this type
         * returns default configuration if none of other type
         *
         * @param {Badge} options.badge
         * @param {Array} options.chosenEmployeesIds
         * @param {String} options.userId
         * @param {boolean} options.isGroupSearchDisabled
         * @returns {Object}
         */
        getSearchReceiverConfig: function getSearchReceiverConfig(options) {
            options.chosenEmployeesIds = options.chosenEmployeesIds || [];

            return {
                url: config.api.search.url,
                data: {
                    exclEmpl: this._defineExcludedEmployeesIds(options),
                    type: this._defineReceiverType(options),
                    badgeId: options.badge.id
                }
            };
        },

        /**
         * @param {Badge} options.badge
         * @param {boolean} options.isGroupSearchDisabled
         * @returns {string}
         * @private
         */
        _defineReceiverType: function _defineReceiverType(options) {
            var isGroupSearchDisabled = options.isGroupSearchDisabled;
            if (!options.badge.isMultipleAssignmentAvailable() || isGroupSearchDisabled) {
                return badgeLibraryConfigs.RECEIVER_TYPES.EMPLOYEE;
            }
        },

        /**
         * @param {Badge} options.badge
         * @param {Array} options.chosenEmployeesIds
         * @param {String} options.userId
         * @returns {Array}
         * @private
         */
        _defineExcludedEmployeesIds: function _defineExcludedEmployeesIds(options) {
            var excludedEmployeesIds = options.chosenEmployeesIds || [];

            if (!options.badge.isSelfAssignmentAvailable()) {
                excludedEmployeesIds = excludedEmployeesIds.concat(options.userId);
            }

            return excludedEmployeesIds;
        }
    };

    return badgeLibraryConfigs;
}).apply(exports, __WEBPACK_AMD_DEFINE_ARRAY__),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));

