/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! backbone */ "./src/main/resources/static-site/js/libs/vendor/backbone/backbone.js"), __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js")))

