/***/ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/util/SvgRoundProgressBarDrawer.js":
/*!*********************************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/portal/core/util/SvgRoundProgressBarDrawer.js ***!
  \*********************************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
/* WEBPACK VAR INJECTION */(function($) {var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var d3 = __webpack_require__(/*! d3 */ "./src/main/resources/static-site/js/libs/vendor/d3/d3.min.js");

    var SvgRoundProgressBarDrawer = {

        defaults: {
            TARGET_ELEMENTS_CLASS_NAME: 'set-item',
            RADIUS: 52,
            INDICATOR_OFFSET: 8,

            /**
             * Duration of the progress bar loading
             */
            ANIMATION_DURATION: 700
        },

        className: 'indicator',
        currentAmountAttribute: 'data-current-amount',
        totalAmountAttribute: 'data-total-amount',

        /**
         * Following script draws round progress bar in the center of elements with target className,
         * in our case it is 'set-item' by default.
         * These elements should have following data attributes: current-amount and total-amount, in
         * other case progress bar will not be added.
         *
         * Also there is possibility to set different radius to the progress bar.
         *
         * @param [targetElementsClassName]
         * @param [progressBarRadius]
         * @param [$context]
         */
        drawProgressBar: function drawProgressBar(targetElementsClassName, progressBarRadius, $context, options) {
            $context = $context || window;

            var targetsClass = targetElementsClassName || this.defaults.TARGET_ELEMENTS_CLASS_NAME;
            var circleRadius = progressBarRadius || this.defaults.RADIUS;
            var $targetsList = $context.$('.' + targetsClass);
            var _this = this;

            _.forEach($targetsList, function (element) {
                _this.drawArcs($(element), circleRadius, options);
            });
        },

        /**
         * Returns cartesian coordinates of point appropriate its polar coordinates
         *
         * @param centerX
         * @param centerY
         * @param radius
         * @param angleInDegrees
         * @returns {*[]}
         */
        polarToCartesian: function polarToCartesian(centerX, centerY, radius, angleInDegrees) {
            var angleInRadians = angleInDegrees * Math.PI / 180.0;
            var x = centerX + radius * Math.cos(angleInRadians);
            var y = centerY + radius * Math.sin(angleInRadians);
            return [x, y];
        },

        /**
         * Returns coordinates of element's center as half of dimensions
         *
         * @param $element
         * @returns {*[]}
         */
        findCenter: function findCenter($element) {
            var width = $element.outerWidth();
            var height = $element.outerHeight();

            return [width / 2, height / 2];
        },

        /**
         * Draws indicator for target elements with defined radius
         *
         * @param $targetElement
         * @param circleRadius
         * @returns {Boolean}
         */
        drawArcs: function drawArcs($targetElement, circleRadius, options) {
            var currentAmount = 0,
                totalAmount = 0,
                data = {};

            data.currentAmount = $targetElement.attr(this.currentAmountAttribute);
            data.totalAmount = $targetElement.attr(this.totalAmountAttribute);

            if (!isNaN(data.currentAmount) && !isNaN(data.totalAmount) && data.currentAmount.length > 0 && data.totalAmount.length > 0) {

                currentAmount = parseInt(data.currentAmount);
                totalAmount = parseInt(data.totalAmount);
            } else {
                //This element not contains data for progressBar
                return false;
            }

            if (currentAmount > totalAmount) {
                //Wrong progressBar parameters, currentAmount couldn't be above than the totalAmount'
                return false;
            } else if (currentAmount === totalAmount) {
                currentAmount -= 0.0001;
            } else if (currentAmount === 0) {
                currentAmount += 0.0001;
            }

            //angle in degrees (arrow position on clock from 12pm)
            var angle = 360 / totalAmount * currentAmount - 90;

            var namespaceURI = 'http://www.w3.org/2000/svg';
            var centerCoordinates = this.findCenter($targetElement);
            var transitionPointCoords = this.polarToCartesian(centerCoordinates[0], centerCoordinates[1], circleRadius, angle);

            var largeArcFlag1 = 0;
            var largeArcFlag2 = 1;

            var svgContainer = document.createElementNS(namespaceURI, 'svg');
            var firstArc = document.createElementNS(namespaceURI, 'path');
            var secondArc = document.createElementNS(namespaceURI, 'path');

            svgContainer.setAttribute('xmlns', namespaceURI);
            svgContainer.setAttribute('xmlns:xlink', namespaceURI);
            svgContainer.setAttribute('class', this.className);

            if (angle > 90) {
                largeArcFlag1 = 1;
                largeArcFlag2 = 0;
            }

            firstArc.setAttributeNS(null, 'd', 'M' + centerCoordinates[0] + ',' + (centerCoordinates[1] - circleRadius) + ' A' + circleRadius + ',' + circleRadius + ' 0 ' + largeArcFlag1 + ',1 ' + transitionPointCoords[0] + ',' + transitionPointCoords[1]);

            secondArc.setAttributeNS(null, 'd', 'M' + centerCoordinates[0] + ',' + (centerCoordinates[1] - circleRadius) + ' A' + circleRadius + ',' + circleRadius + ' 0 ' + largeArcFlag2 + ',0 ' + transitionPointCoords[0] + ',' + transitionPointCoords[1]);

            firstArc.setAttribute('class', 'progress-arc');
            secondArc.setAttribute('class', 'remaining-arc');
            if (options) {
                _.forEach(options, function (value, key) {
                    firstArc.setAttribute(key, value);
                    secondArc.setAttribute(key, value);
                });
            }
            svgContainer.appendChild(firstArc);
            svgContainer.appendChild(secondArc);

            $targetElement.find('.badge-image-wrapper').append(svgContainer);
        },

        /**
         * Creates circle around provided element with d3
         * and animates it
         *
         * @param {String} selector
         * @param {Number} [radius]
         * @param {Number} animationDuration of the animation in ms
         * @returns {jQuery.Deferred.promise}
         */
        animateLoadProgressCircle: function animateLoadProgressCircle(selector, radius, animationDuration) {
            var element = d3.selectAll(selector);
            $(selector).find('.circle-border').remove();
            if (!element.node()) {
                return;
            }

            return SvgRoundProgressBarDrawer._animateProgressCycle(element, {
                animationDuration: animationDuration,
                startAngle: 0,
                startValue: 0,
                endValue: 1,
                radius: radius || element.node().getBoundingClientRect().width / 2
            });
        },

        /**
         * Creates circle around provided element with d3
         * and animates it
         *
         * @param {String} selector
         * @param {Number} [radius]
         * @param {Number} [animationDuration] of the animation in ms
         * @returns {jQuery.Deferred.promise}
         */
        animateUnloadProgressCircle: function animateUnloadProgressCircle(selector, radius, animationDuration) {
            var element = d3.selectAll(selector);
            if (!element.node()) {
                return;
            }

            return SvgRoundProgressBarDrawer._animateProgressCycle(element, {
                animationDuration: animationDuration,
                startAngle: 0,
                startValue: 1,
                endValue: 0,
                radius: radius || element.node().getBoundingClientRect().width / 2
            });
        },

        /**
         * Creates progress bars and animates them according to passed params
         *
         * @param {d3.selection} elements
         * @param {Object} params
         * @param {Number} params.radius
         * @param {Number} params.animationDuration
         * @param {Number} params.startAngle
         * @param {Number} params.startValue
         * @param {Number} params.endValue
         *
         * @returns {jQuery.Deferred.promise}
         */
        _animateProgressCycle: function _animateProgressCycle(elements, params) {
            params.animationDuration = _.isNumber(params.animationDuration) ? params.animationDuration : this.defaults.ANIMATION_DURATION;

            // to organise chains of independent elements
            var $dfd = $.Deferred();
            _.delay(function () {
                $dfd.resolve();
            }, params.animationDuration);

            elements.each(function () {
                d3.select(this).call(_animateOneProgressBar);
            });

            function _animateOneProgressBar(element) {
                var endAngle = function endAngle(data) {
                    return 2 * Math.PI * data.value;
                };
                var arc = SvgRoundProgressBarDrawer._createArc(params.startAngle, endAngle, params.radius);
                var path = element.select('path');
                path = path.node() && path || SvgRoundProgressBarDrawer._createPath(element, { value: params.startValue });

                path.attr('d', arc).transition().duration(params.animationDuration).attrTween('d', SvgRoundProgressBarDrawer._arcTween(arc, { value: params.endValue }));
            }

            return $dfd.promise();
        },

        /**
         * Creates arc object for path
         *
         * @param {Number} startAngle
         * @param {Function|Number} endAngle
         * @param {Number} radius
         *
         * @returns {d3.svg.arc}
         * @private
         */
        _createArc: function _createArc(startAngle, endAngle, radius) {
            return d3.svg.arc().innerRadius(radius).outerRadius(radius).startAngle(startAngle).endAngle(endAngle);
        },

        /**
         * Creates <path> element in svg in arc
         *
         * @param {d3.selection} element
         * @param {Object} [datum]
         * @private
         */
        _createPath: function _createPath(element, datum) {
            var svg = element.append('svg').attr('xmlns', 'http://www.w3.org/2000/svg').attr('class', 'circle-border');

            var svgRect = svg.node().getBoundingClientRect();

            return svg.append('g').attr('transform', 'translate(' + svgRect.width / 2 + ', ' + svgRect.height / 2 + ')').append('path').attr('class', 'arc').datum(datum);
        },

        /**
         * arcTween function returns function that is used
         * for the visualisation of one step of animation
         *
         * @param {Function} arc
         * @param {Object} datumB
         * @returns {Function}
         * @private
         */
        _arcTween: function _arcTween(arc, datumB) {
            return function (datumA) {
                var interpolate = d3.interpolate(datumA, datumB);

                for (var key in datumB) {
                    if (datumB.hasOwnProperty(key)) {
                        datumA[key] = datumB[key];
                    }
                }

                return function (t) {
                    return arc(interpolate(t));
                };
            };
        }
    };

    return SvgRoundProgressBarDrawer;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js")))

/***/ }),

/***/ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/util/Utils.js":
/*!*************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/portal/core/util/Utils.js ***!
  \*************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
/* WEBPACK VAR INJECTION */(function($, jQuery) {var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
/**
 * @class Utils
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {
    /**
     * Resolutions of screens
     * @type {exports}
     */
    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var resolutions = __webpack_require__(/*! com/epam/heroes/portal/core/resolutions */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/resolutions.js");

    __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js");
    __webpack_require__(/*! modernizr */ "./src/main/resources/static-site/js/libs/vendor/modernizr/modernizr.js");

    var Modernizr = window.Modernizr || {}; //Just for testing

    /**
     * Object with handful static functions
     * that are used in different not connected parts of application
     */
    var Utils = {

        /**
         * Retrieves query parameter value by its name.
         *
         * @static
         * @param {String} name
         * @returns {string|undefined}
         */
        getQueryParam: function getQueryParam(name) {
            name = name.replace(/[\[]/, '\\\[').replace(/[\]]/, '\\\]');
            var regexS = '[\\?&]' + name + '=([^&#]*)',
                regex = new RegExp(regexS),
                results = regex.exec(window.location.search);

            if (results) {
                return decodeURIComponent(results[1].replace(/\+/g, ' '));
            }
        },

        /**
         * Defines if resolution of current screen is suitable for tablet
         *
         * @static
         * @returns {Boolean}
         */
        isTouchDevice: function isTouchDevice() {
            return Modernizr.touch || Modernizr.mq('(max-width:' + resolutions.tablet + 'px)');
        },

        /**
         * Defines if resolution of current screen is suitable for mobile
         *
         * @static
         * @returns {Boolean}
         */
        isMobile: function isMobile() {
            return Modernizr.mq('(max-width:' + resolutions.mobile + 'px)');
        },

        /**
         * Defines if resolution of current screen is suitable for mobile
         *
         * @static
         * @returns {Boolean}
         */
        isTablet: function isTablet() {
            return !Utils.isMobile() && Modernizr.mq('(max-width:' + resolutions.tablet + 'px)');
        },

        /**
         * Analyzes screen resolution and depending on its value
         * changes image url to url with higher one resolution
         * and other vise
         *
         * @static
         * @param {String} url
         * @returns {String}
         */
        getBadgeResolution: function getBadgeResolution(url) {
            var highResolutionPostfix = '/original';
            var devicePixelRatio = window.devicePixelRatio;
            var badgeImageUrl = url;
            var isResolutionHigh = Math.round(devicePixelRatio) >= 2;
            var isImageOriginal = badgeImageUrl.indexOf(highResolutionPostfix) > 0;

            if (isResolutionHigh && !isImageOriginal) {
                return badgeImageUrl + highResolutionPostfix;
            }

            if (!isResolutionHigh && isImageOriginal) {
                return badgeImageUrl.replace(highResolutionPostfix, '');
            }

            return badgeImageUrl;
        },

        /**
         * Returns location with excluded office data
         *
         * @static
         * @param {String} location
         * @returns {string}
         */
        getLocationWithoutOffice: function getLocationWithoutOffice(location) {
            var separator = ',';
            return location ? location.split(separator).slice(0, 3).join(separator) : '';
        },

        /**
         * Set exact width to badges' and badgeSets' containers for center alignment
         *
         * @static
         * @param {jQuery} $sections
         * @param {jQuery} $element
         * @return {Number}
         */
        setBadgeSectionsWidth: function setBadgeSectionsWidth($sections, $element) {
            var marginRight = this.extractStyleProperty($element, 'margin-right');
            var marginLeft = this.extractStyleProperty($element, 'margin-left');
            var elementWidth = $element.outerWidth() + marginRight + marginLeft;
            var containerWidth = Math.floor($sections.parent().width() / elementWidth) * elementWidth;

            $sections.css({ width: containerWidth });
            return containerWidth;
        },

        /**
         * Animate number counter from zero to value specified in html
         *
         * @static
         * @param {jQuery} $element
         */
        animateNumbersCounting: function animateNumbersCounting($element) {
            $element.each(function () {
                $(this).animate({
                    counter: $(this).data('number')
                }, {
                    duration: 1500,
                    easing: 'swing',
                    step: function step(now) {
                        $(this).text(Math.ceil(now));
                    }
                });
            });
        },

        /**
         * Extracts css-property number value
         *
         * @static
         * @param {jQuery} $element
         * @param {String} propertyName
         * @returns {Number}
         */
        extractStyleProperty: function extractStyleProperty($element, propertyName) {
            var propertyValue = $element.css(propertyName);

            if (!propertyValue) {
                return 0;
            }

            _.find(['px', 'em', '%'], function (measureStr) {
                if (propertyValue.indexOf(measureStr) >= 0) {
                    propertyValue = +propertyValue.replace(measureStr, '');
                    return true;
                }
            });

            return propertyValue;
        },

        /**
         * Cuts the string to be less than maxSymbols,
         * and doesn't leave the last word there if it's cut
         *
         * @static
         * @param {String} text
         * @param {Number} maxSymbols
         * @param {Number} endOfString - postfix to set at the end of string (exp '...')
         * @returns {String}
         */
        cutExtraLongString: function cutExtraLongString(text, maxSymbols, endOfString, lastWordMaxLength) {
            var resultText = text;
            endOfString = endOfString || '';
            lastWordMaxLength = lastWordMaxLength || 10;

            if (resultText.length > maxSymbols) {
                resultText = resultText.slice(0, maxSymbols - endOfString.length);
                var wordsArray = resultText.split(' ');
                if (_.last(wordsArray).length < lastWordMaxLength) {
                    wordsArray.pop(); //to remove the cut fragment of a last word
                }
                resultText = wordsArray.join(' ') + endOfString;
            }
            return resultText;
        },

        /**
         * Finds ' or " symbols and escape them by html characters
         * @param {String} text
         * @returns {String}
         */
        escapeQuotes: function escapeQuotes(text) {
            return text.replace(/"/g, '&quot;').replace(/'/g, '&#39;');
        },

        /**
         * Chooses the position for popup depends of item's position in layout
         * Don't forget to use mixin _mixins.scss#item-tooltip
         *
         * @param {jQuery} $selectedItem
         * @param {jQuery} $parentElement The element which sets the borders
         */
        calculateTooltipPosition: function calculateTooltipPosition($selectedItem, $parentElement) {
            var SCSS_CLASSES = {
                DEFAULT: '.tooltip-position',
                TOP: 'tooltip-position-top',
                LEFT: 'tooltip-position-left',
                RIGHT: 'tooltip-position-right'
            };
            var _parentElement, parentScrollTop, parentScrollLeft;

            if ($parentElement) {
                _parentElement = $parentElement[0];
                parentScrollTop = _parentElement.scrollTop;
                parentScrollLeft = _parentElement.scrollLeft;
            } else {
                _parentElement = document.documentElement;
                parentScrollTop = window.pageYOffset;
                parentScrollLeft = window.pageXOffset;
            }

            var $popUp = $selectedItem.find(SCSS_CLASSES.DEFAULT);

            var itemPositionCoordinates = $selectedItem.position();
            var itemHeight = $selectedItem.outerHeight(true);
            var itemWidth = $selectedItem.outerWidth(true);
            var tooltipHeight = $popUp.outerHeight(true);
            var tooltipWidth = $popUp.outerWidth(true);
            var tooltipBottomBorderCoordinate = itemPositionCoordinates.top + itemHeight + tooltipHeight;
            var tooltipRightBorderCoordinate = itemPositionCoordinates.left + (tooltipWidth - itemWidth) * 0.5 + tooltipWidth;
            var tooltipLeftBorderCoordinate = itemPositionCoordinates.left - (tooltipWidth - itemWidth) * 0.5;

            /**
             * Compare the coordinates of tooltip's bottom border and bottom bound of client's screen
             */
            if (tooltipBottomBorderCoordinate >= _parentElement.clientHeight + parentScrollTop) {
                $popUp.addClass(SCSS_CLASSES.TOP);
            }

            /**
             * Compare the coordinates of tooltip's right border and right bound of client's screen
             */
            if (tooltipRightBorderCoordinate >= _parentElement.clientWidth + parentScrollLeft) {
                $popUp.addClass(SCSS_CLASSES.LEFT);
            }

            if (tooltipLeftBorderCoordinate <= (tooltipWidth - itemWidth) * 0.5) {
                $popUp.addClass(SCSS_CLASSES.RIGHT);
            }
        },

        /**
         * Checks if local storage is supported
         *
         * @returns {Boolean}
         */
        isLocalStorageAvailable: function isLocalStorageAvailable() {
            try {
                return 'localStorage' in window && window['localStorage'] !== null;
            } catch (e) {
                return false;
            }
        },

        /**
         * Changes arrow position with animation from one pointed element to another.
         *
         * overloading:
         * * positionArrow($arrow, $elementToPoint, $pointedElementBefore, getLeftOffset)
         * * positionArrow($arrow, $elementToPoint, $pointedElementBefore)
         * * positionArrow($arrow, $elementToPoint, getLeftOffset)
         * * positionArrow($arrow, $elementToPoint)
         *
         * @param {jQuery} $arrow
         * @param {jQuery} $elementToPoint
         * @param {jQuery} [$pointedElementBefore]
         * @param {Function} [getLeftOffset] - function($elementToPoint) that defines way of getting left offset
         * @returns {number} left offset
         */
        positionArrow: function positionArrow($arrow, $elementToPoint, $pointedElementBefore, getLeftOffset) {
            var defaultGetLeftOffset = function defaultGetLeftOffset($element) {
                return $element && $element.position() && $element.position().left + parseInt($element.css('padding-left')) || -1;
            };

            getLeftOffset = getLeftOffset || _.isFunction($pointedElementBefore) && $pointedElementBefore || defaultGetLeftOffset;

            $pointedElementBefore = $pointedElementBefore instanceof jQuery && $pointedElementBefore;

            if ($pointedElementBefore) {
                this._setArrowPointTo($arrow, $pointedElementBefore, getLeftOffset);
            }

            $arrow.css({ visibility: 'visible' });

            return this._setArrowPointTo($arrow, $elementToPoint, getLeftOffset, 200);
        },

        /**
         * Sets arrow to point to curtain element,
         * with animation if animation duration is above 0
         *
         * @param {jQuery} $arrow - arrow to set
         * @param {jQuery} $elementToPoint
         * @param {Function} [getLeftOffset] - function($elementToPoint) that defines way of getting left offset
         * @param {Number} [animationDuration]
         * @returns {number}
         * @private
         */
        _setArrowPointTo: function _setArrowPointTo($arrow, $elementToPoint, getLeftOffset, animationDuration) {
            var arrowWidth = $arrow.outerWidth();
            var elementToPointLeftOffset = getLeftOffset($elementToPoint);
            var shift = elementToPointLeftOffset >= 0 ? elementToPointLeftOffset + ($elementToPoint.width() - arrowWidth) / 2 : -arrowWidth;

            if (shift > 0) {
                $arrow.animate({ left: shift }, animationDuration || 0);
            } else {
                $arrow.css({ visibility: 'hidden' });
            }
            return shift;
        },

        /**
         * Checks is string email address
         *
         * @param {String} string to check
         * @returns {Boolean}
         */
        isEmail: function isEmail(string) {
            var emailRegex = new RegExp('^[A-Za-z0-9]+([A-Za-z0-9+\-\_\.]{1}[A-Za-z0-9]+)*@{1}' + '([A-Za-z0-9]+\.)+[A-Za-z0-9]+$', 'g');

            return emailRegex.test(string);
        }
    };

    /***    jQuery add-ons   ***/
    (function ($) {

        /**
         * Sets focus to the the end of the element
         */
        $.fn.endFocus = function () {
            var elem = this[0];
            var elemLen = elem.value.length;
            // For IE Only
            if (document.selection) {
                // Set focus
                elem.focus();
                // Use IE Ranges
                var oSel = document.selection.createRange();
                // Reset position to 0 & then set at end
                oSel.moveStart('character', -elemLen);
                oSel.moveStart('character', elemLen);
                oSel.moveEnd('character', 0);
                oSel.select();
            } else if (elem.selectionStart || elem.selectionStart === 0) {
                // Firefox/Chrome
                elem.selectionStart = elemLen;
                elem.selectionEnd = elemLen;
                elem.focus();
            } // if
        };

        /**
         * Auto-growing textareas; technique ripped from Facebook
         *
         *
         * http://github.com/jaz303/jquery-grab-bag/tree/master/javascripts/jquery.autogrow-textarea.js
         */
        $.fn.autogrow = function (options) {
            return this.filter('textarea').each(function () {
                var self = this;
                var $self = $(self);
                var minHeight = $self.height();
                var noFlickerPad = $self.hasClass('autogrow-short') ? 0 : parseInt($self.css('lineHeight')) || 0;
                var settings = $.extend({
                    preGrowCallback: null,
                    postGrowCallback: null
                }, options);

                var shadow = $('<div></div>').css({
                    position: 'absolute',
                    top: -10000,
                    left: -10000,
                    width: $self.width(),
                    fontSize: $self.css('fontSize'),
                    fontFamily: $self.css('fontFamily'),
                    fontWeight: $self.css('fontWeight'),
                    lineHeight: $self.css('lineHeight'),
                    resize: 'none',
                    'word-wrap': 'break-word'
                }).appendTo(document.body);

                var update = function update(event) {
                    var times = function times(string, number) {
                        for (var i = 0, r = ''; i < number; i++) {
                            r += string;
                        }return r;
                    };

                    var val = self.value.replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;').replace(/\n$/, '<br/>&nbsp;').replace(/\n/g, '<br/>').replace(/ {2,}/g, function (space) {
                        return times('&nbsp;', space.length - 1) + ' ';
                    });

                    // Did enter get pressed?  Resize in this keydown event so that the flicker doesn't occur.
                    if (event && event.data && event.data.event === 'keydown' && event.keyCode === 13) {
                        val += '<br />';
                    }

                    shadow.css('width', $self.width());
                    shadow.html(val + (noFlickerPad === 0 ? '...' : '')); // Append '...' to resize pre-emptively.

                    var newHeight = Math.max(shadow.height() + noFlickerPad, minHeight);
                    if (settings.preGrowCallback != null) {
                        newHeight = settings.preGrowCallback($self, shadow, newHeight, minHeight);
                    }

                    $self.height(newHeight);

                    if (settings.postGrowCallback != null) {
                        settings.postGrowCallback($self);
                    }
                };

                $self.change(update).keyup(update).keydown({ event: 'keydown' }, update);
                $(window).resize(update);

                update();
            });
        };

        $.fn.autoResizeInput = function () {

            function resizeInput() {
                $(this).attr('size', $(this).val().length + 1);
                // + 1 for more space, otherwise name is cut
            }

            $(this).each(resizeInput).keyup(resizeInput);
        };
    })(jQuery);

    return Utils;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js"), __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js")))

/***/ }),

