!(__WEBPACK_AMD_DEFINE_ARRAY__ = [__webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js"), __webpack_require__(/*! com/epam/heroes/portal/core/App */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/App.js"), __webpack_require__(/*! com/epam/heroes/portal/core/view/BaseView */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/BaseView.js"), __webpack_require__(/*! com/epam/heroes/portal/localStorageWrapper */ "./src/main/resources/static-site/js/com/epam/heroes/portal/localStorageWrapper.js"), __webpack_require__(/*! FeatureConfig */ "./src/main/resources/static-site/featuresConfig.js"), __webpack_require__(/*! template/com/epam/heroes/core/Login.soy */ "./src/main/soy/com/epam/heroes/core/Login.soy")], __WEBPACK_AMD_DEFINE_RESULT__ = (function (config, App, BaseView, localStorageWrapper, featuresConfig) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");

    var md5 = __webpack_require__(/*! md5 */ "./node_modules/md5/md5.js");

    var LoginView = BaseView.extend({
        title: 'Heroes - Log In',

        template: com.epam.heroes.template.core.Login,

        events: {
            'click #login-submit': 'login'
        },

        initialize: function initialize() {
            BaseView.prototype.initialize.apply(this, arguments);
            _.bindAll(this, 'successLoginCallback', 'errorLoginCallback');
        },

        afterRender: function afterRender() {
            $('.main-content').css('padding-top', 0).css('overflow', 'hidden');
            $('#global-ask-button').css('display', 'none');
            if (featuresConfig.featureEpamLogoEnabled) {
                $('.login-wrapper').addClass("epam-login-wrapper");
            }
        },

        hideCredsForm: function hideCredsForm() {
            return (window.location.host === 'heroes.epam.com' || window.location.host === 'heroes-stag.epm-ppa.projects.epam.com') && !featuresConfig.customersHasLoginByPassword;
        },

        getModel: function getModel() {
            return {
                showCredsForm: !this.hideCredsForm(),
                showEpamLogin: featuresConfig.featureEpamLoginEnabled,
                showKeycloackLogin: featuresConfig.featureKeycloackLoginEnabled
            };
        },

        login: function login(e) {
            e.preventDefault();

            if (!this.hideCredsForm()) {
                var creds = this.form2json('.creds-login');
                creds.password = md5(creds.password);
                $.ajax({
                    url: config.api.login.url,
                    type: 'POST',
                    data: JSON.stringify(creds),
                    contentType: 'application/json; charset=utf-8',
                    dataType: 'json',
                    success: this.successLoginCallback,
                    error: this.errorLoginCallback
                });
            } else {
                location.reload();
            }

            return false;
        },

        successLoginCallback: function successLoginCallback(response) {
            this.router.navigate('/', {
                trigger: true
            });

            var userId = response.content.id;
            localStorageWrapper.set('userId', userId);

            this.model.set(response.content);
            $('.main-content').css('padding-top', '').css('overflow', '');
            $('#global-ask-button').css('display', 'block');
            this.getRouter().navigateApp();
        },

        errorLoginCallback: function errorLoginCallback(response) {
            var errors = response.responseJSON.errors,
                $credInputs = this.$('.login, .password');
            if (errors && errors.length) {
                var errorMsg = errors[0].message;
                this.notify({
                    body: errorMsg,
                    cssClass: 'error',
                    isAuthenticated: false
                });
                $credInputs.addClass('error');
            } else {
                $credInputs.removeClass('error');
            }
        },

        getTitle: function getTitle() {
            return this.title;
        }
    });

    return LoginView;
}).apply(exports, __WEBPACK_AMD_DEFINE_ARRAY__),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__(/*! jquery */ "./src/main/resources/static-site/js/libs/vendor/jquery/dist/jquery.js")))

