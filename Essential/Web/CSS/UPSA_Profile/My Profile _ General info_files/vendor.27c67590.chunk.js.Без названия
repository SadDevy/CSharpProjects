/***/ "./src/main/resources/static-site/js/com/epam/heroes/portal/localStorageWrapper.js":
/*!*****************************************************************************************!*\
  !*** ./src/main/resources/static-site/js/com/epam/heroes/portal/localStorageWrapper.js ***!
  \*****************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
var __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_RESULT__ = (function (require) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var Utils = __webpack_require__(/*! com/epam/heroes/portal/core/util/Utils */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/util/Utils.js");

    var localStorageWrapper = {

        getUserId: function getUserId() {
            return localStorage.userId;
        },

        getTeamDashboardUsers: function getTeamDashboardUsers() {
            return JSON.parse(localStorage.teamDashboardUsers);
        },

        getCurrentUser: function getCurrentUser() {
            return this.getTeamDashboardUsers()[this.getUserId()];
        },

        set: function set(field, item) {
            if (Utils.isLocalStorageAvailable()) {
                localStorage.setItem(field, item);
            }
        },

        setNewDashboardOption: function setNewDashboardOption(value) {
            this.set('newDashboard', value);
        },

        getNewDashboardOption: function getNewDashboardOption() {
            if (Utils.isLocalStorageAvailable()) {
                return localStorage.newDashboard;
            }
        },

        setTeamDashboardUserOption: function setTeamDashboardUserOption(option, value) {
            if (Utils.isLocalStorageAvailable()) {
                var teamDashboardUsers = localStorage.teamDashboardUsers ? this.getTeamDashboardUsers() : {};
                var currentUser = teamDashboardUsers[this.getUserId()];
                var newObject = {};
                newObject[option] = value;

                if (!currentUser) {
                    currentUser = {};
                }

                teamDashboardUsers[this.getUserId()] = _.extend(currentUser, newObject);
                this.set('teamDashboardUsers', JSON.stringify(teamDashboardUsers));
            }
        },

        getTeamDashboardData: function getTeamDashboardData(userId) {
            if (Utils.isLocalStorageAvailable()) {
                if (!localStorage.teamDashboardUsers) {
                    this.set('teamDashboardUsers', JSON.stringify({}));
                }
                this.set('userId', userId);
            }
        },

        checkItemByUserId: function checkItemByUserId(item) {
            return Utils.isLocalStorageAvailable() && localStorage.teamDashboardUsers && this.getCurrentUser() && this.getCurrentUser()[item];
        },

        setUserProfileFiltersOption: function setUserProfileFiltersOption(option, value, currentEmployeeId) {
            //TODO-Vika: change object structure
            if (Utils.isLocalStorageAvailable()) {
                var userProfileFilters = localStorage.userProfileFilters ? this.getUserProfileFilters() : {};
                var currentUser = userProfileFilters[this.getUserId()] || {};
                var employeeObject = currentUser[currentEmployeeId] || {};
                var optionObject = {};

                optionObject[option] = value;
                currentUser[currentEmployeeId] = _.extend(employeeObject, optionObject);

                userProfileFilters[this.getUserId()] = _.extend(currentUser, currentUser[currentEmployeeId]);
                this.set('userProfileFilters', JSON.stringify(userProfileFilters));
            }
        },

        getUserProfileFilters: function getUserProfileFilters() {
            return JSON.parse(localStorage.userProfileFilters);
        },

        checkProfileFiltersByUserId: function checkProfileFiltersByUserId(item, currentEmployeeId) {
            return Utils.isLocalStorageAvailable() && localStorage.userProfileFilters && this.getUserProfileFilters()[this.getUserId()] && this.getUserProfileFilters()[this.getUserId()][currentEmployeeId] && this.getUserProfileFilters()[this.getUserId()][currentEmployeeId][item];
        },

        removeUserProfileFiltersOption: function removeUserProfileFiltersOption(currentEmployeeId) {
            if (Utils.isLocalStorageAvailable() && localStorage.userProfileFilters && this.getUserProfileFilters()[this.getUserId()] && this.getUserProfileFilters()[this.getUserId()][currentEmployeeId]) {
                var userProfileFilters = this.getUserProfileFilters();
                var currentUserOptions = userProfileFilters[this.getUserId()];
                delete currentUserOptions[currentEmployeeId];

                userProfileFilters[this.getUserId()] = currentUserOptions;
                this.set('userProfileFilters', JSON.stringify(userProfileFilters));
            }
        },

        setNewMovieOption: function setNewMovieOption(value) {
            this.set('newMovie', value);
        },

        getNewMovieOption: function getNewMovieOption() {
            if (Utils.isLocalStorageAvailable()) {
                return localStorage.newMovie;
            }
        }
    };

    return localStorageWrapper;
}).call(exports, __webpack_require__, exports, module),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));

