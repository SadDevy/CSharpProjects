    var inactive__soy69 = opt_data.item.inactive;
    var plainName__soy70 = opt_data.item.plainName;
    output += '<li class="' + ((inactive__soy69) ? ' search-item-inactive ' : '') + '" data-employee-id="' + soy.$$escapeHtml(opt_data.item.id) + '"><a href="#">' + com.epam.heroes.widget.givebadge.template.ItemPhoto({photo: opt_data.item.photo, plainName: opt_data.item.plainName}) + '<span class="employee item-description"><span class="item-name">' + ((firstName__soy66 && lastName__soy67) ? '<span>' + soy.$$filterNoAutoescape(firstName__soy66) + '</span> <span>' + soy.$$filterNoAutoescape(lastName__soy67) + '</span>' : '<span>' + soy.$$filterNoAutoescape(name__soy68) + '</span>') + '</span>' + ((opt_data.item.position) ? '<span class="position small-text">' + soy.$$escapeHtml(opt_data.item.position) + '</span>' : '') + '</span></a></li>';
  }
  return output;
};
if (goog.DEBUG) {
  com.epam.heroes.widget.givebadge.template.EmployeeItem.soyTemplateName = 'com.epam.heroes.widget.givebadge.template.EmployeeItem';
}


com.epam.heroes.widget.givebadge.template.GroupItem = function(opt_data, opt_ignored) {
  var output = '';
  var name__soy104 = opt_data.item.name;
  var plainName__soy105 = opt_data.item.plainName;
  output += '<li data-group-id="' + soy.$$escapeHtml(opt_data.item.id) + '"><a href="#">' + com.epam.heroes.widget.givebadge.template.ItemPhoto({photo: opt_data.item.photo, plainName: opt_data.item.plainName}) + '<span class="group item-description"><span class="item-name">' + soy.$$filterNoAutoescape(name__soy104) + '</span><span class="status small-text">' + soy.$$escapeHtml(opt_data.item.description) + '</span></span></a></li>';
  return output;
};
if (goog.DEBUG) {
  com.epam.heroes.widget.givebadge.template.GroupItem.soyTemplateName = 'com.epam.heroes.widget.givebadge.template.GroupItem';
}


com.epam.heroes.widget.givebadge.template.ItemPhoto = function(opt_data, opt_ignored) {
  return '<i class="item-photo"><span><img src="' + soy.$$escapeHtml(opt_data.photo) + '" alt="' + soy.$$escapeHtml(opt_data.plainName) + '"></span></i>';
};
if (goog.DEBUG) {
  com.epam.heroes.widget.givebadge.template.ItemPhoto.soyTemplateName = 'com.epam.heroes.widget.givebadge.template.ItemPhoto';
}

