  \******************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
/* WEBPACK VAR INJECTION */(function(Backbone) {var __WEBPACK_AMD_DEFINE_ARRAY__, __WEBPACK_AMD_DEFINE_RESULT__;

/*
 * Copyright Â© 2021 EPAM Systems, Inc. All Rights Reserved. All information contained herein is, and remains the
 * property of EPAM Systems, Inc. and/or its suppliers and is protected by international intellectual
 * property law. Dissemination of this information or reproduction of this material is strictly forbidden,
 * unless prior written permission is obtained from EPAM Systems, Inc
 */
!(__WEBPACK_AMD_DEFINE_ARRAY__ = [__webpack_require__(/*! com/epam/heroes/core/config */ "./src/main/resources/static-site/js/com/epam/heroes/core/config.js"), __webpack_require__(/*! com/epam/heroes/portal/core/util/news/backgrounds */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/util/news/backgrounds.js"), __webpack_require__(/*! com/epam/heroes/portal/core/view/Confirmation */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/Confirmation.js"), __webpack_require__(/*! com/epam/heroes/portal/core/view/StackNotification */ "./src/main/resources/static-site/js/com/epam/heroes/portal/core/view/StackNotification.js")], __WEBPACK_AMD_DEFINE_RESULT__ = (function (config, backgrounds, Confirmation, StackNotification) {

    var _ = __webpack_require__(/*! underscore */ "./src/main/resources/static-site/js/libs/vendor/lodash/lodash.js");
    var featuresConfig = __webpack_require__(/*! FeatureConfig */ "./src/main/resources/static-site/featuresConfig.js");

    var defaults = {
        STACK_NOTIFICATION_TIMEOUT: 5000
    };

    var App = _.extend({
        confirm: function confirm(options) {
            new Confirmation({ model: options }).open();
        },

        notify: function notify(options) {
            if (this.currentNotificationTimer) {
                clearTimeout(this.currentNotificationTimer);
                this.currentNotificationTimer = null;
                this.notification.close(_.bind(function () {
                    this.showNotification(options);
                }, this));
            } else {
                this.showNotification(options);
            }
        },

        /**
         * @private
         * @param options
         */
        showNotification: function showNotification(options) {
            var isAuth = !(options.isAuthenticated === false);
            var isAdminTab = window.location.pathname.split('/')[1].toLowerCase() === 'admin';
            if (featuresConfig.featuresReactUuiEnabled && isAuth && !isAdminTab) {
                var showSnackbar = new CustomEvent("SHOW_SNACKBAR", {
                    detail: {
                        options: options
                    }
                });
                window.customEventHandler.add(showSnackbar);
            } else {
                this.notification = new StackNotification({ model: options }).open();
                var timeout = options.timeout ? options.timeout : defaults.STACK_NOTIFICATION_TIMEOUT;
                this.currentNotificationTimer = setTimeout(this.notification.close, timeout);
            }
        },

        /**
         * Function that returns url of background for news-item with provided index
         * or random background otherwise
         *
         * If some theme is set, function returns background according to that theme
         *
         * All backgrounds for theme are situated in "images/%theme%/backgrounds" folder
         * and only default are in "images/backgrounds"
         *
         * @param {Number} [index]
         * @returns {String}
         */
        getBadgeEventBackground: function getBadgeEventBackground(theme, index) {
            var defaultTheme = config.defaultTheme;
            var currentTheme;

            //If there is a special badge theme - make it currentTheme, else - currentTheme is default value
            currentTheme = theme ? theme.toLowerCase() : config.currentTheme;

            var themeBackgrounds = backgrounds[currentTheme];
            var backgroundsUrl = config.portal.news.backgrounds.url;
            var folderSeparator = '/',
                lastFolder,
                folders;

            if (!themeBackgrounds) {
                // then take default theme images
                themeBackgrounds = backgrounds[defaultTheme];
            } else if (currentTheme !== defaultTheme) {
                // Define backgrounds URL according to theme
                // if theme is not default
                folders = backgroundsUrl.split(folderSeparator);
                lastFolder = folders[folders.length - 1] || folders[folders.length - 2];
                backgroundsUrl = backgroundsUrl.replace(lastFolder, currentTheme + folderSeparator + lastFolder);
            }

            var i = index == null ? //i can be == 0
            Math.floor(Math.random() * themeBackgrounds.length) : index % themeBackgrounds.length;
            return backgroundsUrl + themeBackgrounds[i];
        }
    }, Backbone.Events);

    return App;
}).apply(exports, __WEBPACK_AMD_DEFINE_ARRAY__),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
