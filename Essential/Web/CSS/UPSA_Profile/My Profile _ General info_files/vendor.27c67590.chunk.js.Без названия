    var templates = com.epam.heroes.widget.givebadge.template;

    var MOBILE_CLIENT_WIDTH = '768';

    var PointsToAssign = _BaseView2.default.extend({

        template: templates.PointsToAssignField,

        initialize: function initialize() {
            var _this = this;

            if (this.model.get('programSettings')) {

                this.addChildView(new _PointsSliderView2.default({
                    name: 'PointsSlider',
                    model: this.model
                }), '.slider-container');

                this.addChildView(new _PointsChooserView2.default({
                    name: 'PointsChooser',
                    model: this.model
                }), '.points-chooser');

                this.listenTo(_App2.default, 'change:slider', function () {
                    _this.recalculateActivePointStep();
                    _this.validatePoints();
                });

                this.listenTo(this.model, 'change:isBudgetExceeded', this.validatePoints);

                $(window).on('resize', _.throttle(function () {
                    _this.render();
                }, 500));
            }
        },

        afterRender: function afterRender() {
            var _this2 = this;

            if (this.model.get('programSettings') && this.model.get('programSettings').pointsSteps) {

                $(window).on('load', $('.slider-item').each(function (index, el) {
                    _this2.childViews['PointsSlider'].positionDots(el);
                    _this2.childViews['PointsSlider'].paintActiveSliderBone(el);
                }));

                var chosenStep = this.model.get('programSettings').chosenPointStep;

                this.model.getSliders().obsoleteMode ? this.changePointStep(this.getIndexOfStep(chosenStep) || 0) : this.recalculateActivePointStep();

                this.validatePoints();
            }
        },

        getModel: function getModel() {
            return {
                isVisible: this.model.get('programSettings'),
                remainingPoints: this.model.getRemainigPoints(),
                isMobile: this.isMobile()
            };
        },

        isMobile: function isMobile() {
            return this.getClientWidth() < MOBILE_CLIENT_WIDTH;
        },

        getClientWidth: function getClientWidth() {
            return $(document).width();
        },

        recalculateActivePointStep: function recalculateActivePointStep() {
            var steps = this.model.getPointsSteps();
            var recalculatedPoints = _.reduce(this.model.getSliders(), function (sum, slider) {
                return sum += slider.actualPointsAmount || 0;
            }, 0);

            var step = this.findClosestValue(recalculatedPoints, steps);
            this.changePointStep(step.index);
            this.model.get('programSettings').chosenPointStep = step.value;
        },

        findClosestValue: function findClosestValue(actualValue, steps) {
            return _.reduceRight(steps, function (result, step, index) {
                if (Math.abs(step - actualValue) < Math.abs(result.value - actualValue)) {
                    result.index = index;
                    result.value = step;
                }
                return result;
            }, { value: steps[0], index: 0 });
        },

        validatePoints: function validatePoints() {
            var remainingBudget = this.model.getRemainigPoints();
            var activeStep = this.model.get('programSettings').chosenPointStep;

            if (remainingBudget < activeStep) {
                this.model.set({
                    isBudgetExceeded: true
                });

                $('.chooser-container').addClass('exceed-error');
            } else {
                this.model.set({
                    isBudgetExceeded: false
                });

                $('.chooser-container').removeClass('exceed-error');
            }
        },

        changePointStep: function changePointStep(index) {
            $('select.select-steps').val(index).trigger('change');
        },

        getIndexOfStep: function getIndexOfStep(value) {
            return _.indexOf(this.model.get('programSettings').pointsSteps, parseInt(value, 10));
        }
    });

    return PointsToAssign;
